package com.jzy.hio.entity;

import javax.persistence.Column;
import javax.persistence.Table;

import com.jzy.hio.base.entity.BaseEntity;

import lombok.Data;

/**
 * 团队人数字典
 * 
 * @author chc<br/>
 *         2018年4月25日
 */
@Table(name = "im_base_personnel_size")
@Data
public class ImBasePersonnelSize  extends BaseEntity{
	@Column(name = "size_content")
	private String sizeContent;	//规模描述：例：1-99,100-199
	
	@Column(name = "mix_num")
	private Integer mixNum;	//最小人数
	
	@Column(name = "max_num")
	private Integer maxNum;		//最大人数
}
