package com.jzy.hio.entity;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.jzy.hio.base.entity.BaseEntity;

/**
 * 
 * @author 
 *
 */
@Entity
@Table(name = "im_personal_secret")
public class PersonalSecret extends BaseEntity implements Serializable {
	
	private static final long serialVersionUID = 1L;
	
	/**
	 * 用户id
	 */
	@Column(length = 64)
	private String uid;
	/**
	 * 手机日历同步标志，0：否，1：是
	 */
	@Column(name = "mobile_flag", length = 11)
	private Integer mobileFlag;
	
	/**
	 * 日程消息提醒标志，0：否，1：是
	 */
	@Column(name = "remind_flag", length = 11)
	private Integer remindFlag;
	
	/**
	 * 是否允许查看企业0：否，1：是
	 */
	@Column(name = "company_flag", length = 11)
	private Integer companyFlag;
	
	/**
	 * 手机通讯录匹配0：否，1：是
	 */
	@Column(name = "phone_flag", length = 11)
	private Integer phoneFlag;
	
	/**
	 * 对全部好友共享手机号 0：否，1：是
	 */
	@Column(name = "friend_moible_flag", length = 11)
	private Integer friendMoibleFlag;
	
	/**
	 * 对同事隐藏手机号 0：否，1：是
	 */
	@Column(name = "work_moible_flag", length = 11)
	private Integer workMoibleFlag;
	
	@Column(name = "open_running",length = 11)
	private Integer openRunning; // 是否公开运动步数,0：否；1：是
	
	public String getUid() {
		return uid;
	}
	
	public void setUid(String uid) {
		this.uid = uid;
	}
	
	public Integer getMobileFlag() {
		return mobileFlag;
	}
	
	public void setMobileFlag(Integer mobileFlag) {
		this.mobileFlag = mobileFlag;
	}
	
	public Integer getRemindFlag() {
		return remindFlag;
	}
	
	public void setRemindFlag(Integer remindFlag) {
		this.remindFlag = remindFlag;
	}
	
	public Integer getCompanyFlag() {
		return companyFlag;
	}
	
	public void setCompanyFlag(Integer companyFlag) {
		this.companyFlag = companyFlag;
	}
	
	public Integer getPhoneFlag() {
		return phoneFlag;
	}
	
	public void setPhoneFlag(Integer phoneFlag) {
		this.phoneFlag = phoneFlag;
	}
	
	public Integer getFriendMoibleFlag() {
		return friendMoibleFlag;
	}
	
	public void setFriendMoibleFlag(Integer friendMoibleFlag) {
		this.friendMoibleFlag = friendMoibleFlag;
	}
	
	public Integer getWorkMoibleFlag() {
		return workMoibleFlag;
	}
	
	public void setWorkMoibleFlag(Integer workMoibleFlag) {
		this.workMoibleFlag = workMoibleFlag;
	}

	public Integer getOpenRunning() {
		return openRunning;
	}

	public void setOpenRunning(Integer openRunning) {
		this.openRunning = openRunning;
	}
	
	
}
