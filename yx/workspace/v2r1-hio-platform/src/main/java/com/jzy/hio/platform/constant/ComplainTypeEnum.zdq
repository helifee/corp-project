package com.jzy.hio.platform.constant;

/**
 * 投诉类型选项
 * 色情0,欺诈1,广告骚扰2,违法3,政治敏感4,侵权举报5,其他9
 * @author 
 *
 */
public enum ComplainTypeEnum {
	
	SALACITY(0, "色情"),
	CHEAT(1, "欺诈"),
	ADHARASS(2, "广告骚扰"),
	UNLAWFUL(3, "违法"),
	SENSITIVITY(4, "政治敏感"),
	TORT(5, "侵权举报"),
	OTHER(9, "其他");
	
	private int value;
	private String desc;
	
	private ComplainTypeEnum() {}
	
	private ComplainTypeEnum(int value, String desc) {
		this.value = value;
		this.desc = desc;
	}
	
	public int getValue() {
		return value;
	}
	public String getDesc() {
		return desc;
	}
	
	
}
