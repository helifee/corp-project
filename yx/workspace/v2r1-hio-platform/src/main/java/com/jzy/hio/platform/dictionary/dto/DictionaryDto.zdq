package com.jzy.hio.platform.dictionary.dto;

import javax.persistence.Column;

import com.jzy.hio.base.dto.BaseTreeDto;

/**
 * 字典
 * 
 * @author chc<br/>
 *         2018年3月31日
 */
public class DictionaryDto extends BaseTreeDto<DictionaryDto> {
	private String name;	//中文名
	private String parentName;	//父节点
	private Integer isRoot;	//是否根节点
	private Integer type;		//类型，1：提醒时间，2：行业，3：企业类型
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getParentName() {
		return parentName;
	}
	public void setParentName(String parentName) {
		this.parentName = parentName;
	}
	public Integer getIsRoot() {
		return isRoot;
	}
	public void setIsRoot(Integer isRoot) {
		this.isRoot = isRoot;
	}
	public Integer getType() {
		return type;
	}
	public void setType(Integer type) {
		this.type = type;
	}
	
}
