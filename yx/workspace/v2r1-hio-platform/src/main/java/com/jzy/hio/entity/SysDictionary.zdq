package com.jzy.hio.entity;

import javax.persistence.Column;
import javax.persistence.Table;

import com.jzy.hio.base.entity.BaseEntity;

/**
 * 字典
 * 
 * @author chc<br/>
 *         2018年3月31日
 */
@Table(name = "pt_sys_dictionary")
public class SysDictionary extends BaseEntity {
	private String name;	//中文名
	
	@Column(name = "parent_id")
	private String parentId;	//父节点
	
	@Column(name = "is_root")
	private Integer isRoot;	//是否根节点
	
	@Column(name = "type")
	private Integer type;		//类型，1：提醒时间，2：行业，3：企业类型
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getParentId() {
		return parentId;
	}
	public void setParentId(String parentId) {
		this.parentId = parentId;
	}
	public Integer getIsRoot() {
		return isRoot;
	}
	public void setIsRoot(Integer isRoot) {
		this.isRoot = isRoot;
	}
	public Integer getType() {
		return type;
	}
	public void setType(Integer type) {
		this.type = type;
	}
	
}
