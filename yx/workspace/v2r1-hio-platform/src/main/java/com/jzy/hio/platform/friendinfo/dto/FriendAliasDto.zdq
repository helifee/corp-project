package com.jzy.hio.platform.friendinfo.dto;

import com.fasterxml.jackson.annotation.JsonProperty;

import lombok.Data;
/**
 * 好友dto，返回前端
 * @author chc
 *
 */
@Data
public class FriendAliasDto {
	/**
	 * 返回给前端、转义一下名字  "id"是ios的关键字
	 */
	@JsonProperty("sid")
	private String id;
	/**
	 * 用户id
	 */
	private String uid;	
	/**
	 * 好友id
	 */
	private String fid;		
	/**
	 * 好友备注
	 */
	private String aliasName;
	/**
	 * 备注拼音全拼
	 */
	private String aliasNameFull;
	/**
	 * 备注拼音简拼
	 */
	private String aliasNameSimple;
	/**
	 * 好友名称
	 */
	private String fname; 
	/**
	 * 拼音全拼
	 */
	private String nameFull;
	/**
	 * 拼音简拼
	 */
	private String nameSimple;
	/**
	 * 是否共享手机号码，0：否，1：是
	 */
	private Integer sharePhone;	
	/**
	 * 是否被对方解除好友关系，0：否，1：是
	 */
	private Integer isDel;
	
	private String mobile;
	private Integer sex;
	private String resourceImgUrl;
	
}
