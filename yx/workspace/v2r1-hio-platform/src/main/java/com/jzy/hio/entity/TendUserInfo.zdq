package com.jzy.hio.entity;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.jzy.hio.base.entity.BaseEntity;

import lombok.Data;

@Entity
@Table(name = "im_tend_user")
@Data
public class TendUserInfo extends BaseEntity implements Serializable{
	private static final long serialVersionUID = 1L;
	/**
	 * 用户id
	 */
	@Column(length = 64)
	private String uid;	
	/**
	 * 租户id
	 */
	@Column(name = "tend_id" ,length = 64)	
	private String tendId;		
	
	/**
	 * 租户中用户表的id
	 */
	@Column(name = "user_id")
	private String userId;		
	
	/**
	 * 是否默认，0：否，1：是
	 */
	@Column(name = "default_flag")
	private Integer defaultFlag;	
	
	/**
	 * 是否外部联系人，0：否，1：是
	 */
	@Column(name = "outer_linkman")
	private Integer outerLinkman;		
	
	/**
	 * 用户手机号
	 */
	@Column(length = 11)
	private String mobile; 
	
	/**
	 * 企业名称
	 */
	@Column(name = "tend_name")
	private String tendName ; 
	
	/**
	 * 激活状态，1：激活，0：未激活
	 */
	@Column(name = "reserve1")
	private String reserve1;

	
}
