package com.jzy.hio.entity;

import com.jzy.hio.base.entity.BaseEntity;
import lombok.Data;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;
import java.io.Serializable;
import java.sql.Timestamp;


@Data
@Entity
@Table(name = "im_notice_msg_temp")
public class ImNoticeMsgTemp extends BaseEntity implements Serializable {

    /**
     * 消息内容
     */
    @Column(name = "msg")
    private String msg;

	/**
	 * 发送消息类型，text文本类型，cmd透传类型
	 */
	@Column(name = "send_msg_type")
	private String sendMsgType;
    /**
     * 接收用户id
     */
    @Column(name = "to_user_id")
    private String toUserId;

    /**
     * 接收用户名称
     */
    @Column(name = "to_user_name")
    private String toUserName;


    /**
     * 接收人手机号
     */
    @Column(name = "to_user_phone")
    private String toUserPhone;

    /**
     * 发送用户id
     */
    @Column(name = "from_user_id")
    private String fromUserId;

    /**
     * 发送用户名称
     */
    @Column(name = "from_user_name")
    private String fromUserName;

    /**
     * 租户id
     */
    @Column(name = "tend_id")
    private String tendId;

	/**
	 * 租户名称
	 */
	@Column(name = "tend_name")
	private String tendName;

    /**
     * 消息类型
	 * 0：通知消息类     msg
	 * 1：工作类消息     work
	 * 2：系统消息类     sys
     */
    @Column(name = "msg_type")
    private String msgType;

    /**
     * 应用编号
	 * 10001 待审
	 * 10002 待阅
	 * 10003考勤
	 * 10004公告
	 * 10005会议
	 * 10006任务
	 * 10007短信
	 * 10008站内信
     */
    @Column(name = "module_type")
    private String moduleType;

	/**
	 * 应用名称
	 * 10001 待审
	 * 10002 待阅
	 * 10003考勤
	 * 10004公告
	 * 10005会议
	 * 10006任务
	 * 10007短信
	 * 10008站内信
	 */
	@Column(name = "module_name")
    private String moduleName;

    /**
     * 消息发送时间
     */
    @Column(name = "send_date")
    private Timestamp sendDate;

    /**
     * 消息处理时间
     */
    @Column(name = "deal_date")
    private Timestamp dealDate;


    /**
     * 消息连接地址
     */
    @Column(name = "pc_url")
    private String pcUrl;

    /**
     * 移动端的URL
     */
    @Column(name = "mobible_url")
    private String mobibleUrl;

    /**
     * 移动端的参数JSON串
     */
    @Column(name = "mobible_param")
    private String mobibleParam;

    /**
     * 消息是否可以打开, 1-(true):可以打开，0-(false):不可以打开
     */
    @Column(name = "is_open")
    private Boolean isOpen;

	/**
	 * 是否允许端上打开 0不打开，1打开
	 */
	@Column(name = "open_status")
	private int openStatus;

    /**
     * 发送消息状态 1已发送，2，发送成功，3，发送失败
     */
    @Column(name = "status")
    private String status;

	/**
	 * 业务消息自定义扩展
	 */
	@Column(name = "ext")
	private String ext;

}