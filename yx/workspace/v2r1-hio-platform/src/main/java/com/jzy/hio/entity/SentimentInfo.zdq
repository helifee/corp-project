package com.jzy.hio.entity;

import java.io.Serializable;
import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.jzy.hio.base.entity.BaseEntity;

/**
 * 
 * @author 
 *
 */
@Entity
@Table(name = "im_sentiment_info")
public class SentimentInfo extends BaseEntity implements Serializable {
	
	private static final long serialVersionUID = 1L;
	
	/**
	 * 提交人id
	 */
	@Column(name = "send_id", length = 64)
	private String sendId;
	
	/**
	 * 被投诉人id
	 */
	@Column(name = "receiver_id", length = 64)
	private String receiverId;
	
	/**
	 * 舆情信息
	 */
	@Column(name = "content", length = 1024)
	private String content;
	
	/**
	 * 色情0,欺诈1,广告骚扰2,违法3,政治敏感4,侵权举报5,其他9
	 */
	@Column(name = "read_flag", length = 11)
	private Integer readFlag;
	
	/**
	 * 发生时间
	 */
	@Column(name = "happen_time")
	private Date happenTime;

	public String getSendId() {
		return sendId;
	}

	public void setSendId(String sendId) {
		this.sendId = sendId;
	}

	public String getReceiverId() {
		return receiverId;
	}

	public void setReceiverId(String receiverId) {
		this.receiverId = receiverId;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public Integer getReadFlag() {
		return readFlag;
	}

	public void setReadFlag(Integer readFlag) {
		this.readFlag = readFlag;
	}

	public Date getHappenTime() {
		return happenTime;
	}

	public void setHappenTime(Date happenTime) {
		this.happenTime = happenTime;
	}
	
	
}
