package com.jzy.hio.entity;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.jzy.hio.base.entity.BaseEntity;

import lombok.Data;

/**
 * 
 * @author 
 *
 */
@Entity
@Table(name = "im_friend_alias")
@Data
public class ImFriendAlias extends BaseEntity implements Serializable {
	
	private static final long serialVersionUID = 1L;
	
	@Column(name = "uid", length = 64)
	private String uid;
	@Column(name = "fid", length = 64)
	private String fid;
	@Column(name = "alias_name", length =32)
	private String aliasName;
	/**
	 * 备注拼音全拼
	 */
	@Column(name = "alias_name_full", length = 64)
	private String aliasNameFull;
	/**
	 * 备注拼音简拼
	 */
	@Column(name = "alias_name_simple", length = 64)
	private String aliasNameSimple;
	
}
