package com.jzy.hio.platform.constant;

/**
 * 删除标识
 * @author 
 *
 */
public enum DelflagEnum {
	TRUE(1, "已删除"),
	FALSE(0, "未删除");
	
	private int value;
	private String desc;
	
	private DelflagEnum(int value, String desc) {
		this.value = value;
		this.desc = desc;
	}
	
	public int getValue() {
		return value;
	}
	public String getDesc() {
		return desc;
	}
	
}
