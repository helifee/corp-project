<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<artifactId>hio-platform</artifactId>
	<version>2.0.1-SNAPSHOT</version>
	<parent>
		<groupId>com.jzy.hio</groupId>
		<artifactId>hio-parent</artifactId>
		<version>2.0.1-SNAPSHOT</version>
		<relativePath>../v2r1-hio-parent/pom.xml</relativePath>
	</parent>
	<properties>
		<skiptests>true</skiptests>
	</properties>
	<dependencies>

		<dependency>
			<groupId>com.jzy.hio</groupId>
			<artifactId>hio-base</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.jzy.hio</groupId>
			<artifactId>hio-api</artifactId>
			<version>${project.version}</version>
		</dependency>

		<dependency>
			<groupId>org.javassist</groupId>
			<artifactId>javassist</artifactId>
		</dependency>
		<dependency>
			<groupId>commons-beanutils</groupId>
			<artifactId>commons-beanutils</artifactId>
			<version>1.7.0</version>
		</dependency>

		<dependency>
			<groupId>commons-collections</groupId>
			<artifactId>commons-collections</artifactId>
			<version>3.1</version>
		</dependency>
		<dependency>
			<groupId>org.quartz-scheduler</groupId>
			<artifactId>quartz</artifactId>
			<version>2.2.2</version>
		</dependency>
		<!-- coffee add -->
		<dependency>
			<groupId>org.apache.poi</groupId>
			<artifactId>poi</artifactId>
			<version>3.8</version>
		</dependency>
		<dependency>
			<groupId>org.apache.poi</groupId>
			<artifactId>poi-ooxml</artifactId>
			<version>3.8</version>
		</dependency>
		<dependency>
			<groupId>org.projectlombok</groupId>
			<artifactId>lombok</artifactId>
			<version>1.16.20</version>
			<scope>provided</scope>
		</dependency>
		<!--环信IM rest-java-sdk-->
		<!--<dependency>-->
			<!--<groupId>com.easemob</groupId>-->
			<!--<artifactId>rest-java-sdk</artifactId>-->
			<!--<version>1.0.5</version>-->
		<!--</dependency>-->
		<dependency>
			<groupId>com.xinleju.cloud.platform</groupId>
			<artifactId>rest-java-sdk</artifactId>
			<version>0.0.1-SNAPSHOT</version>
		</dependency>
		<!-- 发短信 -->
		<dependency>
			<groupId>com.xinleju.cloud.platform</groupId>
			<artifactId>platform-third-part</artifactId>
			<version>0.0.1-SNAPSHOT</version>
		</dependency>
		<!--上传群头像-->
		<dependency>
			<groupId>com.aliyun.oss</groupId>
			<artifactId>aliyun-sdk-oss</artifactId>
			<version>2.8.2</version>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
				<configuration>
					<classifier>exec</classifier>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.20.1</version>
				<configuration>
					<!-- 不指定单元测试 -->
					<skipTests>true</skipTests>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>