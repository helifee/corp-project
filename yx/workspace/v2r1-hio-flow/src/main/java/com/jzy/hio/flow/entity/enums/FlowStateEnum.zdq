package com.jzy.hio.flow.entity.enums;

public enum FlowStateEnum {

	RUN(1,"审批中"), 
	
	SUCCESS(2,"完成"), 
	
	REJECT( 3,"驳回"),
	
	DRAFT(4,"草稿"),
	
	SKIP(5,"跳过"),
	
	RETURN(6,"退回"),
	
	REJECT_ALREADY_PROCESSED(7,"驳回已处理"),
	
	RETURN_ALREADY_PROCESSED(8,"退回已处理");
	

	Integer state;

	String name;

	private FlowStateEnum(Integer state, String name) {
		this.state = state;
		this.name = name;
	}

	public Integer getState() {
		return state;
	}

	public void setState(Integer state) {
		this.state = state;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
