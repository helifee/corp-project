package com.jzy.hio.flow.entity.enums;

public enum InstanceApproverState {

	NO_APPROVAL(0,"已到达未审批"), 
	
	ALREADY_APPROVAL(1,"已审批"), 

	WAIT_APPROVAL(2,"未到达等待审批"),
	
	REJECT(3,"驳回"),
	
	SKIP(4,"跳过"),

	RETURN(5,"退回");

	private Integer state;
	
	private String name;

	private InstanceApproverState(Integer state, String name) {
		this.state = state;
		this.name = name;
	}

	public Integer getState() {
		return state;
	}

	public void setState(Integer state) {
		this.state = state;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	
}
