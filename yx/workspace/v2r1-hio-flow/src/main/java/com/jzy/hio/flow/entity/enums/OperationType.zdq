package com.jzy.hio.flow.entity.enums;

public enum OperationType {

	REJECT_UPPER_LEVEL(1,"驳回上一级"), 
	
	REJECT_TO_THE_INITIATOR(2,"驳回到发起人"),
	
	RETURN_UPPER_LEVEL(3,"退回上一级"),
	
	RETURN_THE_INITIATOR(4,"退回到发起人");
	
 
	Integer state;

	String name;

	private OperationType(Integer state, String name) {
		this.state = state;
		this.name = name;
	}

	public Integer getState() {
		return state;
	}

	public void setState(Integer state) {
		this.state = state;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}


}
