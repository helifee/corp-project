package com.jzy.hio.flow.entity.enums;

public enum InstanceNodeState {

	NOT_THROUGH(0,"未通过当前环节"), 
	
	HAVE_ARRIV(1,"已到达当前环节,"), 

	PASS(2,"已通过当前环节,"), 
	
	REJECT( 3,"驳回"),
	
	AUTOMATIC_PASS(5,"审批人重复自动通过,"), 
	RETURN(6,"退回"), 
	SKIP(4,"跳过");


	
	Integer state;
	String name;

	private InstanceNodeState(Integer state, String name) {
		this.state = state;
		this.name = name;
	}

	public Integer getState() {
		return state;
	}

	public void setState(Integer state) {
		this.state = state;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
