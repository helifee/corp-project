package com.jzy.hio.flow.entity.enums;

/**
 * 流程审批人类型
 */
public enum  FlowApproverEnum {

	SUPERIORS(1, "直属上级"),

	DEPT_ROLE(2, "角色+部门"),

	USER(3, "用户"),

	CURRENT_DEPT_ROLE(4, "角色+本部门"),

	INITIATOR(5,"发起人");

	private FlowApproverEnum(Integer type, String desc) {
		this.type = type;
		this.desc = desc;
	}

	private Integer type;

	private String desc;

	public Integer getType() {
		return type;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}

	public void setType(Integer type) {
		this.type = type;
	}

}
