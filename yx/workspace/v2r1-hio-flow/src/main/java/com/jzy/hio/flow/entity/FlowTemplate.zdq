package com.jzy.hio.flow.entity;

import javax.persistence.Column;
import javax.persistence.Table;
import javax.persistence.Transient;

/**
 * 流程模板表
 */
@Table(name = "flow_template")
public class FlowTemplate extends FlowBaseEntity {

    /**
     * 流程模板名称
     */
    @Column(name = "name")
    private String name;

    /**
     * 描述
     */
    @Column(name = "description")
    private String description;

    /**
     * 是否启用(启用1,禁用2, 默认禁用)
     */
    @Column(name = "state")
    private Integer state;

    /**
     * 模板编码
     */
    @Column(name = "code")
    private String code;

    /**
     * 流程分类Id
     */
    @Column(name = "flow_category_id")
    private String flowCategoryId;

    /**
     * 环节内审批人是否重复(指流程内 1：不重复 2：重复)
     */
    @Column(name = "is_repeat")
    private Integer isRepeat;

    /**
     * 流程发起方式    (F：自由流 T：模板)
     */
    @Column(name = "free_or_template")
    private String freeOrTemplate;

    /**
     * 流程是否自动完结:1:自动完成0：不自动完结
     */
    @Column(name = "flow_auto_complate")
    private String flowAutoComplate;

    /**
     * 排序
     */
    @Column(name = "sort")
    private Double sort;

    /**
     * 发起标识(预置数据为0，新添加为1)
     */
    @Column(name = "start_sign")
    private Integer  startSign;

    public Integer getStartSign() {
        return startSign;
    }

    public void setStartSign(Integer startSign) {
        this.startSign = startSign;
    }

    public String getFlowAutoComplate() {
        return flowAutoComplate;
    }

    public void setFlowAutoComplate(String flowAutoComplate) {
        this.flowAutoComplate = flowAutoComplate;
    }

    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name == null ? null : name.trim();
    }

    public String getDescription() {
        return description;
    }

    public void setDescription(String description) {
        this.description = description == null ? null : description.trim();
    }

    public Integer getState() {
        return state;
    }

    public void setState(Integer state) {
        this.state = state;
    }


    public String getFlowCategoryId() {
        return flowCategoryId;
    }

    public void setFlowCategoryId(String flowCategoryId) {
        this.flowCategoryId = flowCategoryId == null ? null : flowCategoryId.trim();
    }

    public Integer getIsRepeat() {
        return isRepeat;
    }

    public void setIsRepeat(Integer isRepeat) {
        this.isRepeat = isRepeat;
    }

    public Double getSort() {
        return sort;
    }

    public void setSort(Double sort) {
        this.sort = sort;
    }

    public String getFreeOrTemplate() {
        return freeOrTemplate;
    }

    public void setFreeOrTemplate(String freeOrTemplate) {
        this.freeOrTemplate = freeOrTemplate;
    }
}