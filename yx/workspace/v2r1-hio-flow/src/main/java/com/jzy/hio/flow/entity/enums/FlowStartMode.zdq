package com.jzy.hio.flow.entity.enums;

/**
 * 流程发起方式
 */
public enum FlowStartMode {

	FREE("F", "自由流方式发起"),

	TEMPLATE("T", "模板方式发起");

	private FlowStartMode(String code, String desc) {
		this.code = code;
		this.desc = desc;
	}

	private String code;

	private String desc;

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}
}
