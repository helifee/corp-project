package com.jzy.hio.flow.entity.enums;

/**
 * 表单类型
 * 
 */
public enum FormType {

	FIXED_FORM(1, "固定表单"),

	CUSTOM_FORM(2, "自定义表单");

//	FREE_FORM(3, "自由流表单");

	private FormType(Integer type, String desc) {
		this.type = type;
		this.desc = desc;
	}

	private Integer type;

	private String desc;

	public Integer getType() {
		return type;
	}

	public void setType(Integer type) {
		this.type = type;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}

}
