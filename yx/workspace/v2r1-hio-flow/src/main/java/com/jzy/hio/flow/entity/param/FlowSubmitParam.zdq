package com.jzy.hio.flow.entity.param;

import java.util.List;

import com.jzy.hio.flow.entity.dto.FormItem;
import com.jzy.hio.flow.entity.dto.NodeAddedDto;

/**
 * 流程发起、草稿保存参数Bean
 * 
 * @author chaoyang
 *
 */
public class FlowSubmitParam {

	/**
	 * 草稿删除动作
	 */
	private boolean isRemove;

	/**
	 * 流程实例ID（如果有值说明草稿已经保存过）
	 */
	private String instanceId;

	/**
	 * 流程实例名称（页面输入）
	 */
	private String instanceName;

	/**
	 * 模板ID
	 */
	private String templateId;

	/**
	 * 业务单据ID
	 */
	private String businessId;
	
	/**
	 * 表单定义ID
	 */
	private String customFormId;

	/**
	 * 业务单据（key为表单元素name）
	 */
	private List<FormItem> businessData;
	
	/**
	 * 自定义表单数据Value（对流程不识别）
	 */
	private String valueData;

	/**
	 * 终端类型：ios android web
	 */
	private String terminalType;

	/**
	 * 实例抄送人集合
	 */
	private List<String> copyUserIds;

	/**
	 * 附件ID
	 */
	private String attachmentId;

	/**
	 * 关联项目ID
	 */
	private String projectId;

	/**
	 * 自由流审批环节列表
	 */
	private List<NodeAddedDto> nodeList;

	/**
	 * 审批同意 说明
	 */
	private String desc;
	
	/**
	 * 表单操作类型：new update, delete
	 */
	private String operateType;

	////////////////////////////////////// 流程内部处理用//////////////////////////////////////////////

	/**
	 * 流程是否自动完结:1:自动完成0：不自动完结
	 */
	private String flowAutoComplate;

	/**
	 * 流程实例状态
	 */
	private int instanceState;
	
	/**
	 * 实例发起人
	 * @return
	 */
	private String instanceStartPerson;

	/**
	 *流程组ID
	 */
	private String instanceGroupId;
	
	public List<String> getCopyUserIds() {
		return copyUserIds;
	}

	public void setCopyUserIds(List<String> copyUserIds) {
		this.copyUserIds = copyUserIds;
	}

	public String getInstanceGroupId() {
		return instanceGroupId;
	}

	public void setInstanceGroupId(String instanceGroupId) {
		this.instanceGroupId = instanceGroupId;
	}

	public String getInstanceStartPerson() {
		return instanceStartPerson;
	}

	public void setInstanceStartPerson(String instanceStartPerson) {
		this.instanceStartPerson = instanceStartPerson;
	}

	public boolean isRemove() {
		return isRemove;
	}

	public void setRemove(boolean isRemove) {
		this.isRemove = isRemove;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}

	public String getProjectId() {
		return projectId;
	}

	public void setProjectId(String projectId) {
		this.projectId = projectId;
	}

	public String getBusinessId() {
		return businessId;
	}

	public void setBusinessId(String businessId) {
		this.businessId = businessId;
	}

	public String getTemplateId() {
		return templateId;
	}

	public void setTemplateId(String templateId) {
		this.templateId = templateId;
	}

	public String getTerminalType() {
		return terminalType;
	}

	public void setTerminalType(String terminalType) {
		this.terminalType = terminalType;
	}

	public String getAttachmentId() {
		return attachmentId;
	}

	public void setAttachmentId(String attachmentId) {
		this.attachmentId = attachmentId;
	}

	public String getInstanceId() {
		return instanceId;
	}

	public void setInstanceId(String instanceId) {
		this.instanceId = instanceId;
	}

	public String getInstanceName() {
		return instanceName;
	}

	public void setInstanceName(String instanceName) {
		this.instanceName = instanceName;
	}

	public List<NodeAddedDto> getNodeList() {
		return nodeList;
	}

	public void setNodeList(List<NodeAddedDto> nodeList) {
		this.nodeList = nodeList;
	}

	public List<FormItem> getBusinessData() {
		return businessData;
	}

	public void setBusinessData(List<FormItem> businessData) {
		this.businessData = businessData;
	}

	public int getInstanceState() {
		return instanceState;
	}

	public void setInstanceState(int instanceState) {
		this.instanceState = instanceState;
	}

	public String getFlowAutoComplate() {
		return flowAutoComplate;
	}

	public void setFlowAutoComplate(String flowAutoComplate) {
		this.flowAutoComplate = flowAutoComplate;
	}

	public String getCustomFormId() {
		return customFormId;
	}

	public void setCustomFormId(String customFormId) {
		this.customFormId = customFormId;
	}

	public String getValueData() {
		return valueData;
	}

	public void setValueData(String valueData) {
		this.valueData = valueData;
	}

	@Override
	public String toString() {
		return "FlowSubmitParam [isRemove=" + isRemove + ", instanceId=" + instanceId + ", instanceName=" + instanceName + ", templateId=" + templateId + ", businessId="
				+ businessId + ", customFormId=" + customFormId + ", businessData=" + businessData + ", valueData=" + valueData + ", terminalType=" + terminalType
				+ ", copyUserIds=" + copyUserIds + ", attachmentId=" + attachmentId + ", projectId=" + projectId + ", nodeList=" + nodeList
				+ ", desc=" + desc + ", flowAutoComplate=" + flowAutoComplate + ", instanceState=" + instanceState + ", instanceStartPerson="
				+ instanceStartPerson + "]";
	}

	public String getOperateType() {
		return operateType;
	}

	public void setOperateType(String operateType) {
		this.operateType = operateType;
	}
}
