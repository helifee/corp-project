public enum FlowTemplateRoleEnum {
    DEPARTMENT(1,"部门"),
    ROLE(2,"角色"),
    USER(3,"用户"),
    ALLUSER(4,"所有人");


    Integer type;
    String typeName;

    private FlowTemplateRoleEnum(Integer type, String typeName) {
        this.type = type;
        this.typeName = typeName;
    }

    public Integer getType() {
        return type;
    }

    public void setType(Integer type) {
        this.type = type;
    }

    public String getTypeName() {
        return typeName;
    }

    public void setTypeName(String typeName) {
        this.typeName = typeName;
    }
}
