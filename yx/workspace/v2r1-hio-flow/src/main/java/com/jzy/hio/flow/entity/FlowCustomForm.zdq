package com.jzy.hio.flow.entity;

import javax.persistence.Column;
import javax.persistence.Table;

import org.hibernate.validator.constraints.NotBlank;

/**
 * 自定义表单
 * 
 * @author daoqi
 */
@Table(name = "flow_custom_form")
public class FlowCustomForm extends FlowBaseEntity {

	/**
	 * 编码
	 */
	@Column(name = "code")
	private String code;

	/**
	 * 名称
	 */
	@Column(name = "name")
	private String name;
	
	/**
	 * 1:自定义表单 0：第三方表单
	 */
	@Column(name = "type")
	private Integer type;

	/**
	 * 流程模板ID
	 */
	@Column(name = "flow_template_code")
	private String flowTemplateCode;

	/**
	 * 表单格式JSON
	 */
	@Column(name = "form_layout")
	@NotBlank
	private String layoutData;

	/**
	 * 表单属性JSON
	 */
	@Column(name = "form_properties")
	@NotBlank
	private String propertyData;

	/**
	 * 流程属性JSON
	 */
	@Column(name = "flow_properties")
	private String flowProperties;
	
	/**
	 * 状态
	 */
	@Column(name = "status")
	private Integer status;

	/**
	 * 表单数据保存URL
	 */
	@Column(name = "data_save_url")
	private String dataSaveUrl;
	
	/**
	 * 表单数据删除URL
	 */
	@Column(name = "data_delete_url")
	private String dataDeleteUrl;
	
	/**
	 * 表单状态更新URL
	 */
	@Column(name = "data_status_url")
	private String dataStatusUrl;
	
	/**
	 * 回调方法参数
	 */
	@Column(name = "call_back_param")
	private String callBackParam;

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public String getLayoutData() {
		return layoutData;
	}

	public void setLayoutData(String layoutData) {
		this.layoutData = layoutData;
	}

	public String getPropertyData() {
		return propertyData;
	}

	public void setPropertyData(String propertyData) {
		this.propertyData = propertyData;
	}

	public String getFlowTemplateCode() {
		return flowTemplateCode;
	}

	public void setFlowTemplateCode(String flowTemplateCode) {
		this.flowTemplateCode = flowTemplateCode;
	}

	public String getFlowProperties() {
		return flowProperties;
	}

	public void setFlowProperties(String flowProperties) {
		this.flowProperties = flowProperties;
	}

	public String getDataSaveUrl() {
		return dataSaveUrl;
	}

	public void setDataSaveUrl(String dataSaveUrl) {
		this.dataSaveUrl = dataSaveUrl;
	}

	public String getDataDeleteUrl() {
		return dataDeleteUrl;
	}

	public void setDataDeleteUrl(String dataDeleteUrl) {
		this.dataDeleteUrl = dataDeleteUrl;
	}

	public String getDataStatusUrl() {
		return dataStatusUrl;
	}

	public void setDataStatusUrl(String dataStatusUrl) {
		this.dataStatusUrl = dataStatusUrl;
	}

	public Integer getType() {
		return type;
	}

	public void setType(Integer type) {
		this.type = type;
	}

	public String getCallBackParam() {
		return callBackParam;
	}

	public void setCallBackParam(String callBackParam) {
		this.callBackParam = callBackParam;
	}

}
