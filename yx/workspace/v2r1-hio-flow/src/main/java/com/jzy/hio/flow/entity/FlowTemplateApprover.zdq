package com.jzy.hio.flow.entity;

import javax.persistence.Column;
import javax.persistence.Table;
import javax.persistence.Transient;

/**
 * 流程模板环节审批人
 */
@Table(name = "flow_template_approver")
public class FlowTemplateApprover extends FlowBaseEntity{

    /**
     *模板Id
     */
    @Column(name = "flow_template_id")
    private String flowTemplateId;

    /**
     *流程环节Id
     */
    @Column(name = "flow_ac_id")
    private String flowAcId;

    /**
     *审批人Id
     */
    @Column(name = "approverId")
    private String approverId;

    /**
     *审批人类型（1 直属上级，2 部门+角色，3 用户，4 角色+本部门，5  发起人）
     */
    @Column(name = "approval_type")
    private Integer approvalType;

    /**
     *审批类型名称
     */
    @Column(name = "approval_type_name")
    private String approvalTypeName;

    /**
     * 审批人类型（0-内部   1-外部联系人）
     */
    @Column(name = "user_type")
    private Integer userType;


    /**
     * 审批人角色
     */
    @Transient
    private String roleName;

    public Integer getUserType() {
        return userType;
    }

    public void setUserType(Integer userType) {
        this.userType = userType;
    }


    public String getRoleName() {
        return roleName;
    }

    public void setRoleName(String roleName) {
        this.roleName = roleName;
    }

    public String getFlowTemplateId() {
        return flowTemplateId;
    }

    public void setFlowTemplateId(String flowTemplateId) {
        this.flowTemplateId = flowTemplateId == null ? null : flowTemplateId.trim();
    }

    public String getFlowAcId() {
        return flowAcId;
    }

    public void setFlowAcId(String flowAcId) {
        this.flowAcId = flowAcId == null ? null : flowAcId.trim();
    }

    public String getApproverId() {
        return approverId;
    }

    public void setApproverId(String approverId) {
        this.approverId = approverId;
    }

    public Integer getApprovalType() {
        return approvalType;
    }

    public void setApprovalType(Integer approvalType) {
        this.approvalType = approvalType;
    }

    public String getApprovalTypeName() {
        return approvalTypeName;
    }

    public void setApprovalTypeName(String approvalTypeName) {
        this.approvalTypeName = approvalTypeName == null ? null : approvalTypeName.trim();
    }
}