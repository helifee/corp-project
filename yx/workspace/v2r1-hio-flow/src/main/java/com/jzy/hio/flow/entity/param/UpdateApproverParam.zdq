package com.jzy.hio.flow.entity.param;

import javax.validation.constraints.NotNull;

import org.hibernate.validator.constraints.NotBlank;

public class UpdateApproverParam {

	@NotBlank
	private String instanceId;
	
	private String approverRecordId;
	
	@NotBlank
	private String approverId;
	
	@NotBlank
	private String instanceAcId;
	
	@NotNull
	private Integer type;
	
	public String getInstanceAcId() {
		return instanceAcId;
	}

	public void setInstanceAcId(String instanceAcId) {
		this.instanceAcId = instanceAcId;
	}

	public Integer getType() {
		return type;
	}

	public void setType(Integer type) {
		this.type = type;
	}

	public String getInstanceId() {
		return instanceId;
	}

	public void setInstanceId(String instanceId) {
		this.instanceId = instanceId;
	}

	public String getApproverRecordId() {
		return approverRecordId;
	}

	public void setApproverRecordId(String approverRecordId) {
		this.approverRecordId = approverRecordId;
	}

	public String getApproverId() {
		return approverId;
	}

	public void setApproverId(String approverId) {
		this.approverId = approverId;
	}
	
}
