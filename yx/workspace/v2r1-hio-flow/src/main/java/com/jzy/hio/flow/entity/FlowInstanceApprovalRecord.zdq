package com.jzy.hio.flow.entity;

import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Table;

/**
 * 记录流程审批信息
 */
@Table(name ="flow_instance_approval_record")
public class FlowInstanceApprovalRecord  extends FlowBaseEntity{

    /**
     *流程实例id
     */
    @Column(name = "flow_instance_id")
    private String flowInstanceId;

    /**
     *流程环节Id
     */
    @Column(name = "flow_instance_ac_id")
    private String flowInstanceAcId;

    /**
     *状态（3种）0未审批，1已审批，2 加签未审批   3驳回  4跳过
     */
    @Column(name = "state")
    private Integer state;

    /**
     *审批人
     */
    @Column(name = "approval_user")
    private String approvalUser;
 
/*    *//**
     *职务ID
     *//*
    @Column(name = "postId")
    private String postId;*/

    /**
     *审批意见
     */
    @Column(name = "approval_description")
    private String approvalDescription;

    /**
     *审批时间
     */
    @Column(name = "approval_time")
    private Date approvalTime;
    
    /**
     * 是否是管理员
     */
    @Column(name = "adminId")
    private String adminId;

	public String getAdminId() {
		return adminId;
	}

	public void setAdminId(String adminId) {
		this.adminId = adminId;
	}

	public String getFlowInstanceId() {
        return flowInstanceId;
    }

    public void setFlowInstanceId(String flowInstanceId) {
        this.flowInstanceId = flowInstanceId == null ? null : flowInstanceId.trim();
    }

    public String getFlowInstanceAcId() {
		return flowInstanceAcId;
	}

	public void setFlowInstanceAcId(String flowInstanceAcId) {
		this.flowInstanceAcId = flowInstanceAcId;
	}

	public Integer getState() {
        return state;
    }

    public void setState(Integer state) {
        this.state = state;
    }

    public String getApprovalUser() {
        return approvalUser;
    }

    public void setApprovalUser(String approvalUser) {
        this.approvalUser = approvalUser == null ? null : approvalUser.trim();
    }

    public String getApprovalDescription() {
        return approvalDescription;
    }

    public void setApprovalDescription(String approvalDescription) {
        this.approvalDescription = approvalDescription == null ? null : approvalDescription.trim();
    }

    public Date getApprovalTime() {
        return approvalTime;
    }

    public void setApprovalTime(Date approvalTime) {
        this.approvalTime = approvalTime;
    }

}