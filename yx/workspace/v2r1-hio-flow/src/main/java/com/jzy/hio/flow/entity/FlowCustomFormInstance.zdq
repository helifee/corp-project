package com.jzy.hio.flow.entity;

import java.util.List;

import javax.persistence.Column;
import javax.persistence.Table;
import javax.persistence.Transient;

import com.jzy.hio.flow.entity.dto.FormItem;

/**
 * 自定义表单实例
 * 
 * @author daoqi
 *
 */
@Table(name = "flow_custom_form_instance")
public class FlowCustomFormInstance extends FlowBaseEntity {

	/**
	 * 表单值JSON
	 */
	@Column(name = "form_values")
	private String valueData;
	
	/**
	 * 表单值Map
	 */
	@Transient
	private	List<FormItem> businessData;
	
	/**
	 * 业务ID
	 */
	@Column(name = "businessId")
	private String businessId;

	/**
	 * 表单ID
	 */
	@Column(name = "custom_form_id")
	private String customFormId;

	/**
	 * 状态
	 */
	@Column(name = "status")
	private String status;

	/**
	 * 流程实例id
	 */
	@Column(name = "instance_id")
	private String instanceId;

	/**
	 * 标题
	 */
	@Column(name = "form_title")
	private String formTitle;
	
	/**
	 * 表单布局
	 */
	@Transient
	private	String propertyData;
	
	/**
	 * 表单属性
	 */
	@Transient
	private String layoutData;
	
	/**
	 * 标识数据库新增或更新
	 */
	@Transient
	private boolean saveOrUpdate;

	public String getCustomFormId() {
		return customFormId;
	}

	public void setCustomFormId(String customFormId) {
		this.customFormId = customFormId;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public String getInstanceId() {
		return instanceId;
	}

	public void setInstanceId(String instanceId) {
		this.instanceId = instanceId;
	}

	public String getFormTitle() {
		return formTitle;
	}

	public void setFormTitle(String formTitle) {
		this.formTitle = formTitle;
	}

	public String getPropertyData() {
		return propertyData;
	}

	public void setPropertyData(String propertyData) {
		this.propertyData = propertyData;
	}

	public String getLayoutData() {
		return layoutData;
	}

	public void setLayoutData(String layoutData) {
		this.layoutData = layoutData;
	}

	public String getValueData() {
		return valueData;
	}

	public void setValueData(String valueData) {
		this.valueData = valueData;
	}

	public List<FormItem> getBusinessData() {
		return businessData;
	}

	public void setBusinessData(List<FormItem> businessData) {
		this.businessData = businessData;
	}

	public boolean isSaveOrUpdate() {
		return saveOrUpdate;
	}

	public void setSaveOrUpdate(boolean saveOrUpdate) {
		this.saveOrUpdate = saveOrUpdate;
	}

	public String getBusinessId() {
		return businessId;
	}

	public void setBusinessId(String businessId) {
		this.businessId = businessId;
	}
}
