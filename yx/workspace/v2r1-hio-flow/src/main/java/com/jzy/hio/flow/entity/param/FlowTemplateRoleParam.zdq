package com.jzy.hio.flow.entity.param;

import com.jzy.hio.flow.entity.FlowTemplateRole;

import java.util.List;

public class FlowTemplateRoleParam {
    private List<FlowTemplateRole> personList;//人员列表
    private List<FlowTemplateRole> roleList;//角色列表
    private List<FlowTemplateRole> departmentList;//部门列表
    private List<FlowTemplateRole> allUser;//所有人
    private String flowTemplateId;
    private String code;
    private String sid;

    public List<FlowTemplateRole> getAllUser() {
        return allUser;
    }

    public void setAllUser(List<FlowTemplateRole> allUser) {
        this.allUser = allUser;
    }

    public String getSid() {
        return sid;
    }

    public void setSid(String sid) {
        this.sid = sid;
    }

    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }

    public String getFlowTemplateId() {
        return flowTemplateId;
    }

    public void setFlowTemplateId(String flowTemplateId) {
        this.flowTemplateId = flowTemplateId;
    }

    public List<FlowTemplateRole> getPersonList() {
        return personList;
    }

    public void setPersonList(List<FlowTemplateRole> personList) {
        this.personList = personList;
    }

    public List<FlowTemplateRole> getRoleList() {
        return roleList;
    }

    public void setRoleList(List<FlowTemplateRole> roleList) {
        this.roleList = roleList;
    }

    public List<FlowTemplateRole> getDepartmentList() {
        return departmentList;
    }

    public void setDepartmentList(List<FlowTemplateRole> departmentList) {
        this.departmentList = departmentList;
    }
}
