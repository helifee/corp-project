package com.jzy.hio.flow.entity;

import javax.persistence.Column;
import javax.persistence.Table;
import javax.persistence.Transient;

/**
 * 流程模板环节
 */
@Table(name = "flow_template_ac")
public class FlowTemplateAc extends FlowBaseEntity {

    /**
     * 环节名称
     */
    @Column(name = "name")
    private String name;


    /**
     * 审批类型(1, 审签    2, 会签)
     */
    @Column(name = "approval_type")
    private Integer approvalType;


    /**
     * 模板Id
     */
    @Column(name = "flow_template_id")
    private String flowTemplateId;

    /**
     * 环节类型（1, 开始),(2, 审批节点),(3, 结束),(5, 分支节点), (4, 聚合节点)）
     */
    @Column(name = "node_type")
    private Integer nodeType;

    /**
     * 环节顺序
     */
    @Column(name = "node_sort")
    private Integer nodeSort;

    /**
     * 节点ID
     */
    @Column(name = "node_id")
    private Integer nodeId;

    /**
     * x坐标
     */
    @Column(name = "x")
    private Integer x;

    /**
     * y坐标
     */
    @Column(name = "y")
    private Integer y;

    /**
     * width宽度
     */
    @Column(name = "width")
    private Integer width;

    /**
     * height高度
     */
    @Column(name = "height")
    private Integer height;

    /**
     * 逾期时间
     */
    @Column(name = "overdue_time")
    private Integer overdueTime;

    /**
     * 预期处理方式
     */
    @Column(name = "overdue_handle")
    private String overdueHandle;

    @Transient
    private boolean isTraversaled = false;

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name == null ? null : name.trim();
    }

    public Integer getApprovalType() {
        return approvalType;
    }

    public void setApprovalType(Integer approvalType) {
        this.approvalType = approvalType;
    }


    public String getFlowTemplateId() {
        return flowTemplateId;
    }

    public void setFlowTemplateId(String flowTemplateId) {
        this.flowTemplateId = flowTemplateId == null ? null : flowTemplateId.trim();
    }

    public Integer getNodeType() {
        return nodeType;
    }

    public void setNodeType(Integer nodeType) {
        this.nodeType = nodeType;
    }

    public Integer getNodeSort() {
        return nodeSort;
    }

    public void setNodeSort(Integer nodeSort) {
        this.nodeSort = nodeSort;
    }

    public Integer getNodeId() {
        return nodeId;
    }

    public void setNodeId(Integer nodeId) {
        this.nodeId = nodeId;
    }

    public Integer getX() {
        return x;
    }

    public void setX(Integer x) {
        this.x = x;
    }


    public Integer getWidth() {
        return width;
    }

    public void setWidth(Integer width) {
        this.width = width;
    }

    public Integer getHeight() {
        return height;
    }

    public void setHeight(Integer height) {
        this.height = height;
    }

    public Integer getOverdueTime() {
        return overdueTime;
    }

    public void setOverdueTime(Integer overdueTime) {
        this.overdueTime = overdueTime;
    }

    public String getOverdueHandle() {
        return overdueHandle;
    }

    public void setOverdueHandle(String overdueHandle) {
        this.overdueHandle = overdueHandle == null ? null : overdueHandle.trim();
    }

    public boolean isTraversaled() {
        return isTraversaled;
    }

    public void setTraversaled(boolean isTraversaled) {
        this.isTraversaled = isTraversaled;
    }


    public Integer getY() {
        return y;
    }

    public void setY(Integer y) {
        this.y = y;
    }
}