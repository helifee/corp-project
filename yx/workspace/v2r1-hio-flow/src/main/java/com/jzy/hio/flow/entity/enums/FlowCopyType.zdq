package com.jzy.hio.flow.entity.enums;

/**
 * 流程实例状态
 */
public enum FlowCopyType {

	 

	FORM_COPY(1, "表单上的抄送"),
	TEMPLATE_AC_COPY(2, "环节上的抄送"),
	BUTTON_COPY(3, "按钮上的抄送");
 
	private FlowCopyType(Integer type, String desc) {
		this.type = type;
		this.desc = desc;
	}

	private Integer type;

	private String desc;

	public Integer getType() {
		return type;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}

	public void setType(Integer type) {
		this.type = type;
	}

}
