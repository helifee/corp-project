package com.jzy.hio.flow.entity.enums;

public enum RoleButtonEnum {

	WITHDRAW("withdraw","撤回"),
	
	PASS("pass","通过"),
	
	REJECT("reject","驳回"),

	FRONT_ADD_LABEL("front_add_label","前加签"),

	AFTER_ADD_LABEL("after_add_label","后加签"),
	
	CC("cc","抄送"),
	
	FOLLOW("follow","关注"),
	
	UNFOLLOW("unfollow","取消关注"),

	DELETE_FORM("deleteForm","删除表单"),
	
	EDITOR_FORM("editorForm","编辑表单"),

	SUBMIT_FORM("submitForm","提交表单"),
	
	SAVE_FORM("saveForm","保存"),
	
	UNTREAD("untread","退回"),
	
	ADMIN("admin","是管理员"),

	SKIP("skip","跳过");
 
	String buttonCode;

	String buttonName;
	
	private RoleButtonEnum(String buttonCode, String buttonName) {
		this.buttonCode = buttonCode;
		this.buttonName = buttonName;
	}

	public String getButtonCode() {
		return buttonCode;
	}

	public void setButtonCode(String buttonCode) {
		this.buttonCode = buttonCode;
	}

	public String getButtonName() {
		return buttonName;
	}

	public void setButtonName(String buttonName) {
		this.buttonName = buttonName;
	}

}
