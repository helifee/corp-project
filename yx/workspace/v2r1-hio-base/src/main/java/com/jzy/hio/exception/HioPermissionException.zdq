package com.jzy.hio.exception;

/**
 * 权限不足时抛该异常
 * 
 * @author wt.coffee<br/>
 * 		2018年3月21日下午2:36:39
 */
public class HioPermissionException extends HioException {

	/**
	 * 
	 */
	private static final long serialVersionUID = -1965193913029593039L;

	public HioPermissionException(String exception) {
		super.exception = exception;
	}

	@Override
	public int getCode() {
		return PERMISSION;
	}
}
