package com.jzy.hio.exception;

/**
 * 删除数据库抛的异常
 * 
 * @author wt.coffee<br/>
 *         2018年3月9日上午10:23:52
 */
public class HioMybatisDeleteException extends HioException {

	/**
	 * 
	 */
	private static final long serialVersionUID = -6665206156793778485L;

	public HioMybatisDeleteException(Exception e) {
		super(e);
		super.exception = "删除数据失败";
	}

	public HioMybatisDeleteException(String exception) {
		super(exception);
		super.exception = exception;
	}
	
	@Override
	public int getCode() {
		return MYBATIS_DELETE_FAILED;
	}
	 
}
