package com.jzy.hio.exception;

/**
 * redis 初始化同步异常
 *
 * @author kimchou
 * @date 2018年3月31日下午4:43:15
 */
public class HioRedisSynInitException extends HioException {

    public HioRedisSynInitException() {
        super.exception = "redis初始化同步失败";
    }

    public HioRedisSynInitException(Exception e) {
        super(e);
        super.exception = "redis初始化同步失败";
    }

    @Override
    public int getCode() {
        return REDIS_SYNC_FAILED;
    }
}
