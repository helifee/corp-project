package com.jzy.hio.exception;

/**
 * session为空 或者缺少id等关键信息 需要抛出该异常<br>
 * 构造参数传入sessionMap
 * 
 * @author wt.coffee
 * @date 2018年4月19日下午4:30:18
 */
public class HioSessionException extends HioException {

	/**
	 * 
	 */
	private static final long serialVersionUID = 5199535898980140021L;

	/**
	 * 非法session
	 * 
	 * @param exception
	 */
	public HioSessionException(String exception) {
		super(exception);
	}

	@Override
	public int getCode() {
		return SESSION_INVALID;
	}

}
