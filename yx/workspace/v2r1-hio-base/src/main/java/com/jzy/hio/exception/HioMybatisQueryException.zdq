package com.jzy.hio.exception;

/**
 * 保存数据库抛的异常
 * 
 * @author wt.coffee<br/>
 *         2018年3月9日上午10:23:52
 */
public class HioMybatisQueryException extends HioException {

	/**
	 * 
	 */
	private static final long serialVersionUID = 2542963056788988738L;

	public HioMybatisQueryException(String exception) {
		super(exception);
	}

	public HioMybatisQueryException(Exception e) {
		super(e);
		super.exception = "查询数据失败";
	}

	@Override
	public int getCode() {
		return MYBATIS_QUERY_FAILED;
	}
}
