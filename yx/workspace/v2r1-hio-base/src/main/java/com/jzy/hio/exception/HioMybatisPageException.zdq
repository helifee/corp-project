package com.jzy.hio.exception;

public class HioMybatisPageException extends HioException {
	/**
	 * 
	 */
	private static final long serialVersionUID = 2542963056788988738L;

	public HioMybatisPageException() {
		super.exception = "查询数据失败";
	}

	public HioMybatisPageException(Exception e) {
		super(e);
		super.exception = "查询数据失败";
	}

	@Override
	public int getCode() {
		return MYBATIS_QUERY_FAILED;
	}
}
