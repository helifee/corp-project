package com.jzy.hio.exception;

/**
 * http请求重复提交
 * 
 * @author wt.coffee
 * @date 2018年5月14日下午7:49:23
 */
public class HioHttpRequestDuplicateException extends HioException {
	 

	/**
	 * 
	 */
	private static final long serialVersionUID = 3876044409925813078L;

	public HioHttpRequestDuplicateException(Exception e) {
		super(e);
		super.exception = "重复请求";
	}

	public HioHttpRequestDuplicateException(String exception) {
		super(exception);
		super.exception = exception;
	}

	@Override
	public int getCode() {
		return HTTP_REQUEST_DUPLICATE;
	}
}
