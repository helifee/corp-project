package com.jzy.hio.exception;

/**
 * redis 存储失败
 * 
 * @author wt.coffee
 * @date 2018年3月31日下午4:43:15
 */
public class HioRedisSetException extends HioException {
	/**
	 * 
	 */
	private static final long serialVersionUID = -2638956937940150220L;

	public HioRedisSetException(String message) {
		if (message == null) {
			super.exception = "redis存储失败";
		} else {
			super.exception = message;
		}
	}

	public HioRedisSetException(Exception e) {
		super(e);
		super.exception = "redis存储失败";
	}

	@Override
	public int getCode() {
		return REDIS_SET_FAILED;
	}
}
