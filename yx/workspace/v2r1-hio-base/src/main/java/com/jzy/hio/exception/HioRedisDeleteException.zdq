package com.jzy.hio.exception;

/**
 * redis 删除失败
 * @author wt.coffee
 * @date 2018年3月31日下午4:43:15
 */
public class HioRedisDeleteException extends HioException {
	/**
	 * 
	 */
	private static final long serialVersionUID = -5920988748332489323L;

	public HioRedisDeleteException() {
		super.exception = "redis删除失败";
	}

	public HioRedisDeleteException(Exception e) {
		super(e);
		super.exception = "redis删除失败";
	}

	@Override
	public int getCode() {
		return REDIS_DELETE_FAILED;
	}
}
