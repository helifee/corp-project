package com.jzy.hio.exception;

/**
 * 保存数据库抛的异常
 * 
 * @author wt.coffee<br/>
 *         2018年3月9日上午10:23:52
 */
public class HioMybatisInsertException extends HioException {

	/**
	 * 
	 */
	private static final long serialVersionUID = 2542963056788988738L;

	public HioMybatisInsertException() {
		super.exception = "插入数据失败";
	}

	public HioMybatisInsertException(Exception e) {
		super(e);
		super.exception = "插入数据失败";
	}

	public HioMybatisInsertException(String exception) {
		super.exception = exception;
	}
	
	public HioMybatisInsertException(Exception e, String exception) {
		super(e);
		super.exception = exception;
	}

	@Override
	public int getCode() {
		return MYBATIS_INSERT_FAILED;
	}

}
