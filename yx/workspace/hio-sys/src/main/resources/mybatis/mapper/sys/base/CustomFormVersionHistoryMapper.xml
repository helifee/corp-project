<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper   
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jzy.hio.sys.sys.base.mapper.CustomFormVersionHistoryDao">
	<!-- 新增 -->
	<insert id="save">
		${value}
	</insert>
	<!-- 修改根据Id -->
	<update id="update">
		${value}
	</update>
	<!-- 伪删除根据Id -->
	<update id="deletePseudoObjectById">
		${value}
	</update>
	<!-- 批量伪删除根据Id -->
	<update id="deletePseudoAllObjectByIds">
		${value}
	</update>
	<!-- 删除根据Id -->
	<delete id="deleteById">
		${value}
	</delete>
	<!-- 批量删除根据Id -->
	<delete id="deleteBatchByIds">
		${value}
	</delete>
	<!-- 获取单个对象，根据Id-->
	<select id="get" resultType="com.jzy.hio.sys.sys.base.entity.CustomFormVersionHistory">
		${value}
	</select>
	<!-- 获取列表根据Map查询 -->
	<select id="queryList" resultType="com.jzy.hio.sys.sys.base.entity.CustomFormVersionHistory">
		${value}
	</select>
	<!-- 获取分页根据Map查询 -->
	<select id="queryPageList" resultType="com.jzy.hio.sys.sys.base.entity.CustomFormVersionHistory">
		${value}
	</select>
	<!-- 获取总记录数 -->
	<select id="queryCount" resultType="java.lang.Integer">
		${value}
	</select>
	
	<!-- 获取分页根据Map查询 -->
	<select id="getPageSort" resultType="com.jzy.hio.sys.sys.base.entity.CustomFormVersionHistory" parameterType="java.util.HashMap">
		SELECT
			s.id AS id,
			s.name AS name,
			s.code AS code,
			s.custom_form_id AS customFormId,
			s.version_name AS versionName,
			s.version_create_date AS versionCreateDate,
			s.version_create_person_id AS versionCreatePersonId,
			s.version_create_person_name AS versionCreatePersonName
		FROM
			pt_sys_base_custom_form_version_history s
		WHERE
			s.custom_form_id = #{customFormId} and s.delflag='0'
		ORDER BY
			s.version_create_date DESC
			limit #{start},#{limit}
	</select>
	
	<!-- 获取总记录数 -->
	<select id="getPageSortCount" resultType="java.lang.Integer" parameterType="java.util.HashMap">
		select 
			count(1) 
			FROM
			pt_sys_base_custom_form_version_history s
		WHERE
			s.custom_form_id = #{customFormId} and s.delflag='0'
	</select>
</mapper>