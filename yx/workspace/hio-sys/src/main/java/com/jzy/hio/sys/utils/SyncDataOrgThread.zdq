package com.jzy.hio.sys.utils;

import com.jzy.hio.base.utils.MessageResult;
import com.jzy.hio.sys.sys.sync.service.SyncDataService;


public class SyncDataOrgThread  extends Thread{
	
	private SyncDataService syncDataService;
	
	private String orgId = "";
	
	private String tendCode="";
	
	public SyncDataOrgThread (String orgId,SyncDataService syncDataService,String tendCode){
		this.orgId = orgId;
		this.syncDataService = syncDataService;
		this.tendCode = tendCode;
	}
	public void run() {  
		try{
			//同步数据到第三方系统
			MessageResult resultSync = syncDataService.syncDataOrgOne(orgId,tendCode);
		}catch(Exception e){
		}
		 
	}  
}