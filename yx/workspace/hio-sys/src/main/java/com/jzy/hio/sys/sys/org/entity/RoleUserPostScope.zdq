package com.jzy.hio.sys.sys.org.entity;

import com.jzy.hio.base.entity.BaseEntity;

import javax.persistence.Column;
import javax.persistence.Table;


/**
 * @author admin
 * 通用角色引用对象作用域
 * 
 */

@Table(name="PT_SYS_ORG_ROLE_USER_POST_SCOPE")
public class RoleUserPostScope extends BaseEntity{

	/**
	 * 管辖类型
	 */
	@Column(name="type")
	private String type;

	/**
	 * 管辖id
	 */
	@Column(name="ref_id")
	private String refId;

	/**
	 * 引用对象关系id
	 */
	@Column(name="role_user_id")
	private String roleUserId;
    
  		
		
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
    
  		
	public String getRefId() {
		return refId;
	}
	public void setRefId(String refId) {
		this.refId = refId;
	}
    
  		
	public String getRoleUserId() {
		return roleUserId;
	}
	public void setRoleUserId(String roleUserId) {
		this.roleUserId = roleUserId;
	}
    
  		
	
}
