package com.jzy.hio.sys.sys.res.entity;

import com.jzy.hio.base.entity.BaseEntity;

import javax.persistence.Column;
import javax.persistence.Table;


/**
 * @author admin
 * 数据控制对象
 * 
 */

@Table(name="PT_SYS_RES_DATA_CTRL")
public class DataCtrl extends BaseEntity{

	/**
	 * 编号
	 */
	@Column(name="code")
	private String code;

	/**
	 * 名称
	 */
	@Column(name="name")
	private String name;

	/**
	 * 应用id
	 */
	@Column(name="app_id")
	private String appId;

	/**
	 * 图标
	 */
	@Column(name="icon")
	private String icon;

	/**
	 * 排序
	 */
	@Column(name="sort")
	private Long sort;
    
  		
		
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
    
  		
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
  		
	public String getAppId() {
		return appId;
	}
	public void setAppId(String appId) {
		this.appId = appId;
	}
    
  		
	public String getIcon() {
		return icon;
	}
	public void setIcon(String icon) {
		this.icon = icon;
	}
    
  		
	public Long getSort() {
		return sort;
	}
	public void setSort(Long sort) {
		this.sort = sort;
	}
    
  		
	
}
