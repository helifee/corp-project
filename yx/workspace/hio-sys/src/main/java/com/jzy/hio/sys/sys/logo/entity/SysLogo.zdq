package com.jzy.hio.sys.sys.logo.entity;


import com.jzy.hio.base.entity.BaseEntity;

import javax.persistence.Column;
import javax.persistence.Table;

/**
 * @author admin
 * 平台logo
 * 
 */

@Table(name="PT_SYS_LOGO")
public class SysLogo extends BaseEntity {

	/**
	 * 名称
	 */
	@Column(name="name")
	private String name;

	/**
	 * 图标
	 */
	@Column(name="icon")
	private String icon;

	/**
	 * 地址
	 */
	@Column(name="url")
	private String url;
		
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
    
  		
	public String getIcon() {
		return icon;
	}
	public void setIcon(String icon) {
		this.icon = icon;
	}


	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}
}
