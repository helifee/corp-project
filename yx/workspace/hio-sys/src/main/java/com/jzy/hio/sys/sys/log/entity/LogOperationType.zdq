package com.jzy.hio.sys.sys.log.entity;


import com.jzy.hio.base.entity.BaseEntity;

import javax.persistence.Column;
import javax.persistence.Table;

/**
 * @author admin
 * 日志操作类型
 * 
 */

@Table(name="PT_SYS_LOG_OPERATION_TYPE")
public class LogOperationType extends BaseEntity {

	/**
	 * 编号
	 */
	@Column(name="code")
	private String code;

	/**
	 * 类型名称
	 */
	@Column(name="name")
	private String name;

	/**
	 * 描述
	 */
	@Column(name="remark")
	private String remark;

	/**
	 * 排序
	 */
	@Column(name="sort")
	private Long sort;
    
  		
		
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
    
  		
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	public Long getSort() {
		return sort;
	}
	public void setSort(Long sort) {
		this.sort = sort;
	}
    
  		
	
}
