package com.jzy.hio.sys.sys.base.entity;

import com.jzy.hio.base.entity.BaseEntity;

import javax.persistence.Column;
import javax.persistence.Table;
import java.sql.Timestamp;


/**
 * @author admin
 * 产品类型
 * 
 */

@Table(name = "PT_SYS_BASE_PROJECT_TYPE")
public class BaseProjectType extends BaseEntity {

	/**
	 * 名称
	 */
	@Column(name = "name")
	private String name;

	/**
	 * 编号
	 */
	@Column(name = "code")
	private String code;

	/**
	 * 上级
	 */
	@Column(name = "parent_id")
	private String parentId;

	/**
	 * 状态
	 */
	@Column(name = "status")
	private String status;

	/**
	 * 备注
	 */
	@Column(name = "remark")
	private String remark;

	/**
	 * 排序
	 */
	@Column(name = "sort")
	private String sort;

	/**
	 * 排序id
	 */
	@Column(name = "prefix_id")
	private String prefixId;

	/**
	 * 禁用人
	 */
	@Column(name = "disabled_id")
	private String disabledId;

	/**
	 * 禁用日期
	 */
	@Column(name = "disabled_date")
	private Timestamp disabledDate;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}



	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

/*	public Long getSort() {
		return sort;
	}

	public void setSort(Long sort) {
		this.sort = sort;
	}*/
	
	

	public String getDisabledId() {
		return disabledId;
	}

	public String getSort() {
		return sort;
	}

	public void setSort(String sort) {
		this.sort = sort;
	}

	public void setDisabledId(String disabledId) {
		this.disabledId = disabledId;
	}

	public Timestamp getDisabledDate() {
		return disabledDate;
	}

	public void setDisabledDate(Timestamp disabledDate) {
		this.disabledDate = disabledDate;
	}


	public String getParentId() {
		return parentId;
	}

	public void setParentId(String parentId) {
		this.parentId = parentId;
	}

	public String getPrefixId() {
		return prefixId;
	}

	public void setPrefixId(String prefixId) {
		this.prefixId = prefixId;
	}

	
}
