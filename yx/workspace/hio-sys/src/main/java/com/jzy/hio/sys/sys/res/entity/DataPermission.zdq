package com.jzy.hio.sys.sys.res.entity;

import com.jzy.hio.base.entity.BaseEntity;

import javax.persistence.Column;
import javax.persistence.Table;


/**
 * @author admin
 * 数据授权
 * 
 */

@Table(name="PT_SYS_RES_DATA_PERMISSION")
public class DataPermission extends BaseEntity{

	/**
	 * 角色id
	 */
	@Column(name="role_id")
	private String roleId;

	/**
	 * 控制点id
	 */
	@Column(name="data_point_id")
	private String dataPointId;

	/**
	 * 系统id
	 */
	@Column(name="app_id")
	private String appId;	
		
	public String getRoleId() {
		return roleId;
	}
	public void setRoleId(String roleId) {
		this.roleId = roleId;
	}
    
  		
	public String getDataPointId() {
		return dataPointId;
	}
	public void setDataPointId(String dataPointId) {
		this.dataPointId = dataPointId;
	}
	public String getAppId() {
		return appId;
	}
	public void setAppId(String appId) {
		this.appId = appId;
	}
    
  		
	
}
