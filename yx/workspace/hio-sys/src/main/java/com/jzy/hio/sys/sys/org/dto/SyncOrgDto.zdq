package com.jzy.hio.sys.sys.org.dto;

import java.sql.Timestamp;



/**
 * @author sy
 * 
 *
 */
public class SyncOrgDto{

	//ID
	private String orgId;

	//父节点ID（一级组织父节点设置为null）
	private String parentId;
	
	//租户ID
	private String tendId;
	
	//组织类型（0：公司:1：非公司）
	private int orgType;
    
  	//组织名称
	private String orgName;
	
	//组织排序号
	private int orgOrder;
    
	//组织状态（-1：删除；0：正常；1：停用）
	private int orgState;

	public String getOrgId() {
		return orgId;
	}

	public void setOrgId(String orgId) {
		this.orgId = orgId;
	}

	public String getParentId() {
		return parentId;
	}

	public void setParentId(String parentId) {
		this.parentId = parentId;
	}

	public String getTendId() {
		return tendId;
	}

	public void setTendId(String tendId) {
		this.tendId = tendId;
	}

	public int getOrgType() {
		return orgType;
	}

	public void setOrgType(int orgType) {
		this.orgType = orgType;
	}

	public String getOrgName() {
		return orgName;
	}

	public void setOrgName(String orgName) {
		this.orgName = orgName;
	}

	public int getOrgOrder() {
		return orgOrder;
	}

	public void setOrgOrder(int orgOrder) {
		this.orgOrder = orgOrder;
	}

	public int getOrgState() {
		return orgState;
	}

	public void setOrgState(int orgState) {
		this.orgState = orgState;
	}
	
}
