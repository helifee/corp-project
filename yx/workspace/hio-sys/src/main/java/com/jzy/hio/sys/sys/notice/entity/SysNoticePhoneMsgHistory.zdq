package com.jzy.hio.sys.sys.notice.entity;


import com.jzy.hio.base.entity.BaseEntity;

import javax.persistence.Column;
import javax.persistence.Table;

/**
 * @author admin
 * 手机信息表历史
 * 
 */

@Table(name="PT_SYS_NOTICE_PHONE_MSG_HISTORY")
public class SysNoticePhoneMsgHistory extends BaseEntity {

	/**
	 * 接收方手机号
	 */
	@Column(name="phones")
	private String phones;

    /**
     * 接收方信息
     */
	@Column(name="msg")
	private String msg;

    /**
     * 状态
     */
	@Column(name="status")
	private String status;

    /**
     * 发送次数
     */
	@Column(name="num")
	private Integer num;

    /**
     * 服务器Id
     */
	@Column(name="phonel_server_id")
	private String phonelServerId;

    /**
     * 备注
     */
	@Column(name="remark")
	private String remark;
  		
		
	public String getPhones() {
		return phones;
	}
	public void setPhones(String phones) {
		this.phones = phones;
	}
    
  		
	public String getMsg() {
		return msg;
	}
	public void setMsg(String msg) {
		this.msg = msg;
	}
    
  		
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
    
  		
	public Integer getNum() {
		return num;
	}
	public void setNum(Integer num) {
		this.num = num;
	}
    
  		
	public String getPhonelServerId() {
		return phonelServerId;
	}
	public void setPhonelServerId(String phonelServerId) {
		this.phonelServerId = phonelServerId;
	}
	
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
    
  		
	
}
