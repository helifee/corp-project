package com.jzy.hio.sys.sys.org.entity;

import com.jzy.hio.base.entity.BaseEntity;

import javax.persistence.Column;
import javax.persistence.Table;


/**
 * @author admin
 * 组织结构
 * 
 */

@Table(name="PT_SYS_ORG_ORGNAZATION")
public class Orgnazation extends BaseEntity{

	/**
	 * 名称
	 */
	@Column(name="name")
	private String name;

	/**
	 * 组织机构代码
	 */
	@Column(name="code")
	private String code;

	/**
	 * 全称
	 */
	@Column(name="full_name")
	private String fullName;

	/**
	 * 领导岗位
	 */
	@Column(name="leader_id")
	private String leaderId;

	/**
	 * 上级领导岗位
	 */
	@Column(name="up_leader_id")
	private String upLeaderId;

	/**
	 * 上级
	 */
	@Column(name="parent_id")
	private String parentId;

	/**
	 * 类型
	 */
	@Column(name="type")
	private String type;

	/**
	 * 目录id
	 */
	@Column(name="root_id")
	private String rootId;

	/**
	 * 排序
	 */
	@Column(name="sort")
	private Long sort;

	/**
	 * 图标
	 */
	@Column(name="icon")
	private String icon;

	/**
	 * 状态
	 */
	@Column(name="status")
	private String status;

	/**
	 * 说明
	 */
	@Column(name="remark")
	private String remark;

	/**
	 * 全路径Id
	 */
	@Column(name="prefix_id")
	private String prefixId;

	/**
	 * 全路径名称
	 */
	@Column(name="prefix_name")
	private String prefixName;
    
  		
		
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
    
  		
	public String getFullName() {
		return fullName;
	}
	public void setFullName(String fullName) {
		this.fullName = fullName;
	}
    
  		
	public String getLeaderId() {
		return leaderId;
	}
	public void setLeaderId(String leaderId) {
		this.leaderId = leaderId;
	}
    
  		
	public String getUpLeaderId() {
		return upLeaderId;
	}
	public void setUpLeaderId(String upLeaderId) {
		this.upLeaderId = upLeaderId;
	}
    
  		
	public String getParentId() {
		return parentId;
	}
	public void setParentId(String parentId) {
		this.parentId = parentId;
	}
    
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
    
  		
	public String getRootId() {
		return rootId;
	}
	public void setRootId(String rootId) {
		this.rootId = rootId;
	}
    
  		
	public Long getSort() {
		return sort;
	}
	public void setSort(Long sort) {
		this.sort = sort;
	}
    
  		
	public String getIcon() {
		return icon;
	}
	public void setIcon(String icon) {
		this.icon = icon;
	}
    
  		
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
    
  		
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getPrefixId() {
		return prefixId;
	}
	public void setPrefixId(String prefixId) {
		this.prefixId = prefixId;
	}
	public String getPrefixName() {
		return prefixName;
	}
	public void setPrefixName(String prefixName) {
		this.prefixName = prefixName;
	}
    
  		
	
}
