package com.jzy.hio.sys.sys.res.entity;

import com.jzy.hio.base.entity.BaseEntity;

import javax.persistence.Column;
import javax.persistence.Table;


/**
 * @author admin
 * 数据点角色授权值
 * 
 */

@Table(name="PT_SYS_RES_DATA_POINT_PERMISSION_VAL")
public class DataPointPermissionVal extends BaseEntity{

	/**
	 * 数据授权id
	 */
	@Column(name="data_permission_id")
	private String dataPermissionId;

	/**
	 * 值
	 */
	@Column(name="val")
	private String val;
    
  		
		
	public String getDataPermissionId() {
		return dataPermissionId;
	}
	public void setDataPermissionId(String dataPermissionId) {
		this.dataPermissionId = dataPermissionId;
	}
    
  		
	public String getVal() {
		return val;
	}
	public void setVal(String val) {
		this.val = val;
	}
    
  		
	
}
