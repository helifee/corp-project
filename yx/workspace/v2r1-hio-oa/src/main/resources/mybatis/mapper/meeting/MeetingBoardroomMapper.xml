<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jzy.hio.oa.meeting.mapper.MeetingBoardroomMapper">



	<!-- 通过会议室名称查询,同名称校验 -->
    <select id="getMeetingBoardroomListByTitle" parameterType="com.jzy.hio.entity.MeetingBoardroom"
    		resultType="com.jzy.hio.entity.MeetingBoardroom" >
    		
    	SELECT 
			m.id id,m.name name,m.address address,m.max_person_num				
		FROM meeting_boardroom m
		WHERE 1=1
			AND m.delflag = 0
			AND m.name = #{name}
    		
    </select>
    
  
</mapper>