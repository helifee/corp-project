package com.jzy.hio.oa.dictionary.service.impl;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.jzy.hio.entity.SysTendDictionary;
import com.jzy.hio.oa.OaConstant;
import com.jzy.hio.oa.dictionary.dto.SysDictionaryDto;
import com.jzy.hio.oa.dictionary.mapper.SysTendDictionaryMapper;
import com.jzy.hio.oa.dictionary.service.SysDictionaryService;

/**
 * 数据字典表
 * @author wdy
 *
 */
@Service
public class SysDictionaryServiceImpl implements SysDictionaryService {

	@Autowired
	private SysTendDictionaryMapper sysTendDictionaryMapper;
	
	/**
	 * 数据字典表
	 * @author wdy
	 *
	 */
	@Override
	public List<SysDictionaryDto> querySysDictionaryList() {
		
		SysTendDictionary sysDictionary = new SysTendDictionary();
		sysDictionary.setDicFid(OaConstant.ADVANCETIME_ID);
		List<SysDictionaryDto> sysDictionaryList = sysTendDictionaryMapper.querySysDictionaryList(sysDictionary);
		
		return sysDictionaryList;
	}

}
