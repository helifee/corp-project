package com.jzy.hio.oa.meeting.dto;

import com.jzy.hio.entity.MeetingReserveBoardroom;

/**
 * 会议室预定记录Dto
 * @author shangb
 * @date 2018年3月28日 下午3:11:08
 */

public class MeetingReserveBoardroomDto extends MeetingReserveBoardroom {

	/**
     * 会议室名称
     */
    private String meetingBoardroomName;
    
    /**
     * 使用人姓名
     */
    private String useUserName;
    
    /**
     * 是否显示编辑、删除按钮，1显示，0不显示
     */
    private String existUpdateDeleteButton;

	public String getMeetingBoardroomName() {
		return meetingBoardroomName;
	}

	public void setMeetingBoardroomName(String meetingBoardroomName) {
		this.meetingBoardroomName = meetingBoardroomName;
	}

	public String getUseUserName() {
		return useUserName;
	}

	public void setUseUserName(String useUserName) {
		this.useUserName = useUserName;
	}

	public String getExistUpdateDeleteButton() {
		return existUpdateDeleteButton;
	}

	public void setExistUpdateDeleteButton(String existUpdateDeleteButton) {
		this.existUpdateDeleteButton = existUpdateDeleteButton;
	}
	
}
