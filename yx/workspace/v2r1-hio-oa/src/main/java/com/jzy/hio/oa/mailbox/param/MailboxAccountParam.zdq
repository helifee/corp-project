package com.jzy.hio.oa.mailbox.param;

import com.fasterxml.jackson.annotation.JsonProperty;
import lombok.Data;
import org.hibernate.validator.constraints.Email;
import org.hibernate.validator.constraints.NotBlank;

import javax.validation.constraints.NotNull;

/**
 * <p>
 * 邮箱账号参数对象
 * </p>
 *
 * @author haoqipeng 2018/5/8 21:33
 */
@Data
public class MailboxAccountParam {

    /**
     * 主键
     */
    @JsonProperty("sid")
    private String id;

    /**
     * 邮箱账号
     */
    @NotBlank(message = "邮箱账号不能为空")
    @Email(message = "邮箱账号格式不正确")
    private String account;

    /**
     * 邮箱密码
     */
    @NotBlank(message = "邮箱密码不能为空")
    private String password;

    /**
     * 邮箱确认密码
     */
    //@NotBlank(message = "邮箱确认密码不能为空")
    private String confirmPassword;

    /**
     * 邮箱类型， 1-个人，2-企业
     */
    @NotNull(message = "邮箱类型不能为空")
    private Integer mailboxType;

    /**
     * 邮箱所属用户
     */
    //@NotBlank(message = "邮箱所属用户不能为空")
    private String userId;

    /**
     * 邮箱所属租户ID，企业邮箱时存储此值
     */
    private String tenantId;
}
