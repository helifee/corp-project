package com.jzy.hio.oa.schedule.dto;

import java.util.Date;

/**
 * 查询日程详情出参
 * @author wdy
 *
 */
public class QueryScheduleDetailsDto {

	private String scheduleTitle;//日程标题
	private Date beginTime;//起始时间
	private Date endTime;//结束时间
	private int advanceTime;//提醒前时间
	private String isRepeat;//是否重复 0:重复 1：不重复
	private String repetitionFrequency;//重复频率 0:每天 1:每周 2:每月 3:每年
	private String endFrequency;//结束频率 0：永不（默认3年） 1：次数  2:日期
	private String repetitionResult;//事件重复结果
	private String address;//地点
	private String scheduleDetails;//日程详情
	private Date endFrequencyTime;//结束频率日期
	private int endFrequencyNum;//结束频率次数
	private String createPersonId;//创建id
	private String createPersonName;//创建名称
	private ScheduleSharedDto scheduleSharedDto;//共享人员列表
	private Boolean delete;//是否可以删除
	private String scheduleGroupId;//日程组id
	
	public String getScheduleTitle() {
		return scheduleTitle;
	}
	public void setScheduleTitle(String scheduleTitle) {
		this.scheduleTitle = scheduleTitle;
	}
	public Date getBeginTime() {
		return beginTime;
	}
	public void setBeginTime(Date beginTime) {
		this.beginTime = beginTime;
	}
	public Date getEndTime() {
		return endTime;
	}
	public void setEndTime(Date endTime) {
		this.endTime = endTime;
	}
	public int getAdvanceTime() {
		return advanceTime;
	}
	public void setAdvanceTime(int advanceTime) {
		this.advanceTime = advanceTime;
	}
	public String getIsRepeat() {
		return isRepeat;
	}
	public void setIsRepeat(String isRepeat) {
		this.isRepeat = isRepeat;
	}
	public String getRepetitionFrequency() {
		return repetitionFrequency;
	}
	public void setRepetitionFrequency(String repetitionFrequency) {
		this.repetitionFrequency = repetitionFrequency;
	}
	public String getEndFrequency() {
		return endFrequency;
	}
	public void setEndFrequency(String endFrequency) {
		this.endFrequency = endFrequency;
	}
	public String getRepetitionResult() {
		return repetitionResult;
	}
	public void setRepetitionResult(String repetitionResult) {
		this.repetitionResult = repetitionResult;
	}
	public String getAddress() {
		return address;
	}
	public void setAddress(String address) {
		this.address = address;
	}
	public String getScheduleDetails() {
		return scheduleDetails;
	}
	public void setScheduleDetails(String scheduleDetails) {
		this.scheduleDetails = scheduleDetails;
	}
	public Date getEndFrequencyTime() {
		return endFrequencyTime;
	}
	public void setEndFrequencyTime(Date endFrequencyTime) {
		this.endFrequencyTime = endFrequencyTime;
	}
	public int getEndFrequencyNum() {
		return endFrequencyNum;
	}
	public void setEndFrequencyNum(int endFrequencyNum) {
		this.endFrequencyNum = endFrequencyNum;
	}
	public String getCreatePersonName() {
		return createPersonName;
	}
	public void setCreatePersonName(String createPersonName) {
		this.createPersonName = createPersonName;
	}
	public ScheduleSharedDto getScheduleSharedDto() {
		return scheduleSharedDto;
	}
	public void setScheduleSharedDto(ScheduleSharedDto scheduleSharedDto) {
		this.scheduleSharedDto = scheduleSharedDto;
	}
	public String getCreatePersonId() {
		return createPersonId;
	}
	public void setCreatePersonId(String createPersonId) {
		this.createPersonId = createPersonId;
	}
	public Boolean getDelete() {
		return delete;
	}
	public void setDelete(Boolean delete) {
		this.delete = delete;
	}
	public String getScheduleGroupId() {
		return scheduleGroupId;
	}
	public void setScheduleGroupId(String scheduleGroupId) {
		this.scheduleGroupId = scheduleGroupId;
	}
	
}
