package com.jzy.hio.entity;

import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Table;

import com.jzy.hio.base.entity.BaseEntity;

@Table(name = "news_type")
public class NewsType extends BaseEntity {
    /**
     * 栏目名称
     */
    @Column(name = "name")
    private String name;

    /**
     * 备注
     */
    @Column(name = "remark")
    private String remark;

    /**
     * 排序
     */
    @Column(name = "order_by")
    private Integer orderBy;

    /**
     * 该栏目下新闻数量
     */
    @Column(name = "news_count")
    private Integer newsCount;
    
    /**
     * 栏目拖拽更新时间
     */
    @Column(name = "update_order_time")
    private Date updateOrderTime;


    /**
     * 栏目名称
     * @return name 栏目名称
     */
    public String getName() {
        return name;
    }

    /**
     * 栏目名称
     * @param name 栏目名称
     */
    public void setName(String name) {
        this.name = name == null ? null : name.trim();
    }

    /**
     * 备注
     * @return remark 备注
     */
    public String getRemark() {
        return remark;
    }

    /**
     * 备注
     * @param remark 备注
     */
    public void setRemark(String remark) {
        this.remark = remark == null ? null : remark.trim();
    }

    /**
     * 排序
     * @return order_by 排序
     */
    public Integer getOrderBy() {
        return orderBy;
    }

    /**
     * 排序
     * @param orderBy 排序
     */
    public void setOrderBy(Integer orderBy) {
        this.orderBy = orderBy;
    }

    public Integer getNewsCount() {
		return newsCount;
	}

	public void setNewsCount(Integer newsCount) {
		this.newsCount = newsCount;
	}

	public Date getUpdateOrderTime() {
		return updateOrderTime;
	}

	public void setUpdateOrderTime(Date updateOrderTime) {
		this.updateOrderTime = updateOrderTime;
	}

	/**
     *
     * @mbg.generated
     */
    @Override
    public String toString() {
        StringBuilder sb = new StringBuilder();
        sb.append(getClass().getSimpleName());
        sb.append(" [");
        sb.append("Hash = ").append(hashCode());
        sb.append(", name=").append(name);
        sb.append(", remark=").append(remark);
        sb.append(", orderBy=").append(orderBy);
        sb.append(", newsCount=").append(newsCount);
        sb.append(", updateOrderTime=").append(updateOrderTime);
        sb.append("]");
        return sb.toString();
    }
}