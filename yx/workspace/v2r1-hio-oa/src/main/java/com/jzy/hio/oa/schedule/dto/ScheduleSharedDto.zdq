package com.jzy.hio.oa.schedule.dto;

import java.util.List;

import com.jzy.hio.entity.ScheduleShared;

/**
 * 日程共享对象
 * @author wdy
 *
 */
public class ScheduleSharedDto {

	private List<SharedCreatePersonDto> personList;//人员列表
	private List<SharedCreatePersonDto> roleList;//角色列表
	private List<SharedCreatePersonDto> departmentList;//部门列表
	
	public List<SharedCreatePersonDto> getPersonList() {
		return personList;
	}
	public void setPersonList(List<SharedCreatePersonDto> personList) {
		this.personList = personList;
	}
	public List<SharedCreatePersonDto> getRoleList() {
		return roleList;
	}
	public void setRoleList(List<SharedCreatePersonDto> roleList) {
		this.roleList = roleList;
	}
	public List<SharedCreatePersonDto> getDepartmentList() {
		return departmentList;
	}
	public void setDepartmentList(List<SharedCreatePersonDto> departmentList) {
		this.departmentList = departmentList;
	}
	
}
