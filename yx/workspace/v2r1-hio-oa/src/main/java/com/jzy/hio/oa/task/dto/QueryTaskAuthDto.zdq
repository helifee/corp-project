package com.jzy.hio.oa.task.dto;

/**
 * 查询任务对应操作权限输出对象
 * @author wdy
 *
 */
public class QueryTaskAuthDto {

	private Boolean update;//编辑
	private Boolean follow;//关注
	private Boolean taskFinish;//任务完成
	private Boolean taskClose;//任务关闭
	private Boolean taskReturn;//任务返回
	private Boolean taskNotFinish;//任务未完成
	private Boolean taskActivation;//任务激活
	private Boolean taskDelete;//任务删除
	
	public Boolean getUpdate() {
		return update;
	}
	public void setUpdate(Boolean update) {
		this.update = update;
	}
	public Boolean getFollow() {
		return follow;
	}
	public void setFollow(Boolean follow) {
		this.follow = follow;
	}
	public Boolean getTaskFinish() {
		return taskFinish;
	}
	public void setTaskFinish(Boolean taskFinish) {
		this.taskFinish = taskFinish;
	}
	public Boolean getTaskClose() {
		return taskClose;
	}
	public void setTaskClose(Boolean taskClose) {
		this.taskClose = taskClose;
	}
	public Boolean getTaskReturn() {
		return taskReturn;
	}
	public void setTaskReturn(Boolean taskReturn) {
		this.taskReturn = taskReturn;
	}
	public Boolean getTaskNotFinish() {
		return taskNotFinish;
	}
	public void setTaskNotFinish(Boolean taskNotFinish) {
		this.taskNotFinish = taskNotFinish;
	}
	public Boolean getTaskActivation() {
		return taskActivation;
	}
	public void setTaskActivation(Boolean taskActivation) {
		this.taskActivation = taskActivation;
	}
	public Boolean getTaskDelete() {
		return taskDelete;
	}
	public void setTaskDelete(Boolean taskDelete) {
		this.taskDelete = taskDelete;
	}
	
}
