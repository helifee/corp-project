package com.jzy.hio.oa.schedule.param;

import java.util.List;

import com.jzy.hio.entity.ScheduleShared;

/**
 * 日程共享对象
 * @author wdy
 *
 */
public class ScheduleSharedParam {

	private List<ScheduleShared> personList;//人员列表
	private List<ScheduleShared> roleList;//角色列表
	private List<ScheduleShared> departmentList;//部门列表
	
	public List<ScheduleShared> getPersonList() {
		return personList;
	}
	public void setPersonList(List<ScheduleShared> personList) {
		this.personList = personList;
	}
	public List<ScheduleShared> getRoleList() {
		return roleList;
	}
	public void setRoleList(List<ScheduleShared> roleList) {
		this.roleList = roleList;
	}
	public List<ScheduleShared> getDepartmentList() {
		return departmentList;
	}
	public void setDepartmentList(List<ScheduleShared> departmentList) {
		this.departmentList = departmentList;
	}
	
}
