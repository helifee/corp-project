package com.jzy.hio.oa.task.param;

import java.util.Date;

import org.hibernate.validator.constraints.NotBlank;

import com.jzy.hio.oa.task.dto.TaskParticipantsSharedListDto;
import com.jzy.hio.oa.task.dto.TaskSharedListDto;

/**
 * 修改任务输入项
 * @author wdy
 *
 */
public class UpdateParam {

	@NotBlank
	private String taskId;//任务id
	private String taskName;//任务名称
	private String projectId;//关联项目id
	private String projectstageId;//关联项目阶段id
	private String taskLiableId;//负责人id
	private Date beginDate;//起始日期
	private Date endDate;//到期日 yyyy-MM-dd
	private TaskParticipantsSharedListDto taskParticipantsSharedListDto;//参与人列表
	private TaskSharedListDto taskSharedListDto;//共享人列表
	private Integer taskProgress;//进度
	private String taskUrgentFlag;//紧急程度
	private String describe;//描述
	private Integer advanceTime;//提前时间
	private String remindTime;//提醒时间（分钟）
	private Boolean isremindCreateperson;//是否提醒创建人
	private Boolean isremindLiable;//是否提醒负责人
	private Boolean isremindAffiliated;//是否提醒参与人
	private String imUserId;//发送cc消息userId
	
	public String getTaskId() {
		return taskId;
	}
	public void setTaskId(String taskId) {
		this.taskId = taskId;
	}
	public String getTaskName() {
		return taskName;
	}
	public void setTaskName(String taskName) {
		this.taskName = taskName;
	}
	public String getProjectId() {
		return projectId;
	}
	public void setProjectId(String projectId) {
		this.projectId = projectId;
	}
	public String getProjectstageId() {
		return projectstageId;
	}
	public void setProjectstageId(String projectstageId) {
		this.projectstageId = projectstageId;
	}
	public String getTaskLiableId() {
		return taskLiableId;
	}
	public void setTaskLiableId(String taskLiableId) {
		this.taskLiableId = taskLiableId;
	}
	public Date getBeginDate() {
		return beginDate;
	}
	public void setBeginDate(Date beginDate) {
		this.beginDate = beginDate;
	}
	public Date getEndDate() {
		return endDate;
	}
	public void setEndDate(Date endDate) {
		this.endDate = endDate;
	}
	public TaskParticipantsSharedListDto getTaskParticipantsSharedListDto() {
		return taskParticipantsSharedListDto;
	}
	public void setTaskParticipantsSharedListDto(
			TaskParticipantsSharedListDto taskParticipantsSharedListDto) {
		this.taskParticipantsSharedListDto = taskParticipantsSharedListDto;
	}
	public TaskSharedListDto getTaskSharedListDto() {
		return taskSharedListDto;
	}
	public void setTaskSharedListDto(TaskSharedListDto taskSharedListDto) {
		this.taskSharedListDto = taskSharedListDto;
	}
	public Integer getTaskProgress() {
		return taskProgress;
	}
	public void setTaskProgress(Integer taskProgress) {
		this.taskProgress = taskProgress;
	}
	public String getTaskUrgentFlag() {
		return taskUrgentFlag;
	}
	public void setTaskUrgentFlag(String taskUrgentFlag) {
		this.taskUrgentFlag = taskUrgentFlag;
	}
	public String getDescribe() {
		return describe;
	}
	public void setDescribe(String describe) {
		this.describe = describe;
	}
	public Integer getAdvanceTime() {
		return advanceTime;
	}
	public void setAdvanceTime(Integer advanceTime) {
		this.advanceTime = advanceTime;
	}
	public String getRemindTime() {
		return remindTime;
	}
	public void setRemindTime(String remindTime) {
		this.remindTime = remindTime;
	}
	public Boolean getIsremindCreateperson() {
		return isremindCreateperson;
	}
	public void setIsremindCreateperson(Boolean isremindCreateperson) {
		this.isremindCreateperson = isremindCreateperson;
	}
	public Boolean getIsremindLiable() {
		return isremindLiable;
	}
	public void setIsremindLiable(Boolean isremindLiable) {
		this.isremindLiable = isremindLiable;
	}
	public Boolean getIsremindAffiliated() {
		return isremindAffiliated;
	}
	public void setIsremindAffiliated(Boolean isremindAffiliated) {
		this.isremindAffiliated = isremindAffiliated;
	}
	public String getImUserId() {
		return imUserId;
	}
	public void setImUserId(String imUserId) {
		this.imUserId = imUserId;
	}
	
}
