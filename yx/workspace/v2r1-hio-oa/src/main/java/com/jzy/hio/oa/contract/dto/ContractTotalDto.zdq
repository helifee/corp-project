package com.jzy.hio.oa.contract.dto;

import java.math.BigDecimal;

/**
 * <p>合同信息统计<br>
 * @author wliang
 * @date 2018年4月27日 下午9:30:51
 */
public class ContractTotalDto {
	
	/**
	 * 时间
	 */
	private String totalTime;
	
	/**
	 * 合同金额
	 */
	private BigDecimal totalMoney;

	/**
	 * 合同变更金额
	 */
	private BigDecimal totalChangeMoney;
	
	/**
	 * 合同付款金额
	 */
	private BigDecimal totalPyamentMoney;
	
	
	
	
	

	/**
	 * @return the totalTime
	 */
	public String getTotalTime() {
		return totalTime;
	}

	/**
	 * @param totalTime the totalTime to set
	 */
	public void setTotalTime(String totalTime) {
		this.totalTime = totalTime;
	}

	/**
	 * @return the totalMoney
	 */
	public BigDecimal getTotalMoney() {
		return totalMoney;
	}

	/**
	 * @param totalMoney the totalMoney to set
	 */
	public void setTotalMoney(BigDecimal totalMoney) {
		this.totalMoney = totalMoney;
	}

	/**
	 * @return the totalChangeMoney
	 */
	public BigDecimal getTotalChangeMoney() {
		return totalChangeMoney;
	}

	/**
	 * @param totalChangeMoney the totalChangeMoney to set
	 */
	public void setTotalChangeMoney(BigDecimal totalChangeMoney) {
		this.totalChangeMoney = totalChangeMoney;
	}

	/**
	 * @return the totalPyamentMoney
	 */
	public BigDecimal getTotalPyamentMoney() {
		return totalPyamentMoney;
	}

	/**
	 * @param totalPyamentMoney the totalPyamentMoney to set
	 */
	public void setTotalPyamentMoney(BigDecimal totalPyamentMoney) {
		this.totalPyamentMoney = totalPyamentMoney;
	}
	
	
	

}
