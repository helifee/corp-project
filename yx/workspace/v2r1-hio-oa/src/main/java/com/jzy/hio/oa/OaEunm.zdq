package com.jzy.hio.oa;

/**
 * oa枚举类
 * @author wdy
 * @date 2018年5月16日
 */
public enum OaEunm {

	TASK_STATUS0("0","未完成"),
	TASK_STATUS1("1","已完成"),
	TASK_STATUS2("2","已关闭"),
	TASK_STATUS3("3","超期");
	
	private String type;
	private String name;
	
	private OaEunm(String type, String name) {
		this.type = type;
		this.name = name;
	}
	
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	
}
