package com.jzy.hio.entity;


import javax.persistence.Column;
import javax.persistence.Table;

import com.jzy.hio.base.entity.BaseEntity;
/**
 * @author  yangning
 */
@Table(name = "oa_project_stage_team_person")
public class ProjectStageTeamPerson extends BaseEntity {



    /**
     * 团队人员id
     */
    @Column(name = "stage_team_person_id")
    private String stageTeamPersonId;

    /**
     * 团队人员imid
     */
    @Column(name = "stage_team_person_im_id")
    private String stageTeamPersonImId;
    /**
     * 团队人员名称
     */
    @Column(name = "stage_team_person_name")
    private String stageTeamPersonName;

    /**
     *团队人员标签
     */
    @Column(name = "stage_team_person_tag")
    private String stageTeamPersonTag;


    /**
     * 团队人员部门id
     */
    @Column(name = "stage_team_person_dept_id")
    private String stagTeamPersonDeptId;

    /**
     * 团队人员公司名称
     */
    @Column(name = "stage_team_person_company")
    private String stagTeamPersonCompany;


    /**
     * 项目关联id
     */
    @Column(name = "stage_id")
    private String stageId;

    /**
     * 团队人员类型 1 创建人 2 负责人 3参与人 4共享人
     */
    @Column(name = "stage_team_type")
    private String stageTeamType;


    public String getStageTeamPersonId() {
        return stageTeamPersonId;
    }

    public void setStageTeamPersonId(String stageTeamPersonId) {
        this.stageTeamPersonId = stageTeamPersonId;
    }

    public String getStageTeamPersonName() {
        return stageTeamPersonName;
    }

    public void setStageTeamPersonName(String stageTeamPersonName) {
        this.stageTeamPersonName = stageTeamPersonName;
    }

    public String getStageTeamPersonTag() {
        return stageTeamPersonTag;
    }

    public void setStageTeamPersonTag(String stageTeamPersonTag) {
        this.stageTeamPersonTag = stageTeamPersonTag;
    }

    public String getStagTeamPersonCompany() {
        return stagTeamPersonCompany;
    }

    public void setStagTeamPersonCompany(String stagTeamPersonCompany) {
        this.stagTeamPersonCompany = stagTeamPersonCompany;
    }

    public String getStageId() {
        return stageId;
    }

    public void setStageId(String stageId) {
        this.stageId = stageId;
    }

    public String getStageTeamType() {
        return stageTeamType;
    }

    public void setStageTeamType(String stageTeamType) {
        this.stageTeamType = stageTeamType;
    }

    public String getStageTeamPersonImId() {
        return stageTeamPersonImId;
    }

    public void setStageTeamPersonImId(String stageTeamPersonImId) {
        this.stageTeamPersonImId = stageTeamPersonImId;
    }

    public String getStagTeamPersonDeptId() {
        return stagTeamPersonDeptId;
    }

    public void setStagTeamPersonDeptId(String stagTeamPersonDeptId) {
        this.stagTeamPersonDeptId = stagTeamPersonDeptId;
    }
}