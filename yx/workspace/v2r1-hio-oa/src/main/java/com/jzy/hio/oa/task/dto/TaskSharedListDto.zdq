package com.jzy.hio.oa.task.dto;

import java.util.List;

/**
 * 任务共享人列表输出对象
 * @author wdy
 *
 */
public class TaskSharedListDto {

	private List<TaskSharedDto> personList;//人员列表
	private List<TaskSharedDto> roleList;//角色列表
	private List<TaskSharedDto> departmentList;//部门列表
	
	public List<TaskSharedDto> getPersonList() {
		return personList;
	}
	public void setPersonList(List<TaskSharedDto> personList) {
		this.personList = personList;
	}
	public List<TaskSharedDto> getRoleList() {
		return roleList;
	}
	public void setRoleList(List<TaskSharedDto> roleList) {
		this.roleList = roleList;
	}
	public List<TaskSharedDto> getDepartmentList() {
		return departmentList;
	}
	public void setDepartmentList(List<TaskSharedDto> departmentList) {
		this.departmentList = departmentList;
	}
	
	
}
