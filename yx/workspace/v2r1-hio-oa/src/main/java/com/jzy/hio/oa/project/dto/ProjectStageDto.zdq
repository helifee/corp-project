package com.jzy.hio.oa.project.dto;

import com.jzy.hio.entity.*;

import java.util.Date;
import java.util.List;
/**
 * @author  yangning
 */
public class ProjectStageDto extends ProjectStage {


	/**
	 * 项目阶段名称
	 */
	private String stageName;
	/**
	 * 项目阶段描述
	 */
	private String stageDesc;

	/**
	 * 项目阶段开始时间
	 */
	private Date stageStartTime;
	/**
	 * 项目阶段结束时间
	 */
	private Date stageEndTime;
	/**
	 * 项目阶段负责人id
	 */
	private String stageResponsiblePersonId;
	/**
	 * 项目阶段负责人名称
	 */
	private String stageResponsiblePersonName;


	/**
	 * 项目阶段负责人
	 */
	private ProjectStageTeamPersonDto stageResponsiblePerson;
	/**
	 * 项目阶阶段参与人列表
	 */
	private List<ProjectStageTeamPersonDto> stageParticipantPersonList;

	/**
	 * 关联项目id
	 */
	private String projectId;


	/**
	 * 阶段下任务数量
	 */
	private int taskCount;





	@Override
	public String getStageName() {
		return stageName;
	}

	@Override
	public void setStageName(String stageName) {
		this.stageName = stageName;
	}

	@Override
	public String getStageDesc() {
		return stageDesc;
	}

	@Override
	public void setStageDesc(String stageDesc) {
		this.stageDesc = stageDesc;
	}

	@Override
	public Date getStageStartTime() {
		return stageStartTime;
	}

	@Override
	public void setStageStartTime(Date stageStartTime) {
		this.stageStartTime = stageStartTime;
	}

	@Override
	public Date getStageEndTime() {
		return stageEndTime;
	}

	@Override
	public void setStageEndTime(Date stageEndTime) {
		this.stageEndTime = stageEndTime;
	}

	@Override
	public String getStageResponsiblePersonId() {
		return stageResponsiblePersonId;
	}

	@Override
	public void setStageResponsiblePersonId(String stageResponsiblePersonId) {
		this.stageResponsiblePersonId = stageResponsiblePersonId;
	}

	@Override
	public String getStageResponsiblePersonName() {
		return stageResponsiblePersonName;
	}

	@Override
	public void setStageResponsiblePersonName(String stageResponsiblePersonName) {
		this.stageResponsiblePersonName = stageResponsiblePersonName;
	}


	@Override
	public String getProjectId() {
		return projectId;
	}

	@Override
	public void setProjectId(String projectId) {
		this.projectId = projectId;
	}

	public int getTaskCount() {
		return taskCount;
	}

	public void setTaskCount(int taskCount) {
		this.taskCount = taskCount;
	}

	public ProjectStageTeamPersonDto getStageResponsiblePerson() {
		return stageResponsiblePerson;
	}

	public void setStageResponsiblePerson(ProjectStageTeamPersonDto stageResponsiblePerson) {
		this.stageResponsiblePerson = stageResponsiblePerson;
	}

	public List<ProjectStageTeamPersonDto> getStageParticipantPersonList() {
		return stageParticipantPersonList;
	}

	public void setStageParticipantPersonList(List<ProjectStageTeamPersonDto> stageParticipantPersonList) {
		this.stageParticipantPersonList = stageParticipantPersonList;
	}
}