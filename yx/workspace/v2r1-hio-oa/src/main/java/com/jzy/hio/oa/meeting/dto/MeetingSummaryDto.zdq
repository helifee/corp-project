package com.jzy.hio.oa.meeting.dto;

import java.util.Date;
import java.util.List;

import com.jzy.hio.entity.MeetingActualAttendees;
import com.jzy.hio.entity.MeetingSummary;

/**
 *会议纪要表Dto
 * @author shangb
 * @date 2018年3月27日 下午3:57:43
 */

public class MeetingSummaryDto extends MeetingSummary {
	
	/**
     * 主题
     */
    private String mtTitle;

    /**
     * 预计开始日期
     */
    private Date beginDate;

    /**
     * 预计结束日期
     */
    private Date endDate;
    
    /**
     * 计划会议室地点
     */
    private String boardroomAddressName;
	
    private List<String> meetingAttendeesNameList;// 计划会议参会人员名字
    
	private List<String> actualAttendeesNameList;// 实际参会人名字
	
	/**
	 * 会议实际参会人员表list
	 */
	private List<MeetingActualAttendeesDto> meetingActualAttendeesDtoList;

	/**
	 * 会议实际参会人员userID的list
	 */
	private List<String> meetingActualAttendeesIdList;

    // 预留附件相关字段
	
	/**
	 * 是否显示编辑按钮，1显示，0不显示
	 */
	private String existUpdateButton;

    /**
     * 当前登录人
     */
    private String loginUserId;
    
    /**
     * 记录人姓名
     */
    private String recorderName;
    
    /**
     * 根据表名bean生成Redis里的表主键id
     */
    private AccessoryIdDto accessoryIdDto;
    
	public String getMtTitle() {
		return mtTitle;
	}

	public void setMtTitle(String mtTitle) {
		this.mtTitle = mtTitle;
	}

	public Date getBeginDate() {
		return beginDate;
	}

	public void setBeginDate(Date beginDate) {
		this.beginDate = beginDate;
	}

	public Date getEndDate() {
		return endDate;
	}

	public void setEndDate(Date endDate) {
		this.endDate = endDate;
	}

	public String getBoardroomAddressName() {
		return boardroomAddressName;
	}

	public void setBoardroomAddressName(String boardroomAddressName) {
		this.boardroomAddressName = boardroomAddressName;
	}

	public List<String> getMeetingAttendeesNameList() {
		return meetingAttendeesNameList;
	}

	public void setMeetingAttendeesNameList(List<String> meetingAttendeesNameList) {
		this.meetingAttendeesNameList = meetingAttendeesNameList;
	}

	public List<String> getActualAttendeesNameList() {
		return actualAttendeesNameList;
	}

	public void setActualAttendeesNameList(List<String> actualAttendeesNameList) {
		this.actualAttendeesNameList = actualAttendeesNameList;
	}
	
	public List<MeetingActualAttendeesDto> getMeetingActualAttendeesDtoList() {
		return meetingActualAttendeesDtoList;
	}
	
	public void setMeetingActualAttendeesDtoList(List<MeetingActualAttendeesDto> meetingActualAttendeesDtoList) {
		this.meetingActualAttendeesDtoList = meetingActualAttendeesDtoList;
	}

	public List<String> getMeetingActualAttendeesIdList() {
		return meetingActualAttendeesIdList;
	}

	public void setMeetingActualAttendeesIdList(List<String> meetingActualAttendeesIdList) {
		this.meetingActualAttendeesIdList = meetingActualAttendeesIdList;
	}

	public String getExistUpdateButton() {
		return existUpdateButton;
	}

	public void setExistUpdateButton(String existUpdateButton) {
		this.existUpdateButton = existUpdateButton;
	}

	public String getLoginUserId() {
		return loginUserId;
	}

	public void setLoginUserId(String loginUserId) {
		this.loginUserId = loginUserId;
	}

	public String getRecorderName() {
		return recorderName;
	}

	public void setRecorderName(String recorderName) {
		this.recorderName = recorderName;
	}

	public AccessoryIdDto getAccessoryIdDto() {
		return accessoryIdDto;
	}

	public void setAccessoryIdDto(AccessoryIdDto accessoryIdDto) {
		this.accessoryIdDto = accessoryIdDto;
	}

	
}
