package com.jzy.hio.oa.office.dto;

import java.util.List;

import com.jzy.hio.entity.OfficeInfo;

/**
 * 办公物品详情
 * @author shangb
 * @date 2018年3月20日 上午10:14:39
 */
public class OfficeInfoDto extends OfficeInfo {

	private Integer officeinfoNum;// 本次入库/出库数量
	
    private String typeName;// 类别名称
    
    private String firstHouseId;// 类别名称
    
    /**
     * 查询类型：1:物品类型查询 2:出入库查询
     */
    private String queryType;
    
    /**
	 * 分页大小
	 */
	private Integer pageCount;
	/**
	 * 当前页码
	 */
	private Integer pageNum;
	
	/**
     * 查询排序
     */
    private String orderBy;

    private List<OfficeInOutDto> officeInOutDtoList;// 库存记录

	public Integer getOfficeinfoNum() {
		return officeinfoNum;
	}

	public void setOfficeinfoNum(Integer officeinfoNum) {
		this.officeinfoNum = officeinfoNum;
	}

	public String getTypeName() {
		return typeName;
	}

	public void setTypeName(String typeName) {
		this.typeName = typeName;
	}

	public List<OfficeInOutDto> getOfficeInOutDtoList() {
		return officeInOutDtoList;
	}

	public void setOfficeInOutDtoList(List<OfficeInOutDto> officeInOutDtoList) {
		this.officeInOutDtoList = officeInOutDtoList;
	}

	/**
	 * @return the firstHouseId
	 */
	public String getFirstHouseId() {
		return firstHouseId;
	}

	/**
	 * @param firstHouseId the firstHouseId to set
	 */
	public void setFirstHouseId(String firstHouseId) {
		this.firstHouseId = firstHouseId;
	}

	/**
	 * @return the queryType
	 */
	public String getQueryType() {
		return queryType;
	}

	/**
	 * @param queryType the queryType to set
	 */
	public void setQueryType(String queryType) {
		this.queryType = queryType;
	}

	/**
	 * @return the pageCount
	 */
	public Integer getPageCount() {
		return pageCount;
	}

	/**
	 * @param pageCount the pageCount to set
	 */
	public void setPageCount(Integer pageCount) {
		this.pageCount = pageCount;
	}

	/**
	 * @return the pageNum
	 */
	public Integer getPageNum() {
		return pageNum;
	}

	/**
	 * @param pageNum the pageNum to set
	 */
	public void setPageNum(Integer pageNum) {
		this.pageNum = pageNum;
	}

	/**
	 * @return the orderBy
	 */
	public String getOrderBy() {
		return orderBy;
	}

	/**
	 * @param orderBy the orderBy to set
	 */
	public void setOrderBy(String orderBy) {
		this.orderBy = orderBy;
	}

	
	
	
}
