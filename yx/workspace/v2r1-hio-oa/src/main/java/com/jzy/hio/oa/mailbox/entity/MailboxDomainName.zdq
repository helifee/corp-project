package com.jzy.hio.oa.mailbox.entity;

import com.jzy.hio.base.entity.BaseEntity;
import lombok.Data;

import javax.persistence.Column;
import javax.persistence.Table;

/**
 * 邮箱域名实体类
 *
 * @author haoqipeng 2018/5/8 17:27
 */
@Data
@Table(name = "mailbox_domain_name")
public class MailboxDomainName extends BaseEntity {

    /**
     * 邮箱域名
     */
    @Column(name = "domain_name")
    private String domainName;

    /**
     * IMAP服务器
     */
    @Column(name = "imap_host")
    private String imapHost;

    /**
     * IMAP端口
     */
    @Column(name = "imap_port")
    private Integer imapPort;

    /**
     * IMAP加密
     */
    @Column(name = "imap_secure")
    private Integer imapSecure;

    /**
     * 使用短用户名登录
     */
    @Column(name = "imap_short_login")
    private Boolean imapShortLogin;

    /**
     * SMTP服务器
     */
    @Column(name = "smtp_host")
    private String smtpHost;

    /**
     * SMTP端口
     */
    @Column(name = "smtp_port")
    private Integer smtpPort;

    /**
     * IMAP加密
     */
    @Column(name = "smtp_secure")
    private Integer smtpSecure;

    /**
     * 使用短用户名登录
     */
    @Column(name = "smtp_short_login")
    private Boolean smtpShortLogin;

    /**
     * 使用认证
     */
    @Column(name = "smtp_auth")
    private Boolean smtpAuth;

    /**
     * 使用php mail函数
     */
    @Column(name = "smtp_php_mail")
    private Boolean smtpPhpMail;
}
