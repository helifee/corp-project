
package com.jzy.hio.oa.meeting.service.impl;

import java.util.ArrayList;
import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.jzy.hio.annotation.DataSource;
import com.jzy.hio.base.service.impl.BaseServiceImpl;
import com.jzy.hio.entity.MeetingActualAttendees;
import com.jzy.hio.oa.meeting.mapper.MeetingActualAttendeesMapper;
import com.jzy.hio.oa.meeting.service.MeetingActualAttendeesService;

@Service("meetingActualAttendeesService")
public class MeetingActualAttendeesServiceImpl extends BaseServiceImpl<MeetingActualAttendees> implements MeetingActualAttendeesService {

	@Autowired
	private MeetingActualAttendeesMapper meetingActualAttendeesMapper;
	
	@Override
	public List<String> queryMeetingActualAttendeesUserIdListByMeetingSummaryId(String meetingSummaryId) {
		MeetingActualAttendees meetingActualAttendees = new MeetingActualAttendees();
		meetingActualAttendees.setMeetingSummaryId(meetingSummaryId);
		meetingActualAttendees.setDelflag(0);
		List<MeetingActualAttendees> meetingActualAttendeesList = super.queryList(meetingActualAttendees);
		List<String> meetingActualAttendeesUserIdList = new ArrayList<String>();
		if (meetingActualAttendeesList != null && meetingActualAttendeesList.size() > 0) {
			for (MeetingActualAttendees dbMeetingActualAttendees : meetingActualAttendeesList) {
				String actualAttendeesId = dbMeetingActualAttendees.getActualAttendeesId();// 实际参会人ID
				meetingActualAttendeesUserIdList.add(actualAttendeesId);
			}
		}
		return meetingActualAttendeesUserIdList;
	}
	
	@Override
	public List<MeetingActualAttendees> queryMeetingActualAttendeesListByMeetingSummaryId(String meetingSummaryId) {
		MeetingActualAttendees meetingActualAttendees = new MeetingActualAttendees(); 
		meetingActualAttendees.setMeetingSummaryId(meetingSummaryId);
		meetingActualAttendees.setDelflag(0);
		List<MeetingActualAttendees> meetingActualAttendeesList = queryList(meetingActualAttendees);
		return meetingActualAttendeesList;
	}

}
