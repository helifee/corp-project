package com.jzy.hio.oa.task.param;

import java.util.List;

import com.jzy.hio.entity.TaskParticipants;

/**
 * 任务参与人对象
 * @author wdy
 *
 */
public class TaskParticipantsSharedParam {

	private List<TaskParticipants> personList;//人员列表
	private List<TaskParticipants> roleList;//角色列表
	private List<TaskParticipants> departmentList;//部门列表
	
	public List<TaskParticipants> getPersonList() {
		return personList;
	}
	public void setPersonList(List<TaskParticipants> personList) {
		this.personList = personList;
	}
	public List<TaskParticipants> getRoleList() {
		return roleList;
	}
	public void setRoleList(List<TaskParticipants> roleList) {
		this.roleList = roleList;
	}
	public List<TaskParticipants> getDepartmentList() {
		return departmentList;
	}
	public void setDepartmentList(List<TaskParticipants> departmentList) {
		this.departmentList = departmentList;
	}
	
	
	
}
