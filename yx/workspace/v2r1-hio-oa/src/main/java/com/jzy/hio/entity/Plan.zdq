package com.jzy.hio.entity;

import com.jzy.hio.base.entity.BaseEntity;
import java.util.Date;
import javax.persistence.Column;
import javax.persistence.Table;

@Table(name = "plan")
public class Plan extends BaseEntity {
	
    /**
     * 计划主题
     */
    @Column(name = "plan_title")
    private String planTitle;

    /**
     * 计划年份
     */
    @Column(name = "plan_year")
    private String planYear;

    /**
     * 计划类型：0周，1月，2季度，3年度
     */
    @Column(name = "plan_type")
    private Integer planType;

    /**
     * 计划周期数
     */
    @Column(name = "plan_number")
    private Integer planNumber;

    /**
     * 发布日期
     */
    @Column(name = "publish_date")
    private Date publishDate;

    /**
     * 计划内容
     */
    @Column(name = "plan_content")
    private String planContent;

    /**
     * 计划成果
     */
    @Column(name = "plan_result")
    private String planResult;

    /**
     * 计划总结
     */
    @Column(name = "plan_summary")
    private String planSummary;
    
    /**
     * 周计划开始日期
     */
    @Column(name = "week_plan_start_date")
    private String weekPlanStartDate;
    
    /**
     * 周计划结束日期
     */
    @Column(name = "week_plan_end_date")
    private String weekPlanEndDate;

	public String getPlanTitle() {
		return planTitle;
	}

	public String getWeekPlanStartDate() {
		return weekPlanStartDate;
	}

	public void setWeekPlanStartDate(String weekPlanStartDate) {
		this.weekPlanStartDate = weekPlanStartDate;
	}

	public String getWeekPlanEndDate() {
		return weekPlanEndDate;
	}

	public void setWeekPlanEndDate(String weekPlanEndDate) {
		this.weekPlanEndDate = weekPlanEndDate;
	}

	public void setPlanTitle(String planTitle) {
		this.planTitle = planTitle;
	}

	public String getPlanYear() {
		return planYear;
	}

	public void setPlanYear(String planYear) {
		this.planYear = planYear;
	}

	public Integer getPlanType() {
		return planType;
	}

	public void setPlanType(Integer planType) {
		this.planType = planType;
	}

	public Integer getPlanNumber() {
		return planNumber;
	}

	public void setPlanNumber(Integer planNumber) {
		this.planNumber = planNumber;
	}

	public Date getPublishDate() {
		return publishDate;
	}

	public void setPublishDate(Date publishDate) {
		this.publishDate = publishDate;
	}

	public String getPlanContent() {
		return planContent;
	}

	public void setPlanContent(String planContent) {
		this.planContent = planContent;
	}

	public String getPlanResult() {
		return planResult;
	}

	public void setPlanResult(String planResult) {
		this.planResult = planResult;
	}

	public String getPlanSummary() {
		return planSummary;
	}

	public void setPlanSummary(String planSummary) {
		this.planSummary = planSummary;
	}

}