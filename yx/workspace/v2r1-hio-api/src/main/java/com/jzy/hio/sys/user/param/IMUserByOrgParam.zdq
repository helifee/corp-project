package com.jzy.hio.sys.user.param;

import com.jzy.hio.BaseTendParam;

/**
 * 组织id查询人员param
 *
 * @author kimchou
 * @date 2018/04/19 上午11:08
 */
public class IMUserByOrgParam extends BaseTendParam {
    /**
     * 组织机构ID
     */
    private String organizationId;
    /**
     * 是否包含子部门人员
     */
    private boolean isIncludeChild = false;


    public String getOrganizationId() {
        return organizationId;
    }

    public void setOrganizationId(String organizationId) {
        this.organizationId = organizationId;
    }

    public boolean isIncludeChild() {
        return isIncludeChild;
    }

    public void setIncludeChild(boolean includeChild) {
        isIncludeChild = includeChild;
    }
}
