package com.jzy.hio.sys.user.dto;

import java.util.List;

/**
 * HR 邀请用户返回结果
 * @author kimchou
 * @data 2018/5/9 下午4:17
 */
public class HRUserInviteDto {
    /**
     *  邀请状态：  part-部分邀请成功；success-全部邀请成功;fail-全部邀请失败
     */
    private String status;
    /**
     * 成功的数量
     */
    private int successCnt;
    /**
     * 失败的数量
     */
    private int failCnt;
    /**
     * 所有用户回传，以及邀请失败原因
     */
    private List<HRUserInviteMsgDto> users;

    public String getStatus() {
        return status;
    }

    public void setStatus(String status) {
        this.status = status;
    }

    public int getSuccessCnt() {
        return successCnt;
    }

    public void setSuccessCnt(int successCnt) {
        this.successCnt = successCnt;
    }

    public int getFailCnt() {
        return failCnt;
    }

    public void setFailCnt(int failCnt) {
        this.failCnt = failCnt;
    }


    public List<HRUserInviteMsgDto> getUsers() {
        return users;
    }

    public void setUsers(List<HRUserInviteMsgDto> users) {
        this.users = users;
    }
}
