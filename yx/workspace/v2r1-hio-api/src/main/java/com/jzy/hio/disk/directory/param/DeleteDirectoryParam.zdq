package com.jzy.hio.disk.directory.param;

import org.hibernate.validator.constraints.NotBlank;

/**
 * 
 * 删除目录
 *
 * <p>FileName：DeleteDirectoryParam</p>
 * <p>Author：lvjie</p>
 * <p>Date：2018年4月18日 下午7:52:45</p>
 */
public class DeleteDirectoryParam extends UserInfoParam {
	
	/**
	 * 目录ID
	 */
	@NotBlank(message="目录不能为空")
	private String fileId;
	
	/**
	 * 用户租户ID
	 */
	@NotBlank(message="租户不能为空")
	private String tendId;

	public String getFileId() {
		return fileId;
	}

	public void setFileId(String fileId) {
		this.fileId = fileId;
	}

	public String getTendId() {
		return tendId;
	}

	public void setTendId(String tendId) {
		this.tendId = tendId;
	}
	
}
