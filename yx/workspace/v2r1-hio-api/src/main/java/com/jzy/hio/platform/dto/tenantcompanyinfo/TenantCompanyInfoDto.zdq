package com.jzy.hio.platform.dto.tenantcompanyinfo;

import com.fasterxml.jackson.annotation.JsonProperty;
import lombok.Data;

import javax.validation.constraints.Size;
import java.util.Date;
@Data
public class TenantCompanyInfoDto {
    @JsonProperty("sid")
	private String id;
	@Size(max = 50,message = "企业名称长度最长50个字符")
    private String name;    //团队名称

    private String industry;    //行业

    private String industryChild;//行业子节点

    private String type;    //企业类型

    private String personnelScale;    //人员规模

    private String province;    //所在省份

    private String city;    //所在城市
    private String address;    //企业地址
    private String postcode;//邮政编码
    private String fax;    //企业传真
    private String url;    //企业网址

    private String foundingTime;    //成立时间
    private String logo;    //企业logo

    private String tendId;    //租户id
	private String invoiceTitle;	//发票抬头
	
	private int corpAuthentication;	//企业认证，0：未认证，1：已认证
	
	private Date createDate; // 创建时间
	/**
	 * 行业对象
	 */
	private IndustryDto industryDto; 
	/**
	 * 企业类型对象
	 */
	private TypeDto typeDto;
	/**
	 * 人员规模对象
	 */
	private PersonnelScaleDto personnelScaleDto;
	/**
	 * 省份对象
	 */
	private ProvinceDto privinceDto;
	//纳税人识别号
	private String taxpayerNumber;	
}
