package com.jzy.hio.sys.user.dto;

/**
 * 通过组织查询人员返回bean
 *
 * @author kimchou
 * @date 2018/4/19 上午11:26
 */
public class IMOrgUserDto {
    /**
     * 人员OA ID
     */
    private String sid;
    /**
     * 人员IM userId
     */
    private String imUserId;
    /**
     * 人员名称
     */
    private String name;
    /**
     * 组织机构ID
     */
    private String organizationId;
    /**
     * 组织名称
     */
    private String organizationName;
    /**
     * 组织机构前缀名称
     */
    private String organizationPrefixName;
    /**
     * 职务ID
     */
    private String positionId;
    /**
     * 职务名称
     */
    private String positionName;
    /**
     * 租户ID
     */
    private String tendId;
    /**
     * 是否是主部门
     */
    private boolean isDefault;
    
    /**
     * 在线状态
     */
    private String statusId;

    public String getStatusId() {
		return statusId;
	}

	public void setStatusId(String statusId) {
		this.statusId = statusId;
	}

	public String getSid() {
        return sid;
    }

    public void setSid(String sid) {
        this.sid = sid;
    }

    public String getImUserId() {
        return imUserId;
    }

    public void setImUserId(String imUserId) {
        this.imUserId = imUserId;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getOrganizationId() {
        return organizationId;
    }

    public void setOrganizationId(String organizationId) {
        this.organizationId = organizationId;
    }

    public String getOrganizationName() {
        return organizationName;
    }

    public void setOrganizationName(String organizationName) {
        this.organizationName = organizationName;
    }

    public String getOrganizationPrefixName() {
        return organizationPrefixName;
    }

    public void setOrganizationPrefixName(String organizationPrefixName) {
        this.organizationPrefixName = organizationPrefixName;
    }

    public String getTendId() {
        return tendId;
    }

    public void setTendId(String tendId) {
        this.tendId = tendId;
    }

    public String getPositionId() {
        return positionId;
    }

    public void setPositionId(String positionId) {
        this.positionId = positionId;
    }

    public String getPositionName() {
        return positionName;
    }

    public void setPositionName(String positionName) {
        this.positionName = positionName;
    }

    public boolean isDefault() {
        return isDefault;
    }

    public void setDefault(boolean aDefault) {
        isDefault = aDefault;
    }
}
