package com.jzy.hio.sys.user.param;

import org.hibernate.validator.constraints.NotBlank;

import javax.validation.constraints.Size;

/**
 * 
 * @author kimchou
 * @data 2018/4/24 下午5:36
 */
public class UserPositionParam {
    /**
     * 兼职部门ID
     */
    @NotBlank(message = "请选择部门")
    private String organizationId;
    /**
     * 职务名称
     */
    @Size(max = 20,message = "职务不能超过20个字符")
    private String postName;
    /**
     * 用户职务ID，可为NULL，NULL值说明是新增
     */
    private String userPositionId;
    /**
     * 用户ID,Hr新增需要该字段
     */
    private String userId;
    /**
     * 是否是更新职务（针对HR导入修改人员主职务信息）
     */
    private boolean isUpdate;

    public UserPositionParam(){
        super();
    }

    public UserPositionParam(String organizationId,String postName,String userPositionId){
        this.organizationId = organizationId;
        this.postName = postName;
        this.userPositionId =userPositionId;
    }

    public String getOrganizationId() {
        return organizationId;
    }

    public void setOrganizationId(String organizationId) {
        this.organizationId = organizationId;
    }

    public String getPostName() {
        return postName;
    }

    public void setPostName(String postName) {
        this.postName = postName;
    }


    public String getUserPositionId() {
        return userPositionId;
    }

    public void setUserPositionId(String userPositionId) {
        this.userPositionId = userPositionId;
    }

    public String getUserId() {
        return userId;
    }

    public void setUserId(String userId) {
        this.userId = userId;
    }

    public boolean isUpdate() {
        return isUpdate;
    }

    public void setUpdate(boolean update) {
        isUpdate = update;
    }
}
