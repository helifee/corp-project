package com.jzy.hio.platform.message.param;
/**
 * 发送消息类型
 * @author wufei
 * @create 2018-05-16 9:26
 **/
public enum  SendMsgTypeEnum {
    TEXT("text", "文本类型，"),
    CMD("cmd", "透传类型");

    private String type;

    private String des;

    private SendMsgTypeEnum(String type, String des) {
        this.type = type;
        this.des = des;
    }

    public String getValue() {
        return this.type;
    }

    public String getDes() {
        return this.des;
    }
}
