package com.jzy.hio.sys.organization.dto;

import lombok.Data;

/**
 * 网盘通过角色ID，组织ID，人员ID获取名称 Dto
 *
 * @author kimchou
 * @date 2018-03-24 11:00
 **/
@Data
public class SkyDriveDto {
    /**
     * organization表示组织，role表示角色，user表示用户
     */
    private String type;
    /**
     * 主键ID
     */
    private String id;
    /**
     * 名称
     */
    private String name;
    /**
     * 前缀名称-针对组织
     */
    private String prefixName;
    /**
     * 人员手机号-针对查询人员
     */
    private String mobile;
    /**
     * 删除状态-针对查询人员
     * {@link com.jzy.hio.base.BaseConstants.MYSQL_DELFLAG_NORMAL}
     * {@link com.jzy.hio.base.BaseConstants.MYSQL_DELFLAG_DELETED}
     */
    private Integer delflag;
    /**
     * 人员工作状态-针对查询人员
     * {@link com.jzy.hio.base.BaseConstants.UserWorkEnu}
     */
    private Integer workStatus;
    /**
     * 查询人员-的imUserId-针对查询人员
     */
    private String imUserId;

}
