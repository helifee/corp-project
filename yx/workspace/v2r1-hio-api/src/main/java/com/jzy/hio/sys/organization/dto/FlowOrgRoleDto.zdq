package com.jzy.hio.sys.organization.dto;

/**
 * 组织 角色 Dto
 *
 * @author kimchou
 * @create 2018-03-30 18:00
 **/
public class FlowOrgRoleDto {
    /**
     * 名称
     */
    private String name;
    /**
     * 类型 organization表示组织，role表示角色
     */
    private String type;
    /**
     * id
     */
    private String id;

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getType() {
        return type;
    }

    public void setType(String type) {
        this.type = type;
    }

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }
}
