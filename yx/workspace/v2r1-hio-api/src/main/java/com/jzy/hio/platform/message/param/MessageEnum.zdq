	TOREAD("10002_0","抄送"),
	CLOCKIN("10003", "考勤"),//签到提醒  签退提醒
	NOTICE("10004", "公告"),
	MEETING("10005", "会议"),
	TASK("10006","任务"),
	SMS("10007","短信"),
	MAIL("10008","站内信"),
	FRIENDINVITATION("10009","好友邀请"),
	WORKINVITATION("10010","同事邀请"),
	OUTWORKINVITATION("10011","外部联系人邀请"),
	SECRETEDIT("10012","个人隐私修改"),
	START("10013","特别关注"),
	SCHEDULE("10014", "日程"),
	NEWS("10015", "新闻"),
	HAVEREAD("10016","已办"),
	BROADCAST("10017","广播"),
	REVOCATION("10018", "撤销"),
	PERSONNEL_MATTERS("10019", "人事"),
	TRAIN("10020","培训"),
	ADDFRIEND("10021","对方接受您的好友邀请"),
	ADDBLANK("10022","对方将您拉入黑名单"),
	DELFRIEND("10037","对方将您删除好友"),
	DELBLANK("10023","对方将您拉出黑名单"),
	PROJECT("10024","项目"),
	REMOVETEND("10025","公司解散"),
	MODIFYOWERINFO("10026","修改个人信息"),
	ADDCOMPANY("10027","用户接受贵公司的邀请"),
	RUNNING("10028","动动消息"),
