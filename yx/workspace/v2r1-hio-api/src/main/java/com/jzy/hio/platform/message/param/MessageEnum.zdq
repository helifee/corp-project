	FAlias("10042","添加好友备注");
	private String value;

	private String des;
	
	public static  String  queryDes(String value) {
		
		MessageEnum[] values = MessageEnum.values();
		for (MessageEnum messageEnum : values) {
			if(messageEnum.getValue().equals(value)) {
				
				return messageEnum.getDes();
			}
		}
		return value;
	}
	
	private MessageEnum(String value, String des) {
		this.value = value;
		this.des = des;
	}

 
	public String getDes() {
		return this.des;
	}

	public String getValue() {
		return value;
	}

	public void setValue(String value) {
		this.value = value;
	}

	public void setDes(String des) {
		this.des = des;
	}

}
