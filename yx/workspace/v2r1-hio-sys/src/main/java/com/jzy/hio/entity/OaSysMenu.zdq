package com.jzy.hio.entity;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Table;

import com.jzy.hio.base.BaseConstants;
import com.jzy.hio.base.entity.BaseEntity;

import lombok.Data;
import lombok.EqualsAndHashCode;

/**
 * 菜单
 * 
 * @author wt.coffee
 * @date 2018年5月8日下午5:14:35
 */
@Data
@EqualsAndHashCode(callSuper = false)
@Table(name = BaseConstants.MYSQL_TABLE_MENU)
public class OaSysMenu extends BaseEntity implements Serializable {
	private static final long serialVersionUID = 1L;

	@Column(name = "app_id", length = 32)
	private String appId;

	@Column(length = 100)
	private String code;

	@Column(length = 500)
	private String icon;

	@Column(length = 200)
	private String name;

	@Column(length = 2)
	private String openmode;

	@Column(name = "permission_type")
	private String permissionType;

	@Column(name = "parent_id", length = 32)
	private String parentId;

	@Column(name = "prefix_id", length = 1000)
	private String prefixId;

	@Column(name = "prefix_name", length = 1000)
	private String prefixName;

	@Column(name = "prefix_sort", length = 1000)
	private String prefixSort;

	@Column(length = 2000)
	private String remark;

	@Column(length = 256)
	private String reserve1;

	@Column(length = 256)
	private String reserve2;

	@Column(length = 256)
	private String reserve3;

	@Column(length = 256)
	private String reserve4;

	@Column(length = 256)
	private String reserve5;

	private Integer sort;

	@Column(length = 2)
	private String status;

	@Column(length = 1000)
	private String url;
}