package com.jzy.hio.sys.role.dto;

import java.util.List;

/**
 * 角色相关的整个信息
 * 
 * @author wt.coffee<br/>
 *         2018年3月26日下午1:46:36
 */
public class RoleDetailDto {
	private RoleDto role;
	private List<RoleUserDto> users;
	private List<RolePermissionTreeDto> permissions;

	public RoleDto getRole() {
		return role;
	}

	public void setRole(RoleDto role) {
		this.role = role;
	}

	public List<RoleUserDto> getUsers() {
		return users;
	}

	public void setUsers(List<RoleUserDto> users) {
		this.users = users;
	}

	public List<RolePermissionTreeDto> getPermissions() {
		return permissions;
	}

	public void setPermissions(List<RolePermissionTreeDto> permissions) {
		this.permissions = permissions;
	}
}
