package com.jzy.hio.entity;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Table;

import com.jzy.hio.base.BaseConstants;
import com.jzy.hio.base.entity.BaseEntity;

import lombok.Data;
import lombok.EqualsAndHashCode;

/**
 * 角色定义
 * 
 * @author wt.coffee
 * @date 2018年4月21日下午4:17:43
 */
@Table(name = BaseConstants.MYSQL_TABLE_ROLE)
@Data
@EqualsAndHashCode(callSuper = false)
public class OaSysRole extends BaseEntity implements Serializable {
	private static final long serialVersionUID = 1L;

	@Column(length = 100)
	private String code;

	@Column(length = 500)
	private String icon;

	@Column(length = 200)
	private String name;

	@Column(name = "prefix_id", length = 1000)
	private String prefixId;

	@Column(name = "prefix_name", length = 1000)
	private String prefixName;

	@Column(name = "prefix_sort", length = 1000)
	private String prefixSort;

	@Column(length = 500)
	private String remark;

	/**
	 * 0 默认、1不可见(即无法修改)
	 */
	private Integer type;

	@Column(length = 256)
	private String reserve1;

	@Column(length = 256)
	private String reserve2;

	@Column(length = 256)
	private String reserve3;

	@Column(length = 256)
	private String reserve4;

	@Column(length = 256)
	private String reserve5;

	private Integer sort;

	@Column(length = 2)
	private String status;

	public OaSysRole() {
	}

}