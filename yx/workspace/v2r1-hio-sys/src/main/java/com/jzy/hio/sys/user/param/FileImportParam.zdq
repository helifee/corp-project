package com.jzy.hio.sys.user.param;

import org.hibernate.validator.constraints.NotBlank;
import org.springframework.web.multipart.MultipartFile;

import javax.validation.constraints.NotNull;

/**
 * @author kimchou
 * @create 2018-03-25 18:26
 * @desc
 **/
public class FileImportParam {
    @NotBlank(message = "请选择部门导入")
    private String organizationId;
    @NotNull(message = "请选择excel文件导入")
    private MultipartFile resource;

    public String getOrganizationId() {
        return organizationId;
    }

    public void setOrganizationId(String organizationId) {
        this.organizationId = organizationId;
    }

    public MultipartFile getResource() {
        return resource;
    }

    public void setResource(MultipartFile resource) {
        this.resource = resource;
    }
}
