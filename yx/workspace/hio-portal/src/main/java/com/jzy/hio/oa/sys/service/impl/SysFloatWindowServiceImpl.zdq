package com.jzy.hio.oa.sys.service.impl;

import java.util.List;
import java.util.Map;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.jzy.hio.base.service.impl.BaseServiceImpl;
import com.jzy.hio.oa.sys.entity.SysFloatWindow;
import com.jzy.hio.oa.sys.mapper.SysFloatWindowDao;
import com.jzy.hio.oa.sys.service.SysFloatWindowService;

/**
 * 
 * @author coffee<br/>
 * 		2018年1月5日上午10:45:14
 */
@Service
public class SysFloatWindowServiceImpl extends BaseServiceImpl<String, SysFloatWindow> implements SysFloatWindowService {

	@Autowired
	private SysFloatWindowDao sysFloatWindowDao;

	/**
	 * 模糊查询
	 */
	@Override
	public List<SysFloatWindow> fuzzySearch(Map map) {
		return sysFloatWindowDao.fuzzySearch(map);
	}

	/**
	 * 校验编号唯一
	 */
	@Override
	public Integer checkSerialNo(SysFloatWindow param) {
		final SysFloatWindow floatWindow = sysFloatWindowDao.getFloatWindowBySerialNo(param);
		return floatWindow == null ? 1 : 0;
	}

}
