package com.jzy.hio.oa.sys.entity;

import javax.persistence.Column;
import javax.persistence.Table;
import com.jzy.hio.base.entity.BaseEntity;

/**
 * @author admin
 * 
 *         常用工具表
 */
@Table(name = "OA_SYS_COMMON_TOOLS")
public class SysCommonTools extends BaseEntity {

	/**
	 * 序号
	 */
	@Column(name = "sort")
	private Integer sort;

	/**
	 * 名称
	 */
	@Column(name = "name")
	private String name;

	/**
	 * 图标
	 */
	@Column(name = "icon")
	private String icon;

	/**
	 * 链接
	 */
	@Column(name = "url")
	private String url;

	/**
	 * 编号
	 */
	@Column(name = "code")
	private String code;

	/**
	 * 图片
	 */
	@Column(name = "pic")
	private String pic;

	public Integer getSort() {
		return sort;
	}

	public void setSort(Integer sort) {
		this.sort = sort;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getIcon() {
		return icon;
	}

	public void setIcon(String icon) {
		this.icon = icon;
	}

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getPic() {
		return pic;
	}

	public void setPic(String pic) {
		this.pic = pic;
	}

}
