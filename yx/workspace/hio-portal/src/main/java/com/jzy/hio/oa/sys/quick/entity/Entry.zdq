package com.jzy.hio.oa.sys.quick.entity;

import javax.persistence.Column;
import javax.persistence.Table;
import com.jzy.hio.base.entity.BaseEntity;

/**
 * @author admin
 * 
 *         快速入口表
 */
@Table(name = "OA_SYS_QUICK_ENTRY")
public class Entry extends BaseEntity {

	/**
	 * 编号
	 */
	@Column(name = "code")
	private String code;

	/**
	 * 名称
	 */
	@Column(name = "name")
	private String name;

	/**
	 * 父类ID
	 */
	@Column(name = "parent_id")
	private String parentId;

	/**
	 * 图标
	 */
	@Column(name = "icon")
	private String linkIcon;

	/**
	 * 是否内部链接
	 */
	@Column(name = "is_inner")
	private Integer isInner;

	/**
	 * 资源id
	 */
	@Column(name = "resource_id")
	private String resourceId;

	/**
	 * 资源名称
	 */
	@Column(name = "resource_name")
	private String resourceName;

	/**
	 * 地址
	 */
	@Column(name = "url")
	private String url;

	/**
	 * 状态
	 */
	@Column(name = "status")
	private Integer status;

	/**
	 * 序号
	 */
	@Column(name = "sort")
	private Integer sort;

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getParentId() {
		return parentId;
	}

	public void setParentId(String parentId) {
		this.parentId = parentId;
	}

	public String getLinkIcon() {
		return linkIcon;
	}

	public void setIcon(String linkIcon) {
		this.linkIcon = linkIcon;
	}

	public Integer getIsInner() {
		return isInner;
	}

	public void setIsInner(Integer isInner) {
		this.isInner = isInner;
	}

	public String getResourceId() {
		return resourceId;
	}

	public void setResourceId(String resourceId) {
		this.resourceId = resourceId;
	}

	public String getResourceName() {
		return resourceName;
	}

	public void setResourceName(String resourceName) {
		this.resourceName = resourceName;
	}

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public Integer getSort() {
		return sort;
	}

	public void setSort(Integer sort) {
		this.sort = sort;
	}

}
