package com.jzy.hio.oa.servey.entity;

import javax.persistence.Column;
import javax.persistence.Table;
import com.jzy.hio.base.entity.BaseEntity;

/**
 * @author admin
 * 
 *         调查问卷题目表
 */
@Table(name = "OA_SERVEY_QUESTION")
public class ServeyQuestion extends BaseEntity {

	/**
	 * 问卷ID
	 */
	@Column(name = "servey_id")
	private String serveyId;

	/**
	 * 题目内容
	 */
	@Column(name = "question")
	private String question;

	/**
	 * 题目序号
	 */
	@Column(name = "question_num")
	private String questionNum;

	/**
	 * 题目提示
	 */
	@Column(name = "question_tip")
	private String questionTip;

	/**
	 * 题目类型编码
	 */
	@Column(name = "question_type_code")
	private String questionTypeCode;

	/**
	 * 问题是否必选
	 */
	@Column(name = "question_required")
	private String questionRequired;

	/**
	 * 分数
	 */
	@Column(name = "question_score")
	private String questionScore;

	private int countPartys;

	private int countReployQuestions;

	public int getCountPartys() {
		return countPartys;
	}

	public void setCountPartys(int countPartys) {
		this.countPartys = countPartys;
	}

	public int getCountReployQuestions() {
		return countReployQuestions;
	}

	public void setCountReployQuestions(int countReployQuestions) {
		this.countReployQuestions = countReployQuestions;
	}

	public String getServeyId() {
		return serveyId;
	}

	public void setServeyId(String serveyId) {
		this.serveyId = serveyId;
	}

	public String getQuestion() {
		return question;
	}

	public void setQuestion(String question) {
		this.question = question;
	}

	public String getQuestionTip() {
		return questionTip;
	}

	public void setQuestionTip(String questionTip) {
		this.questionTip = questionTip;
	}

	public String getQuestionTypeCode() {
		return questionTypeCode;
	}

	public void setQuestionTypeCode(String questionTypeCode) {
		this.questionTypeCode = questionTypeCode;
	}

	public String getQuestionNum() {
		return questionNum;
	}

	public void setQuestionNum(String questionNum) {
		this.questionNum = questionNum;
	}

	public String getQuestionRequired() {
		return questionRequired;
	}

	public void setQuestionRequired(String questionRequired) {
		this.questionRequired = questionRequired;
	}

	public String getQuestionScore() {
		return questionScore;
	}

	public void setQuestionScore(String questionScore) {
		this.questionScore = questionScore;
	}

}
