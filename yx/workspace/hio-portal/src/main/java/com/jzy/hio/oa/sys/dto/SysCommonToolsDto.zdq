package com.jzy.hio.oa.sys.dto;

import com.jzy.hio.base.dto.BaseDto;





/**
 * @author admin
 * 
 *
 */
public class SysCommonToolsDto extends BaseDto{

		
	//序号
	private Integer sort;
    
  		
	//名称
	private String name;
    
  		
	//图标
	private String icon;
    
  		
	//链接
	private String url;
    
	private String isDelPic;
  		
	//编号
	private String code;
    
	private byte[] pic;	
		
	public Integer getSort() {
		return sort;
	}
	public void setSort(Integer sort) {
		this.sort = sort;
	}
    
  		
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
    
  		
	public String getIcon() {
		return icon;
	}
	public void setIcon(String icon) {
		this.icon = icon;
	}
    
  		
	public String getUrl() {
		return url;
	}
	public void setUrl(String url) {
		this.url = url;
	}
    
  		
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public byte[] getPic() {
		return pic;
	}
	public void setPic(byte[] pic) {
		this.pic = pic;
	}
	public String getIsDelPic() {
		return isDelPic;
	}
	public void setIsDelPic(String isDelPic) {
		this.isDelPic = isDelPic;
	}
  		
}
