package com.jzy.hio.oa.office.entity;

import javax.persistence.Column;
import javax.persistence.Table;
import com.jzy.hio.base.entity.BaseEntity;

/**
 * @author wangw
 *
 *         办公用品结存表
 */

@Table(name = "OA_OFFICE_INFO")
public class OfficeInfo extends BaseEntity {

	/**
	 * 单位
	 */
	@Column(name = "accout_id")
	private String accoutId;

	/**
	 * 单价
	 */
	@Column(name = "price")
	private String price;

	/**
	 * 分类ID
	 */
	@Column(name = "stock_house_id")
	private String stockHouseId;

	/**
	 * 用品编号
	 */
	@Column(name = "stock_num")
	private String stockNum;

	/**
	 * 名称
	 */
	@Column(name = "stock_name")
	private String stockName;

	/**
	 * 规格
	 */
	@Column(name = "stock_specifications")
	private String stockSpecifications;

	/**
	 * 计量单位
	 */
	@Column(name = "metering_unit")
	private String meteringUnit;

	/**
	 * 品牌
	 */
	@Column(name = "stock_brand")
	private String stockBrand;

	/**
	 * 类别ID
	 */
	@Column(name = "house_id")
	private String houseId;

	/**
	 * 库存数量
	 */
	@Column(name = "stock_count")
	private String stockCount;

	/**
	 * 购置日期
	 */
	@Column(name = "buy_date")
	private String buyDate;

	/**
	 * 最低库存设置
	 */
	@Column(name = "min_count")
	private String minCount;

	/**
	 * 是否预警
	 */
	@Column(name = "warn_flag")
	private String warnFlag;

	/**
	 * 备注
	 */
	@Column(name = "remark")
	private String remark;

	/**
	 * 预借后数量
	 */
	@Column(name = "out_stock_count")
	private String outStockCount;

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getAccoutId() {
		return accoutId;
	}

	public void setAccoutId(String accoutId) {
		this.accoutId = accoutId;
	}

	public String getStockHouseId() {
		return stockHouseId;
	}

	public void setStockHouseId(String stockHouseId) {
		this.stockHouseId = stockHouseId;
	}

	public String getStockNum() {
		return stockNum;
	}

	public void setStockNum(String stockNum) {
		this.stockNum = stockNum;
	}

	public String getStockName() {
		return stockName;
	}

	public void setStockName(String stockName) {
		this.stockName = stockName;
	}

	public String getStockSpecifications() {
		return stockSpecifications;
	}

	public void setStockSpecifications(String stockSpecifications) {
		this.stockSpecifications = stockSpecifications;
	}

	public String getMeteringUnit() {
		return meteringUnit;
	}

	public void setMeteringUnit(String meteringUnit) {
		this.meteringUnit = meteringUnit;
	}

	public String getStockBrand() {
		return stockBrand;
	}

	public void setStockBrand(String stockBrand) {
		this.stockBrand = stockBrand;
	}

	public String getStockCount() {
		return stockCount;
	}

	public void setStockCount(String stockCount) {
		this.stockCount = stockCount;
	}

	public String getBuyDate() {
		return buyDate;
	}

	public void setBuyDate(String buyDate) {
		this.buyDate = buyDate;
	}

	public String getHouseId() {
		return houseId;
	}

	public void setHouseId(String houseId) {
		this.houseId = houseId;
	}

	public String getMinCount() {
		return minCount;
	}

	public void setMinCount(String minCount) {
		this.minCount = minCount;
	}

	public String getWarnFlag() {
		return warnFlag;
	}

	public void setWarnFlag(String warnFlag) {
		this.warnFlag = warnFlag;
	}

	public String getPrice() {
		return price;
	}

	public void setPrice(String price) {
		this.price = price;
	}

	public String getOutStockCount() {
		return outStockCount;
	}

	public void setOutStockCount(String outStockCount) {
		this.outStockCount = outStockCount;
	}

}
