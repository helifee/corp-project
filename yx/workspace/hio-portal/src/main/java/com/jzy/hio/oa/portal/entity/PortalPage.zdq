package com.jzy.hio.oa.portal.entity;

import javax.persistence.Column;
import javax.persistence.Table;
import com.jzy.hio.base.entity.BaseEntity;

/**
 * @author admin
 * 
 *         站点表
 */
@Table(name = "OA_PORTAL_PAGE")
public class PortalPage extends BaseEntity {

	/**
	 * 站点编码
	 */
	@Column(name = "portal_page_code")
	private String portalPageCode;

	/**
	 * 优先级
	 */
	@Column(name = "priority")
	private Integer priority;

	/**
	 * 站点名称
	 */
	@Column(name = "portal_page_name")
	private String portalPageName;

	/**
	 * 站点描述
	 */
	@Column(name = "portal_page_desc")
	private String portalPageDesc;

	/**
	 * 引用站点（源站点）ID
	 */
	@Column(name = "origin_portal_page_id")
	private String originPortalPageId;

	/**
	 * 站点拥有人
	 */
	@Column(name = "portal_page_owner")
	private String portalPageOwner;
	/**
	 * 站点logo
	 */
	@Column(name = "portal_page_logo")
	private String portalPageLogo;
	/**
	 * 启用禁用状态:0:禁用;1:启用;
	 */
	@Column(name = "status")
	private Boolean status;

	public String getPortalPageCode() {
		return portalPageCode;
	}

	public void setPortalPageCode(String portalPageCode) {
		this.portalPageCode = portalPageCode;
	}

	public Integer getPriority() {
		return priority;
	}

	public void setPriority(Integer priority) {
		this.priority = priority;
	}

	public String getPortalPageName() {
		return portalPageName;
	}

	public void setPortalPageName(String portalPageName) {
		this.portalPageName = portalPageName;
	}

	public String getPortalPageDesc() {
		return portalPageDesc;
	}

	public void setPortalPageDesc(String portalPageDesc) {
		this.portalPageDesc = portalPageDesc;
	}

	public String getOriginPortalPageId() {
		return originPortalPageId;
	}

	public void setOriginPortalPageId(String originPortalPageId) {
		this.originPortalPageId = originPortalPageId;
	}

	public String getPortalPageOwner() {
		return portalPageOwner;
	}

	public void setPortalPageOwner(String portalPageOwner) {
		this.portalPageOwner = portalPageOwner;
	}

	public String getPortalPageLogo() {
		return portalPageLogo;
	}

	public void setPortalPageLogo(String portalPageLogo) {
		this.portalPageLogo = portalPageLogo;
	}

	public Boolean getStatus() {
		return status;
	}

	public void setStatus(Boolean status) {
		this.status = status;
	}
}
