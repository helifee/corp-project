package com.jzy.hio.oa.schedule.entity;

import javax.persistence.Column;
import javax.persistence.Table;

import com.jzy.hio.base.entity.BaseEntity;

/**
 * @author admin
 * 
 *         任务包分发
 */
@Table(name = "OA_SCHEDULE_TASK_PACKAGE_DISPATCH")
public class TaskPackageDispatch extends BaseEntity {

	/**
	 * 任务包ID
	 */
	@Column(name = "task_package_id")
	private String taskPackageId;

	/**
	 * 日程ID
	 */
	@Column(name = "work_schedule_id")
	private String workScheduleId;

	/**
	 * 内容
	 */
	@Column(name = "content")
	private String content;

	/**
	 * 责任人
	 */
	@Column(name = "duty_user")
	private String dutyUser;

	/**
	 * 责任人ID
	 */
	@Column(name = "duty_user_id")
	private String dutyUserId;

	/**
	 * 责任部门
	 */
	@Column(name = "duty_dept")
	private String dutyDept;

	/**
	 * 责任部门ID
	 */
	@Column(name = "duty_dept_id")
	private String dutyDeptId;

	/**
	 * 预期完成日期
	 */
	@Column(name = "expect_complete_date")
	private String expectCompleteDate;

	/**
	 * 实际完成日期
	 */
	@Column(name = "actual_complete_date")
	private String actualCompleteDate;

	/**
	 * 备注
	 */
	@Column(name = "remark")
	private String remark;

	/**
	 * 汇报说明
	 */
	@Column(name = "report")
	private String report;

	/**
	 * 汇报状态
	 */
	@Column(name = "report_status")
	private String reportStatus;

	public String getTaskPackageId() {
		return taskPackageId;
	}

	public void setTaskPackageId(String taskPackageId) {
		this.taskPackageId = taskPackageId;
	}

	public String getWorkScheduleId() {
		return workScheduleId;
	}

	public void setWorkScheduleId(String workScheduleId) {
		this.workScheduleId = workScheduleId;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public String getDutyUser() {
		return dutyUser;
	}

	public void setDutyUser(String dutyUser) {
		this.dutyUser = dutyUser;
	}

	public String getDutyUserId() {
		return dutyUserId;
	}

	public void setDutyUserId(String dutyUserId) {
		this.dutyUserId = dutyUserId;
	}

	public String getDutyDept() {
		return dutyDept;
	}

	public void setDutyDept(String dutyDept) {
		this.dutyDept = dutyDept;
	}

	public String getDutyDeptId() {
		return dutyDeptId;
	}

	public void setDutyDeptId(String dutyDeptId) {
		this.dutyDeptId = dutyDeptId;
	}

	public String getExpectCompleteDate() {
		return expectCompleteDate;
	}

	public void setExpectCompleteDate(String expectCompleteDate) {
		this.expectCompleteDate = expectCompleteDate;
	}

	public String getActualCompleteDate() {
		return actualCompleteDate;
	}

	public void setActualCompleteDate(String actualCompleteDate) {
		this.actualCompleteDate = actualCompleteDate;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getReport() {
		return report;
	}

	public void setReport(String report) {
		this.report = report;
	}

	public String getReportStatus() {
		return reportStatus;
	}

	public void setReportStatus(String reportStatus) {
		this.reportStatus = reportStatus;
	}

}
