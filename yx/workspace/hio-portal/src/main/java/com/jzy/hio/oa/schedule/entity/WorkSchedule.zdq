package com.jzy.hio.oa.schedule.entity;

import javax.persistence.Column;
import javax.persistence.Table;
import com.jzy.hio.base.entity.BaseEntity;

/**
 * @author admin
 * 
 *         任务表
 */
@Table(name = "OA_SCHEDULE_WORK_SCHEDULE")
public class WorkSchedule extends BaseEntity {

	/**
	 * 是否周期性事件
	 */
	@Column(name = "period_proceeding")
	private String periodProceeding;

	/**
	 * 内容
	 */
	@Column(name = "content")
	private String content;

	/**
	 * 类型
	 */
	@Column(name = "type")
	private String type;//

	/**
	 * 重复频率
	 */
	@Column(name = "repetition_frequency")
	private String repetitionFrequency;

	/**
	 * 任务开始时间
	 */
	@Column(name = "begin_time")
	private String beginTime;

	/**
	 * 任务结束时间
	 */
	@Column(name = "end_time")
	private String endTime;

	/**
	 * 周期开始时间
	 */
	@Column(name = "period_begin_time")
	private String periodBeginTime;

	/**
	 * 周期结束时间
	 */
	@Column(name = "period_end_time")
	private String periodEndTime;

	/**
	 * 周期任务数
	 */
	@Column(name = "period_task_num")
	private String periodTaskNum;

	/**
	 * 任务归属人
	 */
	@Column(name = "task_owner")
	private String taskOwner;

	/**
	 * 任务归属人ID
	 */
	@Column(name = "task_owner_id")
	private String taskOwnerId;

	/**
	 * 频率类型
	 */
	@Column(name = "frequency_type")
	private String frequencyType;

	/**
	 * 状态
	 */
	@Column(name = "status")
	private String status;//
	/**
	 * 来源
	 */
	@Column(name = "source")
	private String source;
	/**
	 * 关联业务id
	 */
	@Column(name = "business_id")
	private String businessId;

	public String getBusinessId() {
		return businessId;
	}

	public void setBusinessId(String businessId) {
		this.businessId = businessId;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public String getSource() {
		return source;
	}

	public void setSource(String source) {
		this.source = source;
	}

	public String getPeriodProceeding() {
		return periodProceeding;
	}

	public void setPeriodProceeding(String periodProceeding) {
		this.periodProceeding = periodProceeding;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getRepetitionFrequency() {
		return repetitionFrequency;
	}

	public void setRepetitionFrequency(String repetitionFrequency) {
		this.repetitionFrequency = repetitionFrequency;
	}

	public String getBeginTime() {
		return beginTime;
	}

	public void setBeginTime(String beginTime) {
		this.beginTime = beginTime;
	}

	public String getEndTime() {
		return endTime;
	}

	public void setEndTime(String endTime) {
		this.endTime = endTime;
	}

	public String getPeriodBeginTime() {
		return periodBeginTime;
	}

	public void setPeriodBeginTime(String periodBeginTime) {
		this.periodBeginTime = periodBeginTime;
	}

	public String getPeriodEndTime() {
		return periodEndTime;
	}

	public void setPeriodEndTime(String periodEndTime) {
		this.periodEndTime = periodEndTime;
	}

	public String getPeriodTaskNum() {
		return periodTaskNum;
	}

	public void setPeriodTaskNum(String periodTaskNum) {
		this.periodTaskNum = periodTaskNum;
	}

	public String getTaskOwner() {
		return taskOwner;
	}

	public void setTaskOwner(String taskOwner) {
		this.taskOwner = taskOwner;
	}

	public String getTaskOwnerId() {
		return taskOwnerId;
	}

	public void setTaskOwnerId(String taskOwnerId) {
		this.taskOwnerId = taskOwnerId;
	}

	public String getFrequencyType() {
		return frequencyType;
	}

	public void setFrequencyType(String frequencyType) {
		this.frequencyType = frequencyType;
	}

}
