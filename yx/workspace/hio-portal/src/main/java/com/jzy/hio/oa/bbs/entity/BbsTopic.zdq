package com.jzy.hio.oa.bbs.entity;

import javax.persistence.Column;
import javax.persistence.Table;
import javax.persistence.Transient;

import com.jzy.hio.base.entity.BaseEntity;

import javax.persistence.Entity;
import java.util.Date;

/**
 * @author admin
 * 
 *         主题（帖子）
 */
@Entity
@Table(name = "OA_BBS_TOPIC")
public class BbsTopic extends BaseEntity {

	/**
	 * 版块ID
	 */
	@Column(name = "forum_id")
	private String forumId;

	/**
	 * 主题标题
	 */
	@Column(name = "title")
	private String title;

	/**
	 * 主题内容
	 */
	@Column(name = "content")
	private String content;

	/**
	 * 点击量
	 */
	@Column(name = "click_num")
	private Integer clickNum;

	/**
	 * 精华主题（帖子）0:否；1:是；
	 */
	@Column(name = "essence")
	private Boolean essence = Boolean.FALSE;

	/**
	 * 引用主题来源
	 */
	@Column(name = "resource_id")
	private String resourceId;

	/**
	 * 是否匿名：0:否;1:是;
	 */
	@Column(name = "anonymity")
	private Boolean anonymity = Boolean.FALSE;

	/**
	 * 是否接收回帖：0:否;1:是;
	 */
	@Column(name = "receive_reply")
	private Boolean receiveReply = Boolean.TRUE;

	/**
	 * 是否置顶：0:否;1:是
	 */
	@Column(name = "stick")
	private Boolean stick = Boolean.FALSE;

	/**
	 * 置顶序号:从1开始正序排列
	 */
	@Column(name = "stick_sequence")
	private String stickSequence;

	/**
	 * 帖子回复数
	 */
	@Column(name = "reply_num")
	private Integer replyNum;

	/**
	 * 最新回复人
	 */
	@Column(name = "last_reply_user")
	private String lastReplyUser;

	/**
	 * 最新回复人ID
	 */
	@Column(name = "last_reply_user_id")
	private String lastReplyUserId;
	// 版块名称
	//@TableField(exist = false)
	@Transient
	private String forum;

	/**
	 * 状态
	 */
	@Column(name = "status")
	private String status;
	/**
	 * 发布时间
	 */
	@Column(name = "publish_time")
	private Date publishTime;

	/**
	 * 置顶周期
	 */
	@Column(name = "stick_cycle")
	private Integer stickCycle;

	/**
	 * 置顶失效时间
	 */
	@Column(name = "stick_invalid_time")
	private Date stickInvalidTime;

	/**
	 * 序号
	 */
	@Column(name = "sort_num")
	private Integer sortNum;

	/**
	 * 屏蔽原因
	 */
	@Column(name = "screen_reason")
	private String screenReason;

	/**
	 * 屏蔽标识
	 */
	@Column(name = "screen")
	private Boolean screen = Boolean.FALSE;

	/**
	 * 结帖标识
	 */
	@Column(name = "closed")
	private Boolean closed = Boolean.FALSE;

	/**
	 * 最新内容编辑人
	 */
	@Column(name = "last_content_editor")
	private String lastContentEditor;
	/**
	 * 最新内容编辑时间
	 */
	@Column(name = "last_content_edit_time")
	private Date lastContentEditTime;

	public String getForumId() {
		return forumId;
	}

	public void setForumId(String forumId) {
		this.forumId = forumId;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public Integer getClickNum() {
		return clickNum;
	}

	public void setClickNum(Integer clickNum) {
		this.clickNum = clickNum;
	}

	public Boolean getEssence() {
		return essence;
	}

	public void setEssence(Boolean essence) {
		this.essence = essence;
	}

	public Boolean getAnonymity() {
		return anonymity;
	}

	public void setAnonymity(Boolean anonymity) {
		this.anonymity = anonymity;
	}

	public Boolean getReceiveReply() {
		return receiveReply;
	}

	public void setReceiveReply(Boolean receiveReply) {
		this.receiveReply = receiveReply;
	}

	public Boolean getStick() {
		return stick;
	}

	public void setStick(Boolean stick) {
		this.stick = stick;
	}

	public String getResourceId() {
		return resourceId;
	}

	public void setResourceId(String resourceId) {
		this.resourceId = resourceId;
	}

	public String getStickSequence() {
		return stickSequence;
	}

	public void setStickSequence(String stickSequence) {
		this.stickSequence = stickSequence;
	}

	public Integer getReplyNum() {
		return replyNum;
	}

	public void setReplyNum(Integer replyNum) {
		this.replyNum = replyNum;
	}

	public String getLastReplyUser() {
		return lastReplyUser;
	}

	public void setLastReplyUser(String lastReplyUser) {
		this.lastReplyUser = lastReplyUser;
	}

	public String getLastReplyUserId() {
		return lastReplyUserId;
	}

	public void setLastReplyUserId(String lastReplyUserId) {
		this.lastReplyUserId = lastReplyUserId;
	}

	public String getForum() {
		return forum;
	}

	public void setForum(String forum) {
		this.forum = forum;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public Date getPublishTime() {
		return publishTime;
	}

	public void setPublishTime(Date publishTime) {
		this.publishTime = publishTime;
	}

	public Integer getStickCycle() {
		return stickCycle;
	}

	public void setStickCycle(Integer stickCycle) {
		this.stickCycle = stickCycle;
	}

	public Date getStickInvalidTime() {
		return stickInvalidTime;
	}

	public void setStickInvalidTime(Date stickInvalidTime) {
		this.stickInvalidTime = stickInvalidTime;
	}

	public Integer getSortNum() {
		return sortNum;
	}

	public void setSortNum(Integer sortNum) {
		this.sortNum = sortNum;
	}

	public String getScreenReason() {
		return screenReason;
	}

	public void setScreenReason(String screenReason) {
		this.screenReason = screenReason;
	}

	public Boolean getScreen() {
		return screen;
	}

	public void setScreen(Boolean screen) {
		this.screen = screen;
	}

	public Boolean getClosed() {
		return closed;
	}

	public void setClosed(Boolean closed) {
		this.closed = closed;
	}

	public String getLastContentEditor() {
		return lastContentEditor;
	}

	public void setLastContentEditor(String lastContentEditor) {
		this.lastContentEditor = lastContentEditor;
	}

	public Date getLastContentEditTime() {
		return lastContentEditTime;
	}

	public void setLastContentEditTime(Date lastContentEditTime) {
		this.lastContentEditTime = lastContentEditTime;
	}
}
