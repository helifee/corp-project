		return questionOther;
	}
	public void setQuestionOther(String questionOther) {
		this.questionOther = questionOther;
	}
	
	public String getProblemIdNums() {
		return problemIdNums;
	}
	public void setProblemIdNums(String problemIdNums) {
		this.problemIdNums = problemIdNums;
	}
	
	public String getOptionIdNums() {
		return optionIdNums;
	}
	public void setOptionIdNums(String optionIdNums) {
		this.optionIdNums = optionIdNums;
	}
	
	
