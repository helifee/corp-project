package com.jzy.hio.oa.sys.entity;

import javax.persistence.Column;
import javax.persistence.Table;
import com.jzy.hio.base.entity.BaseEntity;

/**
 * @author admin
 * 
 *         快速入口表
 */
@Table(name = "OA_SYS_QUICK_ENTRY")
public class SysQuickEntry extends BaseEntity {

	/**
	 * 所属分类
	 */
	@Column(name = "pname")
	private String pname;

	/**
	 * 编码
	 */
	@Column(name = "code")
	private String code;

	/**
	 * 名称
	 */
	@Column(name = "name")
	private String name;

	/**
	 * 链接
	 */
	@Column(name = "url")
	private String url;

	/**
	 * 父类id
	 */
	@Column(name = "pid")
	private String pid;

	/**
	 * 图标
	 */
	@Column(name = "icon")
	private String icon;

	public String getPname() {
		return pname;
	}

	public void setPname(String pname) {
		this.pname = pname;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	public String getPid() {
		return pid;
	}

	public void setPid(String pid) {
		this.pid = pid;
	}

	public String getIcon() {
		return icon;
	}

	public void setIcon(String icon) {
		this.icon = icon;
	}

}
