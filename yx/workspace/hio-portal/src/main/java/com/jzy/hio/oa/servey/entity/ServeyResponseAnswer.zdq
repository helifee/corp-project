package com.jzy.hio.oa.servey.entity;

import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Table;
import com.jzy.hio.base.entity.BaseEntity;

/**
 * @author admin
 * 
 *         调查反馈答案
 */
@Table(name = "OA_SERVEY_RESPONSE_ANSWER")
public class ServeyResponseAnswer extends BaseEntity {

	/**
	 * 调查反馈ID
	 */
	@Column(name = "servey_response_id")
	private String serveyResponseId;

	/**
	 * 问题ID
	 */
	@Column(name = "question_id")
	private String questionId;

	/**
	 * 矩阵问题ID
	 */
	@Column(name = "sub_question_id")
	private String subQuestionId;

	/**
	 * 问题选项ID(选择题)
	 */
	@Column(name = "option_id")
	private String optionId;

	/**
	 * 问题答案(问答题)
	 */
	@Column(name = "text_response")
	private String textResponse;

	/**
	 * 答题日期
	 */
	@Column(name = "answer_date")
	private Date answerDate;

	public String getServeyResponseId() {
		return serveyResponseId;
	}

	public void setServeyResponseId(String serveyResponseId) {
		this.serveyResponseId = serveyResponseId;
	}

	public String getQuestionId() {
		return questionId;
	}

	public void setQuestionId(String questionId) {
		this.questionId = questionId;
	}

	public String getSubQuestionId() {
		return subQuestionId;
	}

	public void setSubQuestionId(String subQuestionId) {
		this.subQuestionId = subQuestionId;
	}

	public String getOptionId() {
		return optionId;
	}

	public void setOptionId(String optionId) {
		this.optionId = optionId;
	}

	public String getTextResponse() {
		return textResponse;
	}

	public void setTextResponse(String textResponse) {
		this.textResponse = textResponse;
	}

	public Date getAnswerDate() {
		return answerDate;
	}

	public void setAnswerDate(Date answerDate) {
		this.answerDate = answerDate;
	}

}
