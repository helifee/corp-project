package com.jzy.hio.oa.shortcutmenu.dto;

import com.jzy.hio.base.dto.BaseDto;





/**
 * @author admin
 * 
 *
 */
public class ShortcutMenuDto extends BaseDto{

		
	//名称
	private String name;
    
  		
	//编码
	private String code;
    
  		
	//是否内部链接
	private Boolean innerLink;
    
  		
	//资源ID
	private String resourceId;
    
  		
	//资源名称
	private String resourceName;
    
  		
	//链接地址
	private String linkAddr;
    
  		
	//图标
	private byte[] icon;
    
  		
	//菜单归属人ID
	private String ownerId;
    
  		
	//菜单归属人
	private String owner;
	
	private String isDelPic;
	
	//图标展示
	private String iconShow;

	//排序号
	private Integer sortNum;


	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
    
  		
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
    
  		
	public Boolean getInnerLink() {
		return innerLink;
	}
	public void setInnerLink(Boolean innerLink) {
		this.innerLink = innerLink;
	}
    
  		
	public String getResourceId() {
		return resourceId;
	}
	public void setResourceId(String resourceId) {
		this.resourceId = resourceId;
	}
    
  		
	public String getResourceName() {
		return resourceName;
	}
	public void setResourceName(String resourceName) {
		this.resourceName = resourceName;
	}
    
  		
	public String getLinkAddr() {
		return linkAddr;
	}
	public void setLinkAddr(String linkAddr) {
		this.linkAddr = linkAddr;
	}
    
	public byte[] getIcon() {
		return icon;
	}
	public void setIcon(byte[] icon) {
		this.icon = icon;
	}
	
	public String getOwnerId() {
		return ownerId;
	}
	public void setOwnerId(String ownerId) {
		this.ownerId = ownerId;
	}
    
  		
	public String getOwner() {
		return owner;
	}
	public void setOwner(String owner) {
		this.owner = owner;
	}
	
	public String getIsDelPic() {
		return isDelPic;
	}
	
	public void setIsDelPic(String isDelPic) {
		this.isDelPic = isDelPic;
	}
	
	public String getIconShow() {
		return iconShow;
	}
	public void setIconShow(String iconShow) {
		this.iconShow = iconShow;
	}

	public Integer getSortNum() {
		return sortNum;
	}

	public void setSortNum(Integer sortNum) {
		this.sortNum = sortNum;
	}


}
