package com.jzy.hio.oa.servey.entity;

import javax.persistence.Column;
import javax.persistence.Table;
import com.jzy.hio.base.entity.BaseEntity;

/**
 * @author admin
 * 
 *         题目选项
 */
@Table(name = "OA_SURVEY_QUESTION_OPTION")
public class ServeyQuestionOption extends BaseEntity {

	/**
	 * 选项描述
	 */
	@Column(name = "description")
	private String description;

	/**
	 * 题目ID
	 */
	@Column(name = "question_id")
	private String questionId;

	/**
	 * 选项序号
	 */
	@Column(name = "sequence_num")
	private String sequenceNum;

	/**
	 * 权重(分值)
	 */
	@Column(name = "weight_factor")
	private Integer weightFactor;

	/**
	 * 类型
	 */
	@Column(name = "option_type")
	private Integer optionType;

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public String getQuestionId() {
		return questionId;
	}

	public void setQuestionId(String questionId) {
		this.questionId = questionId;
	}

	public String getSequenceNum() {
		return sequenceNum;
	}

	public void setSequenceNum(String sequenceNum) {
		this.sequenceNum = sequenceNum;
	}

	public Integer getWeightFactor() {
		return weightFactor;
	}

	public void setWeightFactor(Integer weightFactor) {
		this.weightFactor = weightFactor;
	}

	public Integer getOptionType() {
		return optionType;
	}

	public void setOptionType(Integer optionType) {
		this.optionType = optionType;
	}

}
