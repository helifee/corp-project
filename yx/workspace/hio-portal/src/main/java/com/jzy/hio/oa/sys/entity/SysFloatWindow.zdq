package com.jzy.hio.oa.sys.entity;

import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Table;
import com.jzy.hio.base.entity.BaseEntity;

/**
 * @author admin
 * 
 *         浮动窗口表
 */

@Table(name = "OA_SYS_FLOAT_WINDOW")
public class SysFloatWindow extends BaseEntity {

	/**
	 * 状态
	 */
	@Column(name = "state")
	private Boolean state;

	/**
	 * 编号
	 */
	@Column(name = "serial_no")
	private String serialNo;

	/**
	 * 名称
	 */
	@Column(name = "name")
	private String name;

	/**
	 * 靠顶坐标
	 */
	@Column(name = "top")
	private Long top;

	/**
	 * 靠左坐标
	 */
	@Column(name = "_left")
	private Long _left;

	/**
	 * 宽带
	 */
	@Column(name = "width")
	private Long width;

	/**
	 * 高度
	 */
	@Column(name = "height")
	private Long height;

	/**
	 * 类型
	 */
	@Column(name = "type")
	private String type;

	/**
	 * 延时打开时间
	 */
	@Column(name = "delay_interval")
	private Integer delayInterval;

	/**
	 * 显示时长
	 */
	@Column(name = "show_interval")
	private Integer showInterval;
	/**
	 * 文件
	 */
	@Column(name = "file")
	private String file;

	/**
	 * 链接
	 */
	@Column(name = "url")
	private String url;

	/**
	 * 自动生效时间
	 */
	@Column(name = "automatic_time")
	private Date automaticTime;

	/**
	 * 自动失效时间
	 */
	@Column(name = "expired_time")
	private Date expiredTime;

	/**
	 * 创建者
	 */
	@Column(name = "creater")
	private String creater;

	/**
	 * 创建时间
	 */
	@Column(name = "create_time")
	private Date createTime;

	/**
	 * 修改者
	 */
	@Column(name = "modifier")
	private String modifier;

	/**
	 * 修改时间
	 */
	@Column(name = "modify_time")
	private Date modifyTime;

	public Integer getShowInterval() {
		return showInterval;
	}

	public void setShowInterval(Integer showInterval) {
		this.showInterval = showInterval;
	}

	public String getSerialNo() {
		return serialNo;
	}

	public void setSerialNo(String serialNo) {
		this.serialNo = serialNo;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Boolean getState() {
		return state;
	}

	public void setState(Boolean state) {
		this.state = state;
	}

	public Long getTop() {
		return top;
	}

	public void setTop(Long top) {
		this.top = top;
	}

	public Long get_left() {
		return _left;
	}

	public void set_left(Long _left) {
		this._left = _left;
	}

	public Long getWidth() {
		return width;
	}

	public void setWidth(Long width) {
		this.width = width;
	}

	public Long getHeight() {
		return height;
	}

	public void setHeight(Long height) {
		this.height = height;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public Integer getDelayInterval() {
		return delayInterval;
	}

	public void setDelayInterval(Integer delayInterval) {
		this.delayInterval = delayInterval;
	}

	public String getFile() {
		return file;
	}

	public void setFile(String file) {
		this.file = file;
	}

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	public Date getAutomaticTime() {
		return automaticTime;
	}

	public void setAutomaticTime(Date automaticTime) {
		this.automaticTime = automaticTime;
	}

	public Date getExpiredTime() {
		return expiredTime;
	}

	public void setExpiredTime(Date expiredTime) {
		this.expiredTime = expiredTime;
	}

	public String getCreater() {
		return creater;
	}

	public void setCreater(String creater) {
		this.creater = creater;
	}

	public Date getCreateTime() {
		return createTime;
	}

	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}

	public String getModifier() {
		return modifier;
	}

	public void setModifier(String modifier) {
		this.modifier = modifier;
	}

	public Date getModifyTime() {
		return modifyTime;
	}

	public void setModifyTime(Date modifyTime) {
		this.modifyTime = modifyTime;
	}

}
