package com.jzy.hio.task.entity;

import javax.persistence.Column;
import javax.persistence.Table;
import com.jzy.hio.base.entity.BaseEntity;


/**
 * 
 * desc="任务参数"
 * 
 * @author admin
 * 
 * 
 */
@Table(name="PT_UNIV_TASK_PARAMETERS")
public class TaskParameters extends BaseEntity{
	
	/**
	 * desc="任务"
	 */
	@Column(name="task_id")
	private String taskId;
    
  	/**
  	 * desc="参数描述"
  	 */
	@Column(name="details")
	private String details;
    
	public String getTaskId() {
		return taskId;
	}
	public void setTaskId(String taskId) {
		this.taskId = taskId;
	}
  		
	public String getDetails() {
		return details;
	}
	public void setDetails(String details) {
		this.details = details;
	}
	
	@Override
	public String toString() {
		return "TaskParameters [taskId=" + taskId + ", details=" + details + "]";
	}
	
}
