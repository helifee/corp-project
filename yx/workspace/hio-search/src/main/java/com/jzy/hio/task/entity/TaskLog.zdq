package com.jzy.hio.task.entity;

import javax.persistence.Column;
import javax.persistence.Table;
import com.jzy.hio.base.entity.BaseEntity;
import java.sql.Timestamp;

/**
 * 
 * desc="任务日志"
 * 
 * @author admin
 * 
 */

@Table(name="PT_UNIV_TASK_LOG")
public class TaskLog extends BaseEntity{
	
	/**
	 * desc="任务"
	 */
	@Column(name="task_id")
	private String taskId;
    
  	/**
  	 * desc="任务运行状态。(1-等待、2-运行、3-错误、4-完成)"
  	 */
	@Column(name="status")
	private String status;
    
  	/**
  	 * desc="最后一次任务开始时间"
  	 */
	@Column(name="last_start_time")
	private Timestamp lastStartTime;
    
  	/**
  	 * desc="最后一次任务结束时间"
  	 */
	@Column(name="last_end_time")
	private Timestamp lastEndTime;
    
  	/**
  	 * desc="下次任务开始时间"
  	 */
	@Column(name="next_start_time")
	private Timestamp nextStartTime;
    
	public String getTaskId() {
		return taskId;
	}
	public void setTaskId(String taskId) {
		this.taskId = taskId;
	}
    
  		
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
    
  		
	public Timestamp getLastStartTime() {
		return lastStartTime;
	}
	public void setLastStartTime(Timestamp lastStartTime) {
		this.lastStartTime = lastStartTime;
	}
    
  		
	public Timestamp getLastEndTime() {
		return lastEndTime;
	}
	public void setLastEndTime(Timestamp lastEndTime) {
		this.lastEndTime = lastEndTime;
	}
    
  		
	public Timestamp getNextStartTime() {
		return nextStartTime;
	}
	public void setNextStartTime(Timestamp nextStartTime) {
		this.nextStartTime = nextStartTime;
	}
	@Override
	public String toString() {
		return "TaskLog [taskId=" + taskId + ", status=" + status + ", lastStartTime=" + lastStartTime
				+ ", lastEndTime=" + lastEndTime + ", nextStartTime=" + nextStartTime + "]";
	}
	
}
