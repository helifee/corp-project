package com.jzy.hio.feign;

import java.util.Map;
import javax.ws.rs.core.MediaType;
import org.springframework.cloud.netflix.feign.FeignClient;
import org.springframework.web.bind.annotation.RequestBody;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RequestMethod;

@FeignClient(name = "SearchService")
public interface SearchFeignService {

	@RequestMapping(value = "/univ/task/taskTest/queryList", method = RequestMethod.POST, consumes = MediaType.APPLICATION_JSON)
	public String queryList(@RequestBody Map<String,Object> paramater);
	
	@RequestMapping(value = "/univ/attachmentServiceImpl/queryListByObject", method = RequestMethod.POST, consumes = MediaType.APPLICATION_JSON)
	public String queryListByObject(@RequestBody Map<String, Object> paramater);
	
	@RequestMapping(value = "/univ/task/taskTest/queryList1", method = RequestMethod.POST, consumes = MediaType.APPLICATION_JSON)
	public String queryList1(@RequestBody Map<String,Object> paramater);
	
	@RequestMapping(value="/univ/attachmentTempServiceImpl/save", method = RequestMethod.POST, consumes = MediaType.APPLICATION_JSON)
    public int attachmentTempSave(@RequestBody String saveJson);
	
}