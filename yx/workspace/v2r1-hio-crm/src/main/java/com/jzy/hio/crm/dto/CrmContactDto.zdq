package com.jzy.hio.crm.dto;

import java.io.Serializable;
import java.util.Date;

/**
 * 联系人
 * @author 
 *
 */
public class CrmContactDto implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 6915094592479293305L;

	private String sid;
	/**
	 * 客户ID
	 */
	private String customerId;
	
	/**
	 * 商机ID
	 */
	private String opportunityId;
	
	/**
	 * 联系人名称
	 */
	private String name;
	
	/**
	 * 是否为主联系人
	 */
	private Integer masterContact;
	
	/**
	 * 手机号码
	 */
	private String phoneNumber;
	
	/**
	 * 职务
	 */
	private String title;
	
	/**
	 * 性别
	 */
	private Integer gendar;
	
	/**
	 * 电子邮件
	 */
	private String email;
	
	/**
	 * 备注
	 */
	private String comment;
	
	/**
	 * 决策关系
	 */
	private Integer decisionRole;
	
	/**
	 * 创建时间
	 */
	private Date createDate;
	
	/**
	 * 更新时间
	 */
	private Date updateDate;
	
	/**
	 * 创建人名称
	 */
	private String createPersonName;
	
	// ---------------分割线---------------
	/**
	 * 客户名称
	 */
	private String customerName;

	public String getSid() {
		return sid;
	}

	public void setSid(String sid) {
		this.sid = sid;
	}

	public String getCustomerId() {
		return customerId;
	}

	public void setCustomerId(String customerId) {
		this.customerId = customerId;
	}

	public String getOpportunityId() {
		return opportunityId;
	}

	public void setOpportunityId(String opportunityId) {
		this.opportunityId = opportunityId;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Integer getMasterContact() {
		return masterContact;
	}

	public void setMasterContact(Integer masterContact) {
		this.masterContact = masterContact;
	}

	public String getPhoneNumber() {
		return phoneNumber;
	}

	public void setPhoneNumber(String phoneNumber) {
		this.phoneNumber = phoneNumber;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public Integer getGendar() {
		return gendar;
	}

	public void setGendar(Integer gendar) {
		this.gendar = gendar;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getComment() {
		return comment;
	}

	public void setComment(String comment) {
		this.comment = comment;
	}

	public Integer getDecisionRole() {
		return decisionRole;
	}

	public void setDecisionRole(Integer decisionRole) {
		this.decisionRole = decisionRole;
	}

	public Date getCreateDate() {
		return createDate;
	}

	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}

	public Date getUpdateDate() {
		return updateDate;
	}

	public void setUpdateDate(Date updateDate) {
		this.updateDate = updateDate;
	}

	public String getCreatePersonName() {
		return createPersonName;
	}

	public void setCreatePersonName(String createPersonName) {
		this.createPersonName = createPersonName;
	}

	public String getCustomerName() {
		return customerName;
	}

	public void setCustomerName(String customerName) {
		this.customerName = customerName;
	}
	
	
}
