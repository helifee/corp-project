package com.jzy.hio.crm.param;

import java.io.Serializable;

/**
 * 验证重复名称
 * @author 
 *
 */

public class CrmValNameParam implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 4157019400954973066L;
	/**
	 * 数据主键
	 */
	private String sid;
	/**
	 * 名称
	 */
	private String name;
	/**
	 * 客户id
	 */
	private String customerId;

	/**
	 * 父级id
	 */
	private String pid;

	public String getSid() {
		return sid;
	}
	public void setSid(String sid) {
		this.sid = sid;
	}
	public String getCustomerId() {
		return customerId;
	}
	public void setCustomerId(String customerId) {
		this.customerId = customerId;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getPid() {
		return pid;
	}
	public void setPid(String pid) {
		this.pid = pid;
	}
	
	
}
