package com.jzy.hio.entity;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

import com.jzy.hio.base.entity.BaseEntity;

/**
 * 客户与销售关系
 * @author 
 *
 */
@Entity
@Table(name="crm_customer_salesman")
public class CrmCustomerSalesman extends BaseEntity implements Serializable {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = -8284693173697790434L;

	/**
	 * 客户ID
	 */
	@Column(name="customer_id")
	private String customerId;
	
	/**
	 * 销售员ID
	 */
	@Column(name="salesman_id")
	private String salesmanId;
	
	/**
	 * 关系类型
	 */
	@Column(name="type")
	private Integer type;

	public String getCustomerId() {
		return customerId;
	}

	public void setCustomerId(String customerId) {
		this.customerId = customerId;
	}

	public String getSalesmanId() {
		return salesmanId;
	}

	public void setSalesmanId(String salesmanId) {
		this.salesmanId = salesmanId;
	}

	public Integer getType() {
		return type;
	}

	public void setType(Integer type) {
		this.type = type;
	}
	
}

