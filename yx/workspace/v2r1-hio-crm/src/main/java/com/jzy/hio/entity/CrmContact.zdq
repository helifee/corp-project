package com.jzy.hio.entity;

import java.io.Serializable;
import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

import com.jzy.hio.base.entity.BaseEntity;

/**
 * 联系人
 * @author 
 *
 */

@Entity
@Table(name="crm_contact")
public class CrmContact extends BaseEntity implements Serializable {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 6336780860387985379L;

	/**
	 * 客户ID
	 */
	@Column(name="customer_id")
	private String customerId;
	
	/**
	 * 商机ID(好像暂时没什么用了)
	 */
	@Column(name="opportunity_id")
	private String opportunityId;
	
	/**
	 * 联系人名称
	 */
	private String name;
	
	/**
	 * 是否为主联系人
	 */
	@Column(name="master_contact")
	private Integer masterContact;
	
	/**
	 * 手机号码
	 */
	@Column(name="phone_number")
	private String phoneNumber;
	
	/**
	 * 职务
	 */
	@Column(name="title")
	private String title;
	
	/**
	 * 性别
	 */
	@Column(name="gendar")
	private Integer gendar;
	
	/**
	 * 电子邮件
	 */
	@Column(name="email")
	private String email;
	
	/**
	 * 备注
	 */
	@Column(name="comment")
	private String comment;
	
	/**
	 * 决策关系
	 */
	@Column(name="decision_role")
	private Integer decisionRole;
	
	/**
	 * 最后跟进时间
	 */
	@Column(name="last_contact_time")
	private Date lastContactTime;

	public String getCustomerId() {
		return customerId;
	}

	public void setCustomerId(String customerId) {
		this.customerId = customerId;
	}

	public String getOpportunityId() {
		return opportunityId;
	}

	public void setOpportunityId(String opportunityId) {
		this.opportunityId = opportunityId;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Integer getMasterContact() {
		return masterContact;
	}

	public void setMasterContact(Integer masterContact) {
		this.masterContact = masterContact;
	}

	public String getPhoneNumber() {
		return phoneNumber;
	}

	public void setPhoneNumber(String phoneNumber) {
		this.phoneNumber = phoneNumber;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public Integer getGendar() {
		return gendar;
	}

	public void setGendar(Integer gendar) {
		this.gendar = gendar;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getComment() {
		return comment;
	}

	public void setComment(String comment) {
		this.comment = comment;
	}

	public Integer getDecisionRole() {
		return decisionRole;
	}

	public void setDecisionRole(Integer decisionRole) {
		this.decisionRole = decisionRole;
	}

	public Date getLastContactTime() {
		return lastContactTime;
	}

	public void setLastContactTime(Date lastContactTime) {
		this.lastContactTime = lastContactTime;
	}

	
}

