package com.jzy.hio.entity;

import java.io.Serializable;
import java.math.BigDecimal;
import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

import com.jzy.hio.base.entity.BaseEntity;

/**
 * 订单
 * @author 
 *
 */
@Entity
@Table(name="crm_order")
public class CrmOrder extends BaseEntity implements Serializable {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 251189839364743316L;
	
	/**
	 * 客户ID
	 */
	@Column(name="customer_id")
	private String customerId;
	
	/**
	 * 客户姓名
	 */
	@Column(name="customer_name")
	private String customerName;
	
	/**
	 * 商机ID
	 */
	@Column(name="opportunity_id")
	private String opportunityId;
	
	/**
	 * 商机名
	 */
	@Column(name="opportunity_name")
	private String opportunityName;
	
	/**
	 * 订单编码
	 */
	private String code;
	
	/**
	 * 订单名称
	 */
	private String name;
	
	/**
	 * 订单类型
	 */
	private Integer type;
	
	/**
	 * 折扣
	 */
	private Integer discount;
	
	/**
	 * 签约金额
	 */
	@Column(name="sign_amount")
	private BigDecimal signAmount;
	
	/**
	 * 合同金额
	 */
	@Column(name="contract_amount")
	private BigDecimal contractAmount;
	
	/**
	 * 签约日期
	 */
	@Column(name="deal_date")
	private Date dealDate;
	
	/**
	 * 合同开始日期
	 */
	@Column(name="contract_start_date")
	private Date contractStartDate;
	
	/**
	 * 合同结束日期
	 */
	@Column(name="contract_end_date")
	private Date contractEndDate;
	
	/**
	 * 对方负责人
	 */
	@Column(name="person_charge_other")
	private String personChargeOther;
	
	/**
	 * 对方负责人姓名
	 */
	@Column(name="person_other_name")
	private String personOtherName;
	
	/**
	 * 我方负责人
	 */
	@Column(name="person_charge_our")
	private String personChargeOur;
	
	/**
	 * 我方负责人姓名
	 */
	@Column(name="person_our_name")
	private String personOurName;
	
	/**
	 * 合同
	 */
	private String contract;
	
	/**
	 * 合同类型
	 */
	@Column(name="contract_type")
	private Integer contractType;
	
	/**
	 * 支付方式
	 */
	@Column(name="payment_channel")
	private Integer paymentChannel;

	/**
	 * 附件
	 */
	private String attachment;

	/**
	 * 订单状态
	 */
	@Column(name="order_status")
	private Integer orderStatus;
	
	/**
	 * 备注
	 */
	private String comment;
	
	public String getCustomerId() {
		return customerId;
	}

	public void setCustomerId(String customerId) {
		this.customerId = customerId;
	}

	public String getCustomerName() {
		return customerName;
	}

	public void setCustomerName(String customerName) {
		this.customerName = customerName;
	}

	public String getOpportunityId() {
		return opportunityId;
	}

	public void setOpportunityId(String opportunityId) {
		this.opportunityId = opportunityId;
	}

	public String getOpportunityName() {
		return opportunityName;
	}

	public void setOpportunityName(String opportunityName) {
		this.opportunityName = opportunityName;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Integer getType() {
		return type;
	}

	public void setType(Integer type) {
		this.type = type;
	}

	public Integer getDiscount() {
		return discount;
	}

	public void setDiscount(Integer discount) {
		this.discount = discount;
	}

	public BigDecimal getSignAmount() {
		return signAmount;
	}

	public void setSignAmount(BigDecimal signAmount) {
		this.signAmount = signAmount;
	}

	public BigDecimal getContractAmount() {
		return contractAmount;
	}

	public void setContractAmount(BigDecimal contractAmount) {
		this.contractAmount = contractAmount;
	}

	public Date getDealDate() {
		return dealDate;
	}

	public void setDealDate(Date dealDate) {
		this.dealDate = dealDate;
	}

	public Date getContractStartDate() {
		return contractStartDate;
	}

	public void setContractStartDate(Date contractStartDate) {
		this.contractStartDate = contractStartDate;
	}

	public Date getContractEndDate() {
		return contractEndDate;
	}

	public void setContractEndDate(Date contractEndDate) {
		this.contractEndDate = contractEndDate;
	}

	public String getPersonChargeOther() {
		return personChargeOther;
	}

	public void setPersonChargeOther(String personChargeOther) {
		this.personChargeOther = personChargeOther;
	}

	public String getPersonOtherName() {
		return personOtherName;
	}

	public void setPersonOtherName(String personOtherName) {
		this.personOtherName = personOtherName;
	}

	public String getPersonChargeOur() {
		return personChargeOur;
	}

	public void setPersonChargeOur(String personChargeOur) {
		this.personChargeOur = personChargeOur;
	}

	public String getPersonOurName() {
		return personOurName;
	}

	public void setPersonOurName(String personOurName) {
		this.personOurName = personOurName;
	}

	public String getContract() {
		return contract;
	}

	public void setContract(String contract) {
		this.contract = contract;
	}

	public Integer getContractType() {
		return contractType;
	}

	public void setContractType(Integer contractType) {
		this.contractType = contractType;
	}

	public Integer getPaymentChannel() {
		return paymentChannel;
	}

	public void setPaymentChannel(Integer paymentChannel) {
		this.paymentChannel = paymentChannel;
	}

	public String getAttachment() {
		return attachment;
	}

	public void setAttachment(String attachment) {
		this.attachment = attachment;
	}

	public Integer getOrderStatus() {
		return orderStatus;
	}

	public void setOrderStatus(Integer orderStatus) {
		this.orderStatus = orderStatus;
	}

	public String getComment() {
		return comment;
	}

	public void setComment(String comment) {
		this.comment = comment;
	}
	
	
}
