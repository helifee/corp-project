package com.jzy.hio.crm.param;

import java.io.Serializable;
import java.math.BigDecimal;

import com.jzy.hio.crm.utils.PageHelper;

/**
 * 商机列表分页
 * @author 
 *
 */
public class CrmOpportunitySearchParam extends PageHelper implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = -2062249298825369835L;

	/**
	 * 用户id
	 */
	private String uid;
	
	/**
	 * 客户id
	 */
	private String customerId;
	
	/**
	 * 商机名称
	 */
	private String opportunityName;
	
	/**
	 * 商机阶段
	 */
	private Integer stage;
	
	/**
	 * 预计成交金额起始
	 */
	private BigDecimal estimateAmountStart;
	
	/**
	 * 预计成交金额结束
	 */
	private BigDecimal estimateAmountEnd;
	
	/**
	 * 最后跟进时间排序
	 */
	private String lastContactTimeSort;
	
	/**
	 * 成交金额排序
	 */
	private String estimateAmountSort;

	public String getUid() {
		return uid;
	}

	public void setUid(String uid) {
		this.uid = uid;
	}

	public String getCustomerId() {
		return customerId;
	}

	public void setCustomerId(String customerId) {
		this.customerId = customerId;
	}

	public String getOpportunityName() {
		return opportunityName;
	}

	public void setOpportunityName(String opportunityName) {
		this.opportunityName = opportunityName;
	}

	public Integer getStage() {
		return stage;
	}

	public void setStage(Integer stage) {
		this.stage = stage;
	}

	public BigDecimal getEstimateAmountStart() {
		return estimateAmountStart;
	}

	public void setEstimateAmountStart(BigDecimal estimateAmountStart) {
		this.estimateAmountStart = estimateAmountStart;
	}

	public BigDecimal getEstimateAmountEnd() {
		return estimateAmountEnd;
	}

	public void setEstimateAmountEnd(BigDecimal estimateAmountEnd) {
		this.estimateAmountEnd = estimateAmountEnd;
	}

	public String getLastContactTimeSort() {
		return lastContactTimeSort;
	}

	public void setLastContactTimeSort(String lastContactTimeSort) {
		this.lastContactTimeSort = lastContactTimeSort;
	}

	public String getEstimateAmountSort() {
		return estimateAmountSort;
	}

	public void setEstimateAmountSort(String estimateAmountSort) {
		this.estimateAmountSort = estimateAmountSort;
	}
	
	
}
