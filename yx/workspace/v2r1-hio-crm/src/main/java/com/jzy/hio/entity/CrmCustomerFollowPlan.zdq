package com.jzy.hio.entity;

import java.io.Serializable;
import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

import com.jzy.hio.base.entity.BaseEntity;

/**
 * 跟进计划
 * @author 
 *
 */
@Entity
@Table(name="crm_customer_follow_plan")
public class CrmCustomerFollowPlan extends BaseEntity implements Serializable {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 129685772423491409L;

	/**
	 * 联系人ID
	 */
	@Column(name="contact_id")
	private String contactId;
	
	/**
	 * 客户ID
	 */
	@Column(name="customer_id")
	private String customerId;
	
	/**
	 * 商机ID
	 */
	@Column(name="opportunity_id")
	private String opportunityId;
	
	/**
	 * 跟进目标
	 */
	@Column(name="target")
	private String target;
	
	/**
	 * 联系时间
	 */
	@Column(name="contact_time")
	private Date contactTime;

	/**
	 * 上次跟进时间
	 */
	@Column(name = "last_contact_time")
	private Date lastContactTime;

	/**
	 * 联系方式
	 */
	@Column(name="contact_way")
	private Integer contactWay;

	public Integer getContactWay() {
		return contactWay;
	}

	public void setContactWay(Integer contactWay) {
		this.contactWay = contactWay;
	}

	public Date getLastContactTime() {
		return lastContactTime;
	}

	public void setLastContactTime(Date lastContactTime) {
		this.lastContactTime = lastContactTime;
	}

	public String getContactId() {
		return contactId;
	}

	public void setContactId(String contactId) {
		this.contactId = contactId;
	}

	public String getCustomerId() {
		return customerId;
	}

	public void setCustomerId(String customerId) {
		this.customerId = customerId;
	}

	public String getOpportunityId() {
		return opportunityId;
	}

	public void setOpportunityId(String opportunityId) {
		this.opportunityId = opportunityId;
	}

	public String getTarget() {
		return target;
	}

	public void setTarget(String target) {
		this.target = target;
	}

	public Date getContactTime() {
		return contactTime;
	}

	public void setContactTime(Date contactTime) {
		this.contactTime = contactTime;
	}

	
}
