package com.jzy.hio.flow.entity;

import javax.persistence.Column;
import javax.persistence.Table;
import com.jzy.hio.base.entity.BaseEntity;


/**
 * @author admin
 * 
 * 流程可阅者
 */

@Table(name="PT_FLOW_INSTANCE_ACCESSIBLE")
public class InstanceAccessible extends BaseEntity{
	
	/**
	 * 	流程实例id
	 */
	@Column(name="fi_id")
	private String fiId;
    
	/**
	 * 可阅者类型: 1:角色,2:岗位,3:人员
	 */
	@Column(name="type")
	private String type;
    
  		
	/**
	 * 可阅者ID
	 */
	@Column(name="accessible_id")
	private String accessibleId;
	
	/**
	 * 可阅者名称
	 */
	@Column(name="accessible_name")
	private String accessibleName;
    
  		
		
	public String getFiId() {
		return fiId;
	}
	public void setFiId(String fiId) {
		this.fiId = fiId;
	}
    
  		
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getAccessibleId() {
		return accessibleId;
	}
	public void setAccessibleId(String accessibleId) {
		this.accessibleId = accessibleId;
	}
	public String getAccessibleName() {
		return accessibleName;
	}
	public void setAccessibleName(String accessibleName) {
		this.accessibleName = accessibleName;
	}
	
}
