			UploadAttachment uploadAttachment = uploadAttachmentService.getObjectById(id);
			String json = JacksonUtils.toJson(uploadAttachment);
			Map<String,Object> oldMap=JacksonUtils.fromJson(json, HashMap.class);
			oldMap.putAll(map);
			String updateJson= JacksonUtils.toJson(oldMap);
			uploadAttachment1 = JacksonUtils.fromJson(updateJson, UploadAttachment.class);
			int updateCount = uploadAttachmentService.update(uploadAttachment1);
