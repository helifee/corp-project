package com.jzy.hio.flow.entity;

import javax.persistence.Column;
import javax.persistence.Table;
import com.jzy.hio.base.entity.BaseEntity;

/**
 * @author admin
 * 
 * 流程传递变量
 */

@Table(name = "PT_FLOW_INSTANCE_VARIABLE")
public class InstanceVariable extends BaseEntity{
	
		
	/**
	 * 变量名称
	 */
	@Column(name = "name")
	private String name;
    
	/**
	 * 变量值
	 */	
	@Column(name = "val")
	private String val;
    
	/**
	 * 流程实例id
	 */
	@Column(name = "fi_id")
	private String fiId;
  		
		
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
    
  		
	public String getVal() {
		return val;
	}
	public void setVal(String val) {
		this.val = val;
	}
    
  		
	public String getFiId() {
		return fiId;
	}
	public void setFiId(String fiId) {
		this.fiId = fiId;
	}
    
  		
	
}
