package com.jzy.hio.flow.entity;
import javax.persistence.Column;
import javax.persistence.Table;
import com.jzy.hio.base.entity.BaseEntity;


/**
 * @author admin
 * 
 * 流程节点连接
 */

@Table(name="PT_FLOW_INSTANCE_STEP")
public class InstanceStep extends BaseEntity{
	
		
	/**
	 * 流程实例id
	 */
	@Column(name="fi_id")
	private String fiId;
    
	/**
	 * 名称
	 */
	@Column(name="name")
	private String name;
    
	/**
	 * 模板连线id
	 */
	@Column(name="step_id")
	private String stepId;
    
	/**
	 * 源节点
	 */
	@Column(name="source_id")
	private String sourceId;
    
	/**
	 * 目标结点
	 */	
	@Column(name="target_id")
	private String targetId;
    
	/**
	 * 开始点x坐标
	 */	
	@Column(name="start_x")
	private Long startX;
    
	/**
	 * 开始点y坐标
	 */		
	@Column(name="start_y")
	private Long startY;
    
	/**
	 * 目标点x坐标
	 */		
	@Column(name="target_x")
	private Long targetX;
    
	/**
	 * 目标点y坐标
	 */	
	@Column(name="target_y")
	private Long targetY;
    
	/**
	 * 状态
	 */		
	@Column(name="status")
	private String status;
    
	/**
	 * 是否有效
	 */	
	@Column(name="disable")
	private Boolean disable;
    
  		
		
	public String getFiId() {
		return fiId;
	}
	public void setFiId(String fiId) {
		this.fiId = fiId;
	}
    
  		
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
   
    
  		
	public String getSourceId() {
		return sourceId;
	}
	public void setSourceId(String sourceId) {
		this.sourceId = sourceId;
	}
    
  		
	public String getTargetId() {
		return targetId;
	}
	public void setTargetId(String targetId) {
		this.targetId = targetId;
	}
    
  		
	public Long getStartX() {
		return startX;
	}
	public void setStartX(Long startX) {
		this.startX = startX;
	}
    
  		
	public Long getStartY() {
		return startY;
	}
	public void setStartY(Long startY) {
		this.startY = startY;
	}
    
  		
	public Long getTargetX() {
		return targetX;
	}
	public void setTargetX(Long targetX) {
		this.targetX = targetX;
	}
    
  		
	public Long getTargetY() {
		return targetY;
	}
	public void setTargetY(Long targetY) {
		this.targetY = targetY;
	}
    
  		
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
    
  		
	public Boolean getDisable() {
		return disable;
	}
	public void setDisable(Boolean disable) {
		this.disable = disable;
	}
	public String getStepId() {
		return stepId;
	}
	public void setStepId(String stepId) {
		this.stepId = stepId;
	}
    
  		
	
}
