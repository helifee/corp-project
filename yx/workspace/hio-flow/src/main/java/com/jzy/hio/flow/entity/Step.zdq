package com.jzy.hio.flow.entity;

import javax.persistence.Column;
import javax.persistence.Table;
import com.jzy.hio.base.entity.BaseEntity;


/**
 * @author admin
 * 
 * 流程模板环节连线
 */

@Table(name ="PT_FLOW_STEP")
public class Step extends BaseEntity{
	
		
	/**
	 * 编号
	 */
	@Column(name ="code")
	private String code;
    
	/**
	 * 条件表达式
	 */
	@Column(name ="condition_expression")
	private String conditionExpression;

	/**
	 * 条件表达式翻译(中文对照)
	 */
	@Column(name = "condition_translation")
	private String conditionTranslation;

	/**
	 * 名称
	 */
	@Column(name ="name")
	private String name;
    
	/**
	 * 流程模板Id
	 */
	@Column(name ="fl_id")
	private String flId;
    
	/**
	 * 源节点
	 */
	@Column(name ="source_id")
	private String sourceId;
    
	/**
	 * 目标结点
	 */
	@Column(name ="target_id")
	private String targetId;
    
	/**
	 * 开始点x坐标
	 */	
	@Column(name ="start_x")
	private Long startX;
    
	/**
	 * 开始点y坐标
	 */	
	@Column(name ="start_y")
	private Long startY;
	
	/**
	 * 目标点x坐标
	 */
	@Column(name ="target_x")
	private Long targetX;
    
	/**
	 * 目标点y坐标
	 */
	@Column(name ="target_y")
	private Long targetY;
	/**
	 * 流程设计图中环节连线ID
	 */
	@Column(name ="node_id")
	private String nodeId;
    
  		
		
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
    
  		
	public String getConditionExpression() {
		return conditionExpression;
	}
	public void setConditionExpression(String conditionExpression) {
		this.conditionExpression = conditionExpression;
	}


	public String getConditionTranslation() {
		return conditionTranslation;
	}
	public void setConditionTranslation(String conditionTranslation) {
		this.conditionTranslation = conditionTranslation;
	}

	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
    
  		
	public String getFlId() {
		return flId;
	}
	public void setFlId(String flId) {
		this.flId = flId;
	}
    
  		
	public String getSourceId() {
		return sourceId;
	}
	public void setSourceId(String sourceId) {
		this.sourceId = sourceId;
	}
    
  		
	public String getTargetId() {
		return targetId;
	}
	public void setTargetId(String targetId) {
		this.targetId = targetId;
	}
    
  		
	public Long getStartX() {
		return startX;
	}
	public void setStartX(Long startX) {
		this.startX = startX;
	}
    
  		
	public Long getStartY() {
		return startY;
	}
	public void setStartY(Long startY) {
		this.startY = startY;
	}
    
  		
	public Long getTargetX() {
		return targetX;
	}
	public void setTargetX(Long targetX) {
		this.targetX = targetX;
	}
    
  		
	public Long getTargetY() {
		return targetY;
	}
	public void setTargetY(Long targetY) {
		this.targetY = targetY;
	}
	public String getNodeId() {
		return nodeId;
	}
	public void setNodeId(String nodeId) {
		this.nodeId = nodeId;
	}
    
}
