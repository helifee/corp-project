package com.jzy.hio.flow.entity;

import javax.persistence.Column;
import javax.persistence.Table;
import com.jzy.hio.base.entity.BaseEntity;


/**
 * @author admin
 * 
 * 系统通知消息用户配置表
 */

@Table(name ="PT_FLOW_SYS_NOTICE_MSG_USER_CONFIG")
public class SysNoticeMsgUserConfig extends BaseEntity{

	/**
	 * 消息推送地址
	 */
	@Column(name ="post_url")
	private String postUrl;

	/**
	 * 消息修改地址
	 */
	@Column(name ="modify_url")
	private String modifyUrl;

	/**
	 * 接收人登录账号
	 */
	@Column(name ="login_name")
	private String loginName;

	/**
	 * 接收人姓名
	 */
	@Column(name ="user_name")
	private String userName;

	/**
	 * 接收人id
	 */
	@Column(name ="user_id")
	private String userId;

	/**
	 * 接收人所在数据库实例名
	 */
	@Column(name ="tend_code")
	private String tendCode;

	public String getPostUrl() {
		return postUrl;
	}

	public void setPostUrl(String postUrl) {
		this.postUrl = postUrl;
	}

	public String getLoginName() {
		return loginName;
	}

	public void setLoginName(String loginName) {
		this.loginName = loginName;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public String getTendCode() {
		return tendCode;
	}

	public void setTendCode(String tendCode) {
		this.tendCode = tendCode;
	}

	public String getModifyUrl() {
		return modifyUrl;
	}

	public void setModifyUrl(String modifyUrl) {
		this.modifyUrl = modifyUrl;
	}
}
