package com.jzy.hio.flow;

import org.mybatis.spring.annotation.MapperScan;
import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.annotation.Import;

import com.jzy.hio.base.init.DynamicDataSourceRegister;
import com.jzy.hio.base.interceptor.DynamicDataSourceAop;

//@EnableDiscoveryClient
@SpringBootApplication
@MapperScan("com.jzy.hio.flow.mapper")
@Import({DynamicDataSourceRegister.class, DynamicDataSourceAop.class})
public class Application {

	public static void main(String[] args) {
		SpringApplication.run(Application.class, args);
		System.out.println("【【【【【【 Flow微服务 】】】】】】已启动.");

	}
}