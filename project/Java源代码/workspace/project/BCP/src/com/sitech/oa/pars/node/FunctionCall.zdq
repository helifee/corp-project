package com.sitech.oa.pars.node;

public class FunctionCall implements Node {
	public NodeToken functionName;
	public ParamList paramList;
	public String path;
	public FunctionCall(NodeToken functionName,ParamList list,String path){
		this.functionName=functionName;
		this.paramList=list;
		this.path=path;
	}
	public void accept(Visitor v) {
		v.visitFunctionCall(this);

	}

}
