package com.sitech.sql.parse.node;

public class Insert implements Node {
	public NodeToken tableNameToken;
	public InsertColumn insertColumn;
	public Values values;
	
	public Insert(NodeToken tableNameToken, InsertColumn insertColumn,
			Values values) {
		this.tableNameToken = tableNameToken;
		this.insertColumn = insertColumn;
		this.values = values;
	}

	public void accept(Visitor v) {
		v.visitInsert(this);

	}

}
