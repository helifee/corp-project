package com.sitech.sql.parse.node;

public class Column implements Node {
	public ColumnExp columnExp;
	public NodeToken otherName;
	public Var var;
	
	public Column(ColumnExp columnExp, NodeToken otherName,Var var) {
		this.columnExp = columnExp;
		this.otherName = otherName;
		this.var=var;
	}

	public void accept(Visitor v) {
		v.visitColumn(this);

	}

}
