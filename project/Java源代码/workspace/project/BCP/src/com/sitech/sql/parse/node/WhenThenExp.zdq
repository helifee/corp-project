package com.sitech.sql.parse.node;

public class WhenThenExp implements Node {
	public WhereColumns whereColumns;
	public ColumnExp columnExp;
	
	public WhenThenExp(WhereColumns whereColumns, ColumnExp columnExp) {
		this.whereColumns = whereColumns;
		this.columnExp = columnExp;
	}

	public void accept(Visitor v) {
		v.visitWhenThenExp(this);

	}

}
