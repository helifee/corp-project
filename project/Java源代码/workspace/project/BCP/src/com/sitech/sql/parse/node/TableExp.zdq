package com.sitech.sql.parse.node;

public class TableExp implements Node {
	public TableExp tableExp;
	public Select select;
	public TableID tableid;
	
	public TableExp(TableExp tableExp, Select select, TableID tableid) {
		this.tableExp = tableExp;
		this.select = select;
		this.tableid = tableid;
	}

	public void accept(Visitor v) {
		v.visitTableExp(this);
		
	}

}
