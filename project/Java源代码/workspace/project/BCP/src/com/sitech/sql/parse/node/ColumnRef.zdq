package com.sitech.sql.parse.node;

public class ColumnRef implements Node {
	public NodeToken tableName;
	public NodeToken columnName;
	
	public ColumnRef(NodeToken tableName, NodeToken columnName) {
		this.tableName = tableName;
		this.columnName = columnName;
	}

	public void accept(Visitor v) {
		v.visitColumnRef(this);
	}

}
