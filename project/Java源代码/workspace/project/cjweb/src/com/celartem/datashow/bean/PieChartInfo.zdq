package com.celartem.datashow.bean;

import java.util.List;
import java.util.Map;

/**
 * 饼图数据（部门/表计）（用于与flex通讯）
 * 
 * @author zhangdaoqiang
 *
 */
public class PieChartInfo {

	/**
	 * 用电量数据
	 */
	private List<Map<String, Object>> eleList;
	
	/**
	 * 用热量数据
	 */
	private List<Map<String, Object>> heatList;
	
	/**
	 * 用冷量数据
	 */
	private List<Map<String, Object>> coolList;
	
	/**
	 * 用冷水量
	 */
	private List<Map<String, Object>> coldWaterList;
	
	/**
	 * 用热水量
	 */
	private List<Map<String, Object>> hotWaterList;
	/**
	 * @return the eleList
	 */
	public List<Map<String, Object>> getEleList() {
		return eleList;
	}
	/**
	 * @param eleList the eleList to set
	 */
	public void setEleList(List<Map<String, Object>> eleList) {
		this.eleList = eleList;
	}
	/**
	 * @return the heatList
	 */
	public List<Map<String, Object>> getHeatList() {
		return heatList;
	}
	/**
	 * @param heatList the heatList to set
	 */
	public void setHeatList(List<Map<String, Object>> heatList) {
		this.heatList = heatList;
	}
	/**
	 * @return the coldWaterList
	 */
	public List<Map<String, Object>> getColdWaterList() {
		return coldWaterList;
	}
	/**
	 * @param coldWaterList the coldWaterList to set
	 */
	public void setColdWaterList(List<Map<String, Object>> coldWaterList) {
		this.coldWaterList = coldWaterList;
	}
	/**
	 * @return the hotWaterList
	 */
	public List<Map<String, Object>> getHotWaterList() {
		return hotWaterList;
	}
	/**
	 * @param hotWaterList the hotWaterList to set
	 */
	public void setHotWaterList(List<Map<String, Object>> hotWaterList) {
		this.hotWaterList = hotWaterList;
	}
	/**
	 * @return the coolList
	 */
	public List<Map<String, Object>> getCoolList() {
		return coolList;
	}
	/**
	 * @param coolList the coolList to set
	 */
	public void setCoolList(List<Map<String, Object>> coolList) {
		this.coolList = coolList;
	}
}
