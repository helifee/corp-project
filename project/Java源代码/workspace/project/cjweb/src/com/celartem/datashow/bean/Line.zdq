package com.celartem.datashow.bean;

import java.util.List;

/**
 * 支路信息
 * 
 * @author zhangdaoqiang
 *
 */
public class Line {

	/**
	 * 支路ID
	 */
	private int bchId;
	
	/**
	 * 支路名称
	 */
	private String bchName;
	
	/**
	 * 父支路ID
	 */
	private int preId;
	
	/**
	 * 支路方向
	 */
	private boolean direction;
	
	/**
	 * 支路相关设备集合
	 */
	private List<Device> devices;
	
	public Line() {
		super();
	}

	public Line(int bchId) {
		super();
		this.bchId = bchId;
	}

	/**
	 * @return the bchId
	 */
	public int getBchId() {
		return bchId;
	}

	/**
	 * @param bchId the bchId to set
	 */
	public void setBchId(int bchId) {
		this.bchId = bchId;
	}

	/**
	 * @return the bchName
	 */
	public String getBchName() {
		return bchName;
	}

	/**
	 * @param bchName the bchName to set
	 */
	public void setBchName(String bchName) {
		this.bchName = bchName;
	}

	/**
	 * @return the preId
	 */
	public int getPreId() {
		return preId;
	}

	/**
	 * @param preId the preId to set
	 */
	public void setPreId(int preId) {
		this.preId = preId;
	}

	/**
	 * @return the direction
	 */
	public boolean isDirection() {
		return direction;
	}

	/**
	 * @param direction the direction to set
	 */
	public void setDirection(boolean direction) {
		this.direction = direction;
	}

	/**
	 * @return the devices
	 */
	public List<Device> getDevices() {
		return devices;
	}

	/**
	 * @param devices the devices to set
	 */
	public void setDevices(List<Device> devices) {
		this.devices = devices;
	}

	/* (non-Javadoc)
	 * @see java.lang.Object#hashCode()
	 */
	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + bchId;
		return result;
	}

	/* (non-Javadoc)
	 * @see java.lang.Object#equals(java.lang.Object)
	 */
	@Override
	public boolean equals(Object obj) {
		if(this == obj) {
			return true;
		}
		if(obj instanceof Line) {
			Line l = (Line)obj;
			if(this.getBchId() == l.getBchId()) {
				return true;
			}
		}
		return false;
	}

}
