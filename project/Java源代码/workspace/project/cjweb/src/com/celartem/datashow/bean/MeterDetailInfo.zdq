package com.celartem.datashow.bean;

/**
 * 表计信息
 * 
 * @author zhangdaoqiang
 *
 */
public class MeterDetailInfo {

	/**
	 * 当日用量
	 */
	private double todayAmount;
	
	/**
	 * 昨日用量
	 */
	private double yesterdayAmount;
	
	/**
	 * 历史最大用量
	 */
	private double maxAmount;
	
	/**
	 * 历史最小用量
	 */
	private double minAmount;
	
	/**
	 * 历史平均用量
	 */
	private double avgAmount;

	/**
	 * @return the todayAmount
	 */
	public double getTodayAmount() {
		return todayAmount;
	}

	/**
	 * @param todayAmount the todayAmount to set
	 */
	public void setTodayAmount(double todayAmount) {
		this.todayAmount = todayAmount;
	}

	/**
	 * @return the yesterdayAmount
	 */
	public double getYesterdayAmount() {
		return yesterdayAmount;
	}

	/**
	 * @param yesterdayAmount the yesterdayAmount to set
	 */
	public void setYesterdayAmount(double yesterdayAmount) {
		this.yesterdayAmount = yesterdayAmount;
	}

	/**
	 * @return the maxAmount
	 */
	public double getMaxAmount() {
		return maxAmount;
	}

	/**
	 * @param maxAmount the maxAmount to set
	 */
	public void setMaxAmount(double maxAmount) {
		this.maxAmount = maxAmount;
	}

	/**
	 * @return the minAmount
	 */
	public double getMinAmount() {
		return minAmount;
	}

	/**
	 * @param minAmount the minAmount to set
	 */
	public void setMinAmount(double minAmount) {
		this.minAmount = minAmount;
	}

	/**
	 * @return the avgAmount
	 */
	public double getAvgAmount() {
		return avgAmount;
	}

	/**
	 * @param avgAmount the avgAmount to set
	 */
	public void setAvgAmount(double avgAmount) {
		this.avgAmount = avgAmount;
	}
}
