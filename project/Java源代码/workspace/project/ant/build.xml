<?xml version="1.0" encoding="utf-8"?>

<project name="myant" basedir="." default="package">
	
	<property name="test" value="test"></property>
	<property name="doc" value="doc"></property>
	<property name="compile" value="compile"></property>

	<target name="init">
		<mkdir dir="${test}"/>
		<mkdir dir="${doc}"/>
		<mkdir dir="${compile}"/>
	</target>

	<target name="preprocess" depends="init">
	
	</target>
	
	<target name="compile" depends="init,preprocess">
		<javac srcdir="src" destdir="compile"></javac>
	</target>
	
	<target name="package" depends="compile">
		<tstamp></tstamp>
		<jar destfile="${DSTAMP}.jar" basedir="${compile}">
			<manifest>
				<attribute name="Build-By" value="${user.name}"/>
				<attribute name="Main-Class" value="com.Ma"/>
			</manifest>
		</jar>
	</target>
	
</project>