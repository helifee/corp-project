package com.langsin.jdk5;

import java.util.ArrayList;
import java.util.Collection;

public class BoxTest
{
	public static void main(String[] args)
	{
		int a = 3;
		Collection<Integer> c = new ArrayList<Integer>();
		c.add(a);
		c.add(a + 3);
		
		for(Integer i : c)
		{
			System.out.println(i);
		}
	} 
}
