						 newTd2.className="";
						 newTd2.innerHTML = "<td class='text1' width='150'>&nbsp;"+objName[i]+"</td>";
					 }
				}
			}
			function imageScan(flag,imagepath,imagename){
				if(imagepath==null||imagepath==""){
					alert("文件路径不存在");
					return;
				}
				if(imagename==null||imagename==""){
					alert("文件不存在");
