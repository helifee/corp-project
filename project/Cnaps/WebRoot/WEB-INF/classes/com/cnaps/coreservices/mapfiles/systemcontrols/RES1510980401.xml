<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>Basic mapping example</description>
	<class name="com.cnaps.coreservices.CommonXMLEntity">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />

		<!-- 原发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="ResponseHead" />
		</field>
		<!--原接收服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="ResponseHead" />
		</field>
		<!--原渠道报文标识号-->
		<field name="messageId">
			<bind-xml name="MessageId" node="element"
				location="ResponseHead" />
		</field>
		<!--原报文发送时间-->
		<field name="sendDateTime">
			<bind-xml name="SendDateTime" node="element"
				location="ResponseHead" />
		</field>
		<!--处理码-->
		<field name="processCode">
			<bind-xml name="ProcessCode" node="element"
				location="ResponseHead" />
		</field>
		<!--处理信息-->
		<field name="processDes">
			<bind-xml name="ProcessDes" node="element"
				location="ResponseHead" />
		</field>


		<!--查询代码-->
		<field name="queryCd">
			<bind-xml name="QueryCd" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageBeginPos">
			<bind-xml name="TurnPageBeginPos" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageShowNum">
			<bind-xml name="TurnPageShowNum" node="element"
				location="ResponseHead" />
		</field>
		
		<field name="recordcount">
			<bind-xml name="RecordCount" node="element"
				location="ResponseHead" />
		</field>
		<field name="results"
			type="com.cnaps.hvps.persistence.messages.Hvpspartyinfo"
			collection="arraylist">
			<bind-xml name="QryResults" node="element"
				location="ResponseFields" />
		</field>
	</class>

	<class
		name="com.cnaps.hvps.persistence.messages.Hvpspartyinfo">
	
		<field name="id" type="java.lang.Long">
			<bind-xml name="id" node="element" />
		</field>
	    <!--参与者行号-->
		<field name="bankcode" type="java.lang.String">
			<bind-xml name="bankcode" node="element" />
		</field>
		<!--全称-->
		<field name="participantname" type="java.lang.String">
			<bind-xml name="participantname" node="element" />
		</field>	
		<!--简称-->	
		<field name="bankaliasename" type="java.lang.String">
			<bind-xml name="participantshortname" node="element" />
		</field>
		<!--行别代码-->	
		<field name="bkctgycd" type="java.lang.String">
			<bind-xml name="bankcategorycode" node="element" />
		</field>
		<!--参与机构类型-->
		<field name="ptytp" type="java.lang.String">
			<bind-xml name="participanttype" node="element" />
		</field>
		<!-- 所属直接参与机构 -->
		<field name="directbankcode" type="java.lang.String">
			<bind-xml name="directbankcode" node="element" />
		</field>
		<!--所属机构ccpp-->
		<field name="bankcenter" type="java.lang.String">
			<bind-xml name="nodecode" node="element" />
		</field>
		<!--状态-->
		<field name="status" type="java.lang.String">
			<bind-xml name="status" node="element" />
		</field>	
		<!--加入日期-->	
		<field name="effdate" type="java.lang.String">
			<bind-xml name="effectivedt" node="element" />
		</field>
		<!--退出日期-->
		<field name="invdate" type="java.lang.String">
			<bind-xml name="expdt" node="element" />
		</field>
		
		<field name="orgstatus" type="java.lang.String">
			<bind-xml name="orgstatus" node="element" />
		</field>
		
	</class>

</mapping>

