<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>票交来报待处理</description>
	<class name="com.cnaps.coreservices.CommonXMLEntity">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />

		<!-- 原发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="ResponseHead" />
		</field>
		<!--原接收服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="ResponseHead" />
		</field>
		<!--原渠道报文标识号-->
		<field name="messageId">
			<bind-xml name="MessageId" node="element"
				location="ResponseHead" />
		</field>
		<!--原报文发送时间-->
		<field name="sendDateTime">
			<bind-xml name="SendDateTime" node="element"
				location="ResponseHead" />
		</field>
		<!--处理码-->
		<field name="processCode">
			<bind-xml name="ProcessCode" node="element"
				location="ResponseHead" />
		</field>
		<!--处理信息-->
		<field name="processDes">
			<bind-xml name="ProcessDes" node="element"
				location="ResponseHead" />
		</field>


		<!--查询代码-->
		<field name="queryCd">
			<bind-xml name="QueryCd" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageBeginPos">
			<bind-xml name="TurnPageBeginPos" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageShowNum">
			<bind-xml name="TurnPageShowNum" node="element"
				location="ResponseHead" />
		</field>
		<field name="recordcount">
			<bind-xml name="RecordCount" node="element"
				location="ResponseHead" />
		</field>
		<field name="results"
			type="com.cnaps.hvps.persistence.cis.CisBepsBkNodeTradeDetail"
			collection="arraylist">
			<bind-xml name="QryResults" node="element"
				location="ResponseFields" />
		</field>
	</class>

	<class name="com.cnaps.hvps.persistence.cis.CisBepsBkNodeTradeDetail">
		<field name="id" type="java.lang.Long"><bind-xml name="id" node="element" /></field>
		<field name="parented" type="java.lang.Long"><bind-xml name="parented" node="element" /></field>
		<field name="endtoendid" type="java.lang.String"><bind-xml name="endtoendid" node="element" /></field>
		<field name="txid" type="java.lang.String"><bind-xml name="txid" node="element" /></field>
		<field name="pmttp" type="java.lang.String"><bind-xml name="pmttp" node="element" /></field>
		<field name="currencycd" type="java.lang.String"><bind-xml name="currencycd" node="element" /></field>
		<field name="amount" type="java.lang.String"><bind-xml name="amount" node="element" /></field>
		<field name="dbtrnm" type="java.lang.String"><bind-xml name="dbtrnm" node="element" /></field>
		<field name="dbtracct" type="java.lang.String"><bind-xml name="dbtracct" node="element" /></field>
		<field name="dbtrissuer" type="java.lang.String"><bind-xml name="dbtrissuer" node="element" /></field>
		<field name="dbtrmmbid" type="java.lang.String"><bind-xml name="dbtrmmbid" node="element" /></field>
		<field name="dbtrbrnchid" type="java.lang.String"><bind-xml name="dbtrbrnchid" node="element" /></field>
		<field name="cdtrmmbid" type="java.lang.String"><bind-xml name="cdtrmmbid" node="element" /></field>
		<field name="cdtrbrnchid" type="java.lang.String"><bind-xml name="cdtrbrnchid" node="element" /></field>
		<field name="cdtrnm" type="java.lang.String"><bind-xml name="cdtrnm" node="element" /></field>
		<field name="cdtracct" type="java.lang.String"><bind-xml name="cdtracct" node="element" /></field>
		<field name="cdtrissuer" type="java.lang.String"><bind-xml name="cdtrissuer" node="element" /></field>
		<field name="pmtkd" type="java.lang.String"><bind-xml name="pmtkd" node="element" /></field>
		<field name="addtlinf" type="java.lang.String"><bind-xml name="addtlinf" node="element" /></field>
		<field name="ustrd" type="java.lang.String"><bind-xml name="ustrd" node="element" /></field>
		<field name="ornglciscnsgndt" type="java.lang.String"><bind-xml name="ornglciscnsgndt" node="element" /></field>
		<field name="ornglcistxid" type="java.lang.String"><bind-xml name="ornglcistxid" node="element" /></field>
		<field name="ornglcisnotesno" type="java.lang.String"><bind-xml name="ornglcisnotesno" node="element" /></field>
		<field name="rcptsts" type="java.lang.String"><bind-xml name="rcptsts" node="element" /></field>
		<field name="txrjctcd" type="java.lang.String"><bind-xml name="txrjctcd" node="element" /></field>
		<field name="txrjctinf" type="java.lang.String"><bind-xml name="txrjctinf" node="element" /></field>
		<field name="prcsts" type="java.lang.String"><bind-xml name="prcsts" node="element" /></field>
		<field name="netgdt" type="java.lang.String"><bind-xml name="netgdt" node="element" /></field>
		<field name="netgrnd" type="java.lang.String"><bind-xml name="netgrnd" node="element" /></field>
		<field name="sttlmdt" type="java.lang.String"><bind-xml name="sttlmdt" node="element" /></field>
	</class>

</mapping>

