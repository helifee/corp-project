<?xml version="1.0" encoding="UTF-8"?>
<mapping>
	<description>Basic mapping example</description>
	<class name="com.cnaps.coreservices.CommonXMLEntity">
		<!-- the name of the xml file's root element  -->
		<map-to xml="CCMS_DATA" />
		<field name="ccms_parameter_count">
			<bind-xml name="CCMS_MSGTYPE_COUNT" node="element"
				location="CCMS_MSGTYPE_DATA" />
		</field>

		<field name="results"
			type="com.cnaps.hvps.persistence.info.CcmsDrctPtyInfo"
			collection="arraylist">
			<bind-xml name="ROW" node="element"
				location="CCMS_MSGTYPE_DATA" />
		</field>
	</class>
	<class name="com.cnaps.hvps.persistence.info.CcmsDrctPtyInfo">
        <field name="syscode" type="java.lang.String">
			<bind-xml name="SYSCODE" node="element" />
		</field>
		<field name="msgtype" type="java.lang.String">
			<bind-xml name="MSGTYPE" node="element" />
		</field>

		<field name="biztype" type="java.lang.String">
			<bind-xml name="BIZTYPE" node="element" />
		</field>
		<field name="bizdtltypepurpose" type="java.lang.String">
			<bind-xml name="BIZDTLTYPEPURPOSE" node="element" />
		</field>
		
	</class>
</mapping>