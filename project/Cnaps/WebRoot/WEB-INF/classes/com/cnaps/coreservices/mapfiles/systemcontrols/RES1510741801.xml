<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>业务种类与类型管理查询</description>
	<class name="com.cnaps.coreservices.CommonXMLEntity">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />

		<!-- 原发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="ResponseHead" />
		</field>
		<!--原接收服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="ResponseHead" />
		</field>
		<!--原渠道报文标识号-->
		<field name="messageId">
			<bind-xml name="MessageId" node="element"
				location="ResponseHead" />
		</field>
		<!--原报文发送时间-->
		<field name="sendDateTime">
			<bind-xml name="SendDateTime" node="element"
				location="ResponseHead" />
		</field>
		<!--处理码-->
		<field name="processCode">
			<bind-xml name="ProcessCode" node="element"
				location="ResponseHead" />
		</field>
		<!--处理信息-->
		<field name="processDes">
			<bind-xml name="ProcessDes" node="element"
				location="ResponseHead" />
		</field>


		<!--查询代码-->
		<field name="queryCd">
			<bind-xml name="QueryCd" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageBeginPos">
			<bind-xml name="TurnPageBeginPos" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageShowNum">
			<bind-xml name="TurnPageShowNum" node="element"
				location="ResponseHead" />
		</field>
		<field name="recordcount">
			<bind-xml name="RecordCount" node="element"
				location="ResponseHead" />
		</field>
		<field name="results"
			type="com.cnaps.hvps.persistence.info.CheckEarmarkManage"
			collection="arraylist">
			<bind-xml name="QryResults" node="element"
				location="ResponseFields" />
		</field>
	</class>

	<class name="com.cnaps.hvps.persistence.info.CheckEarmarkManage">
		<field name="id" type="java.lang.Long"><bind-xml name="id" node="element" /></field>
		<field name="instgpty" type="java.lang.String"><bind-xml name="instgpty" node="element" /></field>
		<field name="instdpty" type="java.lang.String"><bind-xml name="instdpty" node="element" /></field>
		<field name="instgdrctpty" type="java.lang.String"><bind-xml name="instgdrctpty" node="element" /></field>
		<field name="instddrctpty" type="java.lang.String"><bind-xml name="instddrctpty" node="element" /></field>
		<field name="orgnlinstddrctpty" type="java.lang.String"><bind-xml name="orgnlinstddrctpty" node="element" /></field>
		<field name="orgnlmsgid" type="java.lang.String"><bind-xml name="orgnlmsgid" node="element" /></field>
		<field name="orgnlmsgtpcd" type="java.lang.String"><bind-xml name="orgnlmsgtpcd" node="element" /></field>
		<field name="applyorccltp" type="java.lang.String"><bind-xml name="applyorccltp" node="element" /></field>
		<field name="issuedt" type="java.lang.String"><bind-xml name="issuedt" node="element" /></field>
		<field name="notesno" type="java.lang.String"><bind-xml name="notesno" node="element" /></field>
		<field name="drawerbrnchid" type="java.lang.String"><bind-xml name="drawerbrnchid" node="element" /></field>
		<field name="draweracct" type="java.lang.String"><bind-xml name="draweracct" node="element" /></field>
		<field name="amount" type="java.lang.String"><bind-xml name="amount" node="element" /></field>
		<field name="chckmd" type="java.lang.String"><bind-xml name="chckmd" node="element" /></field>
		<field name="chckcd" type="java.lang.String"><bind-xml name="chckcd" node="element" /></field>
		<field name="imagefrontlngth" type="java.lang.Long"><bind-xml name="imagefrontlngth" node="element" /></field>
		<field name="imagefrontdt" type="java.lang.String"><bind-xml name="imagefrontdt" node="element" /></field>
		<field name="orgnlinstgpty" type="java.lang.String"><bind-xml name="orgnlinstgpty" node="element" /></field>
		<field name="direction" type="java.lang.String"><bind-xml name="direction" node="element" /></field>
		<field name="pmtgrpid" type="java.lang.String"><bind-xml name="pmtgrpid" node="element" /></field>
		<field name="workdt" type="java.lang.String"><bind-xml name="workdt" node="element" /></field>
		<field name="msgid" type="java.lang.String"><bind-xml name="msgid" node="element" /></field>
		<field name="credttm" type="java.lang.String"><bind-xml name="credttm" node="element" /></field>
		<field name="recptmsgid" type="java.lang.String"><bind-xml name="recptmsgid" node="element" /></field>
		<field name="recptdttm" type="java.lang.String"><bind-xml name="recptdttm" node="element" /></field>
		<field name="systemcd" type="java.lang.String"><bind-xml name="systemcd" node="element" /></field>
		<field name="ustrd" type="java.lang.String"><bind-xml name="ustrd" node="element" /></field>
		<field name="status" type="java.lang.String"><bind-xml name="status" node="element" /></field>
		<field name="prcsts" type="java.lang.String"><bind-xml name="prcsts" node="element" /></field>
		<field name="prccd" type="java.lang.String"><bind-xml name="prccd" node="element" /></field>
		<field name="rjctcd" type="java.lang.String"><bind-xml name="rjctcd" node="element" /></field>
		<field name="rjctinf" type="java.lang.String"><bind-xml name="rjctinf" node="element" /></field>
		<field name="rjctedptyid" type="java.lang.String"><bind-xml name="rjctedptyid" node="element" /></field>
		<field name="prcdt" type="java.lang.String"><bind-xml name="prcdt" node="element" /></field>
	</class>

</mapping>

