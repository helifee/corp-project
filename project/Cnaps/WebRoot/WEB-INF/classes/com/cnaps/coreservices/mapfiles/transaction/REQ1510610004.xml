<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>往账维护</description>
	<class name="com.cnaps.hvps.form.transfer.TransProcessForm">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />

		<!-- 发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="RequestHead" />
		</field>
		<!--接收服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="RequestHead" />
		</field>
		<!-- 交易码 -->
		<field name="businesscode">
			<bind-xml name="BusinessCode" node="element"
				location="RequestHead" />
		</field>
		<!-- MsgCode -->
		<field name="msgcode">
			<bind-xml name="MsgCode" node="element"
				location="RequestHead" />
		</field>
		<!-- 网点号 -->
		<field name="brnchCd">
			<bind-xml name="BrnchCd" node="element"
				location="RequestHead" />
		</field>
		<!-- 终端号 -->
		<field name="termId">
			<bind-xml name="TermId" node="element"
				location="RequestHead" />
		</field>
		<!-- 操作柜员 -->
		<field name="operTeller">
			<bind-xml name="OperTeller" node="element"
				location="RequestHead" />
		</field>


		<!-- 支付交易组号 -->
		<field name="pmtgrpid">
			<bind-xml name="PmtGrpId" node="element" location="RequestFields" />
		</field>
		
		
		
		<!-- 申请人名称 -->
		<field name="proposernm">
			<bind-xml name="ProposerNm" node="element" location="RequestFields" />
		</field>
		
		<!-- 申请人地址 -->
		<field name="proposeraddr">
			<bind-xml name="ProposerAddr" node="element" location="RequestFields" />
		</field>
		
		<!-- 申请人联系电话 -->
		<field name="proposertel">
			<bind-xml name="ProposerTel" node="element" location="RequestFields" />
		</field>
		
		<!-- 付款人信息 -->
		<!-- 付款人账号 -->
		<field name="dbtracct">
			<bind-xml name="DbtrAcct" node="element" location="RequestFields" />
		</field>
		
		<!-- 付款人名称 -->
		<field name="dbtrnm">
			<bind-xml name="DbtrNm" node="element" location="RequestFields" />
		</field>
		
		<!-- 付款人开户行名称 -->
		<field name="dbtrissuer">
			<bind-xml name="DbtrIssuer" node="element" location="RequestFields" />
		</field>
		
		<!-- 付款清算行行号 -->
		<field name="dbtrmmbid">
			<bind-xml name="DbtrMmbId" node="element" location="RequestFields" />
		</field>
		
		<!-- 付款行行号 -->
		<field name="dbtrbrnchid">
			<bind-xml name="DbtrBrnchId" node="element" location="RequestFields" />
		</field>
		
		<!-- 付款人地址 -->
		<field name="dbtraddr">
			<bind-xml name="DbtrAddr" node="element" location="RequestFields" />
		</field>
		
		
		
		
		<!-- 收款人信息 -->
		<!-- 收款人账号 -->
		<field name="cdtracct">
			<bind-xml name="CdtrAcct" node="element" location="RequestFields" />
		</field>
		
		<!-- 收款人户名 -->
		<field name="cdtrnm">
			<bind-xml name="CdtrNm" node="element" location="RequestFields" />
		</field>
		<!-- 收款人开户行行号 -->
		<field name="cdtrissuer">
			<bind-xml name="CdtrIssuer " node="element" location="RequestFields" />
		</field>
		<!-- 收款清算行行号 -->
		<field name="cdtrmmbid">
			<bind-xml name="CdtrMmbId " node="element" location="RequestFields" />
		</field>
		<!-- 收款行行号 -->
		<field name="cdtrbrnchid">
			<bind-xml name="CdtrBrnchId " node="element" location="RequestFields" />
		</field>
		<!-- 收款人地址 -->
		<field name="cdtraddr">
			<bind-xml name="CdtrAddr " node="element" location="RequestFields" />
		</field>
		
		
		<!-- 其他信息 -->
		
		
	</class>

</mapping>

