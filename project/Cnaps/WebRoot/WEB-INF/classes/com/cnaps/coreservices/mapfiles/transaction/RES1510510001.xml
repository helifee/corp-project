<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>计费与返还通知查询回执</description> 
      <class name="com.cnaps.coreservices.CommonXMLEntity" >
      <!-- the name of the xml file's root element  -->
       <map-to xml="Document"/>
     
         <!-- 原发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="ResponseHead"/>
       </field>
       <!--原接收服务名-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="ResponseHead"/>
       </field>
        <!--原渠道报文标识号 -->
       <field name="messageId">
          <bind-xml name="MessageId" node="element" location="ResponseHead"/>
       </field>
      
        <!--原报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="ResponseHead"/>
       </field>
         <!--处理码-->
       <field name="processCode">
          <bind-xml name="ProcessCode" node="element" location="ResponseHead"/>
       </field>
         <!--处理信息-->
       <field name="processDes">
          <bind-xml name="ProcessDes" node="element" location="ResponseHead"/>
       </field>

       
         <!--查询代码-->
       <field name="queryCd">
          <bind-xml name="QueryCd" node="element" location="ResponseHead"/>
       </field>
       <field name="recordcount">
          <bind-xml name="RecordCount" node="element" location="ResponseHead"/>
       </field>
       <field name="pagecount">
          <bind-xml name="PageCount" node="element" location="ResponseHead"/>
       </field>
       <field name="results" type="com.cnaps.hvps.persistence.clearingAccount.AccountingReturn" collection="arraylist">
          <bind-xml name="QryResults" node="element" location="ResponseFields"/>
       </field>
      </class>

	   <class name="com.cnaps.hvps.persistence.clearingAccount.AccountingReturn">		
       
       <field name="id" type="java.lang.String">
			<bind-xml name="id" node="element" />
		</field>
		<field name="msgId" type="java.lang.String">
			<bind-xml name="msgid" node="element" />
		</field>
		<field name="creDtTm" type="java.lang.String">
			<bind-xml name="credttm" node="element" />
		</field>
		<field name="workDate" type="java.lang.String">
			<bind-xml name="workdate" node="element" />
		</field>
		<field name="sendDrctPty" type="java.lang.String">
			<bind-xml name="senddrctpty" node="element" />
		</field>
		<field name="sendIndrctPty" type="java.lang.String">
			<bind-xml name="sendindrctpty" node="element" />
		</field>
		<field name="recvDrctPty" type="java.lang.String">
			<bind-xml name="recvdrctpty" node="element" />
		</field>
		<field name="recvIndrctPty" type="java.lang.String">
			<bind-xml name="recvindrctpty" node="element" />
		</field>
		<field name="chargeType" type="java.lang.String">
			<bind-xml name="chargetype" node="element" />
		</field>
		<field name="chargeParticipant" type="java.lang.String">
			<bind-xml name="chargeparticipant" node="element" />
		</field>
		<field name="chargeMonth" type="java.lang.String">
			<bind-xml name="chargemonth" node="element" />
		</field>
		<field name="startDate" type="java.lang.String">
			<bind-xml name="startdate" node="element" />
		</field>
		<field name="endDate" type="java.lang.String">
			<bind-xml name="enddate" node="element" />
		</field>
		<field name="totalAmount" type="java.lang.String">
			<bind-xml name="totalamount" node="element" />
		</field>
		<field name="hvpsTotalAmount" type="java.lang.String">
			<bind-xml name="hvpstotalamount" node="element" />
		</field>
		<field name="bepsTotalAmount" type="java.lang.String">
			<bind-xml name="bepstotalamount" node="element" />
		</field>
		<field name="ibpsTotalAmount" type="java.lang.String">
			<bind-xml name="ibpstotalamount" node="element" />
		</field>
       </class>
  </mapping>

