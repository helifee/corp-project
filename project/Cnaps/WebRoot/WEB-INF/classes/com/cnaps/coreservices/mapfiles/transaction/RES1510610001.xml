<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>业务汇总查询回执</description> 
      <class name="com.cnaps.coreservices.CommonXMLEntity" >
      <!-- the name of the xml file's root element  -->
       <map-to xml="Document"/>
     
         <!-- 原发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="ResponseHead"/>
       </field>
       <!--原接收服务名-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="ResponseHead"/>
       </field>
        <!--原渠道报文标识号
       <field name="messageId">
          <bind-xml name="MessageId" node="element" location="ResponseHead"/>
       </field>
       -->
        <!--原报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="ResponseHead"/>
       </field>
         <!--处理码-->
       <field name="processCode">
          <bind-xml name="ProcessCode" node="element" location="ResponseHead"/>
       </field>
         <!--处理信息-->
       <field name="processDes">
          <bind-xml name="ProcessDes" node="element" location="ResponseHead"/>
       </field>
       
       
         <!--查询代码-->
       <field name="queryCd">
          <bind-xml name="QueryCd" node="element" location="ResponseHead"/>
       </field>
       <field name="recordcount">
          <bind-xml name="RecordCount" node="element" location="ResponseHead"/>
       </field>
       <field name="pagecount">
          <bind-xml name="PageCount" node="element" location="ResponseHead"/>
       </field>
       <field name="results" type="com.cnaps.hvps.persistence.transfer.TransProcessEntity" collection="arraylist">
          <bind-xml name="QryResults" node="element" location="ResponseFields"/>
       </field>
      </class>
	
	   <class name="com.cnaps.hvps.persistence.transfer.TransProcessEntity">
	   <!-- 支付交易组号 -->		
       <field name="pmtgrpid" type="java.lang.String">
			<bind-xml name="pmtgrpid" node="element" />
		</field>
	 <!-- 工作日期 -->
		<field name="workdt" type="java.lang.String">
			<bind-xml name="workdt" node="element" />
		</field>
		<!-- 全机构代码 -->
		<field name="brnchid" type="java.lang.String">
			<bind-xml name="brnchid" node="element" />
		</field>
		<!-- 机构代码 -->
		<field name="orgCode" type="java.lang.String">
			<bind-xml name="orgcode" node="element" />
		</field>
		
		<field name="msgtpid" type="java.lang.String">
			<bind-xml name="msgtpid" node="element" />
		</field>
       <field name="pmttp" type="java.lang.String">
			<bind-xml name="pmttp" node="element" />
		</field>
		<field name="sttlmprty" type="java.lang.String">
			<bind-xml name="sttlmprty" node="element" />
		</field>
		
		<field name="applicantacct" type="java.lang.String">
			<bind-xml name="proposeracct" node="element" />
		</field>
		<field name="amount" type="java.lang.String">
			<bind-xml name="amount" node="element" />
		</field>
		<field name="status" type="java.lang.String">
			<bind-xml name="status" node="element" />
		</field>
		<field name="proposercstmrid" type="java.lang.String">
			<bind-xml name="proposercstmrid" node="element" />
		</field>

		<field name="pmttpbgclsotid" type="java.lang.String">
			<bind-xml name="pmttpbgclsotid" node="element" />
		</field>

        <field name="instgpty" type="java.lang.String">
			<bind-xml name="instgpty" node="element" />
		</field>
		  <field name="systemcd" type="java.lang.String">
			<bind-xml name="systemcd" node="element" />
		</field>
		 <field name="msgid" type="java.lang.String">
			<bind-xml name="msgid" node="element" />
		</field>
		 <field name="id" type="java.lang.String">
			<bind-xml name="id" node="element" />
		</field>
		 <field name="txid" type="java.lang.String">
			<bind-xml name="txid" node="element" />
		</field>
		<field name="prcSts" type="java.lang.String">
			<bind-xml name="prcsts" node="element" />
		</field>
		<field name="tablenm" type="java.lang.String">
			<bind-xml name="tablenm" node="element" />
		</field>
		<field name="dbtracct" type="java.lang.String">
			<bind-xml name="dbtracct" node="element" />
		</field>
		<!-- 来帐信息唯一标识号 -->
		<field name="msgIdUnq" type="java.lang.String">
			<bind-xml name="msgidunq" node="element" />
		</field>
		

		
		
		
		 
       </class>
  </mapping>

