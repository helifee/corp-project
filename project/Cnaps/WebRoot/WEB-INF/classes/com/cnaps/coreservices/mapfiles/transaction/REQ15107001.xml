<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>cnaps  定期贷记业务明细建立</description> 
      
      
       <class name="com.cnaps.hvps.form.transfer.RegularCreditForm">
              <map-to xml="Document"/>
                   <!-- 原发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="RequestHead"/>
       </field>
       <!--原接收服务名-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="RequestHead"/>
       </field>
        <!--功能代码-->
       <field name="transactionId">
          <bind-xml name="TransactionId" node="element" location="RequestHead"/>
       </field>
         <!--发起行行号-->
          <!-- 发起参与机构 --><field name="operBankCode"><bind-xml name="OperBankCode" node="element" location="RequestHead"/>
       </field>
      
        <!--原报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="RequestHead"/>
       </field>
           <!--网点号-->
       <field name="brnchCd">
          <bind-xml name="BrnchCd" node="element" location="RequestHead"/>
       </field>
        <!--终端号-->
       <field name="termId">
          <bind-xml name="TermId" node="element" location="RequestHead"/>
       </field>
       <!--操作柜员-->
       <field name="operTeller">
          <bind-xml name="OperTeller" node="element" location="RequestHead"/>
       </field> 
        <!--支付交易组号-->
       <field name="pmtGrpId">
          <bind-xml name="PmtGrpId" node="element" location="RequestHead"/>
       </field> 
       
        
          <!-- 机构号 --><field name="orgCode"><bind-xml name="OrgCode" node="element" location="RequestHead"/>
       </field>
       
       
       
       
       
       
       
       
       
       
       
              <field name="po" type="com.cnaps.hvps.persistence.transfer.RegularCredit" >
              <bind-xml name="RequestFields" node="element" />
             </field>
       </class>
  
       <class name="com.cnaps.hvps.persistence.transfer.RegularCredit">
        	 
		       <!-- 系统编号 -->
		        <field name="systemcd">
		          <bind-xml name="systemCd" node="element" />
		       </field>
                <!-- 申请人证件号 -->
		        <field name="certnum">
		          <bind-xml name="ProposerCertId" node="element" />
		       </field>
		        <!-- 证件类型-->
		        <field name="certsize">
		          <bind-xml name="ProposerCertTp" node="element"/>
		       </field> 
		       <!-- 申请人证件发行国家 -->
		        <field name="issueState">
		          <bind-xml name="ProposerCertIssued" node="element" />
		       </field>
		          <!--申请人账号-->
		        <field name="proposerAcct">
		          <bind-xml name="ProposerAcct" node="element" />
		         </field>
		         
		       <!-- 申请人姓名-->
		       <field name="name">
		          <bind-xml name="ProposerNm" node="element" />
		       </field>
		        <!-- 申请人地址-->
		        <field name="address">
		          <bind-xml name="ProposerAddr" node="element" />
		       </field>	  
		       <!-- 申请人联系电话-->  
		         <field name="phone">
		          <bind-xml name="ProposerTel" node="element" />
		       </field>  
		         <!--申请人客户号1-->
		       <field name="proposerCstmrId">
		          <bind-xml name="ProposerCstmrId" node="element" />
		       </field>
		   
		       
		        <!-- 申请人帐号类型-->
		       <field name="payAccountType">
		          <bind-xml name="ProposerAcctTp" node="element" />
		       </field>
		          <!--申请人账户币种  -->
		        <field name="proposerAcctCcy">
		          <bind-xml name="ProposerAcctCcy" node="element" />
		        </field>
		       
		        
		       <!-- 业务类型编码--> 
		         <field name="businessSizeCode">
		          <bind-xml name="PmtTp" node="element" />
		         </field>  
		         <!--业务种类编码-->
		        <field name="businessClassCode">
		          <bind-xml name="PmtKd" node="element" />
		         </field>
		         
		         
		         <!--付款人账号-->
		        <field name="payerAcount">
		          <bind-xml name="DbtrAcct" node="element" />
		       </field>
		         <!--付款人名称-->
		        <field name="dbtrNm">
		          <bind-xml name="DbtrNm" node="element" />
		       </field>    
		        <!--付款人地址1-->
		        <field name="dbtrAddr">
		          <bind-xml name="DbtrAddr" node="element" />
		       </field>  
		         <!--付款人开户行行号 -->
		        <field name="applyOpenBankNum">
		          <bind-xml name="DbtrIssuer" node="element" />
		       </field> 
		       <!--付款人开户行行名 -->
		        <field name="applyOpenBankName">
		          <bind-xml name="DbtrIssuerNm" node="element" />
		       </field> 
		         <!--付款清算行行号-->
		        <field name="dbtrMmbId">
		          <bind-xml name="DbtrMmbId" node="element" />
		       </field> 
		       
		        <!--付款行行号 -->
		        <field name="dbtrBrnchId">
		          <bind-xml name="DbtrBrnchId" node="element" />
		       </field> 
		      <!--付款行行名-->
		        <field name="dbtrBrnchNm">
		          <bind-xml name="DbtrBrnchNm" node="element" />
		       </field>	 
		       
		       
		       
       </class>
 
    

  </mapping>

