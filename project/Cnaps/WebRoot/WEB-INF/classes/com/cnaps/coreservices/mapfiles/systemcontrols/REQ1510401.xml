<?xml version="1.0" encoding="UTF-8"?>
<mapping>
	<description>Basic mapping example</description>
	<class name="com.cnaps.coreservices.CommonXMLEntity">
		<!-- the name of the xml file's root element  -->
		<map-to xml="CCMS_DATA" />
		<!-- 
		<field name="ccms_parameter_count">
<
			<bind-xml name="BANKCODE_COUNT" node="element"
				location="CCMS_BANK_DATA" />
		</field>
 -->
		<field name="results"
			type="com.cnaps.hvps.persistence.info.CcmsDrctPtyInfo"
			collection="collection">
			<bind-xml name="ROW" node="element"
				location="CCMS_BANK_DATA" />
		</field>
	</class>
	<class name="com.cnaps.hvps.persistence.info.CcmsDrctPtyInfo">

        <field name="bankcode" type="java.lang.String">
			<bind-xml name="BANKCODE" node="element" />
		</field>
        <field name="bankname" type="java.lang.String">
			<bind-xml name="BANKNAME" node="element" />
		</field>
        <field name="bankaliasname" type="java.lang.String">
			<bind-xml name="BANKALIASNAM" node="element" />
		</field>
		<field name="bankcatalog" type="java.lang.String">
			<bind-xml name="BANKCATALOG" node="element" />
		</field>
		<field name="banktype" type="java.lang.String">
			<bind-xml name="BANKTYPE" node="element" />
		</field>
		<field name="pbccode" type="java.lang.String">
			<bind-xml name="PBCCODE" node="element" />
		</field>
		<field name="ccpc" type="java.lang.String">
			<bind-xml name="CCPC" node="element" />
		</field>
		<field name="dreccode" type="java.lang.String">
			<bind-xml name="DRECCODE" node="element" />
		</field>
		<field name="agentsettbank" type="java.lang.String">
			<bind-xml name="AGENTSETTBAN" node="element" />
		</field>
		<field name="suprlist" type="java.lang.String">
			<bind-xml name="SUPRLIST" node="element" />
		</field>
		<field name="sbstitnbk" type="java.lang.String">
			<bind-xml name="SBSTITNBK" node="element" />
		</field>
		<field name="debtorcity" type="java.lang.String">
			<bind-xml name="DEBTORCITY" node="element" />
		</field>
		<field name="addr" type="java.lang.String">
			<bind-xml name="ADDR" node="element" />
		</field>
		<field name="postcode" type="java.lang.String">
			<bind-xml name="POSTCODE" node="element" />
		</field>
		<field name="tel" type="java.lang.String">
			<bind-xml name="TEL" node="element" />
		</field>
		<field name="email" type="java.lang.String">
			<bind-xml name="EMAIL" node="element" />
		</field>
		<field name="effectdate" type="java.lang.String">
			<bind-xml name="EFFECTDATE" node="element" />
		</field>
		<field name="expdate" type="java.lang.String">
			<bind-xml name="EXPDATE" node="element" />
		</field>
		<field name="syscode" type="java.lang.String">
			<bind-xml name="SYSCODE" node="element" />
		</field>

	</class>
</mapping>