<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>业务种类与类型管理查询</description>
	<class name="com.cnaps.coreservices.CommonXMLEntity">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />

		<!-- 原发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="ResponseHead" />
		</field>
		<!--原接收服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="ResponseHead" />
		</field>
		<!--原渠道报文标识号-->
		<field name="messageId">
			<bind-xml name="MessageId" node="element"
				location="ResponseHead" />
		</field>
		<!--原报文发送时间-->
		<field name="sendDateTime">
			<bind-xml name="SendDateTime" node="element"
				location="ResponseHead" />
		</field>
		<!--处理码-->
		<field name="processCode">
			<bind-xml name="ProcessCode" node="element"
				location="ResponseHead" />
		</field>
		<!--处理信息-->
		<field name="processDes">
			<bind-xml name="ProcessDes" node="element"
				location="ResponseHead" />
		</field>


		<!--查询代码-->
		<field name="queryCd">
			<bind-xml name="QueryCd" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageBeginPos">
			<bind-xml name="TurnPageBeginPos" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageShowNum">
			<bind-xml name="TurnPageShowNum" node="element"
				location="ResponseHead" />
		</field>
		<field name="recordcount">
			<bind-xml name="RecordCount" node="element"
				location="ResponseHead" />
		</field>
		<field name="results"
			type="com.cnaps.hvps.persistence.messages.CcmsMsgTpManger"
			collection="arraylist">
			<bind-xml name="QryResults" node="element"
				location="ResponseFields" />
		</field>
	</class>

	<class name="com.cnaps.hvps.persistence.messages.CcmsMsgTpManger">
		<field name="id" type="java.lang.Long"><bind-xml name="id" node="element" /></field>
		<field name="msgid" type="java.lang.String"><bind-xml name="msgid" node="element" /></field>
		<field name="credttm" type="java.lang.String"><bind-xml name="credttm" node="element" /></field>
		<field name="instgpty" type="java.lang.String"><bind-xml name="instgpty" node="element" /></field>
		<field name="instdpty" type="java.lang.String"><bind-xml name="instdpty" node="element" /></field>
		<field name="instgdrctpty" type="java.lang.String"><bind-xml name="instgdrctpty" node="element" /></field>
		<field name="instddrctpty" type="java.lang.String"><bind-xml name="instddrctpty" node="element" /></field>
		<field name="systemcd" type="java.lang.String"><bind-xml name="systemcd" node="element" /></field>
		<field name="changetp" type="java.lang.String"><bind-xml name="changetp" node="element" /></field>
		<field name="effectivetp" type="java.lang.String"><bind-xml name="effectivetp" node="element" /></field>
		<field name="effectivedt" type="java.lang.String"><bind-xml name="effectivedt" node="element" /></field>
		<field name="ineffectivedt" type="java.lang.String"><bind-xml name="ineffectivedt" node="element" /></field>
		<field name="pmttpclctn" type="java.lang.String"><bind-xml name="pmttpclctn" node="element" /></field>
		<field name="pmttpclctnname" type="java.lang.String"><bind-xml name="pmttpclctnname" node="element" /></field>
		<field name="pmtkdclctn" type="java.lang.String"><bind-xml name="pmtkdclctn" node="element" /></field>
		<field name="pmtkdclctnname" type="java.lang.String"><bind-xml name="pmtkdclctnname" node="element" /></field>
		<field name="msgtpidclctn" type="java.lang.String"><bind-xml name="msgtpidclctn" node="element" /></field>
		<field name="msgtpidclctnname" type="java.lang.String"><bind-xml name="msgtpidclctnname" node="element" /></field>
		<field name="ustrd" type="java.lang.String"><bind-xml name="ustrd" node="element" /></field>
		<field name="status" type="java.lang.String"><bind-xml name="status" node="element" /></field>
	</class>

</mapping>

