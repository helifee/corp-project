<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>核心对账处理申请</description> 
       <class name="com.cnaps.hvps.persistence.duizhang.CoreAcctProcess">
              <map-to xml="Document"/>
              <!-- 机构号 --><field name="orgCode"><bind-xml name="OrgCode" node="element" location="RequestHead"/></field>
<!-- 网点号 --><field name="brnchCd"><bind-xml name="BrnchCd" node="element"  location="RequestHead" /></field>
               <!-- 源服务名 -->
		        <field name="source">
		          <bind-xml name="Source" node="element" location="RequestHead" />
		       </field>
		        <!-- 目标服务名 -->
		        <field name="destination">
		          <bind-xml name="Destination" node="element" location="RequestHead" />
		       </field>
		       <!-- 报文标识号 -->
		        <field name="msgId">
		          <bind-xml name="MessageId" node="element" location="RequestHead" />
		       </field>
		         <!-- 功能代码 -->
		        <field name="transactionId">
		          <bind-xml name="TransactionId" node="element" location="RequestHead" />
		       </field>
		       <!-- 发送时间 -->
		        <field name="sendDateTime">
		          <bind-xml name="SendDateTime" node="element" location="RequestHead" />
		       </field>
		       
		       <!-- 前台流水号 -->
		        <field name="txId">
		          <bind-xml name="TxId" node="element" location="RequestFields" />
		       </field>
		       <!-- 账户处理类型 -->
		        <field name="processType">
		          <bind-xml name="ProcessType" node="element" location="RequestFields" />
		       </field>
		       <!-- 原后台流水号 -->
		        <field name="oldTxld">
		          <bind-xml name="OldTxld" node="element" location="RequestFields" />
		       </field>
		        <!-- 账号 -->
		        <field name="dbtrAcct">
		          <bind-xml name="DbtrAcct" node="element" location="RequestFields" />
		       </field>
		         <!-- 名称 -->
		        <field name="dbtrNm">
		          <bind-xml name="DbtrNm" node="element" location="RequestFields" />
		       </field>
		         <!-- 币种 -->
		        <field name="currency">
		          <bind-xml name="Currency" node="element" location="RequestFields" />
		       </field>
		         <!-- 金额-->
		        <field name="amt">
		          <bind-xml name="Amt" node="element" location="RequestFields" />
		       </field>
		         <!-- 手续费-->
		        <field name="charge">
		          <bind-xml name="Charge" node="element" location="RequestFields" />
		       </field>
		         <!-- 邮电费-->
		        <field name="postage">
		          <bind-xml name="Postage" node="element" location="RequestFields" />
		       </field>
		         <!-- 异地加收-->
		        <field name="outstationCharge">
		          <bind-xml name="OutstationCharge" node="element" location="RequestFields" />
		       </field>
		         <!-- 总金额-->
		        <field name="totalAmt">
		          <bind-xml name="TotalAmt" node="element" location="RequestFields" />
		       </field>
		         <!-- 委托日期-->
		        <field name="consignDate">
		          <bind-xml name="ConsignDate" node="element" location="RequestFields" />
		       </field>
		         <!-- 系统编码-->
		        <field name="systemCode">
		          <bind-xml name="SystemCode" node="element" location="RequestFields" />
		       </field>
		      <!-- 附言-->
		        <field name="ustrd">
		          <bind-xml name="Ustrd" node="element" location="RequestFields" />
		       </field>
       </class>

  </mapping>

