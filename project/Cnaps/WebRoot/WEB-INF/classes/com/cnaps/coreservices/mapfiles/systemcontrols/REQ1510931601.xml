<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <class name="com.cnaps.hvps.form.info.BusinessStatusQueryForm">
      <!-- the name of the xml file's root element  -->
       <map-to xml="Document"/>
     
         <!-- 发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="RequestHead"/>
       </field>
       <!--描述-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="RequestHead"/>
       </field>
        <!--功能代码-->
       <field name="transactionId">
          <bind-xml name="TransactionId" node="element" location="RequestHead"/>
       </field>
        <!--渠道报文标识号-->
       <field name="msgId">
          <bind-xml name="MessageId" node="element" location="RequestHead"/>
       </field>
        <!--报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="RequestHead"/>
       </field>
       
       <field name="msgcode">
          <bind-xml name="MsgCode" node="element" location="RequestHead"/>
       </field>
       <!-- 提交代码-->
       <field name="po" type="com.cnaps.hvps.persistence.info.BusinessStatusQuery">
          <bind-xml name="RequestFields" node="element"/>
       </field>
       
     </class>
       
     <class name="com.cnaps.hvps.persistence.info.BusinessStatusQuery">
     	 <field name="sendbranch" type="java.lang.String">
		      <bind-xml name="sendbranch" node="element"/>
		</field>
     	<field name="orimsgid" type="java.lang.String">
			<bind-xml name="OriMsgID" node="element" />
		</field>
		<field name="orisendbranch" type="java.lang.String">
			<bind-xml name="OriSendBranch" node="element" />
		</field>
		<field name="orimsgcode" type="java.lang.String">
			<bind-xml name="OriMsgCode" node="element" />
		</field>
       
	 </class>   

  </mapping>

