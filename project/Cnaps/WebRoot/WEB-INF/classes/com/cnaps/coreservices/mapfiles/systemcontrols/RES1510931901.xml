<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>业务退回应答查询回执报文</description>
	<class name="com.cnaps.coreservices.CommonXMLEntity">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />

		<!-- 原发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="ResponseHead" />
		</field>
		<!--原接收服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="ResponseHead" />
		</field>
		<!--原渠道报文标识号-->
		<field name="messageId">
			<bind-xml name="MessageId" node="element"
				location="ResponseHead" />
		</field>
		<!--原报文发送时间-->
		<field name="sendDateTime">
			<bind-xml name="SendDateTime" node="element"
				location="ResponseHead" />
		</field>
		<!--处理码-->
		<field name="processCode">
			<bind-xml name="ProcessCode" node="element"
				location="ResponseHead" />
		</field>
		<!--处理信息-->
		<field name="processDes">
			<bind-xml name="ProcessDes" node="element"
				location="ResponseHead" />
		</field>
		
		<!--查询代码-->
		<field name="queryCd">
			<bind-xml name="QueryCd" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageBeginPos">
			<bind-xml name="TurnPageBeginPos" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageShowNum">
			<bind-xml name="TurnPageShowNum" node="element"
				location="ResponseHead" />
		</field>
		<field name="recordcount">
			<bind-xml name="RecordCount" node="element"
				location="ResponseHead" />
		</field>
		<field name="results"
			type="com.cnaps.hvps.persistence.info.BusinessReturnAnswer"
			collection="arraylist">
			<bind-xml name="QryResults" node="element"
				location="ResponseFields" />
		</field>
	</class>
	
	<class name="com.cnaps.hvps.persistence.info.BusinessReturnAnswer">
<!--序号--><field name="id" type="java.lang.String" ><bind-xml name="id" node="element"/></field>
<!--支付交易组号--><field name="pmtGrpId" type="java.lang.String" ><bind-xml name="pmtgrpid" node="element"/></field>
<!--报文标识号--><field name="msgID" type="java.lang.String" ><bind-xml name="msgid" node="element"/></field>
<!--报文发送时间--><field name="creDtTm" type="java.lang.String" ><bind-xml name="credttm" node="element"/></field>
<!--回执报文标识号--><field name="recptMsgId" type="java.lang.String" ><bind-xml name="recptmsgid" node="element"/></field>
<!--回复报文时间--><field name="recptDtTm" type="java.lang.String" ><bind-xml name="recptdttm" node="element"/></field>
<!--发起参与机构行号--><field name="instgPty" type="java.lang.String" ><bind-xml name="instgpty" node="element"/></field>
<!--接收参与机构行号--><field name="instdPty" type="java.lang.String" ><bind-xml name="instdpty" node="element"/></field>
<!--发起直接参与机构--><field name="instgDrctPty" type="java.lang.String" ><bind-xml name="instgdrctpty" node="element"/></field>
<!--接收直接参与机构--><field name="instdDrctPty" type="java.lang.String" ><bind-xml name="instddrctpty" node="element"/></field>
<!--原报文标识号--><field name="ornglMsgId" type="java.lang.String" ><bind-xml name="ornglmsgid" node="element"/></field>
<!--原报文类型代码--><field name="ornglMsgTpCd" type="java.lang.String" ><bind-xml name="ornglmsgtpcd" node="element"/></field>
<!--原发起参与机构--><field name="orgnlInstgPty" type="java.lang.String" ><bind-xml name="orgnlinstgpty" node="element"/></field>
<!--退回类型--><field name="backTpCd" type="java.lang.String" ><bind-xml name="backtpcd" node="element"/></field>
<!--业务状态--><field name="status" type="java.lang.String" ><bind-xml name="status" node="element"/></field>
<!--附言--><field name="addtlInf" type="java.lang.String" ><bind-xml name="addtlinf" node="element"/></field>
<!--工作日期--><field name="workDt" type="java.lang.String" ><bind-xml name="workdt" node="element"/></field>
<!--来往标识--><field name="direction" type="java.lang.String" ><bind-xml name="direction" node="element"/></field>
<!--业务处理码--><field name="prcCd" type="java.lang.String" ><bind-xml name="prccd" node="element"/></field>
<!--业务拒绝原因--><field name="rjctInf" type="java.lang.String" ><bind-xml name="rjctinf" node="element"/></field>
<!--拒绝业务的参与机构行号--><field name="rjctedPtyId" type="java.lang.String" ><bind-xml name="rjctedptyid" node="element"/></field>
<!--终态日期--><field name="sttlmDt" type="java.lang.String" ><bind-xml name="sttlmdt" node="element"/></field>
	</class>
	
</mapping>

