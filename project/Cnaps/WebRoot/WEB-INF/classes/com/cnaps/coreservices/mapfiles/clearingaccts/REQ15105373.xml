<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>资金池管理申请录入</description> 
       <class name="com.cnaps.hvps.form.clearingAccount.FundspoolManagementForm">
      <!-- the name of the xml file's root element  -->
       <map-to xml="Document"/>
     
         <!-- 原发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="RequestHead"/>
       </field>
       <!--原接收服务名-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="RequestHead"/>
       </field>
        <!--功能代码-->
       <field name="transactionId">
          <bind-xml name="TransactionId" node="element" location="RequestHead"/>
       </field>
        <!--原报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="RequestHead"/>
       </field>
         <!--网点号-->
       <field name="brnchCd">
          <bind-xml name="BrnchCd" node="element" location="RequestHead"/>
       </field>
       <!-- 终端号 -->
       <field name="termId">
          <bind-xml name="TermId" node="element" location="RequestHead"/>
       </field>
       <!-- 操作柜员 -->
        <field name="operTeller">
          <bind-xml name="OperTeller" node="element" location="RequestHead"/>
       </field>
       
        
          <!-- 发起参与机构 --><field name="operBankCode"><bind-xml name="OperBankCode" node="element" location="RequestHead"/>
       </field>
       <!-- 支付交易组号 -->
        <field name="pmtGrpId">
          <bind-xml name="PmtGrpId" node="element" location="RequestHead"/>
       </field>
       
       <field name="po" type="com.cnaps.hvps.persistence.clearingAccount.FundspoolManagement">
          <bind-xml name="RequestFields" node="element" />
       </field>
      </class>
      
	 <class name="com.cnaps.hvps.persistence.clearingAccount.FundspoolManagement">
	 	<field name="mgrId" type="java.lang.String">
			<bind-xml name="MgrId" node="element" />
		</field>
		<field name="fundsOfPoolManagementType" type="java.lang.String">
			<bind-xml name="FndsOfPoolMgmtTp" node="element" />
		</field>
		<field name="effectiveDate" type="java.lang.String">
			<bind-xml name="FctvDt" node="element" />
		</field>
		<field name="branchQuantity" type="java.lang.String">
			<bind-xml name="BrnchQty" node="element" />
		</field>
		<field name="brnchList" type="java.lang.String" collection="arraylist">
			<bind-xml name="MmbId" node="element" location="BrnchList" />
		</field>
		<field name="remarkInfo" type="java.lang.String">
			<bind-xml name="Rmk" node="element" />
		</field>
       </class>
  </mapping>

