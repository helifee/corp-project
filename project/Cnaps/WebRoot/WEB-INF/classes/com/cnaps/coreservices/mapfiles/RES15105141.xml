<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>Basic mapping example</description> 
      <class name="com.cnaps.coreservices.CommonXMLEntity" >
      <!-- the name of the xml file's root element  -->
       <map-to xml="document"/>
     
         <!-- 原发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="RequestHead"/>
       </field>
       <!--原接收服务名-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="RequestHead"/>
       </field>
        <!--原渠道报文标识号-->
       <field name="messageId">
          <bind-xml name="MessageId" node="element" location="RequestHead"/>
       </field>
        <!--原报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="RequestHead"/>
       </field>
         <!--处理码-->
       <field name="processCode">
          <bind-xml name="ProcessCode" node="element" location="RequestHead"/>
       </field>
         <!--处理信息-->
       <field name="processDes">
          <bind-xml name="ProcessDes" node="element" location="RequestHead"/>
       </field>
       
       
         <!--查询代码-->
       <field name="queryCd">
          <bind-xml name="QueryCd" node="element" location="ResponseFields"/>
       </field>
       <field name="turnPageBeginPos">
          <bind-xml name="TurnPageBeginPos" node="element" location="ResponseFields"/>
       </field>
       <field name="turnPageShowNum">
          <bind-xml name="TurnPageShowNum" node="element" location="ResponseFields"/>
       </field>
       <field name="results" type="com.cnaps.hvps.form.HVPSsend141Form" collection="arraylist">
          <bind-xml name="QryResult" node="element" location="ResponseFields/QryResults"/>
       </field>
      </class>

	 <class name="com.cnaps.hvps.form.HVPSsend141Form">
		<field name="id" type="java.lang.Long">
			<bind-xml name="Id" node="element" />
		</field>
		<field name="msgid" type="java.lang.String">
			<bind-xml name="MsgId" node="element" />
		</field>	
		<field name="credttm" type="java.lang.String">
			<bind-xml name="CreDtTm" node="element" />
		</field>
		     <field name="senddrctpty" type="java.lang.String">
		      <bind-xml name="Senddrctpty" node="element"  />
		    </field>
		     <field name="sendindrctpty" type="java.lang.String">
		      <bind-xml name="Sendindrctpty" node="element"  />
		    </field>
		     <field name="recvdrctpty" type="java.lang.String">
		      <bind-xml name="Recvdrctpty" node="element"  />
		    </field>
		     <field name="recvindrctpty" type="java.lang.String">
		      <bind-xml name="Recvindrctpty" node="element"  />
		    </field>
		     <field name="systemcode" type="java.lang.String">
		      <bind-xml name="Systemcode" node="element"  />
		    </field>
		    
		    
		     <field name="systemcode" type="java.lang.String">
		      <bind-xml name="Systemcode" node="element"  />
		    </field> 
		    <field name="currency" type="java.lang.String">
		      <bind-xml name="Currency" node="element"  />
		    </field>
		     <field name="purchspric" type="java.lang.Double">
		      <bind-xml name="Purchspric" node="element"  />
		    </field> 
		    <field name="buyamt" type="java.lang.Lang">
		      <bind-xml name="buyamt" node="element"  />
		    </field>
		     <field name="remarkinfo" type="java.lang.String">
		      <bind-xml name="Remarkinfo" node="element"  />
		    </field> 
		    <field name="pmtsts" type="java.lang.String">
		      <bind-xml name="Pmtsts" node="element"  />
		    </field>
		     <field name="procode" type="java.lang.String">
		      <bind-xml name="Procode" node="element"  />
		    </field> 
		    <field name="rjctbrchid" type="java.lang.String">
		      <bind-xml name="Rjctbrchid" node="element"  />
		    </field>
		     <field name="rjctrsn" type="java.lang.String">
		      <bind-xml name="Rjctrsn" node="element"  />
		    </field> 
		    <field name="rjctdesc" type="java.lang.String">
		      <bind-xml name="Rjctdesc" node="element"  />
		    </field>
		    
		    <field name="prodate" type="java.lang.String">
		      <bind-xml name="Prodate" node="element"  />
		    </field>
		     <field name="workdate" type="java.lang.String">
		      <bind-xml name="Workdate" node="element"  />
		    </field> 
		    <field name="chnmsgid" type="java.lang.String">
		      <bind-xml name="Chnmsgid" node="element"  />
		    </field>
		    <field name="chndttm" type="java.lang.String">
		      <bind-xml name="Chndttm" node="element"  />
		    </field>
		     <field name="resultmsgid" type="java.lang.String">
		      <bind-xml name="Resultmsgid" node="element"  />
		    </field> 
		    <field name="resultdttm" type="java.lang.String">
		      <bind-xml name="Resultdttm" node="element"  />
		    </field>
  </class>

  </mapping>

