<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>cnaps 通用票据截留</description>


	<class name="com.cnaps.hvps.form.cis.ChequeholdForm">
		<map-to xml="Document" />
		<!-- 原发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="RequestHead" />
		</field>
		<!--原接收服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="RequestHead" />
		</field>
		<!--功能代码-->
		<field name="transactionId">
			<bind-xml name="TransactionId" node="element"
				location="RequestHead" />
		</field>
		<!-- 机构号 -->
		<field name="orgCode">
			<bind-xml name="OrgCode" node="element"
				location="RequestHead" />
		</field>
		<!--发起行行号-->

		<!-- 发起参与机构 -->
		<field name="operBankCode">
			<bind-xml name="OperBankCode" node="element"
				location="RequestHead" />
		</field>

		<!--原报文发送时间-->
		<field name="sendDateTime">
			<bind-xml name="SendDateTime" node="element"
				location="RequestHead" />
		</field>
		<!--网点号-->
		<field name="brnchCd">
			<bind-xml name="BrnchCd" node="element"
				location="RequestHead" />
		</field>
		<!--终端号-->
		<field name="termId">
			<bind-xml name="TermId" node="element"
				location="RequestHead" />
		</field>
		<!--操作柜员-->
		<field name="operTeller">
			<bind-xml name="OperTeller" node="element"
				location="RequestHead" />
		</field>
		<!--支付交易组号-->
		<field name="pmtGrpId">
			<bind-xml name="PmtGrpId" node="element"
				location="RequestHead" />
		</field>

		<field name="bill"
			type="com.cnaps.hvps.persistence.cis.Billhold">
			<bind-xml name="RequestFields" node="element" />
		</field>
	</class>

	<class name="com.cnaps.hvps.persistence.cis.Billhold">


		<!-- 签发模式 -->
		<field name="signMd">
			<bind-xml name="SignMd" node="element" />
		</field>
		<!--授权柜员      -->
		<field name="warrantyId">
			<bind-xml name="WarrantyId" node="element" />
		</field>
		<!-- 手续费-->
		<field name="serviceCharge">
			<bind-xml name="ServiceCharge" node="element" />
		</field>
		<!-- 邮电费 -->
		<field name="postage">
			<bind-xml name="Postage" node="element" />
		</field>
		<!--异地加收-->
		<field name="outstationCharge">
			<bind-xml name="OutstationCharge" node="element" />
		</field>

		<!--工本费-->
		<field name="counterfoil">
			<bind-xml name="Counterfoil" node="element" />
		</field>
		<!-- 减免-->
		<field name="waive">
			<bind-xml name="Waive" node="element" />
		</field>


		<!-- 以下是申请人信息-->




		<!-- 申请人证件号-->
		<field name="proposerCertId">
			<bind-xml name="ProposerCertId" node="element" />
		</field>
		<!-- 申请人证件类型-->
		<field name="proposerCertTp">
			<bind-xml name="ProposerCertTp" node="element" />
		</field>
		<!--申请人证件发行国家-->
		<field name="proposerCertIssued">
			<bind-xml name="ProposerCertIssued" node="element" />
		</field>


		<!-- 申请人账号-->
		<field name="proposerAcct">
			<bind-xml name="ProposerAcct" node="element" />
		</field>
		<!--申请人名称 -->
		<field name="proposerNm">
			<bind-xml name="ProposerNm" node="element" />
		</field>


		<!-- 申请人地址-->
		<field name="proposerAddr">
			<bind-xml name="ProposerAddr" node="element" />
		</field>
		<!--申请人联系电话-->
		<field name="proposerTel">
			<bind-xml name="ProposerTel" node="element" />
		</field>


		<!--申请人客户号-->
		<field name="proposerCstmrId">
			<bind-xml name="ProposerCstmrId" node="element" />
		</field>
		<!--申请人账户类型-->
		<field name="proposerAcctTp">
			<bind-xml name="ProposerAcctTp" node="element" />
		</field>
		<!--申请人账户币种-->
		<field name="proposerAcctCcy">
			<bind-xml name="ProposerAcctCcy" node="element" />
		</field>
		<!--以下是交易信息 -->

		<!--业务类型编码 -->
		<field name="pmtTp">
			<bind-xml name="PmtTp" node="element" />
		</field>
		<!--业务种类编码 -->
		<field name="pmtKd">
			<bind-xml name="PmtKd" node="element" />
		</field>
		<!--货币符号-->
		<field name="currencyCd">
			<bind-xml name="CurrencyCd" node="element" />
		</field>

		<!--交易金额 -->
		<field name="amount">
			<bind-xml name="Amount" node="element" />
		</field>
		<!--附言-->
		<field name="addtlInf">
			<bind-xml name="AddtlInf" node="element" />
		</field>
		<!--合同协议号-->
		<field name="agrmtNb">
			<bind-xml name="AgrmtNb" node="element" />
		</field>

		<!--回执期限-->
		<field name="rcptLtd">
			<bind-xml name="RcptLtd" node="element" />
		</field>

		<!--代理行手续费-->
		<field name="agncyCharge">
			<bind-xml name="AgncyCharge" node="element" />
		</field>
		<!--以下是付款人信息-->

		<!-- 付款人户名-->
		<field name="dbtrNm">
			<bind-xml name="DbtrNm" node="element" />
		</field>

		<!-- 付款人账号-->
		<field name="dbtrAcct">
			<bind-xml name="DbtrAcct" node="element" />
		</field>
		<!-- 付款人地址-->
		<field name="dbtrAddr">
			<bind-xml name="DbtrAddr" node="element" />
		</field>


		<!--付款人开户行行号-->
		<field name="dbtrIssuer">
			<bind-xml name="DbtrIssuer" node="element" />
		</field>
		<!--付款人开户行行名-->
		<field name="dbtrIssuerNm">
			<bind-xml name="DbtrIssuerNm" node="element" />
		</field>
		<!--付款清算行行号-->
		<field name="dbtrMmbId">
			<bind-xml name="DbtrMmbId" node="element" />
		</field>
		<!--付款行行号-->
		<field name="dbtrBrnchId">
			<bind-xml name="DbtrBrnchId" node="element" />
		</field>

		<!--付款行行 名-->
		<field name="dbtrBrnchNm">
			<bind-xml name="DbtrBrnchNm" node="element" />
		</field>

		<!--以下是收款人信息-->

		<!--收款人名称-->
		<field name="cdtrNm">
			<bind-xml name="CdtrNm" node="element" />
		</field>

		<!--收款人账号-->
		<field name="cdtrAcct">
			<bind-xml name="CdtrAcct" node="element" />
		</field>

		<!--收款人地址-->
		<field name="cdtrAddr">
			<bind-xml name="CdtrAddr" node="element" />
		</field>


		<!--收款人开户行行号-->
		<field name="cdtrIssuer">
			<bind-xml name="CdtrIssuer" node="element" />
		</field>
		<!--收款人开户行行名-->
		<field name="cdtrIssuerNm">
			<bind-xml name="CdtrIssuerNm" node="element" />
		</field>
		<!--收款清算行行号-->
		<field name="cdtrMmbId">
			<bind-xml name="CdtrMmbId" node="element" />
		</field>
		<!--收款行行号-->
		<field name="cdtrBrnchId">
			<bind-xml name="CdtrBrnchId" node="element" />
		</field>

		<!--收款行行名-->
		<field name="cdtrBrnchNm">
			<bind-xml name="CdtrBrnchNm" node="element" />
		</field>

		<!--业务类型为小额支付系统支票截留业务-->
		<!--出票日期-->
		<field name="issueDt">
			<bind-xml name="IssueDt" node="element" />
		</field>

		<!--出票金额-->
		<field name="issueAmt">
			<bind-xml name="IssueAmt" node="element" />
		</field>
		<!--提示付款日期-->
		<field name="payDt">
			<bind-xml name="PayDt" node="element" />
		</field>

		<!--票据号码-->
		<field name="notesNo">
			<bind-xml name="NotesNo" node="element" />
		</field>
		<!--支付密码-->
		<field name="pymntPswd">
			<bind-xml name="PymntPswd" node="element" />
		</field>
		<!--汇票到期日期-->
		<field name="maturityDt">
			<bind-xml name="MaturityDt" node="element" />
		</field>
		<!--汇票密押-->
		<field name="billSeal">
			<bind-xml name="BillSeal" node="element" />
		</field>
		<!--承兑协议编号-->
		<field name="accptncagrmntNo">
			<bind-xml name="AccptncagrmntNo" node="element" />
		</field>
		<!--承兑日期-->
		<field name="accptncDt">
			<bind-xml name="AccptncDt" node="element" />
		</field>
		<!--承兑人-->
		<field name="accptncNm">
			<bind-xml name="AccptncNm" node="element" />
		</field>

		<!--票据申请人名称-->
		<field name="applyNm">
			<bind-xml name="ApplyNm" node="element" />
		</field>
		<!--票据申请人账号-->
		<field name="applyAcct">
			<bind-xml name="ApplyAcct" node="element" />
		</field>

		<!--出票人全称-->
		<field name="drawerNm">
			<bind-xml name="DrawerNm" node="element" />
		</field>
		<!--出票人账号-->
		<field name="drawerAcct">
			<bind-xml name="DrawerAcct" node="element" />
		</field>
		<!--交易合同号码-->
		<field name="trnsctnCntrctNo">
			<bind-xml name="TrnsctnCntrctNo" node="element" />
		</field>
		<!--原收款人名称-->
		<field name="oriCrdtrNm">
			<bind-xml name="OriCrdtrNm" node="element" />
		</field>


		<!--用途-->
		<field name="purpose">
			<bind-xml name="Purpose" node="element" />
		</field>
		<!--背书人数-->
		<field name="numOfEndrsr">
			<bind-xml name="NumOfEndrsr" node="element" />
		</field>
		<!-- 图像路径-->
		<field name="imagepath">
			<bind-xml name="imagepath" node="element" />
		</field>
		<!--图像文件名称-->
		<field name="imagename">
			<bind-xml name="imagename" node="element" />
		</field>




		<!--票据图像类型-->
		<field name="imageTp">
			<bind-xml name="ImageTp" node="element" />
		</field>
		<!--票据正面图像长度-->
		<field name="imageFrontLngth">
			<bind-xml name="ImageFrontLngth" node="element" />
		</field>
		<!--票据正面图像数据-->
		<field name="imageFrontData">
			<bind-xml name="ImageFrontDt" node="element" />
		</field>
		<!--票据背面图像长度-->
		<field name="imageBackLngth">
			<bind-xml name="ImageBackLngth" node="element" />
		</field>
		<!--票据背面图像数据-->
		<field name="imageBackData">
			<bind-xml name="ImageBackDt" node="element" />
		</field>
		<!--背书人名称-->
		<field name="endrsrNm" type="strings">
			<bind-xml name="EndrsrNm" node="element"
				location="EndrsrDtl" />
		</field>
	</class>
	<class name="com.cnaps.hvps.persistence.cis.EndrsrNmN">

		<field name="endrsrNm" type="string">
			<bind-xml name="EndrsrNm" node="element" />
		</field>

	</class>


</mapping>

