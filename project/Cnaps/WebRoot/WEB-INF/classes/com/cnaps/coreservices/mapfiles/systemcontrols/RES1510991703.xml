<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>Basic mapping example</description>
	<class name="com.cnaps.coreservices.CommonXMLEntity">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />

		<!-- 原发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="ResponseHead" />
		</field>
		<!--原接收服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="ResponseHead" />
		</field>
		<!--原渠道报文标识号-->
		<field name="messageId">
			<bind-xml name="MessageId" node="element"
				location="ResponseHead" />
		</field>
		<!--原报文发送时间-->
		<field name="sendDateTime">
			<bind-xml name="SendDateTime" node="element"
				location="ResponseHead" />
		</field>
		<!--处理码-->
		<field name="processCode">
			<bind-xml name="ProcessCode" node="element"
				location="ResponseHead" />
		</field>
		<!--处理信息-->
		<field name="processDes">
			<bind-xml name="ProcessDes" node="element"
				location="ResponseHead" />
		</field>


		<!--查询代码-->
		<field name="queryCd">
			<bind-xml name="QueryCd" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageBeginPos">
			<bind-xml name="TurnPageBeginPos" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageShowNum">
			<bind-xml name="TurnPageShowNum" node="element"
				location="ResponseHead" />
		</field>
		<field name="recordcount">
			<bind-xml name="RecordCount" node="element"
				location="ResponseHead" />
		</field>
		<field name="results"
			type="com.cnaps.hvps.persistence.messages.Hvpsbankinfoalterdetail"
			collection="arraylist">
			<bind-xml name="QryResults" node="element"
				location="ResponseFields" />
		</field>
	</class>

	<class name="com.cnaps.hvps.persistence.messages.Hvpsbankinfoalterdetail">
		<field name="id" type="java.lang.Long"><bind-xml name="id" node="element" /></field>
		<field name="parentid" type="java.lang.Long"><bind-xml name="parentid" node="element" /></field>
		<field name="purposetypenotify" type="java.lang.String"><bind-xml name="purposetypenotify" node="element" /></field>
		<field name="effectivetype" type="java.lang.String"><bind-xml name="effectivetype" node="element" /></field>
		<field name="effdate" type="java.lang.String"><bind-xml name="effdate" node="element" /></field>
		<field name="invdate" type="java.lang.String"><bind-xml name="invdate" node="element" /></field>
		<field name="bankcode" type="java.lang.String"><bind-xml name="bankcode" node="element" /></field>
		<field name="participanttype" type="java.lang.String"><bind-xml name="participanttype" node="element" /></field>
		<field name="bankcategorycode" type="java.lang.String"><bind-xml name="bankcategorycode" node="element" /></field>
		<field name="directbankcode" type="java.lang.String"><bind-xml name="directbankcode" node="element" /></field>
		<field name="legalperson" type="java.lang.String"><bind-xml name="legalperson" node="element" /></field>
		<field name="higerparticipant" type="java.lang.String"><bind-xml name="higerparticipant" node="element" /></field>
		<field name="bearbankcode" type="java.lang.String"><bind-xml name="bearbankcode" node="element" /></field>
		<field name="chargebankcode" type="java.lang.String"><bind-xml name="chargebankcode" node="element" /></field>
		<field name="nodecode" type="java.lang.String"><bind-xml name="nodecode" node="element" /></field>
		<field name="citycode" type="java.lang.String"><bind-xml name="citycode" node="element" /></field>
		<field name="participantname" type="java.lang.String"><bind-xml name="participantname" node="element" /></field>
		<field name="participantshortname" type="java.lang.String"><bind-xml name="participantshortname" node="element" /></field>
		<field name="sign" type="java.lang.String"><bind-xml name="sign" node="element" /></field>
		<field name="address" type="java.lang.String"><bind-xml name="address" node="element" /></field>
		<field name="postcode" type="java.lang.String"><bind-xml name="postcode" node="element" /></field>
		<field name="telephone" type="java.lang.String"><bind-xml name="telephone" node="element" /></field>
		<field name="email" type="java.lang.String"><bind-xml name="email" node="element" /></field>
		<field name="remarkinformation" type="java.lang.String"><bind-xml name="remarkinformation" node="element" /></field>
		
	</class>

</mapping>

