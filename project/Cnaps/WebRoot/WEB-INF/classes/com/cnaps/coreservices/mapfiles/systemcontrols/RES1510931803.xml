<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>Basic mapping example</description>
	<class name="com.cnaps.coreservices.CommonXMLEntity">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />

		<!-- 原发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="ResponseHead" />
		</field>
		<!--原接收服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="ResponseHead" />
		</field>
		<!--原渠道报文标识号-->
		<field name="messageId">
			<bind-xml name="MessageId" node="element"
				location="ResponseHead" />
		</field>
		<!--原报文发送时间-->
		<field name="sendDateTime">
			<bind-xml name="SendDateTime" node="element"
				location="ResponseHead" />
		</field>
		<!--处理码-->
		<field name="processCode">
			<bind-xml name="ProcessCode" node="element"
				location="ResponseHead" />
		</field>
		<!--处理信息-->
		<field name="processDes">
			<bind-xml name="ProcessDes" node="element"
				location="ResponseHead" />
		</field>
		
		<!--查询代码-->
		<field name="queryCd">
			<bind-xml name="QueryCd" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageBeginPos">
			<bind-xml name="TurnPageBeginPos" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageShowNum">
			<bind-xml name="TurnPageShowNum" node="element"
				location="ResponseHead" />
		</field>
		<field name="recordcount">
			<bind-xml name="RecordCount" node="element"
				location="ResponseHead" />
		</field>
		<field name="results"
			type="com.cnaps.hvps.persistence.info.ReturnQuestDetails"
			collection="arraylist">
			<bind-xml name="QryResults" node="element"
				location="ResponseFields" />
		</field>
	</class>
	
	
     <class name="com.cnaps.hvps.persistence.info.ReturnQuestDetails">        
        <field name="id" type="java.lang.Long"><bind-xml name="id" node="element"/></field>
		<field name="parentid" type="java.lang.Long"><bind-xml name="parentid" node="element"/></field>
		<field name="orisenddrctpty" type="java.lang.String"><bind-xml name="orisenddrctpty" node="element"/></field>
		<field name="orisendindrctpty" type="java.lang.String"><bind-xml name="orisendindrctpty" node="element"/></field>
		<field name="orirecvdrctpty" type="java.lang.String"><bind-xml name="orirecvdrctpty" node="element"/></field>
		<field name="orirecvindrctpty" type="java.lang.String"><bind-xml name="orirecvindrctpty" node="element"/></field>
		<field name="oritransactionid" type="java.lang.String"><bind-xml name="oritransactionid" node="element"/></field>
		<field name="oritransactiontypecode" type="java.lang.String"><bind-xml name="oritransactiontypecode" node="element"/></field>
		<field name="rtrdintrbksttlmamt" type="java.lang.String"><bind-xml name="rtrdintrbksttlmamt" node="element"/></field>
	 </class>   
	
</mapping>

