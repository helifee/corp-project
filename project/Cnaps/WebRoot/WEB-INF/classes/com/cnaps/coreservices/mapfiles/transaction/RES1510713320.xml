<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>Basic mapping example</description>
	<class name="com.cnaps.coreservices.CommonXMLEntity">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />

		<!-- 原发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="ResponseHead" />
		</field>
		<!--原接收服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="ResponseHead" />
		</field>
		<!--原渠道报文标识号-->
		<field name="messageId">
			<bind-xml name="MessageId" node="element"
				location="ResponseHead" />
		</field>
		<!--原报文发送时间-->
		<field name="sendDateTime">
			<bind-xml name="SendDateTime" node="element"
				location="ResponseHead" />
		</field>
		<!--处理码-->
		<field name="processCode">
			<bind-xml name="ProcessCode" node="element"
				location="ResponseHead" />
		</field>
		<!--处理信息-->
		<field name="processDes">
			<bind-xml name="ProcessDes" node="element"
				location="ResponseHead" />
		</field>
		
		<!--查询代码-->
		<field name="queryCd">
			<bind-xml name="QueryCd" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageBeginPos">
			<bind-xml name="TurnPageBeginPos" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageShowNum">
			<bind-xml name="TurnPageShowNum" node="element"
				location="ResponseHead" />
		</field>
		<field name="recordcount">
			<bind-xml name="RecordCount" node="element"
				location="ResponseHead" />
		</field>
		<field name="results"
			type="com.cnaps.hvps.persistence.transfer.RegularDebitdetails"
			collection="arraylist">
			<bind-xml name="QryResults" node="element"
				location="ResponseFields" />
		</field>
	</class>
	
	<class name="com.cnaps.hvps.persistence.transfer.RegularDebitdetails">
       	<field name="id" type="string"><bind-xml name="id" node="element"/></field>
		<field name="prntid" type="string"><bind-xml name="prntid" node="element"/></field>
		<field name="msgid" type="string"><bind-xml name="msgid" node="element"/></field>
		<field name="recptmsgid" type="string"><bind-xml name="recptmsgid" node="element"/></field>
		<field name="txid" type="string"><bind-xml name="txid" node="element"/></field>
		<field name="pmtgrpid" type="string"><bind-xml name="pmtgrpid" node="element"/></field>
		<field name="pmtkd" type="string"><bind-xml name="pmtkd" node="element"/></field>
		<!--  <field name="endtoendid" type="string"><bind-xml name="endtoendid" node="element"/></field>-->
		<field name="reftxid" type="string"><bind-xml name="reftxid" node="element"/></field>
		<!--   <field name="snglamt" type="string"><bind-xml name="snglamt" node="element"/></field>-->
		<field name="status" type="string"><bind-xml name="status" node="element"/></field>
		<field name="netgdt" type="string"><bind-xml name="netgdt" node="element"/></field>
		<field name="netgrnd" type="string"><bind-xml name="netgrnd" node="element"/></field>
		<field name="sttlmdt" type="string"><bind-xml name="sttlmdt" node="element"/></field>
		<field name="addtlinf" type="string"><bind-xml name="addtlinf" node="element"/></field>
		<field name="ustrd" type="string"><bind-xml name="ustrd" node="element"/></field>
		<field name="pmtagrmtnb" type="string"><bind-xml name="pmtagrmtnb" node="element"/></field>
		<field name="rcptdt" type="string"><bind-xml name="rcptdt" node="element"/></field>
		<field name="txrjctcd" type="string"><bind-xml name="txrjctcd" node="element"/></field>
		<field name="txrjctinf" type="string"><bind-xml name="txrjctinf" node="element"/></field>
		<field name="rcptsts" type="string"><bind-xml name="rcptsts" node="element"/></field>
		<field name="rcptaddtlinf" type="string"><bind-xml name="rcptaddtlinf" node="element"/></field>
		
		<field name="dbtracct" type="string"><bind-xml name="dbtracct" node="element"/></field>
		<field name="dbtrnm" type="string"><bind-xml name="dbtrnm" node="element"/></field>
		<field name="dbtraddr" type="string"><bind-xml name="dbtraddr" node="element"/></field>
		<field name="dbtrissuer" type="string"><bind-xml name="dbtrissuer" node="element"/></field>
		<field name="dbtrissuernm" type="string"><bind-xml name="dbtrissuernm" node="element"/></field>
		<field name="dbtrmmbid" type="string"><bind-xml name="dbtrmmbid" node="element"/></field>
		<field name="dbtrbrnchid" type="string"><bind-xml name="dbtrbrnchid" node="element"/></field>
		<field name="dbtrbrnchnm" type="string"><bind-xml name="dbtrbrnchnm" node="element"/></field>
		
		
		<field name="endToEndId" type="string"><bind-xml name="endtoendid" node="element"/></field>
		 <field name="dtlAmt" type="string"><bind-xml name="snglamt" node="element"/></field>      	
	</class>
	
</mapping>

