<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>小额 汇兑业务</description> 
      
      
       <class name="com.cnaps.hvps.persistence.transfer.TransferOfTransit">
              <map-to xml="Document"/>
               <!-- 发起服务名 -->
		        <field name="source">
		          <bind-xml name="Source" node="element" location="RequestHead" />
		       </field>
		        <!-- 目标服务名 -->
		        <field name="destination">
		          <bind-xml name="Destination" node="element" location="RequestHead" />
		       </field>
		       <!-- 功能代码 -->
		        <field name="transactionId">
		          <bind-xml name="TransactionId" node="element" location="RequestHead" />
		       </field>
		       <!-- 发送时间 -->
		        <field name="sendDateTime">
		          <bind-xml name="SendDateTime" node="element" location="RequestHead" />
		       </field>
		       <!-- 网点号 -->
		        <field name="brnchcd">
		          <bind-xml name="BrnchCd" node="element" location="RequestHead" />
		       </field>
		       <!-- 终端号 -->
		        <field name="termid">
		          <bind-xml name="TermId" node="element" location="RequestHead" />
		       </field>
		       <!-- 操作柜员 -->
		        <field name="operTeller">
		          <bind-xml name="OperTeller" node="element" location="RequestHead" />
		       </field>
		       <!-- 机构号 --><field name="orgCode"><bind-xml name="OrgCode" node="element" location="RequestHead"/></field>
		        <!-- 发起行行号 -->
		        
		          <!-- 发起参与机构 --><field name="operBankCode"><bind-xml name="OperBankCode" node="element" location="RequestHead" />
		       </field>
		        <!-- MsgCode -->
		        <field name="paymentno">
		          <bind-xml name="PmtGrpId" node="element" location="RequestHead" />
		       </field>
		       
		       
		       
		      <!-- 报文基本信息 -->
		      
		      
		      
		       <!-- 端到端标识号 -->
		        <field name="endtoendid">
		          <bind-xml name="EndToEndId" node="element" location="RequestFields" />
		       </field>
		       
		       <!-- 申请人证件号 -->
		        <field name="appcertno">
		          <bind-xml name="ProposerCertId" node="element" location="RequestFields" />
		       </field>
		       
		       <!-- 证件发型国家 -->
		        <field name="certcountry">
		          <bind-xml name="ProposerCertIssued" node="element"  location="RequestFields" />
		       </field>
		       
		       <!-- 证件类型 -->
		        <field name="certtype">
		          <bind-xml name="ProposerCertTp" node="element" location="RequestFields"  />
		       </field>
		       <!-- 申请人名称 -->
		        <field name="appnm">
		          <bind-xml name="ProposerNm" node="element"  location="RequestFields" />
		       </field>
		       <!-- 申请人联系电话 -->
		        <field name="appphone">
		          <bind-xml name="ProposerTel" node="element"  location="RequestFields" />
		       </field>
		        <!-- 申请人客户号 -->
		        <field name="proposercstmrid">
		          <bind-xml name="ProposerCstmrId" node="element"  location="RequestFields" />
		       </field>
		        <!-- 申请人账户类型 -->
		        <field name="proposeraccttp">
		          <bind-xml name="ProposerAcctTp" node="element"  location="RequestFields" />
		       </field>
		        <!-- 申请人账户币种-->
		        <field name="proposeracctccy">
		          <bind-xml name="ProposerAcctCcy" node="element"  location="RequestFields" />
		       </field>
		       
		       
		       <!-- 签发模式 -->
		        <field name="signmd">
		          <bind-xml name="SignMd" node="element" location="RequestFields"  />
		       </field>
		       
		   
		       
		        <!-- 业务类型编码 -->
		        <field name="pmttp">
		          <bind-xml name="PmtTp" node="element" location="RequestFields"  />
		       </field>
		       
		        <!-- 业务种类编码 -->
		        <field name="pmtkd">
		          <bind-xml name="PmtKd" node="element" location="RequestFields"  />
		       </field>
		       
		        <!-- 货币符号 -->
		        <field name="currency">
		          <bind-xml name="CurrencyCd" node="element" location="RequestFields"  />
		       </field>
		       
		        <!-- 汇款金额 -->
		        <field name="ntryamt">
		          <bind-xml name="Amount" node="element" location="RequestFields"  />
		       </field>
		       
		        <!-- 手续费 -->
		        <field name="charge">
		          <bind-xml name="ServiceCharge" node="element"  location="RequestFields" />
		       </field>
		       
		        <!-- 邮电费 -->
		        <field name="postage">
		          <bind-xml name="Postage" node="element" location="RequestFields"  />
		       </field>
		       
		        <!-- 本行户 -->
		        <field name="selfaccount">
		          <bind-xml name="BankCustomer" node="element"  location="RequestFields" />
		       </field>
		        <!-- 减免 -->
		        <field name="waive">
		          <bind-xml name="Waive" node="element"  location="RequestFields" />
		       </field>
		       
		        <!-- 异地加收 -->
		        <field name="otherchange">
		          <bind-xml name="OutstationCharge" node="element"  location="RequestFields" />
		       </field>
		       
		       
		       
		        <!-- 系统号 -->
		        <field name="systemcd">
		          <bind-xml name="SystemType" node="element" location="RequestFields"  />
		       </field>
		       
		        <!-- 业务优先级 -->
		        <field name="sttlmprty">
		          <bind-xml name="SttlmPrty" node="element" location="RequestFields"  />
		       </field>
		       
		       <!-- 附言 -->
		        <field name="remarkinfo">
		          <bind-xml name="AddtlInf" node="element"  location="RequestFields" />
		       </field>
		       
		       <!-- 付款人信息 -->
		       
		       <!-- 付款人账号 -->
		        <field name="dbtracctid">
		          <bind-xml name="DbtrAcct" node="element" location="RequestFields" />
		       </field>
		        <!-- 付款人名称 -->
		        <field name="dbtrnm">
		          <bind-xml name="DbtrNm" node="element" location="RequestFields" />
		       </field>
		        <!-- 付款人地址 -->
		        <field name="dbtraddr">
		          <bind-xml name="DbtrAddr" node="element"  location="RequestFields"/>
		       </field>
		       <!-- 付款人开户行行号 -->
		        <field name="dbtrissr">
		          <bind-xml name="DbtrIssuer" node="element" location="RequestFields" />
		       </field>
		        <!-- 付款人开户行名称 -->
		        <field name="dbtrissrnm">
		          <bind-xml name="DbtrIssuerNm" node="element" location="RequestFields" />
		       </field>
		       <!-- 付款清算行行号 -->
		        <field name="dbtrmmbid">
		          <bind-xml name="DbtrMmbId" node="element" location="RequestFields" />
		       </field>
		       <!-- 付款行行号 -->
		        <field name="dbtrbrchid">
		          <bind-xml name="DbtrBrnchId" node="element"  location="RequestFields"/>
		       </field>
		       <!-- 付款行名称 -->
		        <field name="dbtrbrnchnm">
		          <bind-xml name="DbtrBrnchNm" node="element"  location="RequestFields"/>
		       </field>
		       <!-- 收款人信息 -->
		       
		       <!-- 收款人账号 -->
		        <field name="cdtracctid">
		          <bind-xml name="CdtrAcct" node="element"  location="RequestFields" />
		       </field>
		       
		       <!-- 收款人名称 -->
		        <field name="cdtrnm">
		          <bind-xml name="CdtrNm" node="element" location="RequestFields" />
		       </field>
		       
		      <!-- 收款人地址 -->
		        <field name="cdtraddr">
		          <bind-xml name="CdtrAddr" node="element" location="RequestFields" />
		       </field>
		      
		       <!-- 收款行开户行行号 -->
		        <field name="cdtrissr">
		          <bind-xml name="CdtrIssuer" node="element" location="RequestFields" />
		       </field>
		         <!-- 收款行开户行名称 -->
		        <field name="cdtrissrnm">
		          <bind-xml name="CdtrIssuerNm" node="element" location="RequestFields" />
		       </field>
		       <!-- 收款行行号 -->
		        <field name="cdtrbrchid">
		          <bind-xml name="CdtrBrnchId" node="element" location="RequestFields" />
		       </field>
		        <!-- 收款清算行行号 -->
		        <field name="cdtrmmbid">
		          <bind-xml name="CdtrMmbId" node="element" location="RequestFields" />
		       </field>
		        <!-- 收款行名称 -->
		        <field name="cdtracctnm">
		          <bind-xml name="CdtrBrnchNm" node="element" location="RequestFields" />
		       </field>
		       
		       
		       
		       <!-- 委托收款（划回) -->
		       <!-- 票据日期 -->
		       <field name="collectiondate">
		          <bind-xml name="NotesDt" node="element" location="RequestFields" />
		       </field>
		       <!-- 票据类型 -->
		       <field name="collectiontype">
		          <bind-xml name="NotesTp" node="element"  location="RequestFields"/>
		       </field>
		       <!-- 票据号码 -->
		       <field name="collectionno">
		          <bind-xml name="NotesNo" node="element" location="RequestFields"  />
		       </field>
		       
		       
		       <!-- 托收承付（划回） -->
		        <!-- 票据日期 -->
		       <field name="honourdate">
		          <bind-xml name="NotesDt" node="element" location="RequestFields"/>
		       </field>
		       
		        <!-- 票据号码 -->
		       <field name="honourno">
		          <bind-xml name="NotesNo" node="element" location="RequestFields" />
		       </field>
		        <!-- 赔偿金金额 -->
		       <field name="damages">
		          <bind-xml name="AmndsAmt" node="element" location="RequestFields" />
		       </field>
		        <!-- 拒付金金额 -->
		       <field name="refusechange">
		          <bind-xml name="RjctAmt" node="element" location="RequestFields"/>
		       </field>
		       
		       <!-- 缴费业务 -->
		       <!-- 收费单位流水号 -->
		       <field name="jfmsgid">
		          <bind-xml name="FlowNum" node="element" location="RequestFields"  />
		       </field>
		       <!-- 所属期间 -->
		       <field name="jfdate">
		          <bind-xml name="Term" node="element"  location="RequestFields" />
		       </field>
		       <!-- 缴费类型 -->
		       <field name="jftype">
		          <bind-xml name="PymntTp" node="element" location="RequestFields"  />
		       </field>
		       <!-- 收费附言 -->
		       <field name="jfremarkinfo">
		          <bind-xml name="ChrgRmrk" node="element"  location="RequestFields" />
		       </field>
		       
		   <!-- 国库资金贷记划拨 -->   
		    
		       <!-- 明细汇总金额 -->
		       <field name="gkallchange">
		          <bind-xml name="DtlSmmryAmt" node="element" location="RequestFields" />
		       </field>
		       <!-- 上报国库代码 -->
		       <field name="gksendcode">
		          <bind-xml name="RprtCd" node="element" location="RequestFields" />
		       </field>
		       <!-- /接收国库代码 -->
		       <field name="gkreceivecode">
		          <bind-xml name="RcvCd" node="element" location="RequestFields" />
		       </field>
		       <!-- 报表日期 -->
		       <field name="gktabledate">
		          <bind-xml name="RprtForms" node="element" location="RequestFields" />
		       </field>
		       <!-- 报表序号 -->
		       <field name="gktableid">
		          <bind-xml name="RprtNum" node="element" location="RequestFields" />
		       </field>
		       <!-- 预算级次 -->
		       <field name="gkbudgetlevel">
		          <bind-xml name="BudgetLevel" node="element" location="RequestFields"  />
		       </field>
		       <!-- 调整期标志 -->
		       <field name="gkadjustsign">
		          <bind-xml name="Indicator" node="element"  location="RequestFields" />
		       </field>
		       <!-- 预算种类 -->
		       <field name="gkbudgettype">
		          <bind-xml name="BudgetTp" node="element"  location="RequestFields" />
		       </field>
		        <!-- 明细条数 -->
		       <field name="gkcount">
		          <bind-xml name="NumOfTrnsctns" node="element"  location="RequestFields" />
		       </field>
		       
		       <field name="treasuryTransferList" type="com.cnaps.hvps.persistence.transfer.TreasuryTransfer" collection="arraylist">
              <bind-xml name="NtlTrsrInfDtlsA" node="element" location="RequestFields"/>
             </field>
       </class>
       
        <class name="com.cnaps.hvps.persistence.transfer.TreasuryTransfer">		
       
       <field name="collcode" type="java.lang.String">
			<bind-xml name="TypeCd" node="element" />
		</field>
		<field name="adjustcode" type="java.lang.String">
			<bind-xml name="SbjctCd" node="element" />
		</field>
		<field name="amt" type="java.lang.String">
			<bind-xml name="OccrrdAmt" node="element" />
		</field>
		</class>

  </mapping>

