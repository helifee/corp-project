<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>cnaps CIS通用回执</description>


	<class
		name="com.cnaps.hvps.form.cis.CisCommonReceiptForm">
		<map-to xml="Document" />
		         <!-- 原发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="RequestHead"/>
       </field>
       <!--原接收服务名-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="RequestHead"/>
       </field>
        <!--功能代码-->
       <field name="transactionId">
          <bind-xml name="TransactionId" node="element" location="RequestHead"/>
       </field>
         
        <!--原报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="RequestHead"/>
       </field>
      
       
       
       <!-- 机构号 --><field name="orgCode"><bind-xml name="OrgCode" node="element" location="RequestHead"/></field>
         <!--网点号-->
       <field name="brnchCd">
          <bind-xml name="BrnchCd" node="element" location="RequestHead"/>
       </field>
        <!--终端号-->
       <field name="termId">
          <bind-xml name="TermId" node="element" location="RequestHead"/>
       </field>
       <!--操作柜员-->
       <field name="operTeller">
          <bind-xml name="OperTeller" node="element" location="RequestHead"/>
       </field> 
       
         <!--发起行行号-->
       
          <!-- 发起参与机构 --><field name="operBankCode"><bind-xml name="OperBankCode" node="element" location="RequestHead"/>
       </field> 
       <!--支付交易组号-->
       <field name="pmtGrpId" type="string">
       <bind-xml name="PmtGrpId" node="element" location="RequestHead"/>
       </field>
		
        
		
		
		
		
		<field name="po"
			type="com.cnaps.hvps.persistence.cis.CisCommonReceipt">
			<bind-xml name="RequestFields" node="element"  />
		</field>
		
		
	</class>
<!--  
		 <class
		name="com.cnaps.hvps.persistence.cis.CisCommonReceipt">
		<field name="cc"
			type="com.cnaps.hvps.persistence.cis.CisCopy">
			<bind-xml name="CdtTrfTxInf" node="element"  />
		</field>
		</class>
-->
		
	<class
		name="com.cnaps.hvps.persistence.cis.CisCommonReceipt">
             
		<!-- 端对端表示号-->
		<field name="endToEndId">
			<bind-xml name="EndToEndId" node="element"  />
		</field>
        <!--明细标识号      -->
		<field name="txid">
			<bind-xml name="TxId" node="element" />
		</field>
        
        <!--业务类型编码-->
		<field name="pmttp">
			<bind-xml name="PmtTp" node="element" />
		</field>
		<!--货币符号-->
		<field name="currencyCd">
			<bind-xml name="CurrencyCd" node="element" />
		</field>
			<!--结算金额 -->
		<field name="ntryAmt">
			<bind-xml name="NtryAmt" node="element" />
		</field>
		 	<!--付款人名称 -->
		<field name="dbtrNm">
			<bind-xml name="DbtrNm" node="element" />
		</field>
		 
	 	<!--付款人账户 -->
		<field name="dbtrAcct">
			<bind-xml name="DbtrAcct" node="element" />
		</field>
		 <!--付款人开户行行号 -->
		<field name="dbtrIssuer">
			<bind-xml name="DbtrIssuer" node="element" />
		</field>
		 
		
		  <!--付款清算行行号 -->
		<field name="dbtrMmbId">
			<bind-xml name="DbtrMmbId" node="element" />
		</field>

		  <!--付款行行号 -->
		<field name="dbtrBrnchId">
			<bind-xml name="DbtrBrnchId" node="element" />
		</field>
		  <!--收款清算行行号 -->
		<field name="cdtrMmbId">
			<bind-xml name="CdtrMmbId" node="element" />
		</field>
		 <!--收款行号-->
		<field name="cdtrBrnchId">
			<bind-xml name="CdtrBrnchId" node="element" />
		</field>
		
		 <!--收款人名称-->
		<field name="cdtrNm">
			<bind-xml name="CdtrNm" node="element" />
		</field>
		 <!--收款人账户-->
		<field name="cdtrAcct">
			<bind-xml name="CdtrAcct" node="element" />
		</field>
		 <!--收款人开户行行号-->
		<field name="cdtrIssuer">
			<bind-xml name="CdtrIssuer" node="element" />
		</field>
		 <!--业务种类编码-->
		<field name="pmtkd">
			<bind-xml name="PmtKd" node="element" />
		</field>
		 <!--原cis委托日期-->
		<field name="ornglCisCnsgnDt">
			<bind-xml name="OrnglCisCnsgnDt" node="element" />
		</field>
		 <!--原cis交易序号-->
		<field name="ornglCisTxId">
			<bind-xml name="OrnglCisTxId" node="element" />
		</field>
		 <!--原cis票据号码-->
		<field name="ornglCisNotesNo">
			<bind-xml name="OrnglCisNotesNo" node="element" />
		</field>
		 <!--备注-->
		<field name="ustrd">
			<bind-xml name="Ustrd" node="element" />
		</field>
		 <!--附言-->
		<field name="addtlInf">
			<bind-xml name="AddtlInf" node="element" />
		</field>
		
		 <!--新增以下三项 应答状态-->
		<field name="answerStatus">
			<bind-xml name="PrcSts" node="element" />
		</field>
		 <!--拒绝码-->
		<field name="resuseNo">
			<bind-xml name="TxRjctCd" node="element" />
		</field>
		 <!--拒绝信息-->
		<field name="txrjctinf">
			<bind-xml name="TxRjctInf" node="element" />
		</field>
		
		

		<!--付款人地址-->
		<field name="dbtrAddr">
			<bind-xml name="DbtrAddr" node="element" />
		</field>
		
	
	
	

	<!-- 付款人开户行名称 -->
	<field name="dbtrIssuerNm">
			<bind-xml name="DbtrIssuerNm" node="element" />
		</field>
	<!--  收款人地址-->
	<field name="cdtrAddr">
			<bind-xml name="CdtrAddr" node="element" />
		</field>
	
	

	<!--  证件类型-->
	<field name="certsize">
			<bind-xml name="ProposerCertTp " node="element" />
		</field>
	<!--  付款人证件号-->
	<field name="certnum">
			<bind-xml name="ProposerCertId" node="element" />
		</field>
	<!-- 付款人联系电话 -->
	<field name="phone">
			<bind-xml name="ProposerTel" node="element" />
		</field>
		<!-- 付款行名称 -->
		<field name="dbtrbrnchnm">
			<bind-xml name="DbtrBrnchNm" node="element" />
		</field>
		<!-- 收款行名称 -->
		<field name="receBankName">
			<bind-xml name="CdtrBrnchNm" node="element" />
		</field>
		 <!--收款人开户行名称-->
		<field name="cdtrissuernm">
			<bind-xml name="CdtrIssuerNm" node="element" />
		</field>
		 <!--账户币种-->
		<field name="proposeracctccy">
			<bind-xml name="ProposerAcctCcy" node="element" />
		</field>
		 <!--总额-->
		<field name="amount">
			<bind-xml name="Amount" node="element" />
		</field>
		
		
	</class>



</mapping>

