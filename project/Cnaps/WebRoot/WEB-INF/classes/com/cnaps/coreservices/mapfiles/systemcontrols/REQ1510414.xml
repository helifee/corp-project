<?xml version="1.0" encoding="UTF-8"?>
<mapping>
	<description>Basic mapping example</description>
	<class name="com.cnaps.coreservices.CommonXMLEntity">
		<!-- the name of the xml file's root element  -->
		<map-to xml="CCMS_DATA" />
		<field name="ccms_parameter_count">
			<bind-xml name="BANK_COUNT" node="element"
				location="CCMS_BANK_DATA" />
		</field>

		<field name="results"
			type="com.cnaps.hvps.persistence.info.CcmsDrctPtyInfo"
			collection="arraylist">
			<bind-xml name="ROW" node="element"
				location="CCMS_BANK_DATA" />
		</field>
	</class>
	<class name="com.cnaps.hvps.persistence.info.CcmsDrctPtyInfo">
	<field name="bankcode" type="java.lang.String">
			<bind-xml name="BANKCODE" node="element" />
		</field>
        <field name="certdn" type="java.lang.String">
			<bind-xml name="CERTDN" node="element" />
		</field>
		<field name="certsn" type="java.lang.String">
			<bind-xml name="CERTSN" node="element" />
		</field>
        <field name="certeffdate" type="java.lang.String">
			<bind-xml name="CERTEFFDATE" node="element" />
		</field>
		<field name="certinvdate" type="java.lang.String">
			<bind-xml name="CERTINVDATE" node="element" />
		</field>
		<field name="certbody" type="java.lang.String">
			<bind-xml name="CERTBODY" node="element" />
		</field>
	</class>
</mapping>