<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>Basic mapping example</description>
	<class name="com.cnaps.coreservices.CommonXMLEntity">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />

		<!-- 原发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="ResponseHead" />
		</field>
		<!--原接收服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="ResponseHead" />
		</field>
		<!--原渠道报文标识号-->
		<field name="messageId">
			<bind-xml name="MessageId" node="element"
				location="ResponseHead" />
		</field>
		<!--原支付交易组号-->
		<field name="pmtgrpid">
			<bind-xml name="PmtGrpId" node="element"
				location="ResponseHead" />
		</field>
		<!--原报文发送时间-->
		<field name="sendDateTime">
			<bind-xml name="SendDateTime" node="element"
				location="ResponseHead" />
		</field>
		<!--处理码-->
		<field name="processCode">
			<bind-xml name="ProcessCode" node="element"
				location="ResponseHead" />
		</field>
		<!--处理信息-->
		<field name="processDes">
			<bind-xml name="ProcessDes" node="element"
				location="ResponseHead" />
		</field>
		   <!--查询代码-->
       <field name="queryCd">
          <bind-xml name="QueryCd" node="element" location="ResponseHead"/>
       </field>
       <field name="recordcount">
          <bind-xml name="RecordCount" node="element" location="ResponseHead"/>
       </field>
       <field name="pagecount">
          <bind-xml name="PageCount" node="element" location="ResponseHead"/>
       </field>
		<field name="results"
			type="com.cnaps.hvps.persistence.info.Informationquery"
			collection="arraylist">
			<bind-xml name="QryResults" node="element"
				location="ResponseFields" />
		</field>
	</class>
	
	<class name="com.cnaps.hvps.persistence.info.Informationquery">
		<field name="credttm" type="java.lang.String"><bind-xml name="credttm" node="element"/></field>
		<field name="msgid" type="java.lang.String"><bind-xml name="msgid" node="element"/></field>
		<field name="sendbranch" type="java.lang.String"><bind-xml name="instgpty" node="element"/></field>
		<field name="recvbranch" type="java.lang.String"><bind-xml name="instdpty" node="element"/></field>
		<field name="workdate" type="java.lang.String"><bind-xml name="workdt" node="element"/></field>
		<field name="prodate" type="java.lang.String"><bind-xml name="prodate" node="element"/></field>
		<field name="pmtsts" type="java.lang.String"><bind-xml name="pmtsts" node="element"/></field>
		<field name="systemcd" type="java.lang.String"><bind-xml name="systemcd" node="element"/></field>
		<field name="direction" type="java.lang.String"><bind-xml name="direction" node="element"/></field>
		
	 </class> 
	
</mapping>

