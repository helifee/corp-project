		</field>
		<!-- 申请人证件类型 -->
		<field name="proposercertissued" type="string">
			<bind-xml name="ProposerCertIssued" node="element" />
		</field>
		<!--申请人账号  -->
		<field name="proposeracct" type="string">
			<bind-xml name="ProposerAcct" node="element" />
		</field>
		<!-- 申请人名称 -->
		<field name="proposernm" type="string">
			<bind-xml name="ProposerNm" node="element" />
		</field>
		<!-- 申请人地址 -->
		<field name="proposeraddr" type="string">
			<bind-xml name="ProposerAddr" node="element" />
		</field>
		<!-- 申请人电话 -->
		<field name="proposertel" type="string">
			<bind-xml name="ProposerTel" node="element" />
		</field>
		<!-- 申请人客户号 -->
		<field name="proposercstmrid" type="string">
			<bind-xml name="ProposerCstmrId" node="element" />
		</field>
		<!-- 申请人账户类型 -->
		<field name="proposeraccttp" type="string">
			<bind-xml name="ProposerAcctTp" node="element" />
		</field>
		<!-- 申请人账户币种 -->
		<field name="proposeracctccy" type="string">
			<bind-xml name="ProposerAcctCcy" node="element" />
		</field>
		<!-- 付款人账号 -->
		<field name="dbtracct" type="string">
			<bind-xml name="DbtrAcct" node="element" />
		</field>
		<!-- 付款人名称 -->
		<field name="dbtrnm" type="string">
			<bind-xml name="DbtrNm" node="element" />
		</field>
		<!-- 扣款人账号 -->
		<field name="dbtramtacctid" type="string">
			<bind-xml name="Dbtramtacctid" node="element" />
		</field>
		<!-- 扣款人名称 -->
		<field name="dbamtnm" type="string">
			<bind-xml name="DbamtNm" node="element" />
		</field>
		<!-- 付款人地址 -->
		<field name="dbtraddr" type="string">
			<bind-xml name="DbtrAddr" node="element" />
		</field>
		<!-- 付款人开户行行号 -->
		<field name="dbtrissuer" type="string">
			<bind-xml name="DbtrIssuer" node="element" />
		</field>
		<!-- 付款人开户行名称 -->
		<field name="dbtrissuernm" type="string">
			<bind-xml name="DbtrIssuerNm" node="element" />
		</field>
		<!-- 付款清算行行号 -->
		<field name="dbtrmmbid" type="string">
			<bind-xml name="DbtrMmbId" node="element" />
		</field>
		<!-- 付款行行号 -->
		<field name="dbtrbrnchid" type="string">
			<bind-xml name="DbtrBrnchId" node="element" />
		</field>
		<!-- 付款行名称 -->
		<field name="dbtrbrnchnm" type="string">
			<bind-xml name="DbtrBrnchNm" node="element" />
		</field>
		<!-- 收款人账号 -->
		<field name="cdtracct" type="string">
			<bind-xml name="CdtrAcct" node="element" />
		</field>
		<!-- 收款人名称 -->
		<field name="cdtrnm" type="string">
			<bind-xml name="CdtrNm" node="element" />
		</field>
		<!-- 收款人地址 -->
		<field name="cdtraddr" type="string">
			<bind-xml name="CdtrAddr" node="element" />
		</field>
		<!--收款人开户行行号 -->
		<field name="cdtrissuer" type="string">
			<bind-xml name="CdtrIssuer" node="element" />
		</field>
		<!-- 收款人开户行名称 -->
		<field name="cdtrissuernm" type="string">
			<bind-xml name="CdtrIssuerNm" node="element" />
		</field>
		<!-- 收款清算行行号 -->
		<field name="cdtrmmbid" type="string">
			<bind-xml name="CdtrMmbId" node="element" />
		</field>
		<!-- 收款行行号 -->
		<field name="cdtrbrnchid" type="string">
			<bind-xml name="CdtrBrnchId" node="element" />
		</field>
		<!-- 收款行名称 -->
		<field name="cdtrbrnchnm" type="string">
			<bind-xml name="CdtrBrnchNm" node="element" />
		</field>
		<!-- 手续费 -->
		<field name="servicecharge" type="string">
			<bind-xml name="ServiceCharge" node="element" />
		</field>
		<!-- 邮电费 -->
		<field name="postage" type="string">
			<bind-xml name="Postage" node="element" />
		</field>
		<!-- 异地加收 -->
		<field name="outstationcharge" type="string">
			<bind-xml name="OutstationCharge" node="element" />
		</field>
		<!-- 是否减免 -->
		<field name="waive" type="string">
			<bind-xml name="Waive" node="element" />
		</field>
		
		
		<!-- 回执期限 -->
		<field name="rcptltd" type="string">
			<bind-xml name="RcptLtd" node="element" />
		</field>
		
		<!-- 代理行手续费 -->
		<field name="agncycharge" type="string">
			<bind-xml name="AgncyCharge" node="element" />
		</field>
		<field name="gk_id" type="string">
			<bind-xml name="Id" node="element" />
		</field>
		<field name="gk_prntid" type="string">
			<bind-xml name="PrntId" node="element" />
		</field>
		<!-- 预算级次 -->
		<field name="gk_budgetlevel" type="string">
