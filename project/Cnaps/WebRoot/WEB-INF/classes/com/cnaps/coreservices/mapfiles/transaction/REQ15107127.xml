<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>BEPS 127</description>
	<class name="com.cnaps.hvps.form.transfer.GeneralDebitForm">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />

		<!-- 发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="RequestHead" />
		</field>
		<!--描述-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="RequestHead" />
		</field>
		<!--功能代码-->
		<field name="transactionId">
			<bind-xml name="TransactionId" node="element"
				location="RequestHead" />
		</field>
		<!--报文发送时间-->
		<field name="sendDateTime">
			<bind-xml name="SendDateTime" node="element"
				location="RequestHead" />
		</field>
		<!--网点号-->
		<field name="brnchCd">
			<bind-xml name="BrnchCd" node="element"
				location="RequestHead" />
		</field>
		<!-- 终端号 -->
		<field name="termId">
			<bind-xml name="TermId" node="element"
				location="RequestHead" />
		</field>
		<!-- 操作柜员 -->
		<field name="operTeller">
			<bind-xml name="OperTeller" node="element"
				location="RequestHead" />
		</field>
		
		<!-- 机构号 --><field name="orgCode"><bind-xml name="OrgCode" node="element" location="RequestHead"/></field>
			<!-- 发起参与机构 --><field name="operBankCode"><bind-xml name="OperBankCode" node="element"
				location="RequestHead" />
		</field>
		<!-- 支付交易组号 -->
		<field name="pmtGrpId" type="string">
			<bind-xml name="PmtGrpId" node="element" location="RequestHead"/>
		</field>
		<!-- 提交代码-->
		
		<field name="po"
			type="com.cnaps.hvps.persistence.transfer.GeneralDebit">
			<bind-xml name="RequestFields" node="element" />
		</field>

	</class>

	<class name="com.cnaps.hvps.persistence.transfer.GeneralDebit">
		
		
		<!-- 签发模式 -->
		<field name="signmd" type="string">
			<bind-xml name="SignMd" node="element" />
		</field>
		<!--授权柜员      -->
		<field name="warrantyId">
			<bind-xml name="WarrantyId" node="element" />
		</field>
		<!-- 业务种类编码 -->
		<field name="pmtkd" type="string">
			<bind-xml name="PmtKd" node="element" />
		</field>
		<!-- 附言 -->
		<field name="addtlinf" type="string">
			<bind-xml name="AddtlInf" node="element" />
		</field>
		
		<!-- 合同协议号 -->
		<field name="agrmtnb" type="string">
			<bind-xml name="AgrmtNb" node="element" />
		</field>
	
		<!-- 业务类型编码 -->
		<field name="pmttp" type="string">
			<bind-xml name="PmtTp" node="element" />
		</field>
		<!-- 货币符号 -->
		<field name="currencycd" type="string">
			<bind-xml name="CurrencyCd" node="element" />
		</field>
		<!--交易金额  -->
		<field name="amount" type="string">
			<bind-xml name="Amount" node="element" />
		</field>
		
		
		<!-- 申请人证件号 -->
		<field name="proposercertid" type="string">
			<bind-xml name="ProposerCertId" node="element" />
		</field>
		<!-- 申请人证件类型 -->
		<field name="proposercerttp" type="string">
