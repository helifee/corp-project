		</field>
		<!-- 调整期标志 -->
		<field name="gk_indicator" type="string">
			<bind-xml name="Indicator" node="element" />
		</field>
		<!-- 预算种类-->
		<field name="gk_budgettp" type="string">
			<bind-xml name="BudgetTp" node="element" />
		</field>
		<!-- 信息流水号 -->
		<field name="gk_flowno" type="string">
			<bind-xml name="FlowNo" node="element" />
		</field>
		<!-- 明细汇总金额 -->
		<field name="gk_dtlsmmryamt" type="string">
			<bind-xml name="DtlSmmryAmt" node="element" />
		</field>
		<!-- 上报国库代码 -->
		<field name="gk_rprtcd" type="string">
			<bind-xml name="RprtCd" node="element" />
		</field>
		<!-- 接收国库代码 -->
		<field name="gk_rcvcd" type="string">
			<bind-xml name="RcvCd" node="element" />
		</field>
		<!-- 报表日期 -->
		<field name="gk_rprtforms" type="string">
			<bind-xml name="RprtForms" node="element" />
		</field>
		<!-- 报表序号 -->
		<field name="gk_rprtnum" type="string">
			<bind-xml name="RprtNum" node="element" />
		</field>
		
		<!-- 明细信息 -->
		<field name="tftd"
			type="com.cnaps.hvps.persistence.transfer.TreasuryFundsTransferDebit"
			collection="arraylist">
			<bind-xml name="NtlTrsrInfDtlsA" node="element" />
		</field>
		<field name="gz_id" type="string">
			<bind-xml name="Id" node="element" />
		</field>
		<field name="gz_prntid" type="string">
