		</field>
		<field name="gz_dtlsmmryamt" type="string">
			<bind-xml name="DtlSmmryAmt" node="element" />
		</field>
		<field name="gz_rprtcd" type="string">
			<bind-xml name="RprtCd" node="element" />
		</field>
		<field name="gz_rcvcd" type="string">
			<bind-xml name="RcvCd" node="element" />
		</field>
		<field name="gz_rprtforms" type="string">
			<bind-xml name="RprtForms" node="element" />
		</field>
		<field name="gz_rprtnum" type="string">
			<bind-xml name="RprtNum" node="element" />
		</field>
		
		<!-- 明细条数 -->
		<field name="numOfTrnsctns" type="string">
			<bind-xml name="NumOfTrnsctns" node="element" />
		</field>
		<field name="tftdp"
			type="com.cnaps.hvps.persistence.transfer.TreasuryFundsTransferDebitPayment"
			collection="arraylist">
			<bind-xml name="NtlTrsrInfDtlsB" node="element" />
		</field>
	</class>

	<class
		name="com.cnaps.hvps.persistence.transfer.TreasuryFundsTransferDebit">
		<field name="id" type="string">
			<bind-xml name="Id" node="element" />
		</field>
		<field name="dtlid" type="string">
			<bind-xml name="DtlId" node="element" />
		</field>
		<field name="typecd" type="string">
			<bind-xml name="TypeCd" node="element" />
		</field>
		<field name="sbjctcd" type="string">
			<bind-xml name="SbjctCd" node="element" />
		</field>
		<field name="occrrdamt" type="string">
			<bind-xml name="OccrrdAmt" node="element" />
		</field>
	</class>

	<class
		name="com.cnaps.hvps.persistence.transfer.TreasuryFundsTransferDebitPayment">
		<field name="mxid" type="string">
			<bind-xml name="Id" node="element" />
		</field>
		<field name="dtlid" type="string">
			<bind-xml name="DtlId" node="element" />
		</field>
		<field name="typecd" type="string">
			<bind-xml name="TypeCd" node="element" />
		</field>
		<field name="cptlcd" type="string">
			<bind-xml name="CptlCd" node="element" />
		</field>
		<field name="cptlamt" type="string">
			<bind-xml name="CptlAmt" node="element" />
		</field>
		<field name="accrlcd" type="string">
			<bind-xml name="AccrlCd" node="element" />
		</field>
		<field name="accrlamt" type="string">
			<bind-xml name="AccrlAmt" node="element" />
		</field>
	</class>

</mapping>
