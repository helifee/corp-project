<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>Basic mapping example</description>
	<class name="com.cnaps.coreservices.CommonXMLEntity">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />

		<!-- 原发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="ResponseHead" />
		</field>
		<!--原接收服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="ResponseHead" />
		</field>
		<!--原渠道报文标识号-->
		<field name="messageId">
			<bind-xml name="MessageId" node="element"
				location="ResponseHead" />
		</field>
		<!--原报文发送时间-->
		<field name="sendDateTime">
			<bind-xml name="SendDateTime" node="element"
				location="ResponseHead" />
		</field>
		<!--处理码-->
		<field name="processCode">
			<bind-xml name="ProcessCode" node="element"
				location="ResponseHead" />
		</field>
		<!--处理信息-->
		<field name="processDes">
			<bind-xml name="ProcessDes" node="element"
				location="ResponseHead" />
		</field>

		<!--查询代码-->
		<field name="queryCd">
			<bind-xml name="QueryCd" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageBeginPos">
			<bind-xml name="TurnPageBeginPos" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageShowNum">
			<bind-xml name="TurnPageShowNum" node="element"
				location="ResponseHead" />
		</field>
		<field name="recordcount">
			<bind-xml name="RecordCount" node="element"
				location="ResponseHead" />
		</field>
		<field name="results"
			type="com.cnaps.hvps.persistence.info.BusinessReturn"
			collection="arraylist">
			<bind-xml name="QryResults" node="element"
				location="ResponseFields" />
		</field>
	</class>


	<class name="com.cnaps.hvps.persistence.info.BusinessReturn">
		<!-- 主键 -->
		<field name="id" >
			<bind-xml name="id" node="element" />
		</field>
		<!-- 来往标识 -->
		<field name="direction" type="java.lang.String">
			<bind-xml name="direction" node="element" />
		</field>
		<!-- 原业务类型编码 -->
		<field name="ornglPmtTp" type="java.lang.String">
			<bind-xml name="ornglpmttp" node="element" />
		</field>
		<!-- 原业务机构代码 -->
		<field name="orgCode" type="java.lang.String">
			<bind-xml name="orgcode" node="element" />
		</field>
		<!-- 原业务报文标识号 -->
		<field name="ornglMsgId" type="java.lang.String">
			<bind-xml name="ornglmsgid" node="element" />
		</field>
		<!-- 原业务应答标识-->
		<field name="status" type="java.lang.String">
			<bind-xml name="status" node="element" />
		</field>
		<!-- 原发起直接参与机构-->
		<field name="ornglInstgDrctPty" type="java.lang.String">
			<bind-xml name="ornglinstgdrctpty" node="element" />
		</field>
		<!-- 原发起参与机构-->
		<field name="orgnlInstgPty" type="java.lang.String">
			<bind-xml name="ornglinstgpty" node="element" />
		</field>
		<!-- 原接收参与机构-->
		<field name="ornglInstdPty" type="java.lang.String">
			<bind-xml name="ornglinstdpty" node="element" />
		</field>
		
		<!-- 原接收直接参与机构-->
		<field name="ornglInstdDrctPty" type="java.lang.String">
			<bind-xml name="ornglinstddrctpty" node="element" />
		</field>
		<!-- 原明细标识号-->
		<field name="orngltxid" type="java.lang.String">
			<bind-xml name="orngltxid" node="element" />
		</field>
		<!-- 父主键 -->
		<field name="parentid" type="java.lang.String">
			<bind-xml name="parentid" node="element" />
		</field>
		
	</class>

</mapping>

