<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>银行汇票申请解付</description> 
      
      
       <class name="com.cnaps.hvps.form.draft.JieFuForm">
              <map-to xml="Document"/>
              
               <!-- 发起服务名 -->
		        <field name="source">
		          <bind-xml name="Source" node="element" location="RequestHead" />
		       </field>
		        <!-- 目标服务名 -->
		        <field name="destination">
		          <bind-xml name="Destination" node="element" location="RequestHead" />
		       </field>
		       <!-- 功能代码 -->
		        <field name="transactionId">
		          <bind-xml name="TransactionId" node="element" location="RequestHead" />
		       </field>
		       <!-- 发送时间 -->
		        <field name="sendDateTime">
		          <bind-xml name="SendDateTime" node="element" location="RequestHead" />
		       </field>
		       <!-- 网点号 -->
		        <field name="brnchCd">
		          <bind-xml name="BrnchCd" node="element" location="RequestHead" />
		       </field>
		       <!-- 终端号 -->
		        <field name="termId">
		          <bind-xml name="TermId" node="element" location="RequestHead" />
		       </field>
		       <!-- 操作柜员 -->
		        <field name="operTeller">
		          <bind-xml name="OperTeller" node="element" location="RequestHead" />
		       </field>
		       <!-- 机构号 --><field name="orgCode"><bind-xml name="OrgCode" node="element" location="RequestHead"/></field>
		        <!-- 发起行行号 -->
		        
		          <!-- 发起参与机构 --><field name="operBankCode"><bind-xml name="OperBankCode" node="element" location="RequestHead" />
		       </field>
		        <!-- MsgCode -->
		        <field name="pmtGrpId">
		          <bind-xml name="PmtGrpId" node="element" location="RequestHead" />
		       </field>
		       
		       
		       
		      <!-- 报文基本信息 -->
		      
		      
		      
		       <!-- 备注 -->
		        <field name="ustrd">
		          <bind-xml name="Ustrd" node="element" location="RequestFields" />
		       </field>
		       
		       <!-- 出票日期 -->
		        <field name="dbtracct">
		          <bind-xml name="DbtrAcct" node="element" location="RequestFields" />
		       </field>
		        <!-- 汇票号码 -->
		        <field name="notesno">
		          <bind-xml name="NotesNo" node="element" location="RequestFields" />
		       </field>
		        <!-- 汇票密押 -->
		        <field name="billseal">
		          <bind-xml name="BillSeal" node="element" location="RequestFields" />
		       </field>
		        <!-- 汇票种类 -->
		        <field name="billtp">
		          <bind-xml name="BillTp" node="element" location="RequestFields" />
		       </field>
		       
		        <!-- 出票金额 -->
		        <field name="issueamt">
		          <bind-xml name="IssueAmt" node="element" location="RequestFields" />
		       </field>
		        <!-- 汇票签发行行号 -->
		        <field name="issuerbk">
		          <bind-xml name="IssuerBk" node="element" location="RequestFields" />
		       </field>
		        <!-- 汇票申请人账号 -->
		        <field name="issueracct">
		          <bind-xml name="IssuerAcct" node="element" location="RequestFields" />
		       </field>
		        <!-- 汇票申请人名称 -->
		        <field name="issuernm">
		          <bind-xml name="IssuerNm" node="element" location="RequestFields" />
		       </field>
		        <!--汇票收款人名称 -->
		        <field name="rcvrnm">
		          <bind-xml name="RcvrNm" node="element" location="RequestFields" />
		       </field>
		       
		        <!--原现金汇票兑付行 -->
		        <field name="orgnlpmtbk">
		          <bind-xml name="OrgnlPmtBk" node="element" location="RequestFields" />
		       </field>
		       

		        
		       
       </class>
  </mapping>

