package com.cnaps.hvps.form.info;

import java.util.ArrayList;
import java.util.List;

import com.bancstone.form.BaseActionForm;
import com.cnaps.hvps.persistence.info.GeneralSignInfo;
@SuppressWarnings("serial")
public class GeneralSignInfoForm extends BaseActionForm{
	
	
	private GeneralSignInfo po=new GeneralSignInfo();
	@SuppressWarnings("unchecked")
	private List conditions = new ArrayList();
	private String msgid;
	
	@SuppressWarnings("unchecked")
	public List getConditions() {
		return conditions;
	}
	@SuppressWarnings("unchecked")
	public void setConditions(List conditions) {
		this.conditions = conditions;
	}
	public GeneralSignInfo getPo() {
		return po;
	}
	public void setPo(GeneralSignInfo po) {
		this.po = po;
	}
	/**
	 * @return the msgid
	 */
	public String getMsgid() {
		return msgid;
	}
	/**
	 * @param msgid the msgid to set
	 */
	public void setMsgid(String msgid) {
		this.msgid = msgid;
	}

}
