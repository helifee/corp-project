package com.cnaps.hvps.form.info;

import java.util.ArrayList;

import com.bancstone.form.BaseActionForm;
import com.cnaps.hvps.persistence.info.RealTimeReversal;

public class RealTimeReversalForm extends BaseActionForm{

	private static final long serialVersionUID = 1L;
	private ArrayList<String> conditions=new ArrayList<String>();

	private String instdPty;

	/**
	 * 
	 */
	//查询条件保存
	private String msgid;

	private RealTimeReversal rtr=new RealTimeReversal();

	private String status;

	private String workDt;

	/**
	 * @return the conditions
	 */
	public ArrayList<String> getConditions() {
		return conditions;
	}

	/**
	 * @return the instdPty
	 */
	public String getInstdPty() {
		return instdPty;
	}

	/**
	 * @return the msgid
	 */
	public String getMsgid() {
		return msgid;
	}
	public RealTimeReversal getRtr() {
		return rtr;
	}
	/**
	 * @return the status
	 */
	public String getStatus() {
		return status;
	}
	/**
	 * @return the workDt
	 */
	public String getWorkDt() {
		return workDt;
	}
	
	/**
	 * @param conditions the conditions to set
	 */
	public void setConditions(ArrayList<String> conditions) {
		this.conditions = conditions;
	}

	/**
	 * @param instdPty the instdPty to set
	 */
	public void setInstdPty(String instdPty) {
		this.instdPty = instdPty;
	}

	/**
	 * @param msgid the msgid to set
	 */
	public void setMsgid(String msgid) {
		this.msgid = msgid;
	};
	
	public void setRtr(RealTimeReversal rtr) {
		this.rtr = rtr;
	}

	/**
	 * @param status the status to set
	 */
	public void setStatus(String status) {
		this.status = status;
	}

	/**
	 * @param workDt the workDt to set
	 */
	public void setWorkDt(String workDt) {
		this.workDt = workDt;
	}
	
}
