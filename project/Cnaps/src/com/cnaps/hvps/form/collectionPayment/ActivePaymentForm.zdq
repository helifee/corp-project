package com.cnaps.hvps.form.collectionPayment;

import java.util.ArrayList;
import java.util.List;

import com.bancstone.form.BaseActionForm;
import com.cnaps.hvps.persistence.collectionPayment.ActivePaymentNotify;
import com.cnaps.hvps.persistence.collectionPayment.ActivePaymentQuery;
import com.cnaps.hvps.persistence.collectionPayment.InvoicePrintApply;

/**
 * @包名   com.cnaps.hvps.form.collectionPayment
 * @文件名 ActivePaymentForm.java
 * @作者   zcx
 * @创建日期 Aug 30, 2011
 * @版本 V 1.0
 */
public class ActivePaymentForm extends BaseActionForm {
	private List conditions=new ArrayList();
    private 	ActivePaymentQuery po=new ActivePaymentQuery();
    private   ActivePaymentNotify  poNotify=new ActivePaymentNotify();
    private   InvoicePrintApply poPrint=new InvoicePrintApply();//发票打印申请
	/**
	 * @return the conditions
	 */
	public List getConditions() {
		return conditions;
	}
	/**
	 * @param conditions the conditions to set
	 */
	public void setConditions(List conditions) {
		this.conditions = conditions;
	}
	/**
	 * @return the po
	 */
	public ActivePaymentQuery getPo() {
		return po;
	}
	/**
	 * @param po the po to set
	 */
	public void setPo(ActivePaymentQuery po) {
		this.po = po;
	}
	/**
	 * @return the poNotify
	 */
	public ActivePaymentNotify getPoNotify() {
		return poNotify;
	}
	/**
	 * @param poNotify the poNotify to set
	 */
	public void setPoNotify(ActivePaymentNotify poNotify) {
		this.poNotify = poNotify;
	}
	/**
	 * @return the poPrint
	 */
	public InvoicePrintApply getPoPrint() {
		return poPrint;
	}
	/**
	 * @param poPrint the poPrint to set
	 */
	public void setPoPrint(InvoicePrintApply poPrint) {
		this.poPrint = poPrint;
	}
}
