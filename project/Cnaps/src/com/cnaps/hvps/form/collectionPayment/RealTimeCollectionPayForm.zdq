package com.cnaps.hvps.form.collectionPayment;

import java.util.ArrayList;
import java.util.List;

import com.bancstone.form.BaseActionForm;
import com.cnaps.hvps.persistence.collectionPayment.BatchCustomers;
import com.cnaps.hvps.persistence.collectionPayment.BatchCustomersChild;
import com.cnaps.hvps.persistence.collectionPayment.RealPay;

/**
 * @包名   com.cnaps.hvps.form.collectionPayment
 * @文件名 RealTimeCollectionPayForm.java
 * @作者   zcx
 * @创建日期 Sep 8, 2011
 * @版本 V 1.0
 */
public class RealTimeCollectionPayForm extends BaseActionForm{
	private List conditions=new ArrayList();
	private RealPay po=new RealPay();
	private BatchCustomers customers=new BatchCustomers();
	private BatchCustomersChild cusChild=new BatchCustomersChild();
	/**
	 * @return the conditions
	 */
	public List getConditions() {
		return conditions;
	}
	/**
	 * @param conditions the conditions to set
	 */
	public void setConditions(List conditions) {
		this.conditions = conditions;
	}
	/**
	 * @return the po
	 */
	public RealPay getPo() {
		return po;
	}
	/**
	 * @param po the po to set
	 */
	public void setPo(RealPay po) {
		this.po = po;
	}
	/**
	 * @return the customers
	 */
	public BatchCustomers getCustomers() {
		return customers;
	}
	/**
	 * @param customers the customers to set
	 */
	public void setCustomers(BatchCustomers customers) {
		this.customers = customers;
	}
	/**
	 * @return the cusChild
	 */
	public BatchCustomersChild getCusChild() {
		return cusChild;
	}
	/**
	 * @param cusChild the cusChild to set
	 */
	public void setCusChild(BatchCustomersChild cusChild) {
		this.cusChild = cusChild;
	}
}
