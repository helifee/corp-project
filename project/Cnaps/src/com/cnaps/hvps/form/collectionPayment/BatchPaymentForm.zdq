package com.cnaps.hvps.form.collectionPayment;

import java.util.ArrayList;
import java.util.List;

import com.bancstone.form.BaseActionForm;
import com.cnaps.hvps.persistence.collectionPayment.BatchCollection;
import com.cnaps.hvps.persistence.collectionPayment.BatchPayment;
import com.cnaps.hvps.persistence.collectionPayment.BatchPaymentDetails;

/**
 * @包名   com.cnaps.hvps.form.collectionPayment
 * @文件名 BatchPaymentForm.java
 * @作者   zcx
 * @创建日期 Sep 6, 2011
 * @版本 V 1.0
 */
public class BatchPaymentForm extends BaseActionForm{
	private List conditions=new ArrayList();
	private BatchPayment po=new BatchPayment();
	 private BatchPaymentDetails poo=new BatchPaymentDetails();
	 private BatchCollection bc=new BatchCollection();
	 
	   
	/**
	 * @return the conditions
	 */
	public List getConditions() {
		return conditions;
	}
	/**
	 * @param conditions the conditions to set
	 */
	public void setConditions(List conditions) {
		this.conditions = conditions;
	}
	/**
	 * @return the po
	 */
	public BatchPayment getPo() {
		return po;
	}
	/**
	 * @param po the po to set
	 */
	public void setPo(BatchPayment po) {
		this.po = po;
	}
	/**
	 * @return the poo
	 */
	public BatchPaymentDetails getPoo() {
		return poo;
	}
	/**
	 * @param poo the poo to set
	 */
	public void setPoo(BatchPaymentDetails poo) {
		this.poo = poo;
	}
	/**
	 * @return the bc
	 */
	public BatchCollection getBc() {
		return bc;
	}
	/**
	 * @param bc the bc to set
	 */
	public void setBc(BatchCollection bc) {
		this.bc = bc;
	}
}
