package com.cnaps.hvps.form.cashier;

import java.util.ArrayList;
import java.util.List;

import com.bancstone.form.BaseActionForm;
import com.cnaps.hvps.persistence.cashier.BianYa;
import com.cnaps.hvps.persistence.cis.CisCommonReceipt;

/**
 * @包名   com.cnaps.hvps.form.cashier
 * @文件名 BianYaForm.java
 * @作者   zcx
 * @创建日期 Nov 7, 2011
 * @版本 V 1.0
 */
public class BianYaForm extends BaseActionForm{
  private BianYa po=new BianYa();
	private List conditions=new ArrayList();
	private CisCommonReceipt cs=new CisCommonReceipt();
	/**
	 * @return the po
	 */
	public BianYa getPo() {
		return po;
	}
	/**
	 * @param po the po to set
	 */
	public void setPo(BianYa po) {
		this.po = po;
	}
	/**
	 * @return the conditions
	 */
	public List getConditions() {
		return conditions;
	}
	/**
	 * @param conditions the conditions to set
	 */
	public void setConditions(List conditions) {
		this.conditions = conditions;
	}
	/**
	 * @return the cs
	 */
	public CisCommonReceipt getCs() {
		return cs;
	}
	/**
	 * @param cs the cs to set
	 */
	public void setCs(CisCommonReceipt cs) {
		this.cs = cs;
	}
}
