package com.cnaps.hvps.form.cis;

import java.util.ArrayList;
import java.util.List;

import org.apache.struts.upload.FormFile;

import com.bancstone.form.BaseActionForm;
import com.cnaps.hvps.persistence.cis.Billhold;
import com.cnaps.hvps.persistence.cis.Chequehold;
import com.cnaps.hvps.persistence.cis.CisCommonReceipt;

/**
 * @包名   com.cnaps.hvps.form.cis
 * @文件名 ChequeholdForm.java
 * @作者   zcx
 * @创建日期 Sep 1, 2011
 * @版本 V 1.0
 */
public class ChequeholdForm extends BaseActionForm{
	protected FormFile files;
	private List conditions=new ArrayList();
	private Chequehold po=new Chequehold();
	private Billhold bill=new Billhold();
	CisCommonReceipt cis=new CisCommonReceipt();
	/**
	 * @return the conditions
	 */
	public List getConditions() {
		 
		return conditions;
	}
	/**
	 * @param conditions the conditions to set
	 */
	public void setConditions(List conditions) {
		this.conditions = conditions;
	}
	/**
	 * @return the po
	 */
	public Chequehold getPo() {
		return po;
	}
	/**
	 * @param po the po to set
	 */
	public void setPo(Chequehold po) {
		this.po = po;
	}
	/**
	 * @return the files
	 */
	public FormFile getFiles() {
		return files;
	}
	/**
	 * @param files the files to set
	 */
	public void setFiles(FormFile files) {
		this.files = files;
	}
	/**
	 * @return the bill
	 */
	public Billhold getBill() {
		return bill;
	}
	/**
	 * @param bill the bill to set
	 */
	public void setBill(Billhold bill) {
		this.bill = bill;
	}
	/**
	 * @return the cis
	 */
	public CisCommonReceipt getCis() {
		return cis;
	}
	/**
	 * @param cis the cis to set
	 */
	public void setCis(CisCommonReceipt cis) {
		this.cis = cis;
	}
}
