		return totalAmount;
	}
	public void setTotalAmount(String totalAmount) {
		this.totalAmount = totalAmount;
	}
	public String getHvpsTotalAmount() {
		return hvpsTotalAmount;
	}
	public void setHvpsTotalAmount(String hvpsTotalAmount) {
		this.hvpsTotalAmount = hvpsTotalAmount;
	}
	public String getBepsTotalAmount() {
		return bepsTotalAmount;
	}
	public void setBepsTotalAmount(String bepsTotalAmount) {
		this.bepsTotalAmount = bepsTotalAmount;
	}
	public String getIbpsTotalAmount() {
		return ibpsTotalAmount;
	}
	public void setIbpsTotalAmount(String ibpsTotalAmount) {
		this.ibpsTotalAmount = ibpsTotalAmount;
