package com.cnaps.hvps.persistence.clearingAccount;

/**
 * 计费与返还查询
 * @author dell
 *
 */
public class AccountingReturn {

	private String id               ;       //序号							
	private String workDate         ;       //工作日期					
	private String msgId            ;       //报文标识号				
	private String creDtTm          ;       //报文发送时间			
	private String sendDrctPty      ;       //发起直接参与机构	
	private String sendIndrctPty    ;       //发起间接参与机构	
	private String recvDrctPty      ;       //接收直接参与机构	
	private String recvIndrctPty    ;       //接收间接参与机构	
	private String systemCode       ;       //系统编号	   			
	private String remarkInfo       ;       //备注							
	private String chargeType       ;       //计费与返还类型 		
	private String chargeParticipant;       //计费直接参与者		
	private String chargeMonth      ;       //计费月份					
	private String startDate        ;       //计费开始日期			
	private String endDate          ;       //计费终止日期			
	private String totalAmount      ;       //计费/返还总金额		
	private String hvpsTotalAmount	;		//计费/返还总金额（大额系统）	
	private String bepsTotalAmount	;		//计费/返还总金额（小额系统）	
	private String ibpsTotalAmount	;		///计费/返还总金额（IBPS）			
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getWorkDate() {
		return workDate;
	}
	public void setWorkDate(String workDate) {
		this.workDate = workDate;
	}
	public String getMsgId() {
		return msgId;
	}
	public void setMsgId(String msgId) {
		this.msgId = msgId;
	}
	public String getCreDtTm() {
		return creDtTm;
	}
	public void setCreDtTm(String creDtTm) {
		this.creDtTm = creDtTm;
	}
	public String getSendDrctPty() {
		return sendDrctPty;
	}
	public void setSendDrctPty(String sendDrctPty) {
		this.sendDrctPty = sendDrctPty;
	}
	public String getSendIndrctPty() {
		return sendIndrctPty;
	}
	public void setSendIndrctPty(String sendIndrctPty) {
		this.sendIndrctPty = sendIndrctPty;
