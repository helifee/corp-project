package com.cnaps.hvps.persistence.duizhang;

/**
 * 对账异常处理类
 * @author dell
 *
 */
public class ExceptionEntity {

	private String pmtGrpId;//支付交易组号
	private String txId;//支付业务序号
	private String pmtTp;//业务类型编码
	private String msgId;//报文标识号
	private String msgTpCd;//报文类型编码
	private String amount;//汇款金额
	private String sttlmDt;//清算日期
	private String prcSts;//业务状态
	private String direction;//来往标识
	private String netgDt;//轧差日期
	private String netgRnd;//轧差场次
	private String totalAmt;//总金额
	private String proposerAcct;//申请人账号
	private String instgPty;//发起参与机构
	
	public String getPmtGrpId() {
		return pmtGrpId;
	}
	public void setPmtGrpId(String pmtGrpId) {
		this.pmtGrpId = pmtGrpId;
	}
	public String getPmtTp() {
		return pmtTp;
	}
	public void setPmtTp(String pmtTp) {
		this.pmtTp = pmtTp;
	}
	public String getMsgId() {
		return msgId;
	}
	public void setMsgId(String msgId) {
		this.msgId = msgId;
	}
	public String getMsgTpCd() {
		return msgTpCd;
	}
	public void setMsgTpCd(String msgTpCd) {
		this.msgTpCd = msgTpCd;
	}
	public String getAmount() {
		return amount;
	}
	public void setAmount(String amount) {
		this.amount = amount;
	}
	public String getSttlmDt() {
		return sttlmDt;
	}
	public void setSttlmDt(String sttlmDt) {
		this.sttlmDt = sttlmDt;
	}
	public String getPrcSts() {
		return prcSts;
	}
	public void setPrcSts(String prcSts) {
		this.prcSts = prcSts;
	}
	public String getDirection() {
		return direction;
	}
	public void setDirection(String direction) {
		this.direction = direction;
	}
	public String getTxId() {
		return txId;
	}
	public void setTxId(String txId) {
		this.txId = txId;
	}
	public String getNetgDt() {
		return netgDt;
	}
	public void setNetgDt(String netgDt) {
		this.netgDt = netgDt;
	}
	public String getNetgRnd() {
		return netgRnd;
	}
	public void setNetgRnd(String netgRnd) {
		this.netgRnd = netgRnd;
	}
	public String getTotalAmt() {
		return totalAmt;
	}
	public void setTotalAmt(String totalAmt) {
		this.totalAmt = totalAmt;
	}
	public String getProposerAcct() {
		return proposerAcct;
	}
	public void setProposerAcct(String proposerAcct) {
		this.proposerAcct = proposerAcct;
	}
	public String getInstgPty() {
		return instgPty;
	}
	public void setInstgPty(String instgPty) {
		this.instgPty = instgPty;
	}
	
}
