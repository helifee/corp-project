package com.cnaps.hvps.persistence.duizhang;

import java.util.ArrayList;
import java.util.List;

public class TotalAcctEntity {

	private List duizhangList = new ArrayList();//对账结果列表
	private String msgTpCd;//报文类型编码
	private String pmtTp;//业务类型编码
	private String chckdt;//对账日期
	private String chckResult;//对账结果
	
	private String msgTp;//大额对账中为业务类型编码，小额对账中为报文类型编码
	
	public String getMsgTp() {
		return msgTp;
	}
	public void setMsgTp(String msgTp) {
		this.msgTp = msgTp;
	}
	public List getDuizhangList() {
		return duizhangList;
	}
	public void setDuizhangList(List duizhangList) {
		this.duizhangList = duizhangList;
	}
	public String getPmtTp() {
		return pmtTp;
	}
	public void setPmtTp(String pmtTp) {
		this.pmtTp = pmtTp;
	}
	public String getChckdt() {
		return chckdt;
	}
	public void setChckdt(String chckdt) {
		this.chckdt = chckdt;
	}
	public String getChckResult() {
		return chckResult;
	}
	public void setChckResult(String chckResult) {
		this.chckResult = chckResult;
	}
	public String getMsgTpCd() {
		return msgTpCd;
	}
	public void setMsgTpCd(String msgTpCd) {
		this.msgTpCd = msgTpCd;
	}
	
	
	
}
