package com.cnaps.hvps.persistence.transfer;

public class TreasuryFundsTransferDebitPayment 
{
	private String mxid;
	private String dtlid;
	private String typecd;
	private String cptlcd;
	private String cptlamt;
	private String accrlcd;
	private String accrlamt;
	
	public String getMxid() {
		return mxid;
	}
	public void setMxid(String mxid) {
		this.mxid = mxid;
	}
	public String getDtlid() {
		return dtlid;
	}
	public void setDtlid(String dtlid) {
		this.dtlid = dtlid;
	}
	public String getTypecd() {
		return typecd;
	}
	public void setTypecd(String typecd) {
		this.typecd = typecd;
	}
	public String getCptlcd() {
		return cptlcd;
	}
	public void setCptlcd(String cptlcd) {
		this.cptlcd = cptlcd;
	}
	public String getCptlamt() {
		return cptlamt;
	}
	public void setCptlamt(String cptlamt) {
		this.cptlamt = cptlamt;
	}
	public String getAccrlcd() {
		return accrlcd;
	}
	public void setAccrlcd(String accrlcd) {
		this.accrlcd = accrlcd;
	}
	public String getAccrlamt() {
		return accrlamt;
	}
	public void setAccrlamt(String accrlamt) {
		this.accrlamt = accrlamt;
	}
	
}
