package com.cnaps.hvps.persistence.transfer;

/**
 * 来账待处理交易处理类
 * @author dell
 *
 */
public class RecvTransProcessEntity {
    private String pid ;
	private String id               ; //序号         
	private String inwardDt         ; //来报日期     
	private String msgTpCd          ; //报文类型编码 
	private String pmtTp            ; //业务类型编码 
	private String msgSts           ; //报文状态     
	private String pmtGrpId;//支付交易组号
	private String txId             ; //支付业务序号 
	private String issuerBk         ; //发起行行号   
	private String amount           ; //汇款金额     
	private String actionCd         ; //行动代码     
	private String rjctRsn          ; //失败理由     
	private String nbOfTxs          ; //明细笔数     
	private String sucflRspedTxsAmt ; //成功回执金额 
	private String nbOfSucflRspedTxs; //成功回执笔数 
	
	private String prcSts; //处理状态
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getInwardDt() {
		return inwardDt;
	}
	public void setInwardDt(String inwardDt) {
		this.inwardDt = inwardDt;
	}
	public String getMsgTpCd() {
		return msgTpCd;
	}
	public void setMsgTpCd(String msgTpCd) {
		this.msgTpCd = msgTpCd;
	}
	public String getPmtTp() {
		return pmtTp;
	}
	public void setPmtTp(String pmtTp) {
		this.pmtTp = pmtTp;
	}
	public String getMsgSts() {
		return msgSts;
	}
	public void setMsgSts(String msgSts) {
		this.msgSts = msgSts;
	}
	public String getTxId() {
		return txId;
	}
	public void setTxId(String txId) {
		this.txId = txId;
	}
	public String getIssuerBk() {
		return issuerBk;
	}
	public void setIssuerBk(String issuerBk) {
		this.issuerBk = issuerBk;
	}
	public String getAmount() {
		return amount;
	}
	public void setAmount(String amount) {
		this.amount = amount;
	}
	public String getActionCd() {
		return actionCd;
	}
	public void setActionCd(String actionCd) {
		this.actionCd = actionCd;
	}
	public String getRjctRsn() {
		return rjctRsn;
	}
	public void setRjctRsn(String rjctRsn) {
		this.rjctRsn = rjctRsn;
	}
	public String getNbOfTxs() {
		return nbOfTxs;
	}
	public void setNbOfTxs(String nbOfTxs) {
		this.nbOfTxs = nbOfTxs;
	}
	public String getSucflRspedTxsAmt() {
		return sucflRspedTxsAmt;
	}
	public void setSucflRspedTxsAmt(String sucflRspedTxsAmt) {
		this.sucflRspedTxsAmt = sucflRspedTxsAmt;
	}
	public String getNbOfSucflRspedTxs() {
		return nbOfSucflRspedTxs;
	}
	public void setNbOfSucflRspedTxs(String nbOfSucflRspedTxs) {
		this.nbOfSucflRspedTxs = nbOfSucflRspedTxs;
	}
	public String getPmtGrpId() {
		return pmtGrpId;
	}
	public void setPmtGrpId(String pmtGrpId) {
		this.pmtGrpId = pmtGrpId;
	}
	public String getPrcSts() {
		return prcSts;
	}
	public void setPrcSts(String prcSts) {
		this.prcSts = prcSts;
	}
	/**
	 * @return the pid
	 */
	public String getPid() {
		return pid;
	}
	/**
	 * @param pid the pid to set
	 */
	public void setPid(String pid) {
		this.pid = pid;
	}
	

}
