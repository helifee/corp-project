package com.cnaps.hvps.persistence.pub;

import java.io.Serializable;

/**
 * @包名   com.cnaps.hvps.persistence.pub
 * @文件名 ChargeInfo.java
 * @作者   zcx
 * @创建日期 Oct 24, 2011
 * @版本 V 1.0
 */
public class ChargeInfo implements Serializable {

	private String msgtpcd;//报文类型编码
	private String charge;//手续费
	private String youdianfei;//邮电费
	private String yidijiashou;//异地加收
	private String gongbenfei;//工本费
	private String pmttp;//业务
	private String money;//汇款金额
	
	private String hvpsmoney;//大额金额下限
	private String bepsmoney;//小额金额上限
	private String sttlmprty;//优先级
	
	
	
	
	/**
	 * @return the money
	 */
	public String getMoney() {
		return money;
	}
	/**
	 * @param money the money to set
	 */
	public void setMoney(String money) {
		this.money = money;
	}
	/**
	 * @return the pmttp
	 */
	public String getPmttp() {
		return pmttp;
	}
	/**
	 * @param pmttp the pmttp to set
	 */
	public void setPmttp(String pmttp) {
		this.pmttp = pmttp;
	}
	public String getMsgtpcd() {
		return msgtpcd;
	}
	public void setMsgtpcd(String msgtpcd) {
		this.msgtpcd = msgtpcd;
	}
	public String getCharge() {
		return charge;
	}
	public void setCharge(String charge) {
		this.charge = charge;
	}
	public String getYoudianfei() {
		return youdianfei;
	}
	public void setYoudianfei(String youdianfei) {
		this.youdianfei = youdianfei;
	}
	public String getYidijiashou() {
		return yidijiashou;
	}
	public void setYidijiashou(String yidijiashou) {
		this.yidijiashou = yidijiashou;
	}
	public String getHvpsmoney() {
		return hvpsmoney;
	}
	public void setHvpsmoney(String hvpsmoney) {
		this.hvpsmoney = hvpsmoney;
	}
	public String getBepsmoney() {
		return bepsmoney;
	}
	public void setBepsmoney(String bepsmoney) {
		this.bepsmoney = bepsmoney;
	}
	public String getSttlmprty() {
		return sttlmprty;
	}
	public void setSttlmprty(String sttlmprty) {
		this.sttlmprty = sttlmprty;
	}
	public String getGongbenfei() {
		return gongbenfei;
	}
	public void setGongbenfei(String gongbenfei) {
		this.gongbenfei = gongbenfei;
	}
	
}
