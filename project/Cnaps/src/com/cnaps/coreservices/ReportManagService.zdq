	/*
	 * 添加公共参数信息
	 */
	public boolean addPartyinfo(List list){
		boolean result = true;
		if(list.size()<=0){
			return false;
		}
		try {
			for(int i=0;i<list.size();i++){
				this.getCommondao().createByClazz((Object)list.get(i));
			}
		} catch (RuntimeException e) {
			e.printStackTrace();
			result = false;
		}
		return result;
	}
	
	
