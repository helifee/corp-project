			HashMap hashMap = new HashMap();
//			String path1 = this.getClass().getClassLoader().getResource("/").getFile();
//			String filepath = path1+ "com/cnaps/coreservices/ireport/HVPS101.japser";
			
			JasperReport report = (JasperReport)JRLoader.loadObject(path);
			             
			bytes = JasperRunManager.runReportToPdf(report,hashMap,dataSource);
