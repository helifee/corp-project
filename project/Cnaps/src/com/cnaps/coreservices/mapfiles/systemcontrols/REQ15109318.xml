<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<!-- 业务退回申请mappingfile -->
<mapping>
	<class name="com.cnaps.hvps.form.info.BusinessReturnForm">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />
		<!-- 发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="RequestHead" />
		</field>
		<!--接收服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="RequestHead" />
		</field>
		<!--功能代码-->
		<field name="transactionId">
			<bind-xml name="TransactionId" node="element"
				location="RequestHead" />
		</field>
		<!--报文发送时间-->
		<field name="sendDateTime">
			<bind-xml name="SendDateTime" node="element"
				location="RequestHead" />
		</field>
		<!--网点号-->
		<field name="brnchCd">
			<bind-xml name="BrnchCd" node="element"
				location="RequestHead" />
		</field>
		<!-- 终端号 -->
		<field name="termId">
			<bind-xml name="TermId" node="element"
				location="RequestHead" />
		</field>
		<!-- 操作柜员 -->
		<field name="operTeller">
			<bind-xml name="OperTeller" node="element"
				location="RequestHead" />
		</field>


		<!-- 发起参与机构 -->
		<field name="operBankCode">
			<bind-xml name="OperBankCode" node="element"
				location="RequestHead" />
		</field>
		<!-- 机构号 -->
		<field name="orgCode">
			<bind-xml name="OrgCode" node="element"
				location="RequestHead" />
		</field>
		<!--支付交易组号-->
		<field name="pmtGrpId">
			<bind-xml name="PmtGrpId" node="element"
				location="RequestHead" />
		</field>

		<!-- 退回申请-->
		<field name="po"
			type="com.cnaps.hvps.persistence.info.BusinessReturn">
			<bind-xml name="RequestFields" node="element" />
		</field>
	</class>


	<class name="com.cnaps.hvps.persistence.info.BusinessReturn">
		<!-- 原报文标识号 -->
		<field name="OrnglMsgId" type="java.lang.String">
			<bind-xml name="OrgnlMsgId" node="element" />
		</field>

		<!-- 退回类型-->
		<field name="rtrnTp" type="java.lang.String">
			<bind-xml name="RtrnTp" node="element" />
		</field>
		<!-- 业务明细信息-->
		<field name="list" type="java.lang.String"
			collection="arraylist">
			<bind-xml name="OriTrnsctnId" node="element" />
		</field>
	</class>


</mapping>