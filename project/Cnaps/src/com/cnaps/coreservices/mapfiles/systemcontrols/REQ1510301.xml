<?xml version="1.0" encoding="UTF-8"?>
<mapping>
	<description>Basic mapping example</description>
	<class name="com.cnaps.coreservices.CommonXMLEntity">
		<!-- the name of the xml file's root element  -->
		<map-to xml="CCMS_DATA" />
        <field name="ccms_parameter_count">
			<bind-xml name="CCMS_PARAMETER_COUNT" node="element"
				location="CCMS_PARAMETER_DATA" />
		</field>
		
		<field name="results"
			type="com.cnaps.hvps.persistence.info.CcmsDrctPtyInfo"
			collection="arraylist">
			<bind-xml name="PARAMETER" node="element"
				location="CCMS_PARAMETER_DATA" />
		</field>
	</class>
<class name="com.cnaps.hvps.persistence.info.CcmsDrctPtyInfo">
       <field name="varcode" type="java.lang.String">
			<bind-xml
			 name="VARCODE" node="element" />
		</field>
		<field name="varname" type="java.lang.String">
			<bind-xml
			 name="VARNAME" node="element" />
		</field>
      <field name="varvalue" type="java.lang.String">
			<bind-xml name="VARVALUE" node="element" />
		</field>
	 <field name="vartype" type="java.lang.String">
			<bind-xml name="VARTYPE" node="element" />
		</field>
	 <field name="altstyle" type="java.lang.String">
			<bind-xml name="VARTYPE" node="element" />
		</field>
	 <field name="syscode" type="java.lang.String">
			<bind-xml name="SYSCODE" node="element" />
		</field>
	</class>
</mapping>