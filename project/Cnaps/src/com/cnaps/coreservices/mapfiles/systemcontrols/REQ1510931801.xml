<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>业务退回查询(发送)</description>
	<class name="com.cnaps.hvps.form.info.BusinessReturnForm">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />

		<!-- 发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="RequestHead" />
		</field>
		<!--目标服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="RequestHead" />
		</field>
		<!--功能代码-->
		<field name="transactionId">
			<bind-xml name="TransactionId" node="element"
				location="RequestHead" />
		</field>
		<!--报文发送时间-->
		<field name="sendDateTime">
			<bind-xml name="SendDateTime" node="element"
				location="RequestHead" />
		</field>
		<!--网点号-->
		<field name="brnchCd">
			<bind-xml name="BrnchCd" node="element"
				location="RequestHead" />
		</field>
			<!-- 终端号 -->
		<field name="termId">
			<bind-xml name="TermId" node="element"
				location="RequestHead" />
		</field>
		<!-- 操作柜员 -->
		<field name="operTeller">
			<bind-xml name="OperTeller" node="element"
				location="RequestHead" />
		</field>
		<!-- 发起参与机构 -->
		<field name="operBankCode">
			<bind-xml name="OperBankCode" node="element"
				location="RequestHead" />
		</field>


		<!-- 机构号 -->
		<field name="orgCode">
			<bind-xml name="OrgCode" node="element"
				location="RequestHead" />
		</field>
		<!--查询代码-->
		<field name="queryCd">
			<bind-xml name="QueryCd" node="element"
				location="RequestHead" />
		</field>
		<!-- 分页开始位置 -->
		<field name="turnPageBeginPos">
			<bind-xml name="TurnPageBeginPos" node="element"
				location="RequestHead" />
		</field>
		<!-- 每页显示记录数目 -->
		<field name="turnPageShowNum">
			<bind-xml name="TurnPageShowNum" node="element"
				location="RequestHead" />
		</field>
		<!-- 查询条件 -->
		<field name="conditions" type="java.lang.String"
			collection="arraylist">
			<bind-xml name="Field" node="element"
				location="RequestFields" />
		</field>
	</class>

</mapping>