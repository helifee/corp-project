<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description> 通用信息查询应答报文 </description> 
      <class name="com.cnaps.hvps.form.info.GeneralSignInfoAnswerForm" >
      <!-- the name of the xml file's root element  -->
       <map-to xml="Document"/>
     
         <!-- 发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="RequestHead"/>
       </field>
       <!--描述-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="RequestHead"/>
       </field>
        <!--功能代码-->
       <field name="transactionId">
          <bind-xml name="TransactionId" node="element" location="RequestHead"/>
       </field>
        <!--报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="RequestHead"/>
       </field>
         <!--网点号-->
       <field name="brnchCd">
          <bind-xml name="BrnchCd" node="element" location="RequestHead"/>
       </field>
       <!-- 终端号 -->
       <field name="termId">
          <bind-xml name="TermId" node="element" location="RequestHead"/>
       </field>
       <!-- 操作柜员 -->
        <field name="operTeller">
          <bind-xml name="OperTeller" node="element" location="RequestHead"/>
       </field>
       
        
          <!-- 发起参与机构 --><field name="operBankCode"><bind-xml name="OperBankCode" node="element" location="RequestHead"/>
       </field>
       
        <!-- 提交代码-->
       <field name="po" type="com.cnaps.hvps.persistence.info.GeneralSignInfoAnswer">
          <bind-xml name="RequestFields" node="element"/>
       </field>
       
     </class>
 
     <class name="com.cnaps.hvps.persistence.info.GeneralSignInfoAnswer">
       <field name="status" type="java.lang.String">
			<bind-xml name="Status" node="element" />
		</field>
       <field name="rejectCode" type="java.lang.String">
			<bind-xml name="RjctCd" node="element" />
		</field>	
		<field name="processParty" type="java.lang.String">
			<bind-xml name="PrcPty" node="element" />
		</field>
		<field name="rejectInformation" type="java.lang.String">
			<bind-xml name="RjctInf" node="element" />
		</field>
		<field name="ustrd" type="java.lang.String">
			<bind-xml name="Ustrd" node="element" />
		</field>
       <field name="orgnlInstgPty" type="java.lang.String">
			<bind-xml name="OrgnlInstgPty" node="element" />
		</field>	
		<field name="orgnlMsgId" type="java.lang.String">
			<bind-xml name="OrgnlMsgId" node="element" />
		</field>
		<field name="orgnlMT" type="java.lang.String">
			<bind-xml name="OrgnlMT"  node="element" />
			
		</field>
         
	 </class>   

  </mapping>

