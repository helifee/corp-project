<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>Basic mapping example</description>
	<class name="com.cnaps.coreservices.CommonXMLEntity">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />

		<!-- 原发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="ResponseHead" />
		</field>
		<!--原接收服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="ResponseHead" />
		</field>
		<!--原渠道报文标识号-->
		<field name="messageId">
			<bind-xml name="MessageId" node="element"
				location="ResponseHead" />
		</field>
		<!--原报文发送时间-->
		<field name="sendDateTime">
			<bind-xml name="SendDateTime" node="element"
				location="ResponseHead" />
		</field>
		<!--处理码-->
		<field name="processCode">
			<bind-xml name="ProcessCode" node="element"
				location="ResponseHead" />
		</field>
		<!--处理信息-->
		<field name="processDes">
			<bind-xml name="ProcessDes" node="element"
				location="ResponseHead" />
		</field>
		
		<!--查询代码-->
		<field name="queryCd">
			<bind-xml name="QueryCd" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageBeginPos">
			<bind-xml name="TurnPageBeginPos" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageShowNum">
			<bind-xml name="TurnPageShowNum" node="element"
				location="ResponseHead" />
		</field>
		<field name="recordcount">
			<bind-xml name="RecordCount" node="element"
				location="ResponseHead" />
		</field>
		<field name="results"
			type="com.cnaps.hvps.persistence.info.GeneralSignInfo"
			collection="arraylist">
			<bind-xml name="QryResults" node="element"
				location="ResponseFields" />
		</field>
	</class>
	
	<class name="com.cnaps.hvps.persistence.info.GeneralSignInfo">
       <!--序号-->
       <field name="id" type="java.lang.Long">
       	<bind-xml name="id" node="element" />
       </field>
       <!--工作日期-->
       <field name="workdate" type="java.lang.String">
       	<bind-xml name="workdt" node="element" />
       </field>
       <!--来往标识-->
       <field name="direction" type="java.lang.String">
       	<bind-xml name="direction" node="element" />
       </field>
       <!--支付交易组号-->
       <field name="pmtGrpId" type="java.lang.String">
       	<bind-xml name="pmtgrpid" node="element" />
       </field>
       <!--报文标识号-->
       <field name="msgid" type="java.lang.String">
       	<bind-xml name="msgid" node="element" />
       </field>
       <!--报文发送时间-->
       <field name="credttm" type="java.lang.String">
       	<bind-xml name="credttm" node="element" />
       </field>
       <!--回执报文标识号-->
       <field name="resultmsgid" type="java.lang.String">
       	<bind-xml name="recptmsgid" node="element" />
       </field>
       <!--回复报文时间-->
       <field name="resultdttm" type="java.lang.String">
       	<bind-xml name="recptdttm" node="element" />
       </field>
       <!--发起参与机构行号-->
       <field name="sendindrctpty" type="java.lang.String">
       	<bind-xml name="instgpty" node="element" />
       </field>
       <!--接收参与机构行号-->
       <field name="recvindrctpty" type="java.lang.String">
       	<bind-xml name="instdpty" node="element" />
       </field>
       <!--发起直接参与机构-->
       <field name="senddrctpty" type="java.lang.String">
       	<bind-xml name="instgdrctpty" node="element" />
       </field>
       <!--接收直接参与机构-->
       <field name="recvdrctpty" type="java.lang.String">
       	<bind-xml name="instddrctpty" node="element" />
       </field>
       <!--系统编号-->
       <field name="systemcode" type="java.lang.String">
       	<bind-xml name="systemcd" node="element" />
       </field>
       <!--备注-->
       <field name="remarkinfo" type="java.lang.String">
       	<bind-xml name="ustrd" node="element" />
       </field>
       <!--业务类型编码-->
       <field name="ctgypurpcd" type="java.lang.String">
       	<bind-xml name="pmttp" node="element" />
       </field>
       <!--业务种类编码-->
       <field name="txtpcd" type="java.lang.String">
       	<bind-xml name="pmtkd" node="element" />
       </field>
       <!--信息标题-->
       <field name="title" type="java.lang.String">
       	<bind-xml name="msgttl" node="element" />
       </field>
       <!--信息内容-->
       <field name="content" type="java.lang.String">
       	<bind-xml name="msgcntnt" node="element" />
       </field>
       <!--附件长度-->
       <field name="attachmentlength" type="java.lang.Long">
       	<bind-xml name="attchmntlen" node="element" />
       </field>
       <!--附件名称-->
       <field name="attachmentname" type="java.lang.String">
       	<bind-xml name="attchmntnm" node="element" />
       </field>
       <!--附件内容-->
       <field name="attachmentcontent" type="java.lang.String">
       	<bind-xml name="attchmntcntnt" node="element" />
       </field>
       <!--业务状态-->
       <field name="pmtsts" type="java.lang.String">
       	<bind-xml name="status" node="element" />
       </field>
       <!--业务处理码-->
       <field name="procode" type="java.lang.String">
       	<bind-xml name="procd" node="element" />
       </field>
       <!--业务拒绝信息-->
       <field name="rjctdesc" type="java.lang.String">
       	<bind-xml name="rjctinf" node="element" />
       </field>
       <!--业务处理参与机构-->
       <field name="rjctedPtyId" type="java.lang.String">
       	<bind-xml name="rjctedptyid" node="element" />
       </field>
	</class>
	
</mapping>

