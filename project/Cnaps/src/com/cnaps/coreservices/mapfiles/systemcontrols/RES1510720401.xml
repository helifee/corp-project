<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>票交来报待处理</description>
	<class name="com.cnaps.coreservices.CommonXMLEntity">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />

		<!-- 原发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="ResponseHead" />
		</field>
		<!--原接收服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="ResponseHead" />
		</field>
		<!--原渠道报文标识号-->
		<field name="messageId">
			<bind-xml name="MessageId" node="element"
				location="ResponseHead" />
		</field>
		<!--原报文发送时间-->
		<field name="sendDateTime">
			<bind-xml name="SendDateTime" node="element"
				location="ResponseHead" />
		</field>
		<!--处理码-->
		<field name="processCode">
			<bind-xml name="ProcessCode" node="element"
				location="ResponseHead" />
		</field>
		<!--处理信息-->
		<field name="processDes">
			<bind-xml name="ProcessDes" node="element"
				location="ResponseHead" />
		</field>


		<!--查询代码-->
		<field name="queryCd">
			<bind-xml name="QueryCd" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageBeginPos">
			<bind-xml name="TurnPageBeginPos" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageShowNum">
			<bind-xml name="TurnPageShowNum" node="element"
				location="ResponseHead" />
		</field>
		<field name="recordcount">
			<bind-xml name="RecordCount" node="element"
				location="ResponseHead" />
		</field>
		<field name="results"
			type="com.cnaps.hvps.persistence.cis.CisBepsBkNodeTrade"
			collection="arraylist">
			<bind-xml name="QryResults" node="element"
				location="ResponseFields" />
		</field>
	</class>

	<class name="com.cnaps.hvps.persistence.cis.CisBepsBkNodeTrade">
		<field name="id" type="java.lang.Long"><bind-xml name="id" node="element" /></field>
		<field name="workdt" type="java.lang.String"><bind-xml name="workdt" node="element" /></field>
		<field name="msgid" type="java.lang.String"><bind-xml name="msgid" node="element" /></field>
		<field name="credttm" type="java.lang.String"><bind-xml name="credttm" node="element" /></field>
		<field name="instgpty" type="java.lang.String"><bind-xml name="instgpty" node="element" /></field>
		<field name="instdpty" type="java.lang.String"><bind-xml name="instdpty" node="element" /></field>
		<field name="nboftxsofscs" type="java.lang.String"><bind-xml name="nboftxsofscs" node="element" /></field>
		<field name="ctrlsum" type="java.lang.String"><bind-xml name="ctrlsum" node="element" /></field>
		<field name="nboftxs" type="java.lang.String"><bind-xml name="nboftxs" node="element" /></field>
		<field name="status" type="java.lang.String"><bind-xml name="status" node="element" /></field>
		<field name="prcsts" type="java.lang.String"><bind-xml name="prcsts" node="element" /></field>
		<field name="prccd" type="java.lang.String"><bind-xml name="prccd" node="element" /></field>
		<field name="rjctcd" type="java.lang.String"><bind-xml name="rjctcd" node="element" /></field>
		<field name="rjctinf" type="java.lang.String"><bind-xml name="rjctinf" node="element" /></field>
		<field name="rjctedptyid" type="java.lang.String"><bind-xml name="rjctedptyid" node="element" /></field>
		<field name="sttlmdt" type="java.lang.String"><bind-xml name="sttlmdt" node="element" /></field>
		<field name="msgidunq" type="java.lang.String"><bind-xml name="msgidunq" node="element" /></field>
		<field name="pmtkd" type="java.lang.String"><bind-xml name="pmtkd" node="element" /></field>
	</class>

</mapping>

