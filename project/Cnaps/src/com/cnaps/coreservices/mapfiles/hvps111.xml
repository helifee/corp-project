<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>Basic mapping example</description> 
      <class name="com.cnaps.xly.hvps.ClientCambiumForm" >
      <!-- the name of the xml file's root element  -->
       <map-to xml="BussinessRequest"/>
     
         <!-- 报文标识号 -->
        <field name="msgId">
          <bind-xml name="MsgId" node="element" location="BussinessRequest/RequestFields"/>
       </field>
        <!-- 交易信息 -->
        <!-- 端到端标识号 -->
        <field name="endToEndId">
          <bind-xml name="EndToEndId" node="element" location="BussinessRequest/RequestFields/PmtInfo"/>
       </field>
        <!-- 业务类型编码 -->
        <field name="pmtTp">
          <bind-xml name="PmtTp" node="element" location="BussinessRequest/RequestFields/PmtInfo"/>
       </field>
        <!-- 业务种类编码 -->
        <field name="pmtKd">
          <bind-xml name="PmtKd" node="element" location="BussinessRequest/RequestFields/PmtInfo"/>
       </field>
       <!-- 货币符号 -->
        <field name="currency">
          <bind-xml name="Currency" node="element" location="BussinessRequest/RequestFields/PmtInfo"/>
       </field>
        <!-- 交易金额 -->
        <field name="ntryAmt">
          <bind-xml name="NtryAmt" node="element" location="BussinessRequest/RequestFields/PmtInfo"/>
       </field>
        <!--手续费金额 -->
        <field name="charge">
          <bind-xml name="Charge" node="element" location="BussinessRequest/RequestFields/PmtInfo"/>
       </field>
        <!-- 业务优先级 -->
        <field name="sttlmPrty">
          <bind-xml name="SttlmPrty" node="element" location="BussinessRequest/RequestFields/PmtInfo"/>
       </field>
        <!-- 备注 -->
        <field name="rmkInf">
          <bind-xml name="RmkInf" node="element" location="BussinessRequest/RequestFields/PmtInfo"/>
       </field>
       
       <!-- 付款人信息 -->
       <!-- 付款人账号 -->
        <field name="dbtrAcctId">
          <bind-xml name="AcctId" node="element" location="BussinessRequest/RequestFields/DbtrInfo"/>
       </field>
       <!-- 付款人账号 -->
        <field name="dbtrNm">
          <bind-xml name="Nm" node="element" location="BussinessRequest/RequestFields/DbtrInfo"/>
       </field>
        <!-- 付款开户行名称 -->
        <field name="dbtrIssr">
          <bind-xml name="Issr" node="element" location="BussinessRequest/RequestFields/DbtrInfo"/>
       </field>
       <!-- 付款人清算行行号 -->
        <field name="dbtrMmbId">
          <bind-xml name="MmbId" node="element" location="BussinessRequest/RequestFields/DbtrInfo"/>
       </field>
        <!-- 付款行行号 -->
        <field name="dbtrBrnchId">
          <bind-xml name="BrnchId" node="element" location="BussinessRequest/RequestFields/DbtrInfo"/>
       </field>
       
        <!-- 收款人信息 -->
       <!-- 收款人账号 -->
        <field name="cdtrAcctId">
          <bind-xml name="AcctId" node="element" location="BussinessRequest/RequestFields/CdtrInfo"/>
       </field>
       <!-- 收款人名称-->
        <field name="cdtrNm">
          <bind-xml name="Nm" node="element" location="BussinessRequest/RequestFields/CdtrInfo"/>
       </field>
        <!-- 收款开户行名称 -->
        <field name="cdtrIssr">
          <bind-xml name="Issr" node="element" location="BussinessRequest/RequestFields/CdtrInfo"/>
       </field>
       <!-- 收付款人清算行行号 -->
        <field name="cdtrMmbId">
          <bind-xml name="MmbId" node="element" location="BussinessRequest/RequestFields/CdtrInfo"/>
       </field>
        <!-- 收款行行号 -->
        <field name="cdtrBrnchId">
          <bind-xml name="BrnchId" node="element" location="BussinessRequest/RequestFields/CdtrInfo"/>
       </field>
       
      </class>

  </mapping>

