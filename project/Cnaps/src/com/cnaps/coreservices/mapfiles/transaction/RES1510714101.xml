<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>即时转账查询回执</description> 
      <class name="com.cnaps.coreservices.CommonXMLEntity" >
      <!-- the name of the xml file's root element  -->
       <map-to xml="Document"/>
     
         <!-- 原发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="ResponseHead"/>
       </field>
       <!--原接收服务名-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="ResponseHead"/>
       </field>
        <!--原渠道报文标识号
       <field name="messageId">
          <bind-xml name="MessageId" node="element" location="ResponseHead"/>
       </field>
       -->
        <!--原报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="ResponseHead"/>
       </field>
         <!--处理码-->
       <field name="processCode">
          <bind-xml name="ProcessCode" node="element" location="ResponseHead"/>
       </field>
         <!--处理信息-->
       <field name="processDes">
          <bind-xml name="ProcessDes" node="element" location="ResponseHead"/>
       </field>
       
       
         <!--查询代码-->
       <field name="queryCd">
          <bind-xml name="QueryCd" node="element" location="ResponseHead"/>
       </field>
       <field name="recordcount">
          <bind-xml name="RecordCount" node="element" location="ResponseHead"/>
       </field>
       <field name="pagecount">
          <bind-xml name="PageCount" node="element" location="ResponseHead"/>
       </field>
       <field name="results" type="com.cnaps.hvps.persistence.transfer.InstantTransfer" 
       	collection="arraylist">
          <bind-xml name="QryResults" node="element" location="ResponseFields"/>
       </field>
      </class>

	
	
	
	   <class name="com.cnaps.hvps.persistence.transfer.InstantTransfer">		
       
			<!--序号             --> <field name="id" type="java.lang.String"><bind-xml name="id" node="element" /></field>
			<!--报文标识号       --> <field name="msgId" type="java.lang.String"><bind-xml name="msgid" node="element" /></field>
			<!--回执报文标识号   --> <field name="recptMsgId" type="java.lang.String"><bind-xml name="recptmsgid" node="element" /></field>
			<!--支付业务序号     --> <field name="txId" type="java.lang.String"><bind-xml name="txid" node="element" /></field>
			<!--端到端标识号     --> <field name="endToEndId" type="java.lang.String"><bind-xml name="endtoendid" node="element" /></field>
			<!--业务类型编码     --> <field name="pmtTp" type="java.lang.String"><bind-xml name="pmttp" node="element" /></field>
			<!--币种代码         --> <field name="currencyCd" type="java.lang.String"><bind-xml name="currencycd" node="element" /></field>
			<!--汇款金额         --> <field name="amount" type="java.lang.String"><bind-xml name="amount" node="element" /></field>
			<!--业务优先级       --> <field name="sttlmPrty" type="java.lang.String"><bind-xml name="sttlmprty" node="element" /></field>
			<!--系统工作日       --> <field name="workDt" type="java.lang.String"><bind-xml name="workdt" node="element" /></field>
			<!--业务类型编码集   --> <field name="pmtTpClctn" type="java.lang.String"><bind-xml name="pmttpclctn" node="element" /></field>
			<!--业务种类编码集   --> <field name="pmtKdClctn" type="java.lang.String"><bind-xml name="pmtkdclctn" node="element" /></field>
			<!--发起参与机构     --> <field name="instgPty" type="java.lang.String"><bind-xml name="instgpty" node="element" /></field>
			<!--发起直接参与机构 --> <field name="instgDrctPty" type="java.lang.String"><bind-xml name="instgdrctpty" node="element" /></field>
			
			
			<!--付款人账号       --> <field name="dbtrAcct" type="java.lang.String"><bind-xml name="dbtracct" node="element" /></field>
			<!--付款人名称       --> <field name="dbtrNm" type="java.lang.String"><bind-xml name="dbtrnm" node="element" /></field>
			<!--付款人地址       --> <field name="dbtrAddr" type="java.lang.String"><bind-xml name="dbtraddr" node="element" /></field>
			<!--付款人开户行行号 --> <field name="dbtrIssuer" type="java.lang.String"><bind-xml name="dbtrissuer" node="element" /></field>
			<!--付款人开户行行名 --> <field name="dbtrIssuerNm" type="java.lang.String"><bind-xml name="dbtrissuernm" node="element" /></field>
			<!--付款清算行行号   --> <field name="dbtrMmbId" type="java.lang.String"><bind-xml name="dbtrmmbid" node="element" /></field>
			<!--付款行行号       --> <field name="dbtrBrnchId" type="java.lang.String"><bind-xml name="dbtrbrnchid" node="element" /></field>
			<!--付款行行名       --> <field name="dbtrBrnchNm" type="java.lang.String"><bind-xml name="dbtrbrnchnm" node="element" /></field>
			
			
			
			<!--收款人账号       --> <field name="cdtrAcct" type="java.lang.String"><bind-xml name="cdtracct" node="element" /></field>
			<!--收款人名称       --> <field name="cdtrNm" type="java.lang.String"><bind-xml name="cdtrnm" node="element" /></field>
			<!--收款人地址       --> <field name="cdtrAddr" type="java.lang.String"><bind-xml name="cdtraddr" node="element" /></field>
			<!--收款人开户行行号 --> <field name="cdtrIssuer" type="java.lang.String"><bind-xml name="cdtrissuer" node="element" /></field>
			<!--收款人开户行行名 --> <field name="cdtrIssuerNm" type="java.lang.String"><bind-xml name="cdtrissuernm" node="element" /></field>
			<!--收款清算行行号   --> <field name="cdtrMmbId" type="java.lang.String"><bind-xml name="cdtrmmbid" node="element" /></field>
			<!--收款行行号       --> <field name="cdtrBrnchId" type="java.lang.String"><bind-xml name="cdtrbrnchid" node="element" /></field>
			<!--收款行行名       --> <field name="cdtrBrnchNm" type="java.lang.String"><bind-xml name="cdtrbrnchnm" node="element" /></field>
			
			<!--特许参与者       --> <field name="spclPrmssnMmbId" type="java.lang.String"><bind-xml name="spclprmssnmmbid" node="element" /></field>
			<!--打印次数         --> <field name="printTimes" type="java.lang.String"><bind-xml name="printtimes" node="element" /></field>
			
			<!--业务状态         --> <field name="status" type="java.lang.String"><bind-xml name="printtimes" node="element" /></field>
		
       </class>
  </mapping>

