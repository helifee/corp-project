<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<class name="com.cnaps.hvps.form.transfer.RegularDebitForm">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />

		<!-- 原发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="RequestHead" />
		</field>
		<!--原接收服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="RequestHead" />
		</field>
		<!--功能代码-->
		<field name="transactionId">
			<bind-xml name="TransactionId" node="element"
				location="RequestHead" />
		</field>

		<!--原报文发送时间-->
		<field name="sendDateTime">
			<bind-xml name="SendDateTime" node="element"
				location="RequestHead" />
		</field>
		<!--网点号-->
		<field name="brnchCd">
			<bind-xml name="BrnchCd" node="element"
				location="RequestHead" />
		</field>
		<!--终端号-->
		<field name="termId">
			<bind-xml name="TermId" node="element"
				location="RequestHead" />
		</field>
		<!--操作柜员-->
		<field name="operTeller">
			<bind-xml name="OperTeller" node="element"
				location="RequestHead" />
		</field>
<!-- 机构号 --><field name="orgCode"><bind-xml name="OrgCode" node="element" location="RequestHead"/></field>
		<!--发起行行号-->
		
			<!-- 发起参与机构 --><field name="operBankCode"><bind-xml name="OperBankCode" node="element"
				location="RequestHead" />
		</field>
		<!--支付交易组号-->
		<field name="pmtGrpId" type="string">
			<bind-xml name="PmtGrpId" node="element"
				location="RequestHead" />
		</field>

		<!-- 提交代码-->
		<field name="poDetails"
			type="com.cnaps.hvps.persistence.transfer.RegularDebitdetails">
			<bind-xml name="RequestFields" node="element" />
		</field>

	</class>

	<class
		name="com.cnaps.hvps.persistence.transfer.RegularDebitdetails">
		<field name="id" type="string">
			<bind-xml name="Id" node="element" />
		</field>
		<field name="prntid" type="string">
			<bind-xml name="PrntId" node="element" />
		</field>
		<field name="msgid" type="string">
			<bind-xml name="MsgId" node="element" />
		</field>
		<field name="recptmsgid" type="string">
			<bind-xml name="RecptMsgId" node="element" />
		</field>
		<field name="txid" type="string">
			<bind-xml name="TxId" node="element" />
		</field>
		<field name="pmtkd" type="string">
			<bind-xml name="PmtKd" node="element" />
		</field>
		<field name="endtoendid" type="string">
			<bind-xml name="EndToEndId" node="element" />
		</field>
		<field name="reftxid" type="string">
			<bind-xml name="RefTxId" node="element" />
		</field>
		<field name="snglamt" type="string">
			<bind-xml name="SnglAmt" node="element" />
		</field>
		<field name="status" type="string">
			<bind-xml name="Status" node="element" />
		</field>
		<field name="netgdt" type="string">
			<bind-xml name="NetgDt" node="element" />
		</field>
		<field name="netgrnd" type="string">
			<bind-xml name="NetgRnd" node="element" />
		</field>
		<field name="sttlmdt" type="string">
			<bind-xml name="SttlmDt" node="element" />
		</field>
		<field name="addtlinf" type="string">
			<bind-xml name="AddtlInf" node="element" />
		</field>
		<field name="ustrd" type="string">
			<bind-xml name="Ustrd" node="element" />
		</field>
		<field name="pmtagrmtnb" type="string">
			<bind-xml name="PmtAgrmtNb" node="element" />
		</field>
		<field name="rcptdt" type="string">
			<bind-xml name="RcptDt" node="element" />
		</field>
		<field name="txrjctcd" type="string">
			<bind-xml name="TxRjctCd" node="element" />
		</field>
		<field name="txrjctinf" type="string">
			<bind-xml name="TxRjctInf" node="element" />
		</field>
		<field name="rcptsts" type="string">
			<bind-xml name="RcptSts" node="element" />
		</field>
		<field name="rcptaddtlinf" type="string">
			<bind-xml name="RcptAddtlInf" node="element" />
		</field>
		<field name="dbtracct" type="string">
			<bind-xml name="DbtrAcct" node="element" />
		</field>
		<field name="dbtrnm" type="string">
			<bind-xml name="DbtrNm" node="element" />
		</field>
		<field name="dbtraddr" type="string">
			<bind-xml name="DbtrAddr" node="element" />
		</field>
		<field name="dbtrissuer" type="string">
			<bind-xml name="DbtrIssuer" node="element" />
		</field>
		<field name="dbtrissuernm" type="string">
			<bind-xml name="DbtrIssuerNm" node="element" />
		</field>
		<field name="dbtrmmbid" type="string">
			<bind-xml name="DbtrMmbId" node="element" />
		</field>
		<field name="dbtrbrnchid" type="string">
			<bind-xml name="DbtrBrnchId" node="element" />
		</field>
		<field name="dbtrbrnchnm" type="string">
			<bind-xml name="DbtrBrnchNm" node="element" />
		</field>

	</class>

</mapping>
