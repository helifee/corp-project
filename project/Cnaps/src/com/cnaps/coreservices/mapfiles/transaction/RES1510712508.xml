<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>定期贷记业务明细建立 回执</description> 
      <class name="com.cnaps.coreservices.CommonXMLEntity" >
      <!-- the name of the xml file's root element  -->
       <map-to xml="Document"/>
     
         <!-- 原发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="ResponseHead"/>
       </field>
       <!--原接收服务名-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="ResponseHead"/>
       </field>
        <!--原渠道报文标识号
       <field name="messageId">
          <bind-xml name="MessageId" node="element" location="ResponseHead"/>
       </field>
       -->
        <!--原报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="ResponseHead"/>
       </field>
         <!--处理码-->
       <field name="processCode">
          <bind-xml name="ProcessCode" node="element" location="ResponseHead"/>
       </field>
         <!--处理信息-->
       <field name="processDes">
          <bind-xml name="ProcessDes" node="element" location="ResponseHead"/>
       </field>
       
       
         <!--查询代码-->
       <field name="queryCd">
          <bind-xml name="QueryCd" node="element" location="ResponseHead"/>
       </field>
       <field name="recordcount">
          <bind-xml name="RecordCount" node="element" location="ResponseHead"/>
       </field>
       <field name="pagecount">
          <bind-xml name="PageCount" node="element" location="ResponseHead"/>
       </field>
       <field name="results" type="com.cnaps.hvps.persistence.transfer.RegularCreditPersonDetail" collection="arraylist">
          <bind-xml name="QryResults" node="element" location="ResponseFields"/>
       </field>
      </class>
            <class name="com.cnaps.hvps.persistence.transfer.RegularCreditPersonDetail">
		<field name="id" type="java.lang.String">
			<bind-xml name="id" node="element" />
		</field>
		<!-- 收款人账号 -->
		<field name="receAcount" type="java.lang.String">
			<bind-xml name="cdtracct" node="element" />
		</field>
		
		<!-- 收款人名称-->
		<field name="receName" type="java.lang.String">
			<bind-xml name="cdtrnm" node="element" />
		</field>
		<!-- 收款行行号-->	
		<field name="cdtrBrnchId" type="java.lang.String">
			<bind-xml name="cdtrbrnchid" node="element" />
		</field>
		<!-- 收款行名称	-->
		 <field name="receBankName" type="java.lang.String">
			<bind-xml name="cdtrbrnchnm" node="element" />
		</field>
		<!-- 收款人开户行行号-->
		 <field name="receOpenBankNum" type="java.lang.String">
			<bind-xml name="cdtrissuer" node="element" />
		</field>  
		<!--收款人开户行名称-->
		<field name="receOpenBankName" type="java.lang.String">
			<bind-xml name="cdtrissuernm" node="element" />
		</field> 
		<!-- 收款人清算行号-->
		<field name="cdtrMmbId" type="java.lang.String">
			<bind-xml name="cdtrmmbid" node="element" />
		</field>
		<!--  收款人地址-->
		<field name="receAddress" type="java.lang.String">
			<bind-xml name="cdtraddr" node="element" />
		</field> 
		  <!-- 发送金额单笔金额-->   
		         <field name="money">
		          <bind-xml name="snglamt" node="element" />
		       </field>   
		        <!--端到端标识号   -->
		        <field name="endToEndId">
		          <bind-xml name="endtoendid" node="element" />
		       </field>
		        <!--附言  -->
		       <field name="postscript">
		          <bind-xml name="addtlinf " node="element" />
		       </field>
		
  </class>
			

  </mapping>

