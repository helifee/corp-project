<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>定期贷记业务明细建立 回执</description> 
      <class name="com.cnaps.coreservices.CommonXMLEntity" >
      <!-- the name of the xml file's root element  -->
       <map-to xml="Document"/>
     
         <!-- 原发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="ResponseHead"/>
       </field>
       <!--原接收服务名-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="ResponseHead"/>
       </field>
        <!--原渠道报文标识号
       <field name="messageId">
          <bind-xml name="MessageId" node="element" location="ResponseHead"/>
       </field>
       -->
        <!--原报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="ResponseHead"/>
       </field>
         <!--处理码-->
       <field name="processCode">
          <bind-xml name="ProcessCode" node="element" location="ResponseHead"/>
       </field>
         <!--处理信息-->
       <field name="processDes">
          <bind-xml name="ProcessDes" node="element" location="ResponseHead"/>
       </field>
       
       
         <!--查询代码-->
       <field name="queryCd">
          <bind-xml name="QueryCd" node="element" location="ResponseHead"/>
       </field>
       <field name="recordcount">
          <bind-xml name="RecordCount" node="element" location="ResponseHead"/>
       </field>
       <field name="pagecount">
          <bind-xml name="PageCount" node="element" location="ResponseHead"/>
       </field>
       <field name="results" type="com.cnaps.hvps.persistence.transfer.RegularCredit" collection="arraylist">
          <bind-xml name="QryResults" node="element" location="ResponseFields"/>
       </field>
      </class>
            <class name="com.cnaps.hvps.persistence.transfer.RegularCredit">
		<field name="id" type="java.lang.String">
			<bind-xml name="id" node="element" />
		</field>
		
		   <!-- 系统编号 -->
		        <field name="systemcd">
		          <bind-xml name="systemcd" node="element" />
		       </field>
                <!-- 申请人证件号 -->
		        <field name="certnum">
		          <bind-xml name="proposercertid" node="element" />
		       </field>
		        <!-- 证件类型-->
		        <field name="certsize">
		          <bind-xml name="proposercerttp" node="element"/>
		       </field> 
		       <!-- 申请人证件发行国家 -->
		        <field name="issueState">
		          <bind-xml name="proposercertissued" node="element" />
		       </field>
		          <!--申请人账号-->
		        <field name="proposerAcct">
		          <bind-xml name="proposeracct" node="element" />
		         </field>
		         
		       <!-- 申请人姓名-->
		       <field name="name">
		          <bind-xml name="proposernm" node="element" />
		       </field>
		        <!-- 申请人地址-->
		        <field name="address">
		          <bind-xml name="proposeraddr" node="element" />
		       </field>	  
		       <!-- 申请人联系电话-->  
		         <field name="phone">
		          <bind-xml name="proposertel" node="element" />
		       </field>  
		         <!--申请人客户号1-->
		       <field name="proposerCstmrId">
		          <bind-xml name="proposercstmrid" node="element" />
		       </field>
		       
		       
		        <!-- 申请人帐号类型-->
		       <field name="payAccountType">
		          <bind-xml name="proposeraccttp" node="element" />
		       </field>
		          <!--申请人账户币种  -->
		        <field name="proposerAcctCcy">
		          <bind-xml name="proposeracctccy" node="element" />
		        </field>
		       
		        
		       <!-- 业务类型编码--> 
		         <field name="businessSizeCode">
		          <bind-xml name="pmttp" node="element" />
		         </field>  
		         <!--业务种类编码-->
		        <field name="businessClassCode">
		          <bind-xml name="pmtkd" node="element" />
		         </field>
		        
		         
		        
		        
		         
		         <!--付款人账号-->
		        <field name="payerAcount">
		          <bind-xml name="dbtracct" node="element" />
		       </field>
		         <!--付款人名称-->
		        <field name="dbtrNm">
		          <bind-xml name="dbtrnm" node="element" />
		       </field>    
		        <!--付款人地址1-->
		        <field name="dbtrAddr">
		          <bind-xml name="dbtraddr" node="element" />
		       </field>  
		         <!--付款人开户行行号 -->
		        <field name="applyOpenBankNum">
		          <bind-xml name="dbtrissuer" node="element" />
		       </field> 
		       <!--付款人开户行行名 -->
		        <field name="applyOpenBankName">
		          <bind-xml name="dbtrissuernm" node="element" />
		       </field> 
		         <!--付款清算行行号-->
		        <field name="dbtrMmbId">
		          <bind-xml name="dbtrmmbid" node="element" />
		       </field> 
		       
		       
		       
		       <!--付款行行号 -->
		        <field name="dbtrBrnchId">
		          <bind-xml name="dbtrbrnchid" node="element" />
		       </field> 
		      <!--付款行行名-->
		        <field name="dbtrBrnchNm">
		          <bind-xml name="dbtrbrnchnm" node="element" />
		       </field>	 
		       
		       <!--  支付组号-->
		<field name="paymentGroupNum" type="java.lang.String">
			<bind-xml name="pmtgrpid" node="element" />
		</field> 
		
		
			<!--累计录入笔数--> 
		<field name="totalentry" type="java.lang.String">
			<bind-xml name="nboftxs" node="element" />
		</field>
			<!--累计录入金额--> 
		<field name="totalentryMoney" type="java.lang.String">
			<bind-xml name="aclmtamt" node="element" />
		</field>
		<!--  扣款金额-->  
		<field name="chargamount" type="java.lang.String">
			<bind-xml name="amount" node="element" />
		</field>
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	 
  </class>
			

  </mapping>

