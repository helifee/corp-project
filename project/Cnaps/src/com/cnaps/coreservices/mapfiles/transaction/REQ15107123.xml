<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>cnaps 个人现金跨行通存</description>


	<class
		name="com.cnaps.hvps.form.transfer.PersonCrashSpanBankForm">
		<map-to xml="Document" />
		         <!-- 原发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="RequestHead"/>
       </field>
       <!--原接收服务名-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="RequestHead"/>
       </field>
        <!--功能代码-->
       <field name="transactionId">
          <bind-xml name="TransactionId" node="element" location="RequestHead"/>
       </field>
         
        <!--原报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="RequestHead"/>
       </field>
      
       
       
       <!-- 机构号 --><field name="orgCode"><bind-xml name="OrgCode" node="element" location="RequestHead"/></field>
         <!--网点号-->
       <field name="brnchCd">
          <bind-xml name="BrnchCd" node="element" location="RequestHead"/>
       </field>
        <!--终端号-->
       <field name="termId">
          <bind-xml name="TermId" node="element" location="RequestHead"/>
       </field>
       <!--操作柜员-->
       <field name="operTeller">
          <bind-xml name="OperTeller" node="element" location="RequestHead"/>
       </field> 
       
         <!--发起行行号-->
       
          <!-- 发起参与机构 --><field name="operBankCode"><bind-xml name="OperBankCode" node="element" location="RequestHead"/>
       </field> 
       <!--支付交易组号-->
       <field name="pmtGrpId" type="string">
       <bind-xml name="PmtGrpId" node="element" location="RequestHead"/>
       </field>
		
        
		
		
		
		
		
		
		<field name="po"
			type="com.cnaps.hvps.persistence.transfer.PersonCrashSpanBank">
			<bind-xml name="RequestFields" node="element" />
		</field>
	</class>

	<class
		name="com.cnaps.hvps.persistence.transfer.PersonCrashSpanBank">
             
		<!-- 签发模式 1-->
		<field name="signMd">
			<bind-xml name="SignMd" node="element" />
		</field>
       <!--以下是手续费信息      -->
        <!--手续费 1-->
		<field name="handingCharge">
			<bind-xml name="ServiceCharge" node="element" />
		</field>
		<!--邮电费 1-->
		<field name="postCharge">
			<bind-xml name="Postage" node="element" />
		</field>
			<!--异地加收1 -->
		<field name="otherPlaceCharge">
			<bind-xml name="OutstationCharge" node="element" />
		</field>
		<!--减免-->
		<field name="waive">
			<bind-xml name="Waive" node="element" />
		</field>
		<!--以下是申请人信息-->
		<!-- 申请人证件号1 -->
		<field name="certnum">
			<bind-xml name="ProposerCertId" node="element" />
		</field>
		<!-- 证件类型1-->
		<field name="certsize">
			<bind-xml name="ProposerCertTp " node="element" />
		</field>
		<!-- 申请人证件发行国家1 -->
		<field name="issueState">
			<bind-xml name="ProposerCertIssued" node="element" />
		</field>
		<!-- 申请人账号-->
		<field name="proposerAcct">
			<bind-xml name="ProposerAcct" node="element" />
		</field>
		<!-- 申请人姓名1-->
		<field name="name">
			<bind-xml name="ProposerNm" node="element" />
		</field>
		<!-- 申请人地址1-->
		<field name="address">
			<bind-xml name="ProposerAddr" node="element" />
		</field>
		<!-- 申请人联系电话1-->
		<field name="phone">
			<bind-xml name="ProposerTel" node="element" />
		</field>
		<!-- 申请人客户号-->
		<field name="proposerCstmrId">
			<bind-xml name="ProposerCstmrId" node="element" />
		</field>
		<!-- 申请人账户类型-->
		<field name="proposerAcctTp">
			<bind-xml name="ProposerAcctTp" node="element" />
		</field>
		<!-- 申请人账户币种-->
		<field name="proposerAcctCcy">
			<bind-xml name="ProposerAcctCcy" node="element" />
		</field>
		
		
		<!-- //以下是交易信息-->
		<!--端到端标识号 1  -->
		<field name="endToEndId">
			<bind-xml name="EndToEndId" node="element" />
		</field>
		<!--业务种类编码      -->
		<field name="businessClassCode">
			<bind-xml name="PmtKd" node="element" />
		</field>
		<!-- 业务类型编码1-->
		<field name="businesssizenum">
			<bind-xml name="PmtTp" node="element" />
		</field>
		<!--币种代码1 -->
		<field name="moneyClassCode">
			<bind-xml name="CurrencyCd" node="element" />
		</field>
		<!--汇款金额 1-->
		<field name="moneyNum">
			<bind-xml name="Amount" node="element" />
		</field>
		
		<!--附言  1-->
		<field name="postscript">
			<bind-xml name="AddtlInf " node="element" />
		</field>
		<!--备注 1 -->
		<field name="remark">
			<bind-xml name="Ustrd" node="element" />
		</field>
		
		
		<!-- 扣款账号信息 -->
				<!-- 扣帐账号 -->
		<field name="dbtramtacctid">
			<bind-xml name="Dbtramtacctid" node="element"
				 />
		</field>
		
		<!-- 扣帐户名 -->
		<field name="dbamtnm">
			<bind-xml name="DbamtNm" node="element"
				 />
		</field>
		
		
		
		
		<!--以下是付款人信息-->
		
		<!--付款人账号1-->
		<field name="payerNum">
			<bind-xml name="DbtrAcct" node="element" />
		</field>
		<!-- 付款人名称-->
		<field name="dbtrNm">
			<bind-xml name="DbtrNm" node="element" />
		</field>
		<!-- 付款人地址-->
		<field name="dbtrAddr">
			<bind-xml name="DbtrAddr" node="element" />
		</field>
		<!-- 付款人开户行行号 -->
		<field name="dbtrIssuer"> 
			<bind-xml name="DbtrIssuer" node="element" />
		</field>
		<!--付款人开户行名称  -->
		<field name="dbtrIssuerNm">
			<bind-xml name="DbtrIssuerNm" node="element" />
		</field>
		
		
		<!--付款清算行行号1-->
		<field name="dbtrMmbId">
			<bind-xml name="DbtrMmbId" node="element" />
		</field>
		<!--付款行行号1-->
		<field name="dbtrBrnchId">
			<bind-xml name="DbtrBrnchId" node="element" />
		</field>
		<!--付款行行名-->
		<field name="dbtrBrnchN">
			<bind-xml name="DbtrBrnchNm" node="element" />
		</field>
		
		
		
		<!--以下是收款人信息-->
		<!--收款人帐号1-->
		<field name="receNum">
			<bind-xml name="CdtrAcct" node="element" />
		</field>
		<!--收款人名称1-->
		<field name="receName">
			<bind-xml name="CdtrNm" node="element" />
		</field>
		<!--  收款人地址1-->
		<field name="receAddress">
			<bind-xml name="CdtrAddr" node="element" />
		</field>
		<!-- 收款人开户行行号1-->
		<field name="receOpenBankNum">
			<bind-xml name="CdtrIssuer" node="element" />
		</field>
		<!-- 收款人开户行名称1-->
		<field name="receOpenBankName">
			<bind-xml name="CdtrIssuerNm" node="element" />
		</field>
		
		<!-- 收款人清算行行号1-->
		<field name="cdtrMmbId">
			<bind-xml name="CdtrMmbId" node="element" />
		</field>
		<!--收款行行号1-->
		<field name="receBankNum">
			<bind-xml name="CdtrBrnchId" node="element" />
		</field>
		<!--收款行名1称-->
		<field name="receBankName">
			<bind-xml name="CdtrBrnchNm" node="element" />
		</field>
		
		
		<!--业务类型为个人储蓄通存业务  -->
		
		
		<!--付款人账户类型1  -->
		<field name="payAccountType">
			<bind-xml name="DbtrAcctTp" node="element" />
		</field>
	 	<!--收款人账户类型1   -->
		<field name="receAccountType">
			<bind-xml name="CdtrAcctTp" node="element" />
		</field>
		
		
		
		
		
		
		
		
		


		
		
		
		

		<!-- 系统编号 -->
		<field name="systemcd">
			<bind-xml name="systemCd" node="element" />
		</field>
		
		<!-- 支付组号1-->
		<field name="paymentGroupNum">
			<bind-xml name="PmtGrpId" node="element" />
		</field>
		

		
		
		
		
		<!--本行户1-->
		<field name="payBankNum">
			<bind-xml name="BankCustomer" node="element" />
		</field>
	
		<!--异地加收
			<field name="totalMoney">
			<bind-xml name="totalMoney" node="element" />
			</field> -->
		
		

		
		<!--总额 1  -->
		<field name="totalMoney">
			<bind-xml name="TotalAmt" node="element" />
		</field>

		
	</class>



</mapping>

