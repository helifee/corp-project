<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>cnaps  批量代付</description> 
      
      
       <class name="com.cnaps.hvps.form.collectionPayment.BatchPaymentForm">
              <map-to xml="Document"/>
                   <!-- 原发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="RequestHead"/>
       </field>
       <!--原接收服务名-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="RequestHead"/>
       </field>
        <!--功能代码-->
       <field name="transactionId">
          <bind-xml name="TransactionId" node="element" location="RequestHead"/>
       </field>
         <!--发起行行号-->
       
          <!-- 发起参与机构 --><field name="operBankCode"><bind-xml name="OperBankCode" node="element" location="RequestHead"/>
       </field>
      
        <!--原报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="RequestHead"/>
       </field>
           <!--网点号-->
       <field name="brnchCd">
          <bind-xml name="BrnchCd" node="element" location="RequestHead"/>
       </field>
        <!--终端号-->
       <field name="termId">
          <bind-xml name="TermId" node="element" location="RequestHead"/>
       </field>
       <!--操作柜员-->
       <field name="operTeller">
          <bind-xml name="OperTeller" node="element" location="RequestHead"/>
       </field> 
        <!--支付交易组号-->
       <field name="pmtGrpId">
          <bind-xml name="PmtGrpId" node="element" location="RequestHead"/>
       </field> 
       
       <!-- 机构号 --><field name="orgCode"><bind-xml name="OrgCode" node="element" location="RequestHead"/></field>
       
       
       
       
       
       
       
       
       
              <field name="po" type="com.cnaps.hvps.persistence.collectionPayment.BatchPayment" >
              <bind-xml name="RequestFields" node="element" />
             </field>
       </class>
  
       <class name="com.cnaps.hvps.persistence.collectionPayment.BatchPayment">
        	 
		      
                <!-- 发起参与机构行号 -->
		        <field name="instgPty">
		          <bind-xml name="InstgPty" node="element" />
		       </field>
		        <!-- 接收参与机构行号-->
		        <field name="instdPty">
		          <bind-xml name="InstdPty" node="element"/>
		       </field> 
		       <!-- 发起直接参与机构 -->
		        <field name="instgDrctPty">
		          <bind-xml name="InstgDrctPty" node="element" />
		       </field>
		          <!--接收直接参与机构-->
		        <field name="instdDrctPty">
		          <bind-xml name="InstdDrctPty" node="element" />
		         </field>
		         
		       <!-- 系统编号-->
		       <field name="systemCd">
		          <bind-xml name="SystemCd" node="element" />
		       </field>
		       
		       
		       
		       
		       
		        <!--备注-->
		        <field name="ustrd">
		          <bind-xml name="Ustrd" node="element" />
		       </field>	  
		       <!-- 批次序号-->  
		         <field name="btchNb">
		          <bind-xml name="BtchNb" node="element" />
		       </field>  
		         <!--回执期限-->
		       <field name="rcptLtd">
		          <bind-xml name="RcptLtd" node="element" />
		       </field>
		   
		       
		        <!-- 接收标识-->
		       <field name="rcvTp">
		          <bind-xml name="RcvTp" node="element" />
		       </field>
		          <!--收款人名称 -->
		        <field name="cdtrNm">
		          <bind-xml name="CdtrNm" node="element" />
		        </field>
		       
		        
		       <!-- 收款人账号--> 
		         <field name="cdtrAcct">
		          <bind-xml name="CdtrAcct" node="element" />
		         </field>  
		         <!--付款清算行行号-->
		        <field name="dbtrMmbId">
		          <bind-xml name="DbtrMmbId" node="element" />
		         </field>
		         
		         
		         <!--付款行行号-->
		        <field name="dbtrBrnchId">
		          <bind-xml name="DbtrBrnchId" node="element" />
		       </field>
		         <!--收款清算行行号-->
		        <field name="cdtrMmbId">
		          <bind-xml name="CdtrMmbId" node="element" />
		       </field>    
		        <!--总金额-->
		        <field name="ttlAmt">
		          <bind-xml name="TtlAmt" node="element" />
		       </field>  
		        
		       
		         <!--业务类型编码 -->
		        <field name="pmtTp">
		          <bind-xml name="PmtTp" node="element" />
		       </field> 
		        <!--付款人数目-->
		        <field name="cdtrNb">
		          <bind-xml name="CdtrNb" node="element" />
		       </field> 
		       
		       
		       
		       
		       
		      
		        
		     
		  
		     
		      <!--     <field name="zcx"
			type="com.cnaps.hvps.persistence.cis.EndrsrNmN"
			collection="arraylist">-->
		       <field name="poo"
			type="com.cnaps.hvps.persistence.collectionPayment.BatchPaymentDetails"
			 >
			<bind-xml name="TxsDtls" node="element" />
		</field>
       </class>
 <class
		name="com.cnaps.hvps.persistence.collectionPayment.BatchPaymentDetails">
		 <!--业务类型编码 --> 
		 <field name="pmtkd" type="string">
			<bind-xml name="Pmtkd" node="element" />
		</field>
		  <!--付收款行行号 --> 
		 <field name="cdtrBrnchId" type="string">
			<bind-xml name="CdtrBrnchId" node="element" />
		</field>
		  <!--收款人名称 --> 
		 <field name="cdtrNm" type="string">
			<bind-xml name="CdtrNm" node="element" />
		</field>
		  <!--收款人账号 --> 
		 <field name="cdtrAcct" type="string">
			<bind-xml name="CdtrAcct" node="element" />
		</field>
		 <!--货币符号 --> 
		 <field name="currencyCd" type="string">
			<bind-xml name="CurrencyCd" node="element" />
		</field>
		<!--金额 --> 
		 <field name="amount" type="string">
			<bind-xml name="Amount" node="element" />
		</field>
		<!--合同（协议）号 --> 
		 <field name="agrmtNb" type="string">
			<bind-xml name="AgrmtNb" node="element" />
		</field>
		<!--核验标识 --> 
		 <field name="chckFlg" type="string">
			<bind-xml name="ChckFlg" node="element" />
		</field>
		<!--附言 --> 
		 <field name="addtlInf" type="string">
			<bind-xml name="AddtlInf" node="element" />
		</field>
	</class>
    

  </mapping>

