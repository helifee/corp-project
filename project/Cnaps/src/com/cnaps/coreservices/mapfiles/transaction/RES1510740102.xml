<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>Basic mapping example</description>
	<class name="com.cnaps.coreservices.CommonXMLEntity">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />

		<!-- 原发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="ResponseHead" />
		</field>
		<!--原接收服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="ResponseHead" />
		</field>
		<!--原渠道报文标识号-->
		<field name="messageId">
			<bind-xml name="MessageId" node="element"
				location="ResponseHead" />
		</field>
		<!--原报文发送时间-->
		<field name="sendDateTime">
			<bind-xml name="SendDateTime" node="element"
				location="ResponseHead" />
		</field>
		<!--处理码-->
		<field name="processCode">
			<bind-xml name="ProcessCode" node="element"
				location="ResponseHead" />
		</field>
		<!--处理信息-->
		<field name="processDes">
			<bind-xml name="ProcessDes" node="element"
				location="ResponseHead" />
		</field>
		
		<!--查询代码-->
		<field name="queryCd">
			<bind-xml name="QueryCd" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageBeginPos">
			<bind-xml name="TurnPageBeginPos" node="element"
				location="ResponseHead" />
		</field>
		<field name="turnPageShowNum">
			<bind-xml name="TurnPageShowNum" node="element"
				location="ResponseHead" />
		</field>
		<field name="recordcount">
			<bind-xml name="RecordCount" node="element"
				location="ResponseHead" />
		</field>
		<field name="results"
			type="com.cnaps.hvps.persistence.transfer.RealAccount"
			collection="arraylist">
			<bind-xml name="QryResults" node="element"
				location="ResponseFields" />
		</field>
	</class>
	
	<class name="com.cnaps.hvps.persistence.transfer.RealAccount">
	     <!-- 支付交易组号 -->
       	<field name="pmtGrpId" type="string">
       	<bind-xml name="pmtgrpid" node="element"/>
       	</field>
		   <!-- 发起参与机构行号 -->
       	<field name="instgPty" type="string">
       	<bind-xml name="instgpty" node="element"/>
       	</field>
       	  <!-- 接收参与机构行号 -->
       	<field name="instdPty" type="string">
       	<bind-xml name="instdpty" node="element"/>
       	</field>
       	  <!-- 发起直接参与机构 -->
       	<field name="instgDrctPty" type="string">
       	<bind-xml name="instgdrctpty" node="element"/>
       	</field>
       	 <!-- 接收直接参与机构 -->
       	<field name="instdDrctPty" type="string">
       	<bind-xml name="instddrctpty" node="element"/>
       	</field>
       		 <!-- 系统编号 -->
       	<field name="systemCd" type="string">
       	<bind-xml name="systemcd" node="element"/>
       	</field>
       		 <!-- 备注 -->
       	<field name="ustrd" type="string">
       	<bind-xml name="ustrd" node="element"/>
       	</field>
       	 <!-- 账户支付类型 -->
       	<field name="acctPmtTp" type="string">
       	<bind-xml name="acctpmttp" node="element"/>
       	</field>
       	<!-- 客户账户账号-->
       	<field name="cstmrAcctId" type="string">
       	<bind-xml name="cstmracctid" node="element"/>
       	</field>
       	<!-- 客户账户名称-->
       	<field name="cstmrAcctNm" type="string">
       	<bind-xml name="cstmracctnm" node="element"/>
       	</field>
       	<!--校验模式（密码验证码算法）-->
       	<field name="chckMd" type="string">
       	<bind-xml name="chckmd" node="element"/>
       	</field>
       	<!--/密码验证码值-->
       	<field name="chckCdVal" type="string">
       	<bind-xml name="chckcdval" node="element"/>
       	</field>
       	<!--/密码验证码长度-->
       	<field name="ChckCdLen" type="string">
       	<bind-xml name="chckcdLen" node="element"/>
       	</field>
       	 	<!--查询余额或状态-->
       	<field name="qryBalOrStsTp" type="string">
       	<bind-xml name="qrybalorststp" node="element"/>
       	</field>
       	
       	 	 
       	<field name="id" type="string">
       	<bind-xml name="id" node="element"/>
       	</field>
       		<!--工作日期-->
       	<field name="workDt" type="string">
       	<bind-xml name="workdt" node="element"/>
       	</field>
       		<!--报文标识号-->
       	<field name="msgId" type="string">
       	<bind-xml name="msgid" node="element"/>
       	</field>
       		<!--报文发送时间-->
       	<field name="creDtTm" type="string">
       	<bind-xml name="credttm" node="element"/>
       	</field>
       		<!--回执报文标识号-->
       	<field name="recptMsgId" type="string">
       	<bind-xml name="recptmsgid" node="element"/>
       	</field>
       		<!--回执报文时间-->
       	<field name="recptDtTm" type="string">
       	<bind-xml name="recptdttm" node="element"/>
       	</field>
       		<!--当前余额-->
       	<field name="crrntBal" type="string">
       	<bind-xml name="crrntbal" node="element"/>
       	</field>
       		<!--当前账户状态-->
       	<field name="crrntAcctSts" type="string">
       	<bind-xml name="crrntacctsts" node="element"/>
       	</field>
       	<!-- 业务状态-->
       	<field name="status" type="string">
       	<bind-xml name="status" node="element"/>
       	</field>
       	<!-- 处理状态-->
       	<field name="prcSts" type="string">
       	<bind-xml name="prcsts" node="element"/>
       	</field>
       	  	<!-- 处理码-->
       	<field name="prcCd" type="string">
       	<bind-xml name="prccd" node="element"/>
       	</field>
       	 	<!-- 拒绝信息-->
       	<field name="rjctCd" type="string">
       	<bind-xml name="rjctcd" node="element"/>
       	</field>
       		<!-- 拒绝业务的参与机构行号-->
       	<field name="rjctedPtyId" type="string">
       	<bind-xml name="rjctedptyid" node="element"/>
       	</field>
       		<!-- 处理日期-->
       	<field name="prcDt" type="string">
       	<bind-xml name="prcdt" node="element"/>
       	</field>
	</class>
	
</mapping>

