<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>定期贷记收款人信息修改</description> 
       <class name="com.cnaps.hvps.form.transfer.RegularCreditForm">
      <!-- the name of the xml file's root element  -->
       <map-to xml="Document"/>
     
           <!-- 原发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="RequestHead"/>
       </field>
       <!--原接收服务名-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="RequestHead"/>
       </field>
        <!--功能代码
       <field name="transactionId">
          <bind-xml name="TransactionId" node="element" location="RequestHead"/>
       </field>-->
        <!--原渠道报文标识号-->
       <field name="msgId">
          <bind-xml name="MessageId" node="element" location="RequestHead"/>
       </field>
        <!--原报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="RequestHead"/>
       </field>
        <!--交易码-->
       <field name="businesscode">
          <bind-xml name="BusinessCode" node="element" location="RequestHead"/>
       </field>
       
       <!-- 机构号 --><field name="orgCode"><bind-xml name="OrgCode" node="element" location="RequestHead"/></field>
         <!--网点号-->
       <field name="brnchCd">
          <bind-xml name="BrnchCd" node="element" location="RequestHead"/>
       </field>
        <!--终端号-->
       <field name="termId">
          <bind-xml name="TermId" node="element" location="RequestHead"/>
       </field>
       <!--操作柜员-->
       <field name="operTeller">
          <bind-xml name="OperTeller" node="element" location="RequestHead"/>
       </field> 
       
        <!--报文标识号-->
       <field name="msgcode">
          <bind-xml name="MsgCode" node="element" location="RequestHead"/>
       </field>
             <!-- 提交代码-->
       <field name="regularCreditPersonDetail" type="com.cnaps.hvps.persistence.transfer.RegularCreditPersonDetail">
          <bind-xml name="RequestFields" node="element"/>
       </field>
       
      
      </class>
  <class name="com.cnaps.hvps.persistence.transfer.RegularCreditPersonDetail">
       <!-- 父表id更新操作需要同时更新父表信息-->
     <field name="parentid" type="java.lang.String">
			<bind-xml name="PrntId" node="element" />
		</field>
		  <!-- 本表id-->
     <field name="id" type="java.lang.String">
			<bind-xml name="Id" node="element" />
		</field>
		 <!--收款人账号-->
		<field name="receAcount" type="java.lang.String">
			<bind-xml name="CdtrAcct" node="element" />
		</field>
		 <!--收款人名称-->
		<field name="receName" type="java.lang.String">
			<bind-xml name="CdtrNm" node="element" />
		</field>	
		<!--收款行行号-->
		<field name="cdtrBrnchId" type="java.lang.String">
			<bind-xml name="CdtrBrnchId" node="element" />
		</field>
		<!--收款行名称-->
		 <field name="receBankName" type="java.lang.String">
			<bind-xml name="CdtrBrnchNm" node="element" />
		</field>
		<!--收款人开户行行号-->
		 <field name="receOpenBankNum" type="java.lang.String">
			<bind-xml name="CdtrIssuer" node="element" />
		</field>  
		<!--收款人开户行名称-->
		<field name="receOpenBankName" type="java.lang.String">
			<bind-xml name="CdtrIssuerNm" node="element" />
		</field> 
		 <!--业务种类编码-->
		        <field name="pmtKd">
		          <bind-xml name="PmtKd" node="element" />
		         </field>
		       <!-- 发送金额单笔金额-->   
		         <field name="money">
		          <bind-xml name="SnglAmt" node="element" />
		       </field>    
		   <!-- 支付组号-->   
		         <field name="paymentGroupNum">
		          <bind-xml name="PmtGrpId" node="element" />
		       </field>        
		<!--收款清算行行号-->
		 <field name="cdtrMmbId" type="java.lang.String">
			<bind-xml name="CdtrMmbId" node="element" />
		</field>  
		<!--收款人地址-->
		<field name="receAddress" type="java.lang.String">
			<bind-xml name="CdtrAddr" node="element" />
		</field> <!--
		<field name="payAccountType" type="java.lang.String">
			<bind-xml name="contractoraccttp" node="element" />
		</field>
		<field name="moneyClassCode" type="java.lang.String">
			<bind-xml name="contractoracctccy" node="element" />
		</field> 
		-->
		 <!--端到端标识号   -->
		        <field name="endToEndId">
		          <bind-xml name="EndToEndId" node="element" />
		       </field>
		       <!--附言   -->
		        <field name="postscript">
		          <bind-xml name="AddtlInf" node="element" />
		       </field>
	 </class>   
  </mapping>