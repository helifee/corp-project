<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>账户查询</description> 
      <class name="com.cnaps.coreservices.CommonXMLEntity" >
      <!-- the name of the xml file's root element  -->
       <map-to xml="Document"/>
     
         <!-- 原发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="ResponseHead"/>
       </field>
       <!--原接收服务名-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="ResponseHead"/>
       </field>
        <!--原渠道报文标识号
       <field name="messageId">
          <bind-xml name="MessageId" node="element" location="ResponseHead"/>
       </field>
       -->
        <!--原报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="ResponseHead"/>
       </field>
         <!--处理码-->
       <field name="processCode">
          <bind-xml name="ProcessCode" node="element" location="ResponseHead"/>
       </field>
         <!--处理信息-->
       <field name="processDes">
          <bind-xml name="ProcessDes" node="element" location="ResponseHead"/>
       </field>
       
       
         <!--查询代码-->
       <field name="queryCd">
          <bind-xml name="QueryCd" node="element" location="ResponseHead"/>
       </field>
       <field name="recordcount">
          <bind-xml name="RecordCount" node="element" location="ResponseHead"/>
       </field>
       <field name="pagecount">
          <bind-xml name="PageCount" node="element" location="ResponseHead"/>
       </field>
       <field name="results" type="com.cnaps.hvps.persistence.pub.PubQueryAccount" collection="arraylist">
          <bind-xml name="QryResults" node="element" location="ResponseFields"/>
       </field>
      </class>
	
	   <class name="com.cnaps.hvps.persistence.pub.PubQueryAccount">		
       <field name="acctid" type="java.lang.String">
			<bind-xml name="acctid" node="element" />
		</field>
       <field name="mm" type="java.lang.String">
			<bind-xml name="nm" node="element" />
		</field>
       <field name="acctissr" type="java.lang.String">
			<bind-xml name="acctissr" node="element" />
		</field>
       <field name="bankcode" type="java.lang.String">
			<bind-xml name="bankcode" node="element" />
		</field>
       <field name="citysubdvsn" type="java.lang.String">
			<bind-xml name="citysubdvsn" node="element" />
		</field>
       <field name="accttp" type="java.lang.String">
			<bind-xml name="accttp" node="element" />
		</field>
       <field name="accountstatus" type="java.lang.String">
			<bind-xml name="accountstatus" node="element" />
		</field>
		<field name="certip" type="java.lang.String">
			<bind-xml name="certtp" node="element" />
		</field>
		<field name="certid" type="java.lang.String">
			<bind-xml name="certid" node="element" />
		</field>
		<field name="certissued" type="java.lang.String">
			<bind-xml name="certissued" node="element" />
		</field>
		<field name="tel" type="java.lang.String">
			<bind-xml name="tel" node="element" />
		</field>
		<field name="cstmrid" type="java.lang.String">
			<bind-xml name="cstmrid" node="element" />
		</field>
		<field name="addr" type="java.lang.String">
			<bind-xml name="addr" node="element" />
		</field>
       </class>
  </mapping>

