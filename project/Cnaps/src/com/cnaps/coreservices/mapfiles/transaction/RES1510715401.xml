<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>取现回执查询回执</description> 
      <class name="com.cnaps.coreservices.CommonXMLEntity" >
      <!-- the name of the xml file's root element  -->
       <map-to xml="Document"/>
     
         <!-- 原发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="ResponseHead"/>
       </field>
       <!--原接收服务名-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="ResponseHead"/>
       </field>
        <!--原渠道报文标识号
       <field name="messageId">
          <bind-xml name="MessageId" node="element" location="ResponseHead"/>
       </field>
       -->
        <!--原报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="ResponseHead"/>
       </field>
         <!--处理码-->
       <field name="processCode">
          <bind-xml name="ProcessCode" node="element" location="ResponseHead"/>
       </field>
         <!--处理信息-->
       <field name="processDes">
          <bind-xml name="ProcessDes" node="element" location="ResponseHead"/>
       </field>
       
       
         <!--查询代码-->
       <field name="queryCd">
          <bind-xml name="QueryCd" node="element" location="ResponseHead"/>
       </field>
       <field name="recordcount">
          <bind-xml name="RecordCount" node="element" location="ResponseHead"/>
       </field>
       <field name="pagecount">
          <bind-xml name="PageCount" node="element" location="ResponseHead"/>
       </field>
       <field name="results" type="com.cnaps.hvps.persistence.transfer.CashReceipt" 
       	collection="arraylist">
          <bind-xml name="QryResults" node="element" location="ResponseFields"/>
       </field>
      </class>

	
	
	
	   <class name="com.cnaps.hvps.persistence.transfer.CashReceipt">		
       		<!--序号-->
       		<field name="id" type="java.lang.String">
       			<bind-xml name="id" node="element" />
       		</field>
       		<!--工作日期-->
       		<field name="workDt" type="java.lang.String">
       			<bind-xml name="workdt" node="element" />
       		</field>
       		<!--报文标识号-->
       		<field name="msgId" type="java.lang.String">
       			<bind-xml name="msgid" node="element" />
       		</field>
       		<!--报文发送时间-->
       		<field name="creDtTm" type="java.lang.String">
       			<bind-xml name="credttm" node="element" />
       		</field>
       		<!--发起参与机构行号-->
       		<field name="instgPty" type="java.lang.String">
       			<bind-xml name="instgpty" node="element" />
       		</field>
       		<!--接收参与机构行号-->
       		<field name="instdPty" type="java.lang.String">
       			<bind-xml name="instdpty" node="element" />
       		</field>
       		<!--发起直接参与机构-->
       		<field name="instgDrctPty" type="java.lang.String">
       			<bind-xml name="instgdrctpty" node="element" />
       		</field>
       		<!--接收直接参与机构-->
       		<field name="instdDrctPty" type="java.lang.String">
       			<bind-xml name="instddrctpty" node="element" />
       		</field>
       		<!--回执报文标识号-->
       		<field name="recptMsgId" type="java.lang.String">
       			<bind-xml name="recptmsgid" node="element" />
       		</field>
       		<!--回复报文时间-->
       		<field name="recptDtTm" type="java.lang.String">
       			<bind-xml name="recptdttm" node="element" />
       		</field>
       		<!--系统编号-->
       		<field name="systemCd" type="java.lang.String">
       			<bind-xml name="systemcd" node="element" />
       		</field>
       		<!--备注-->
       		<field name="ustrd" type="java.lang.String">
       			<bind-xml name="ustrd" node="element" />
       		</field>
       		<!--ACS核算主体代码-->
       		<field name="acctgSbjtCd" type="java.lang.String">
       			<bind-xml name="acctgsbjtcd" node="element" />
       		</field>
       		<!--ACS核算主体名称-->
       		<field name="acctgSbjtNm" type="java.lang.String">
       			<bind-xml name="acctgsbjtnm" node="element" />
       		</field>
       		<!--ACS网点代码-->
       		<field name="brnchCd" type="java.lang.String">
       			<bind-xml name="brnchcd" node="element" />
       		</field>
       		<!--ACS网点名称-->
       		<field name="brnchNm" type="java.lang.String">
       			<bind-xml name="brnchnm" node="element" />
       		</field>
       		<!--交易标识号-->
       		<field name="txId" type="java.lang.String">
       			<bind-xml name="txid" node="element" />
       		</field>
       		<!--原交易标识号-->
       		<field name="orgnlTxId" type="java.lang.String">
       			<bind-xml name="orgnltxid" node="element" />
       		</field>
       		<!--取现清算行行号-->
       		<field name="mmbId" type="java.lang.String">
       			<bind-xml name="mmbid" node="element" />
       		</field>
       		<!--取现行行号（本行、他行）-->
       		<field name="brnchId" type="java.lang.String">
       			<bind-xml name="brnchid" node="element" />
       		</field>
       		<!--ACS记账账号-->
       		<field name="pstngAcct" type="java.lang.String">
       			<bind-xml name="pstngacct" node="element" />
       		</field>
       		<!--ACS记账账户名称-->
       		<field name="pstngNm" type="java.lang.String">
       			<bind-xml name="pstngnm" node="element" />
       		</field>
       		<!--货金机构代码-->
       		<field name="instnId" type="java.lang.String">
       			<bind-xml name="instnid" node="element" />
       		</field>
       		<!--货金机构名称-->
       		<field name="instnNm" type="java.lang.String">
       			<bind-xml name="instnnm" node="element" />
       		</field>
       		<!--业务类型编码-->
       		<field name="pmtTp" type="java.lang.String">
       			<bind-xml name="pmttp" node="element" />
       		</field>
       		<!--业务种类编码-->
       		<field name="pmtKd" type="java.lang.String">
       			<bind-xml name="pmtkd" node="element" />
       		</field>
       		<!--业务优先级-->
       		<field name="sttlmPrty" type="java.lang.String">
       			<bind-xml name="sttlmprty" node="element" />
       		</field>
       		<!--货币符号-->
       		<field name="currencyCd" type="java.lang.String">
       			<bind-xml name="currencycd" node="element" />
       		</field>
       		<!--金额-->
       		<field name="amount" type="java.lang.String">
       			<bind-xml name="amount" node="element" />
       		</field>
       		<!--支取方式-->
       		<field name="drawTp" type="java.lang.String">
       			<bind-xml name="drawtp" node="element" />
       		</field>
       		<!--库房类别                -->
       		<field name="strHsTp" type="java.lang.String">
       			<bind-xml name="strhstp" node="element" />
       		</field>
	   </class>
  </mapping>

