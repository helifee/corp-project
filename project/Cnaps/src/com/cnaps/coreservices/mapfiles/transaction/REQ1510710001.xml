<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">
<mapping>
	<description>小额往账复核 审核 审批</description>
	<class name="com.cnaps.hvps.form.transfer.TransProcessForm">
		<!-- the name of the xml file's root element  -->
		<map-to xml="Document" />

		<!-- 发起服务名 -->
		<field name="source">
			<bind-xml name="Source" node="element"
				location="RequestHead" />
		</field>
		<!--接收服务名-->
		<field name="destination">
			<bind-xml name="Destination" node="element"
				location="RequestHead" />
		</field>
		<!-- 交易码 -->
		<field name="businesscode">
			<bind-xml name="BusinessCode" node="element"
				location="RequestHead" />
		</field>
		<!-- MsgCode -->
		<field name="msgcode">
			<bind-xml name="MsgCode" node="element"
				location="RequestHead" />
		</field>
		<!-- 网点号 -->
		<field name="brnchCd">
			<bind-xml name="BrnchCd" node="element"
				location="RequestHead" />
		</field>
		<!-- 终端号 -->
		<field name="termId">
			<bind-xml name="TermId" node="element"
				location="RequestHead" />
		</field>
		<!-- 操作柜员 -->
		<field name="operTeller">
			<bind-xml name="OperTeller" node="element"
				location="RequestHead" />
		</field>

<!-- 机构号 --><field name="orgCode"><bind-xml name="OrgCode" node="element" location="RequestHead"/></field>
		
		<field name="operway">
			<bind-xml name="OperWay" node="element"
				location="RequestFields" />
		</field>
		<field name="systemcd">
			<bind-xml name="Systemcd" node="element"
				location="RequestFields" />
		</field>
		<field name="pmtgrpid">
			<bind-xml name="PmtgrpId" node="element"
				location="RequestFields" />
		</field>
	</class>

</mapping>

