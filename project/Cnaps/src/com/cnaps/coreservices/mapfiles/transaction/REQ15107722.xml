<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd">

<mapping>
	 <!-- 小额业务对账申请  -->
      <class name="com.cnaps.hvps.form.duizhang.BepsBusinessDetailDuizhangForm" >
      <!-- the name of the xml file's root element  -->
       <map-to xml="Document"/>
     
         <!-- 发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="RequestHead"/>
       </field>
       <!--接收服务名-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="RequestHead"/>
       </field>
        <!--功能代码-->
       <field name="transactionId">
          <bind-xml name="TransactionId" node="element" location="RequestHead"/>
       </field>
        <!--报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="RequestHead"/>
       </field>
        <!--网点号-->
       <field name="brnchCd">
          <bind-xml name="BrnchCd" node="element" location="RequestHead"/>
       </field>
       <!-- 终端号 -->
       <field name="termId">
          <bind-xml name="TermId" node="element" location="RequestHead"/>
       </field>
       <!-- 操作柜员 -->
        <field name="operTeller">
          <bind-xml name="OperTeller" node="element" location="RequestHead"/>
       </field>
       
        <!-- 机构号 --><field name="orgCode"><bind-xml name="OrgCode" node="element" location="RequestHead"/></field>
          <!-- 发起参与机构 --><field name="operBankCode"><bind-xml name="OperBankCode" node="element" location="RequestHead"/>
       </field>
        <!--支付交易组号-->
       <field name="pmtGrpId">
          <bind-xml name="PmtGrpId" node="element" location="RequestHead"/>
       </field>
       
        <!-- 小额业务对账申请-->
       <field name="po" type="com.cnaps.hvps.persistence.duizhang.BepsBusinessDetailDuizhang">
          <bind-xml name="RequestFields" node="element"/>
       </field>
       
	 </class>   
	 
	  <class name="com.cnaps.hvps.persistence.duizhang.BepsBusinessDetailDuizhang">
	 <!--   发起参与机构行号    -->
	 <field name="instgPty" type="java.lang.String">
	 	<bind-xml name="InstgPty" node="element" />
	 </field>
	 <!-- 接收参与机构行号      -->
	 <field name="instdPty" type="java.lang.String">
	 	<bind-xml name="InstdPty" node="element" />
	 </field>
	 <!--     发起直接参与机构  -->
	 <field name="instgDrctPty" type="java.lang.String">
	 	<bind-xml name="InstgDrctPty" node="element" />
	 </field>
	 <!--    接收直接参与机构   -->
	 <field name="instdDrctPty" type="java.lang.String">
	 	<bind-xml name="InstdDrctPty" node="element" />
	 </field>
	 <!-- 系统编号              -->
	 <field name="systemCd" type="java.lang.String">
	 	<bind-xml name="SystemCd" node="element" />
	 </field>
	 <!--备注                   -->
	 <field name="ustrd" type="java.lang.String">
	 	<bind-xml name="Ustrd" node="element" />
	 </field>
	 <!--     对账日期          -->
	 <field name="chckDt" type="java.lang.String">
	 	<bind-xml name="ChckDt" node="element" />
	 </field>
	 <!--     业务包核对申请信息   -->
	 <field name="po1" type="com.cnaps.hvps.persistence.duizhang.BepsBusinessDetailDtlChckPmtInf">
	 	<bind-xml name="DtlChckPmtInf " node="element" />
	 </field>
	  <!--     信息报文核对申请信息  -->
	 <field name="po2" type="com.cnaps.hvps.persistence.duizhang.BepsBusinessDetailDtlChckMsg">
	 	<bind-xml name="DtlChckMsg " node="element" />
	 </field>

	  </class>

	 <!--     业务包核对申请信息   -->
	  <class
	  	name="com.cnaps.hvps.persistence.duizhang.BepsBusinessDetailDtlChckPmtInf">
	  	<!--   申请明细数目小于等于8条    -->
	  	<field name="nbOfTxs" type="java.lang.String">
	  		<bind-xml name="NbOfTxs" node="element" />
	  	</field>
	  	<!--  申请明细清单    -->
	  	<field name="xDetails"
	  		type="com.cnaps.hvps.persistence.duizhang.DtlChckPmtInfDtlChckPmtDtls"
	  		collection="arraylist">
	  		<bind-xml name="DtlChckMsgDtls" node="element" />
	  	</field>
	  </class>
	  
	  <!--     信息报文核对申请信息  -->
	  <class name="com.cnaps.hvps.persistence.duizhang.BepsBusinessDetailDtlChckMsg">
	  	<!--   申请明细数目    -->
	  	<field name="nbOfTxs" type="java.lang.String">
	  		<bind-xml name="NbOfTxs" node="element" />
	  	</field>
	  	<!--  申请明细清单    -->
	  	<field name="xdetails"
	  		type="com.cnaps.hvps.persistence.duizhang.DtlChckMsgDtlChckMsgDtls"
	  		collection="arraylist">
	  		<bind-xml name="DtlChckMsgDtls" node="element" />
	  	</field>
	 </class>
	 

	 
	 	 <!--     业务包核对申请信息的明细   -->
	  <class name="com.cnaps.hvps.persistence.duizhang.DtlChckPmtInfDtlChckPmtDtls">
	  	<!--       -->
	  	<field name="sndRcvTp" type="java.lang.String">
	  		<bind-xml name="SndRcvTp" node="element" />
	  	</field>
	  	<!--       -->
	  	<field name="txNetgDt" type="java.lang.String">
	  		<bind-xml name="TxNetgDt" node="element" />
	  	</field>
	  	<!--       -->
	  	<field name="txNetgRnd" type="java.lang.String">
	  		<bind-xml name="TxNetgRnd" node="element" />
	  	</field>
	  	<!--       -->
	  	<field name="msgTpCd" type="java.lang.String">
	  		<bind-xml name="MsgTpCd" node="element" />
	  	</field>
	  	<!--       -->
	  	<field name="prcSts" type="java.lang.String">
	  		<bind-xml name="PrcSts" node="element" />
	  	</field>
	  </class>

  </mapping>