<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>银行汇票查询回执</description> 
      <class name="com.cnaps.coreservices.CommonXMLEntity" >
      <!-- the name of the xml file's root element  -->
       <map-to xml="Document"/>
     
         <!-- 原发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="ResponseHead"/>
       </field>
       <!--原接收服务名-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="ResponseHead"/>
       </field>
        <!--原渠道报文标识号
       <field name="messageId">
          <bind-xml name="MessageId" node="element" location="ResponseHead"/>
       </field>
       -->
        <!--原报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="ResponseHead"/>
       </field>
         <!--处理码-->
       <field name="processCode">
          <bind-xml name="ProcessCode" node="element" location="ResponseHead"/>
       </field>
         <!--处理信息-->
       <field name="processDes">
          <bind-xml name="ProcessDes" node="element" location="ResponseHead"/>
       </field>
       
       
         <!--查询代码-->
       <field name="queryCd">
          <bind-xml name="QueryCd" node="element" location="ResponseHead"/>
       </field>
       <field name="recordcount">
          <bind-xml name="RecordCount" node="element" location="ResponseHead"/>
       </field>
       <field name="pagecount">
          <bind-xml name="PageCount" node="element" location="ResponseHead"/>
       </field>
       <field name="results" type="com.cnaps.hvps.persistence.draft.ApplyReturned" collection="arraylist">
          <bind-xml name="QryResults" node="element" location="ResponseFields"/>
       </field>
        <field name="addInfo"
			type="com.cnaps.hvps.persistence.transfer.AddtnlData"
			collection="arraylist">
			<bind-xml name="AddtnlData" node="element"
				location="ResponseFields" />
		</field>
      </class>

	
	
	
	   <class name="com.cnaps.hvps.persistence.draft.ApplyReturned">

       <!--序号-->
       <field name="id" type="java.lang.String">
       	<bind-xml name="id" node="element" />
       </field>
       <!--工作日期-->
       <field name="workDt" type="java.lang.String">
       	<bind-xml name="workdate" node="element" />
       </field>
       <!--报文标识号-->
       <field name="msgId" type="java.lang.String">
       	<bind-xml name="msgid" node="element" />
       </field>
       <!--报文发送时间-->
       <field name="creDtTm" type="java.lang.String">
       	<bind-xml name="credttm" node="element" />
       </field>
       <!--发起参与机构行号-->
       <field name="instgPty" type="java.lang.String">
       	<bind-xml name="instgpty" node="element" />
       </field>
       <!--接收参与机构行号-->
       <field name="instdPty" type="java.lang.String">
       	<bind-xml name="instdpty" node="element" />
       </field>
       <!--发起直接参与机构-->
       <field name="instgDrctPty" type="java.lang.String">
       	<bind-xml name="instgdrctpty" node="element" />
       </field>
       <!--接收直接参与机构-->
       <field name="instdDrctPty" type="java.lang.String">
       	<bind-xml name="instddrctpty" node="element" />
       </field>
       <!--系统编号-->
       <field name="systemCd" type="java.lang.String">
       	<bind-xml name="systemcd" node="element" />
       </field>
       <!--备注-->
       <field name="ustrd" type="java.lang.String">
       	<bind-xml name="ustrd" node="element" />
       </field>
       <!--出票日期-->
       <field name="issueDt" type="java.lang.String">
       	<bind-xml name="issuedt" node="element" />
       </field>
       <!--票据号码-->
       <field name="notesNo" type="java.lang.String">
       	<bind-xml name="notesno" node="element" />
       </field>
       <!--汇票密押-->
       <field name="billSeal" type="java.lang.String">
       	<bind-xml name="billseal" node="element" />
       </field>
       <!--汇票种类-->
       <field name="billTp" type="java.lang.String">
       	<bind-xml name="billtp" node="element" />
       </field>
       <!--出票金额-->
       <field name="issueAmt" type="java.lang.String">
       	<bind-xml name="issueamt" node="element" />
       </field>
       <!--汇票签发行行号-->
       <field name="issuerBk" type="java.lang.String">
       	<bind-xml name="issuerbk" node="element" />
       </field>
       <!--汇票申请人账号-->
       <field name="issuerAcct" type="java.lang.String">
       	<bind-xml name="issueracct" node="element" />
       </field>
       <!--汇票申请人名称-->
       <field name="issuerNm" type="java.lang.String">
       	<bind-xml name="issuernm" node="element" />
       </field>
       <!--票面记载的收款人名称-->
       <field name="rcvrNm" type="java.lang.String">
       	<bind-xml name="rcvrnm" node="element" />
       </field>
       <!--业务状态-->
       <field name="status" type="java.lang.String">
       	<bind-xml name="status" node="element" />
       </field>   
       
       
       <!-- asdfasfdasdf -->


	   </class>
	  
