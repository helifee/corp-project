<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>银行本票查询回执</description> 
      <class name="com.cnaps.coreservices.CommonXMLEntity" >
      <!-- the name of the xml file's root element  -->
       <map-to xml="Document"/>
     
         <!-- 原发起服务名 -->
        <field name="source">
          <bind-xml name="Source" node="element" location="ResponseHead"/>
       </field>
       <!--原接收服务名-->
       <field name="destination">
          <bind-xml name="Destination" node="element" location="ResponseHead"/>
       </field>
        <!--原渠道报文标识号
       <field name="messageId">
          <bind-xml name="MessageId" node="element" location="ResponseHead"/>
       </field>
       -->
        <!--原报文发送时间-->
       <field name="sendDateTime">
          <bind-xml name="SendDateTime" node="element" location="ResponseHead"/>
       </field>
         <!--处理码-->
       <field name="processCode">
          <bind-xml name="ProcessCode" node="element" location="ResponseHead"/>
       </field>
         <!--处理信息-->
       <field name="processDes">
          <bind-xml name="ProcessDes" node="element" location="ResponseHead"/>
       </field>
       
       
         <!--查询代码-->
       <field name="queryCd">
          <bind-xml name="QueryCd" node="element" location="ResponseHead"/>
       </field>
       <field name="recordcount">
          <bind-xml name="RecordCount" node="element" location="ResponseHead"/>
       </field>
       <field name="pagecount">
          <bind-xml name="PageCount" node="element" location="ResponseHead"/>
       </field>
       <field name="results" type="com.cnaps.hvps.persistence.transfer.PersonCrashSpanBankExchange" collection="arraylist">
          <bind-xml name="QryResults" node="element" location="ResponseFields"/>
       </field>
      </class>

	
	
	
	   <class name="com.cnaps.hvps.persistence.transfer.PersonCrashSpanBankExchange">		
         <!-- 以下是申请人信息 -->
		<!--1 申请人证件号 -->
		<field name="certnum">
			<bind-xml name="proposercertid" node="element" />
		</field>
		<!-- 1申请人证件类型 -->
		<field name="certsize">
			<bind-xml name="proposercerttp" node="element" />
		</field>
		<!-- 1申请人证件发行国家 -->
		<field name="issueState">
			<bind-xml name="proposercertissued" node="element" />
		</field>
		<!-- 1申请人账号(收款人帐号)-->
		<field name="proposerAcct">
			<bind-xml name="proposeracct" node="element" />
		</field>
		<!--1 申请人名称 -->
		<field name="proposerNm">
			<bind-xml name="proposernm" node="element" />
		</field>
		<!--  1 申请人联系电话 -->
		<field name="phone">
			<bind-xml name="proposertel" node="element" />
		</field>
        <!--  签发模式m -->
		<field name="signMd">
			<bind-xml name="signmd" node="element" />
		</field>
		<!--  手续费 -->
		<field name="handingCharge">
			<bind-xml name="ServiceCharge" node="element" />
		</field>
		<!-- 本票种类-->
		<field name="businessSizeCode">
			<bind-xml name="pyntp" node="element" />
		</field>
		<!-- 1业务种类编码 -->
		<field name="businessClassCode">
			<bind-xml name="PmtKd" node="element" />
		</field>
        <!-- 1货币符号M -->
		<field name="moneyClassCode">
			<bind-xml name="currencycd" node="element" />
		</field>
		<!-- 1交易金额M -->
		<field name="moneyNum">
			<bind-xml name="Amount" node="element" />
		</field>
		
		<!-- 减免 -->
		<field name="waive">
			<bind-xml name="waive" node="element" />
		</field>
		<!-- 附言 -->
		<field name="postscript">
			<bind-xml name="addtlinf" node="element"/>
		</field>

		<!-- 1付款行行号 -->
		<field name="payBankNum">
			<bind-xml name="dbtrbrnchid" node="element" />
		</field>
		<!--  付款行名称 -->
		<field name="payBankName">
			<bind-xml name="dbtrbrnchnm" node="element" />
		</field>
		
		<!-- 1收款人户名M -->
		<field name="name">
			<bind-xml name="cdtrnm" node="element" />
		</field>
			<!--密押-->
		<field name="billseal">
			<bind-xml name="billseal" node="element" />
		</field>

		<!-- 本票冠字码 -->
		<field name="password">
			<bind-xml name="chckcd" node="element" />
		</field>
		
		<!-- 本票号码 -->
		<field name="honourno">
			<bind-xml name="notesno" node="element"/>
		</field>
		
		<!-- 本票流水号 -->
		<field name="paymentGrouppo">
			<bind-xml name="paymentgrouppo" node="element"/>
		</field>
		<!-- 出票日期 -->
		<field name="issueDt">
			<bind-xml name="workdt" node="element"/>
		</field>
		<!--本票状态-->
		<field name="billstatus">
			<bind-xml name="billStatus" node="element" />
		</field>
		<!--本票状态2不知为啥2个状态,太2了-->
		<field name="status">
			<bind-xml name="status" node="element" />
		</field>
		 <!-- 出票金额 -->
		<field name="issueAmt">
			<bind-xml name="amount" node="element" />
		</field>
       </class>
  </mapping>

