<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE databases PUBLIC 
  "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
  "http://castor.exolab.org/mapping.dtd"> 
  <mapping>
      <description>汇兑业务 国内现金汇款  客户</description> 
      
      
       <class name="com.cnaps.hvps.form.transfer.TransferOfLedgerForm">
              <map-to xml="Document"/>
               <!-- 源服务名 -->
		        <field name="source">
		          <bind-xml name="Source" node="element" location="RequestHead" />
		       </field>
		        <!-- 目标服务名 -->
		        <field name="destination">
		          <bind-xml name="Destination" node="element" location="RequestHead" />
		       </field>
		       <!-- 报文标识号 -->
		        <field name="msgId">
		          <bind-xml name="MessageId" node="element" location="RequestHead" />
		       </field>
		       
		       <!-- 发送时间 -->
		        <field name="sendDateTime">
		          <bind-xml name="SendDateTime" node="element" location="RequestHead" />
		       </field>
		        <!-- 交易码 -->
		        <field name="businesscode">
		          <bind-xml name="BusinessCode" node="element" location="RequestHead" />
		       </field>
		        <!-- MsgCode -->
		        <field name="msgcode">
		          <bind-xml name="MsgCode" node="element" location="RequestHead" />
		       </field>
		       <!-- 机构号 --><field name="orgCode"><bind-xml name="OrgCode" node="element" location="RequestHead"/></field>
<!-- 网点号 --><field name="brnchCd"><bind-xml name="BrnchCd" node="element"  location="RequestHead" /></field>
		       
		      <!-- 报文基本信息 -->
		       <!-- 申请人证件号 -->
		        <field name="appcertno">
		          <bind-xml name="Appcertno" node="element" location="RequestFields" />
		       </field>
		       
		       <!-- 证件发型国家 -->
		        <field name="certcountry">
		          <bind-xml name="Certcountry" node="element"  location="RequestFields" />
		       </field>
		       
		       <!-- 证件类型 -->
		        <field name="certtype">
		          <bind-xml name="Certtype" node="element" location="RequestFields"  />
		       </field>
		       <!-- 申请人名称 -->
		        <field name="appnm">
		          <bind-xml name="Appnm" node="element"  location="RequestFields" />
		       </field>
		       <!-- 申请人联系电话 -->
		        <field name="appphone">
		          <bind-xml name="Appphone" node="element"  location="RequestFields" />
		       </field>
		       
		       
		       
		        <!-- 支付组号 -->
		        <field name="paymentno">
		          <bind-xml name="Paymentno" node="element" location="RequestFields"  />
		       </field>
		       
		        <!-- 业务类型编码 -->
		        <field name="pmttp">
		          <bind-xml name="Pmttp" node="element" location="RequestFields"  />
		       </field>
		       
		        <!-- 业务种类编码 -->
		        <field name="pmtkd">
		          <bind-xml name="Pmtkd" node="element" location="RequestFields"  />
		       </field>
		       
		        <!-- 币种 -->
		        <field name="currency">
		          <bind-xml name="Currency" node="element" location="RequestFields"  />
		       </field>
		       
		        <!-- 汇款金额 -->
		        <field name="ntryamt">
		          <bind-xml name="Ntryamt" node="element" location="RequestFields"  />
		       </field>
		       
		        <!-- 手续费 -->
		        <field name="charge">
		          <bind-xml name="Charge" node="element"  location="RequestFields" />
		       </field>
		       
		        <!-- 邮电费 -->
		        <field name="postage">
		          <bind-xml name="Postage" node="element" location="RequestFields"  />
		       </field>
		       
		        <!-- 本行户 -->
		        <field name="selfaccount">
		          <bind-xml name="Selfaccount" node="element"  location="RequestFields" />
		       </field>
		       
		        <!-- 异地加收 -->
		        <field name="otherchange">
		          <bind-xml name="Otherchange" node="element"  location="RequestFields" />
		       </field>
		       
		        <!-- 总额 -->
		        <field name="allchange">
		          <bind-xml name="Allchange" node="element"  location="RequestFields" />
		       </field>
		       
		        <!-- 系统号 -->
		        <field name="systemno">
		          <bind-xml name="Systemno" node="element" location="RequestFields"  />
		       </field>
		       
		        <!-- 业务优先级 -->
		        <field name="sttlmprty">
		          <bind-xml name="Sttlmprty" node="element" location="RequestFields"  />
		       </field>
		       
		       <!-- 附言 -->
		        <field name="remarkinfo">
		          <bind-xml name="Remarkinfo" node="element"  location="RequestFields" />
		       </field>
		       
		       <!-- 付款人信息 -->
		       
		       <!-- 付款人账号 -->
		        <field name="dbtracctid">
		          <bind-xml name="Dbtracctid" node="element" location="RequestFields" />
		       </field>
		        <!-- 付款人名称 -->
		        <field name="dbtrnm">
		          <bind-xml name="Dbtrnm" node="element" location="RequestFields" />
		       </field>
		        <!-- 付款人地址 -->
		        <field name="dbtraddr">
		          <bind-xml name="Dbtraddr" node="element"  location="RequestFields"/>
		       </field>
		       <!-- 付款人开户行行号 -->
		        <field name="dbtrissr">
		          <bind-xml name="Dbtrissr" node="element" location="RequestFields" />
		       </field>
		       <!-- 付款清算行行号 -->
		        <field name="dbtrmmbid">
		          <bind-xml name="Dbtrmmbid" node="element" location="RequestFields" />
		       </field>
		       <!-- 付款行行号 -->
		        <field name="dbtrbrchid">
		          <bind-xml name="Dbtrbrchid" node="element"  location="RequestFields"/>
		       </field>
		       
		       <!-- 收款人信息 -->
		       
		       <!-- 收款人账号 -->
		        <field name="cdtracctid">
		          <bind-xml name="Cdtracctid" node="element"  location="RequestFields" />
		       </field>
		       
		       <!-- 收款人名称 -->
		        <field name="cdtrnm">
		          <bind-xml name="Cdtrnm" node="element" location="RequestFields" />
		       </field>
		       <!-- 收款行行号 -->
		        <field name="cdtrbrchid">
		          <bind-xml name="Cdtrbrchid" node="element" location="RequestFields" />
		       </field>
		       <!-- 收款行名称 -->
		        <field name="cdtracctnm">
		          <bind-xml name="Cdtracctnm" node="element" location="RequestFields" />
		       </field>
		       <!-- 收款行开户行行号 -->
		        <field name="cdtrissr">
		          <bind-xml name="Cdtrissr" node="element" location="RequestFields" />
		       </field>
		        <!-- 收款行开户行名称 -->
		        <field name="cdtrissrnm">
		          <bind-xml name="Cdtrissrnm" node="element" location="RequestFields" />
		       </field>
		        <!-- 收款人地址 -->
		        <field name="cdtraddr">
		          <bind-xml name="Cdtraddr" node="element" location="RequestFields" />
		       </field>
		       
		        <!-- 收款清算行行号 -->
		        <field name="cdtrmmbid">
		          <bind-xml name="Cdtrmmbid" node="element" location="RequestFields" />
		       </field>
		       
		       <!-- 委托收款（划回 -->
		       <!-- 票据日期 -->
		       <field name="collectiondate">
		          <bind-xml name="Collectiondate" node="element" location="RequestFields" />
		       </field>
		       <!-- 票据类型 -->
		       <field name="collectiontype">
		          <bind-xml name="Collectiontype" node="element"  location="RequestFields"/>
		       </field>
		       <!-- 票据号码 -->
		       <field name="collectionno">
		          <bind-xml name="Collectionno" node="element" location="RequestFields"  />
		       </field>
		       
		       
		       <!-- 托收承付（划回） -->
		        <!-- 票据日期 -->
		       <field name="honourdate">
		          <bind-xml name="Honourdate" node="element" location="RequestFields"/>
		       </field>
		       
		        <!-- 票据号码 -->
		       <field name="honourno">
		          <bind-xml name="Honourno" node="element" location="RequestFields" />
		       </field>
		        <!-- 赔偿金金额 -->
		       <field name="damages">
		          <bind-xml name="Damages" node="element" location="RequestFields" />
		       </field>
		        <!-- 拒付金金额 -->
		       <field name="refusechange">
		          <bind-xml name="Refusechange" node="element" location="RequestFields"/>
		       </field>
		       
		       <!-- 缴费业务 -->
		       <!-- 收费单位流水号 -->
		       <field name="jfmsgid">
		          <bind-xml name="Jfmsgid" node="element" location="RequestFields"  />
		       </field>
		       <!-- 所属期间 -->
		       <field name="jfdate">
		          <bind-xml name="Jfdate" node="element"  location="RequestFields" />
		       </field>
		       <!-- 缴费类型 -->
		       <field name="jftype">
		          <bind-xml name="Jftype" node="element" location="RequestFields"  />
		       </field>
		       <!-- 收费附言 -->
		       <field name="jfremarkinfo">
		          <bind-xml name="Jfremarkinfo" node="element"  location="RequestFields" />
		       </field>
		       
		   <!-- 国库资金贷记划拨 -->   
		    
		       <!-- 明细汇总金额 -->
		       <field name="gkallchange">
		          <bind-xml name="Gkallchange" node="element" location="RequestFields" />
		       </field>
		       <!-- 上报国库代码 -->
		       <field name="gksendcode">
		          <bind-xml name="Gksendcode" node="element" location="RequestFields" />
		       </field>
		       <!-- /接收国库代码 -->
		       <field name="gkreceivecode">
		          <bind-xml name="Gkreceivecode" node="element" location="RequestFields" />
		       </field>
		       <!-- 报表日期 -->
		       <field name="gktabledate">
		          <bind-xml name="Gktabledate" node="element" location="RequestFields" />
		       </field>
		       <!-- 报表序号 -->
		       <field name="gktableid">
		          <bind-xml name="Gktableid" node="element" location="RequestFields" />
		       </field>
		       <!-- 预算级次 -->
		       <field name="gkbudgetlevel">
		          <bind-xml name="Gkbudgetlevel" node="element" location="RequestFields"  />
		       </field>
		       <!-- 调整期标志 -->
		       <field name="gkadjustsign">
		          <bind-xml name="Gkadjustsign" node="element"  location="RequestFields" />
		       </field>
		       <!-- 预算种类 -->
		       <field name="gkbudgettype">
		          <bind-xml name="Gkbudgettype" node="element"  location="RequestFields" />
		       </field>
		        <!-- 明细条数 -->
		       <field name="gkcount">
		          <bind-xml name="Gkcount" node="element"  location="RequestFields" />
		       </field>
		       
		       
 
             
       </class>
 
       
       
     

  </mapping>

