public class Systemroleandgrouprelation implements java.io.Serializable {

	// Fields

	/**
	 * <ul>
	 * <li>1、字段类型：long</li>
	 * <li>2、字段名称：Systemroleandgrouprelation.java</li>
	 * </ul>
	 */
	private static final long serialVersionUID = 1L;
	private Long indentifier;
	private String roleindentifier;
	private String groupindentifier;
	private String commn;
	private String descriptiion;

	// Constructors

	/** default constructor */
	public Systemroleandgrouprelation() {
	}

	/** minimal constructor */
	public Systemroleandgrouprelation(String roleindentifier,
			String groupindentifier) {
		this.roleindentifier = roleindentifier;
		this.groupindentifier = groupindentifier;
	}

	/** full constructor */
	public Systemroleandgrouprelation(String roleindentifier,
			String groupindentifier, String commn, String descriptiion) {
		this.roleindentifier = roleindentifier;
		this.groupindentifier = groupindentifier;
		this.commn = commn;
		this.descriptiion = descriptiion;
	}

	// Property accessors

	public Long getIndentifier() {
		return this.indentifier;
	}

	public void setIndentifier(Long indentifier) {
		this.indentifier = indentifier;
	}

	public String getRoleindentifier() {
		return this.roleindentifier;
	}

	public void setRoleindentifier(String roleindentifier) {
		this.roleindentifier = roleindentifier;
	}

	public String getGroupindentifier() {
		return this.groupindentifier;
	}

	public void setGroupindentifier(String groupindentifier) {
		this.groupindentifier = groupindentifier;
	}

	public String getCommn() {
		return this.commn;
	}

	public void setCommn(String commn) {
		this.commn = commn;
	}

	public String getDescriptiion() {
		return this.descriptiion;
	}

	public void setDescriptiion(String descriptiion) {
		this.descriptiion = descriptiion;
	}

}