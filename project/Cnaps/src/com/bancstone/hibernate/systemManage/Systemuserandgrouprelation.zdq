public class Systemuserandgrouprelation implements java.io.Serializable {

    // Fields    

     private SystemuserandgrouprelationId id;
     private String description;
     private String common;


    // Constructors

    /** default constructor */
    public Systemuserandgrouprelation() {
    }

    
    /** full constructor */
    public Systemuserandgrouprelation(String description, String common) {
        this.description = description;
        this.common = common;
    }

   
    // Property accessors

    public SystemuserandgrouprelationId getId() {
        return this.id;
    }
    
    public void setId(SystemuserandgrouprelationId id) {
        this.id = id;
    }

    public String getDescription() {
        return this.description;
    }
    
    public void setDescription(String description) {
        this.description = description;
    }

    public String getCommon() {
        return this.common;
    }
    
    public void setCommon(String common) {
        this.common = common;
    }
   




}