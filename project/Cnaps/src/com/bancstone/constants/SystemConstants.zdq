		} else {
			if (PICP_CUNDAI_SIGN_PROCESS_STATUS_00.equals(productCode.trim())) {
				productName = "预受理";
			}
			if (PICP_CUNDAI_SIGN_PROCESS_STATUS_01.equals(productCode.trim())) {
				productName = "待审核";
			}
			if (PICP_CUNDAI_SIGN_PROCESS_STATUS_02.equals(productCode.trim())) {
				productName = "待复审";
			}
			if (PICP_CUNDAI_SIGN_PROCESS_STATUS_03.equals(productCode.trim())) {
				productName = "待终审";
			}
			if (PICP_CUNDAI_SIGN_PROCESS_STATUS_04.equals(productCode.trim())) {
				productName = "已生效";
			}
			if (PICP_CUNDAI_SIGN_PROCESS_STATUS_05.equals(productCode.trim())) {
				productName = "账号未找到";
			}
			if (PICP_CUNDAI_SIGN_PROCESS_STATUS_06.equals(productCode.trim())) {
				productName = "已过期";
			}
			if (PICP_CUNDAI_SIGN_PROCESS_STATUS_07.equals(productCode.trim())) {
				productName = "终审已取消";
			}
			if (PICP_CUNDAI_SIGN_PROCESS_STATUS_08.equals(productCode.trim())) {
				productName = "销户成功";
			}
			if (PICP_CUNDAI_SIGN_PROCESS_STATUS_09.equals(productCode.trim())) {
				productName = "已冻结";
			}
		}
		return productName;
	}

