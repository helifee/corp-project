	public static String getProcessresultNamebycode(String code) {
		if ("".equals(code) || code == null) {
			code = "";
		} else {
			if (code.equals(SystemConstants.PROCESSRESULT_CODE2)) {
				code = "失败";
			}
			if (code.equals(SystemConstants.PROCESSRESULT_CODE1)) {
				code = "成功";
			}
			if (code.equals(SystemConstants.PROCESSRESULT_CODE0)) {
				code = "待处理";
			}
		}
		return code;
	}

