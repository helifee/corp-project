public class Material {
	public String name;
	public String type;
	public  int copys;
	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}
	/**
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}
	/**
	 * @return the type
	 */
	public String getType() {
		return type;
	}
	/**
	 * @param type the type to set
	 */
	public void setType(String type) {
		this.type = type;
	}
	/**
	 * @return the copys
	 */
	public int getCopys() {
		return copys;
	}
	/**
	 * @param copys the copys to set
	 */
	public void setCopys(int copys) {
		this.copys = copys;
	}
 
}
