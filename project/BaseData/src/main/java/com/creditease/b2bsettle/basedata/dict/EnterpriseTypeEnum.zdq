package com.creditease.b2bsettle.basedata.dict;

/** 
 * Description: 企业类型 枚举类
 * 			1-有限责任公司   2-国有企业   3-私营企业   4-合伙企业   5-农民专业合作社   6-个体工商户   7-分公司  8-农民专业合作社分支机构
 *
 * @author leileiyuan
 * Create Date: 2015年6月27日 下午1:04:39
 */
public enum EnterpriseTypeEnum {
	limitedLiabilityCompany("1", "有限责任公司"),
	stateOwnedEnterprise("2", "国有企业"), 
	privateEnterprise("3", "私营企业"), 
	jointEnterprise("4", "合伙企业"), 
	ruralCooperative("5", "农民专业合作社"), 
	privatelyBusiness("6", "个体工商户"), 
	branchCompany("7", "分公司"), 
	ruralCooperativeBranch("8", "农民专业合作社分支机构");
	
	private String value;
	private String name;
	
	private EnterpriseTypeEnum(String value, String name) {
		this.value = value;
		this.name = name;
	}
	
	public String getValue(){
		return value;
	}
	
	public String getName(){
		return name;
	}
	
	public static String getName(String value){
		for (EnterpriseTypeEnum biz : EnterpriseTypeEnum.values()) {
			if(biz.getValue().equals(value)){
				return biz.name;
			}
		}
		return null;
	}
}
