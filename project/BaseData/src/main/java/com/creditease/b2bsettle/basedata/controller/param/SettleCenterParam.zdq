package com.creditease.b2bsettle.basedata.controller.param;


/**
 * 
 * @author daoqiangzhang
 *
 */
public class SettleCenterParam extends PageParam {

	private long id;
	private String text;
	private String centerCorpName;
	private int status;
	private String opation;
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public String getText() {
		return text;
	}
	public void setText(String text) {
		this.text = text;
	}
	public String getCenterCorpName() {
		return centerCorpName;
	}
	public void setCenterCorpName(String centerCorpName) {
		this.centerCorpName = centerCorpName;
	}
	public int getStatus() {
		return status;
	}
	public void setStatus(int status) {
		this.status = status;
	}
	public String getOpation() {
		return opation;
	}
	public void setOpation(String opation) {
		this.opation = opation;
	}
}
