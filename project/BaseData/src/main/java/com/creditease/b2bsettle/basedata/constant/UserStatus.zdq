package com.creditease.b2bsettle.basedata.constant;

/**
 * 用户状态
 * @author daoqiangzhang
 *
 */
public enum UserStatus {
	
	Normal("0"), 		//正常状态
	LockManual("1"), 	//手动锁定
	LockAuto("2"),		//自动锁定
	ModifyAudit("3"),	//修改审核中
	DelAudit("4");		//删除审核中
	
	private String value;
	
	UserStatus(String value) {
		this.value = value;
	}
	
	public String toString() {
		return this.value;
	}
	
	public String value() {
		return this.value;
	}
}
