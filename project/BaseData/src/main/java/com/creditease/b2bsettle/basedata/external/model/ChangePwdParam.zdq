package com.creditease.b2bsettle.basedata.external.model;

/**
 * 修改密码请求参数Bean
 * 
 * @author daoqiangzhang
 *
 */
public class ChangePwdParam extends ParamBean {

	//原始密码
	private String orgPwd;
	
	//新密码
	private String newPwd;

	public String getOrgPwd() {
		return orgPwd;
	}
	public void setOrgPwd(String orgPwd) {
		this.orgPwd = orgPwd;
	}
	public String getNewPwd() {
		return newPwd;
	}
	public void setNewPwd(String newPwd) {
		this.newPwd = newPwd;
	}
}
