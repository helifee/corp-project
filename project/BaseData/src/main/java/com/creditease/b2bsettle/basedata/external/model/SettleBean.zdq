package com.creditease.b2bsettle.basedata.external.model;

import org.apache.commons.lang3.builder.HashCodeBuilder;
import org.apache.commons.lang3.builder.ReflectionToStringBuilder;
import org.apache.commons.lang3.builder.ToStringStyle;

public class SettleBean {

	private String settleCenterId;
	private String settleCenterNm;
	private String centerENm;
	private String centerEId;
	public String getSettleCenterId() {
		return settleCenterId;
	}
	public void setSettleCenterId(String settleCenterId) {
		this.settleCenterId = settleCenterId;
	}
	public String getSettleCenterNm() {
		return settleCenterNm;
	}
	public void setSettleCenterNm(String settleCenterNm) {
		this.settleCenterNm = settleCenterNm;
	}
	public String getCenterENm() {
		return centerENm;
	}
	public void setCenterENm(String centerENm) {
		this.centerENm = centerENm;
	}
	public String getCenterEId() {
		return centerEId;
	}
	public void setCenterEId(String centerEId) {
		this.centerEId = centerEId;
	}
	
	public String toString() {
		return ReflectionToStringBuilder.toString(this, ToStringStyle.MULTI_LINE_STYLE);
	}
	
	@Override
	public int hashCode() {
		return HashCodeBuilder.reflectionHashCode(this); 
	}
}
