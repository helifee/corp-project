package com.creditease.b2bsettle.basedata.dict;

/** 
 * Description: 营业地段 枚举类 
 * 				1-商业区  2-工业区   3-住宅区    4-科技园区   5-城区   6-郊区   7-偏远地区
 *
 * @author leileiyuan
 * Create Date: 2015年6月27日 下午1:33:23
 */
public enum BizCommodity {
	businessZone("1", "商业区"),
	industrialArea("2", "工业区"), 
	uptown("3", "住宅区"), 
	sciencePark("4", "科技园区"), 
	cityProper("5", "城区"), 
	suburb("6", "郊区"), 
	remoteAreas("7", "偏远地区");
	
	private String value;
	private String name;
	
	private BizCommodity(String value, String name) {
		this.value = value;
		this.name = name;
	}
	
	public String getValue(){
		return value;
	}
	
	public String getName(){
		return name;
	}
	
	public static String getName(String value){
		for (BizCommodity biz : BizCommodity.values()) {
			if(biz.getValue().equals(value)){
				return biz.name;
			}
		}
		return null;
	}
}
