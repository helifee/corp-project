package com.creditease.b2bsettle.basedata.dict;

/** 
 * Description: 产品 业务类型 枚举类  包含  1-企业付款    2-代发工资    3-企业报销    4-资金划拨
 *
 * @author leileiyuan
 * Create Date: 2015年6月27日 下午2:03:20
 */
public enum ProductBusinessType {
	businessType1("1","企业付款"),
	businessType2("2","代发工资"),
	businessType3("3","企业报销"),
	businessType4("4","资金划拨");
	
	private String value;
	private String name;
	
	private ProductBusinessType(String value,String name){
		this.value = value;
		this.name = name;
	}
	
	public String getValue() {
		return value;
	}

	public String getName(){
		return name;
	}

	public static String getName(String value) {
		for (ProductBusinessType status : ProductBusinessType.values()) {
			if(status.getValue().equals(value)){
				return status.name;
			}
		}
		return null;
	}
}
