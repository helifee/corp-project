package com.creditease.b2bsettle.basedata.model;

import javax.persistence.Entity;
import javax.persistence.Table;

import com.creditease.b2bsettle.basedata.base.BaseEntity;

/**
 * 平台响应码实体
 * 
 * @author daoqiangzhang
 *
 */
@Entity
@Table(name = "plat_resp_code")
public class ResponseCodePlatform extends BaseEntity {

	private static final long serialVersionUID = 455959577610542180L;

	private String code;
	private String info;

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getInfo() {
		return info;
	}

	public void setInfo(String info) {
		this.info = info;
	}
}
