package com.creditease.b2bsettle.basedata.model;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

import com.creditease.b2bsettle.basedata.base.BaseEntity;

/**
 * 结算中心实体
 * 
 * @author daoqiangzhang
 *
 */
@Entity
@Table(name = "settle_center")
public class SettleCenter extends BaseEntity implements Serializable {

	private static final long serialVersionUID = -3353454013473619119L;

	@Column(name = "settle_center_name")
	private String name;
	
	@Column(name = "settle_center_status")
	private int status;
	
	@Column(name = "corp_id")
	private String centerCorpNo;
	
	@Column(name = "corp_name")
	private String centerCorpName;
	
	@Column(name = "parent_id")
	private long parentId;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getStatus() {
		return status;
	}
	public void setStatus(int status) {
		this.status = status;
	}
	public String getCenterCorpNo() {
		return centerCorpNo;
	}
	public void setCenterCorpNo(String centerCorpNo) {
		this.centerCorpNo = centerCorpNo;
	}
	public String getCenterCorpName() {
		return centerCorpName;
	}
	public void setCenterCorpName(String centerCorpName) {
		this.centerCorpName = centerCorpName;
	}
	public long getParentId() {
		return parentId;
	}
	public void setParentId(long parentId) {
		this.parentId = parentId;
	}
	
}
