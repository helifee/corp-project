package com.creditease.b2bsettle.basedata.model;


import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

import com.creditease.b2bsettle.basedata.base.BaseEntityAudit;

@Entity
@Table(name = "bank_plat_province_map_audit")
public class BankPlatProvinceMapAudit extends BaseEntityAudit {

	private static final long serialVersionUID = -2944393008211871852L;

	//编号
	@Column
	private long id;
	
	//银行编码
	@Column(name = "bank_code")
	private String bankCode;
	
	//银行名称
	@Column(name = "bank_name")
	private String bankName;
 
	//通道省级编码
	@Column(name = "province_code_c")
	private String provinceCodeC;
	
	//通道省级名称
	@Column(name = "province_name_c")
	private String provinceNameC;
	
	//平台省级编码
	@Column(name = "province_code_p")
	private String provinceCodeP;
	
	//平台省级名称
	@Column(name = "province_name_p")
	private String provinceNameP;

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public String getBankCode() {
		return bankCode;
	}

	public void setBankCode(String bankCode) {
		this.bankCode = bankCode;
	}

	public String getBankName() {
		return bankName;
	}

	public void setBankName(String bankName) {
		this.bankName = bankName;
	}

	public String getProvinceCodeC() {
		return provinceCodeC;
	}

	public void setProvinceCodeC(String provinceCodeC) {
		this.provinceCodeC = provinceCodeC;
	}

	public String getProvinceNameC() {
		return provinceNameC;
	}

	public void setProvinceNameC(String provinceNameC) {
		this.provinceNameC = provinceNameC;
	}

	public String getProvinceCodeP() {
		return provinceCodeP;
	}

	public void setProvinceCodeP(String provinceCodeP) {
		this.provinceCodeP = provinceCodeP;
	}

	public String getProvinceNameP() {
		return provinceNameP;
	}

	public void setProvinceNameP(String provinceNameP) {
		this.provinceNameP = provinceNameP;
	}
 
 

}
