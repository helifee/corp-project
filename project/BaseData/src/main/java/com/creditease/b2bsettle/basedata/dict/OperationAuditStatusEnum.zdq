package com.creditease.b2bsettle.basedata.dict;

/** 
 * Description: 企业审核状态(待运营审核，运营审核拒绝，待风控审核，风控审核通过，风控审核拒绝)
 *
 * @author leileiyuan
 * Create Date: 2015年6月25日 下午8:08:34
 */
public enum OperationAuditStatusEnum {
	chkStatus1("1","待运营审核"),
	chkStatus2("2","运营审核拒绝"),
	chkStatus3("3","待风控审核"),
	chkStatus4("4","风控审核通过"),
	chkStatus5("5","风控审核拒绝");
	
	private String value;
	private String name;
	
	private OperationAuditStatusEnum(String value,String name){
		this.value = value;
		this.name = name;
	}
	
	public String getValue() {
		return value;
	}
	
	public String getName(){
		return name;
	}

	public static String getName(String value) {
		for (OperationAuditStatusEnum status : OperationAuditStatusEnum.values()) {
			if(status.getValue().equals(value)){
				return status.name;
			}
		}
		return null;
	}
}
