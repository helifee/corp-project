package com.creditease.b2bsettle.basedata.dict;

/** 
 * Description: 企业管理系统后台使用的 操作类型枚举 包含1-新增,2-修改
 *
 * @author leileiyuan
 * Create Date: 2015年6月26日 下午3:29:09
 */
public enum EnterpriseOperateTypeEnum {
	add("1", "新增"), modify("2", "修改");
	
	private String value;
	private String name;
	
	private EnterpriseOperateTypeEnum(String value, String name) {
		this.value = value;
		this.name = name;
	}
	
	public String getValue(){
		return value;
	}
	
	public String getName(){
		return name;
	}
	
}
