package com.creditease.b2bsettle.basedata.dict;

/** 
 * Description:  信用等级 枚举类 
 * 				1-一级  2-二级   3-三级  4-四级  5-五级
 *
 * @author leileiyuan
 * Create Date: 2015年6月27日 下午1:38:45
 */
public enum CreditLevel {
	one("1", "一级"),
	two("2", "二级"), 
	three("3", "三级"), 
	four("4", "四级"), 
	five("5", "五级");
	
	private String value;
	private String name;
	
	private CreditLevel(String value, String name) {
		this.value = value;
		this.name = name;
	}
	
	public String getValue(){
		return value;
	}
	
	public String getName(){
		return name;
	}
	
	public static String getName(String value){
		for (CreditLevel biz : CreditLevel.values()) {
			if(biz.getValue().equals(value)){
				return biz.name;
			}
		}
		return null;
	}
}
