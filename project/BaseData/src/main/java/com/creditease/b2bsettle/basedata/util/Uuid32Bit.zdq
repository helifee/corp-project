package com.creditease.b2bsettle.basedata.util;

import java.util.UUID;

/** 
 * Description: 32位UUID，Java生成的UUID为36位长度
 *
 * @author leileiyuan
 * Create Date: 2015年6月29日 下午1:53:53
 */
public class Uuid32Bit {
	public static String getUuid32Bit(){
		String uuid = UUID.randomUUID().toString();
		return uuid.replaceAll("-", "");
	}
}
