package com.creditease.b2bsettle.basedata.constant;

/**
 * 对数据的操作类型，记录在审核表中
 * 
 * @author daoqiangzhang
 *
 */
public enum OperateType {
	
	ADD("0"), 		//新增
	UPDATE("1"), 	//修改
	DELETE("2"), 	//删除
	ENABLE("3"),	//启用
	DISABLE("4");	//停用
	
	private String value;
	
	OperateType(String value) {
		this.value = value;
	}
	
	public String toString() {
		return this.value;
	}
	
	public String value() {
		return this.value;
	}
}
