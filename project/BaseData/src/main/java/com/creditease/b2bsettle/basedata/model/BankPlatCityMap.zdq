package com.creditease.b2bsettle.basedata.model;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Table;

import com.creditease.b2bsettle.basedata.base.BaseEntity;

@Entity
@Table(name="bank_plat_city_map")
public class BankPlatCityMap extends BaseEntity{
	
	private static final long serialVersionUID = 4216974064957823022L;

	
	//银行编码
	@Column(name="bank_code")
	private String bankCode;

	//银行名称
	@Column(name="bank_name")
	private String bankName;
	
	//通道市级编码
	@Column(name="city_code_c")
	private String cityCodeC;
	
	//通道市级名称
	@Column(name="city_name_c")
	private String cityNameC;
	
	//平台市级编码
	@Column(name="city_code_p")
	private String cityCodeP;
	
	//平台市级名称
	@Column(name="city_name_p")
	private String cityNameP;

	public String getBankCode() {
		return bankCode;
	}

	public void setBankCode(String bankCode) {
		this.bankCode = bankCode;
	}

	public String getBankName() {
		return bankName;
	}

	public void setBankName(String bankName) {
		this.bankName = bankName;
	}

	public String getCityCodeC() {
		return cityCodeC;
	}

	public void setCityCodeC(String cityCodeC) {
		this.cityCodeC = cityCodeC;
	}

	public String getCityNameC() {
		return cityNameC;
	}

	public void setCityNameC(String cityNameC) {
		this.cityNameC = cityNameC;
	}

	public String getCityCodeP() {
		return cityCodeP;
	}

	public void setCityCodeP(String cityCodeP) {
		this.cityCodeP = cityCodeP;
	}

	public String getCityNameP() {
		return cityNameP;
	}

	public void setCityNameP(String cityNameP) {
		this.cityNameP = cityNameP;
	}
 
 
	
 
	

}
