package com.creditease.b2bsettle.basedata.constant;

/**
 * 审核状态(审核中、通过、拒绝)
 * @author daoqiangzhang
 *
 */
public enum AuditStatus {
	Audit("0"), Through("1"), Refuse("2");
	
	private String value;
	
	AuditStatus(String value) {
		this.value = value;
	}
	
	public String toString() {
		return this.value;
	}
	
	public String value() {
		return this.value;
	}
}
