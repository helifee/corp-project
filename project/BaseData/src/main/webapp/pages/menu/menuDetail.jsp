<%@ page language="java" contentType="text/html; charset=UTF-8"  pageEncoding="UTF-8"%>
<div style="padding:10px 60px 20px 60px">
<form id="ff" method="post">
	<table cellpadding="5">
		<tr>
			<td>菜单名称:</td>
			<td><input class="easyui-textbox" type="text" name="name" disabled="true"></input></td>
		</tr>
		<tr>
			<td>菜单Url:</td>
			<td><input class="easyui-textbox" type="text" name="url" disabled="true"></input></td>
		</tr>
		<tr>
			<td>菜单位置:</td>
			<td><input class="easyui-textbox" type="text" name="location" disabled="true"></input></td>
		</tr>
		<tr>
			<td>创建人:</td>
			<td><input class="easyui-textbox" name="creator" disabled="true"></input></td>
		</tr>
		<tr>
			<td>创建时间:</td>
			<td><input class="easyui-textbox" name="createdTime" disabled="true"></input></td>
		</tr>
		<tr>
			<td>修改人:</td>
			<td><input class="easyui-textbox" name="modifier" disabled="true"></input></td>
		</tr>
		<tr>
			<td>修改时间:</td>
			<td><input class="easyui-textbox" name="modifyTime" disabled="true"></input></td>
		</tr>
		<tr>
			<td>备注:</td>
			<td><input class="easyui-textbox" name="comments" disabled="true"></input></td>
		</tr>
	</table>
</form>
	