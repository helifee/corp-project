	@Test
	public void testSendMessage(){
		
		QueryEsEnterpriseAuthorityByEIdRequest request = new QueryEsEnterpriseAuthorityByEIdRequest();
		request.seteId("60000000");
		request.setMerchId("3333");
		request.setProductId("5");
		request.setSendTime("201506182010201");
		
		QueryEsEnterpriseAuthorityByEIdResponse response = enEnterpriseInfoAuthorityService.queryEnEnterpriseInfoAuthorityByEId(request);
	   
		System.out.println("EID="+response.geteId());
