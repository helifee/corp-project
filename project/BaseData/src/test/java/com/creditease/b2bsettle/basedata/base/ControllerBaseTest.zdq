package com.creditease.b2bsettle.basedata.base;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.web.client.RestTemplate;

/**
 * 控制器测试基类
 * @author daoqiangzhang
 *
 */
public class ControllerBaseTest extends BaseTest {
	
    @Autowired
    private RestTemplate restUtil;

	public void postAndPrint(String url, Object param, Class<?> clazz) {
		Object retVo = this.restUtil.postForObject(url, param, clazz);
		System.out.println(retVo);
	}
}
