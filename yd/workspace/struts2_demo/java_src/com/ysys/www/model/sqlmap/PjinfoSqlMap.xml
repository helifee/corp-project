<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="Pjinfo">

    <typeAlias alias="pjinfo" type="com.ysys.www.model.Pjinfo"/>

    <resultMap id="pjinfoResult" class="pjinfo">
        <result property="projectid" column="PROJECTID"/>
        <result property="enddate" column="ENDDATE"/>
        <result property="startdate" column="STARTDATE"/>
        <result property="prjmei" column="PRJMEI"/>
        <result property="kyakuname" column="KYAKUNAME"/>
        <result property="cancelflag" column="CANCELFLAG"/>
        <result property="intimestp" column="INTIMESTP"/>
        <result property="uptimestp" column="UPTIMESTP"/>
        <result property="projecttyuname" column="PROJECTTYUNAME"/>
        <result property="projecteiname" column="PROJECTEINAME"/>
        <result property="muserid" column="MUSERID"/>
        <result property="ystartdate" column="YSTARTDATE"/>
        <result property="yenddate" column="YENDDATE"/>
        <result property="usernum" column="USERNUM"/>
        <result property="pjgaiyou" column="PJGAIYOU"/>
        <result property="pjem" column="PJEM"/>
        <result property="statusflg" column="STATUSFLG"/>
        <result property="supuserid" column="SUPUSERID"/>
        <result property="biko" column="BIKO"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	PROJECTID ,
        	ENDDATE ,
        	STARTDATE ,
        	PRJMEI ,
        	KYAKUNAME ,
        	CANCELFLAG ,
        	INTIMESTP ,
        	UPTIMESTP ,
        	PROJECTTYUNAME ,
        	PROJECTEINAME ,
        	MUSERID ,
        	YSTARTDATE ,
        	YENDDATE ,
        	USERNUM ,
        	PJGAIYOU ,
        	PJEM ,
        	STATUSFLG ,
        	SUPUSERID ,
        	BIKO 
	    ]]>
	</sql>
	
    <insert id="insert" parameterClass="pjinfo">
    <![CDATA[
        INSERT INTO
        pjinfo (
        	PROJECTID ,
        	ENDDATE ,
        	STARTDATE ,
        	PRJMEI ,
        	KYAKUNAME ,
        	CANCELFLAG ,
        	INTIMESTP ,
        	UPTIMESTP ,
        	PROJECTTYUNAME ,
        	PROJECTEINAME ,
        	MUSERID ,
        	YSTARTDATE ,
        	YENDDATE ,
        	USERNUM ,
        	PJGAIYOU ,
        	PJEM ,
        	STATUSFLG ,
        	SUPUSERID ,
        	BIKO 
        ) VALUES (
        	#projectid# ,
        	#enddate# ,
        	#startdate# ,
        	#prjmei# ,
        	#kyakuname# ,
        	#cancelflag# ,
        	#intimestp# ,
        	#uptimestp# ,
        	#projecttyuname# ,
        	#projecteiname# ,
        	#muserid# ,
        	#ystartdate# ,
        	#yenddate# ,
        	#usernum# ,
        	#pjgaiyou# ,
        	#pjem# ,
        	#statusflg# ,
        	#supuserid# ,
        	#biko# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
        <selectKey resultClass="java.lang.Long" type="post" keyProperty="projectid" >
            select last_insert_id()
        </selectKey>
    </insert>

    <update id="update" parameterClass="pjinfo">
    <![CDATA[
        UPDATE pjinfo SET
	        ENDDATE = #enddate# ,
	        STARTDATE = #startdate# ,
	        PRJMEI = #prjmei# ,
	        KYAKUNAME = #kyakuname# ,
	        CANCELFLAG = #cancelflag# ,
	        INTIMESTP = #intimestp# ,
	        UPTIMESTP = #uptimestp# ,
	        PROJECTTYUNAME = #projecttyuname# ,
	        PROJECTEINAME = #projecteiname# ,
	        MUSERID = #muserid# ,
	        YSTARTDATE = #ystartdate# ,
	        YENDDATE = #yenddate# ,
	        USERNUM = #usernum# ,
	        PJGAIYOU = #pjgaiyou# ,
	        PJEM = #pjem# ,
	        STATUSFLG = #statusflg# ,
	        SUPUSERID = #supuserid# ,
	        BIKO = #biko# 
        WHERE 
	        PROJECTID = #projectid# 
    ]]>
    </update>

    <delete id="delete" parameterClass="java.lang.Integer">
    <![CDATA[
        delete from pjinfo where
        PROJECTID = #projectid# 
    ]]>
    </delete>
    
    <select id="getById" resultMap="pjinfoResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from pjinfo 
	        where 
		        PROJECTID = #projectid# 
	    ]]>
    </select>

	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	       <isNotEmpty prepend="AND" property="projectid">
	             PROJECTID = #projectid#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="enddate">
	             ENDDATE = #enddate#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="startdate">
	             STARTDATE = #startdate#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="prjmei">
	             PRJMEI = #prjmei#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="kyakuname">
	             KYAKUNAME = #kyakuname#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="cancelflag">
	             CANCELFLAG = #cancelflag#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="intimestp">
	             INTIMESTP = #intimestp#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="uptimestp">
	             UPTIMESTP = #uptimestp#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="projecttyuname">
	             PROJECTTYUNAME = #projecttyuname#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="projecteiname">
	             PROJECTEINAME = #projecteiname#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="muserid">
	             MUSERID = #muserid#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="ystartdate">
	             YSTARTDATE = #ystartdate#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="yenddate">
	             YENDDATE = #yenddate#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="usernum">
	             USERNUM = #usernum#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="pjgaiyou">
	             PJGAIYOU = #pjgaiyou#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="pjem">
	             PJEM = #pjem#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="statusflg">
	             STATUSFLG = #statusflg#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="supuserid">
	             SUPUSERID = #supuserid#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="biko">
	             BIKO = #biko#
	       </isNotEmpty>
	    </dynamic>	
	</sql>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from pjinfo
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="pjinfoResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from pjinfo 
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
</sqlMap>
