public class Emailaddress extends BaseEntity {
	
	//alias
	public static final String TABLE_ALIAS = "Emailaddress";
	public static final String ALIAS_ID = "id";
	public static final String ALIAS_EMAILADDRESS = "emailaddress";
	
	//date formats
	
	//columns START
	private java.lang.Integer id;
	private java.lang.String emailaddress;
	//columns END

	public Emailaddress(){
	}

	public Emailaddress(
		java.lang.Integer id
	){
		this.id = id;
	}

	public void setId(java.lang.Integer value) {
		this.id = value;
	}
	
	public java.lang.Integer getId() {
		return this.id;
	}
	public void setEmailaddress(java.lang.String value) {
		this.emailaddress = value;
	}
	
	public java.lang.String getEmailaddress() {
		return this.emailaddress;
	}

	public String toString() {
		return new ToStringBuilder(this)
			.append("Id",getId())
			.append("Emailaddress",getEmailaddress())
			.toString();
	}
	
	public int hashCode() {
		return new HashCodeBuilder()
			.append(getId())
			.append(getEmailaddress())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof Emailaddress == false) return false;
		if(this == obj) return true;
		Emailaddress other = (Emailaddress)obj;
		return new EqualsBuilder()
			.append(getId(),other.getId())
			.append(getEmailaddress(),other.getEmailaddress())
			.isEquals();
	}
}

