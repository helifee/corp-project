public class Pjpginfo extends BaseEntity {
	
	//alias
	public static final String TABLE_ALIAS = "Pjpginfo";
	public static final String ALIAS_USERID = "userid";
	public static final String ALIAS_PROJECTID = "projectid";
	public static final String ALIAS_INTIMESTP = "intimestp";
	public static final String ALIAS_UPTIMESTP = "uptimestp";
	public static final String ALIAS_CANCELFLAG = "cancelflag";
	
	//date formats
	
	//columns START
	private java.lang.Integer userid;
	private java.lang.String projectid;
	private java.lang.String intimestp;
	private java.lang.String uptimestp;
	private java.lang.String cancelflag;
	//columns END

	public Pjpginfo(){
	}

	public Pjpginfo(
		java.lang.Integer userid,
		java.lang.String projectid
	){
		this.userid = userid;
		this.projectid = projectid;
	}

	public void setUserid(java.lang.Integer value) {
		this.userid = value;
	}
	
	public java.lang.Integer getUserid() {
		return this.userid;
	}
	public void setProjectid(java.lang.String value) {
		this.projectid = value;
	}
	
	public java.lang.String getProjectid() {
		return this.projectid;
	}
	public void setIntimestp(java.lang.String value) {
		this.intimestp = value;
	}
	
	public java.lang.String getIntimestp() {
		return this.intimestp;
	}
	public void setUptimestp(java.lang.String value) {
		this.uptimestp = value;
	}
	
	public java.lang.String getUptimestp() {
		return this.uptimestp;
	}
	public void setCancelflag(java.lang.String value) {
		this.cancelflag = value;
	}
	
	public java.lang.String getCancelflag() {
		return this.cancelflag;
	}

	public String toString() {
		return new ToStringBuilder(this)
			.append("Userid",getUserid())
			.append("Projectid",getProjectid())
			.append("Intimestp",getIntimestp())
			.append("Uptimestp",getUptimestp())
			.append("Cancelflag",getCancelflag())
			.toString();
	}
	
	public int hashCode() {
		return new HashCodeBuilder()
			.append(getUserid())
			.append(getProjectid())
			.append(getIntimestp())
			.append(getUptimestp())
			.append(getCancelflag())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof Pjpginfo == false) return false;
		if(this == obj) return true;
		Pjpginfo other = (Pjpginfo)obj;
		return new EqualsBuilder()
			.append(getUserid(),other.getUserid())
			.append(getProjectid(),other.getProjectid())
			.append(getIntimestp(),other.getIntimestp())
			.append(getUptimestp(),other.getUptimestp())
			.append(getCancelflag(),other.getCancelflag())
			.isEquals();
	}
}

