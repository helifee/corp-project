public class TabMcyl extends BaseEntity {
	
	//alias
	public static final String TABLE_ALIAS = "TabMcyl";
	public static final String ALIAS_JNID = "机能ID";
	public static final String ALIAS_HMID = "画面ID";
	public static final String ALIAS_XMID = "项目ID";
	public static final String ALIAS_XMSX = "项目顺序";
	public static final String ALIAS_XMMC = "项目名称";
	public static final String ALIAS_SCQF = "删除区分";
	public static final String ALIAS_UPDATETIME = "更新时间";
	
	//date formats
	public static final String FORMAT_UPDATETIME = DATE_TIME_FORMAT;
	
	//columns START
	private java.lang.String jnid;
	private java.lang.String hmid;
	private java.lang.String xmid;
	private java.lang.String xmsx;
	private java.lang.String xmmc;
	private java.lang.String scqf;
	private java.sql.Timestamp updatetime;
	//columns END

	public TabMcyl(){
	}

	public TabMcyl(
		java.lang.String jnid,
		java.lang.String hmid,
		java.lang.String xmid,
		java.lang.String xmsx
	){
		this.jnid = jnid;
		this.hmid = hmid;
		this.xmid = xmid;
		this.xmsx = xmsx;
	}

	public void setJnid(java.lang.String value) {
		this.jnid = value;
	}
	
	public java.lang.String getJnid() {
		return this.jnid;
	}
	public void setHmid(java.lang.String value) {
		this.hmid = value;
	}
	
	public java.lang.String getHmid() {
		return this.hmid;
	}
	public void setXmid(java.lang.String value) {
		this.xmid = value;
	}
	
	public java.lang.String getXmid() {
		return this.xmid;
	}
	public void setXmsx(java.lang.String value) {
		this.xmsx = value;
	}
	
	public java.lang.String getXmsx() {
		return this.xmsx;
	}
	public void setXmmc(java.lang.String value) {
		this.xmmc = value;
	}
	
	public java.lang.String getXmmc() {
		return this.xmmc;
	}
	public void setScqf(java.lang.String value) {
		this.scqf = value;
	}
	
	public java.lang.String getScqf() {
		return this.scqf;
	}
	public String getUpdatetimeString() {
		return date2String(getUpdatetime(), FORMAT_UPDATETIME);
	}
	public void setUpdatetimeString(String value) {
		setUpdatetime(string2Date(value, FORMAT_UPDATETIME,java.sql.Timestamp.class));
	}
	
	public void setUpdatetime(java.sql.Timestamp value) {
		this.updatetime = value;
	}
	
	public java.sql.Timestamp getUpdatetime() {
		return this.updatetime;
	}

	public String toString() {
		return new ToStringBuilder(this)
			.append("Jnid",getJnid())
			.append("Hmid",getHmid())
			.append("Xmid",getXmid())
			.append("Xmsx",getXmsx())
			.append("Xmmc",getXmmc())
			.append("Scqf",getScqf())
			.append("Updatetime",getUpdatetime())
			.toString();
	}
	
	public int hashCode() {
		return new HashCodeBuilder()
			.append(getJnid())
			.append(getHmid())
			.append(getXmid())
			.append(getXmsx())
			.append(getXmmc())
			.append(getScqf())
			.append(getUpdatetime())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof TabMcyl == false) return false;
		if(this == obj) return true;
		TabMcyl other = (TabMcyl)obj;
		return new EqualsBuilder()
			.append(getJnid(),other.getJnid())
			.append(getHmid(),other.getHmid())
			.append(getXmid(),other.getXmid())
			.append(getXmsx(),other.getXmsx())
			.append(getXmmc(),other.getXmmc())
			.append(getScqf(),other.getScqf())
			.append(getUpdatetime(),other.getUpdatetime())
			.isEquals();
	}
}

