package com.ysys.www.model;

import org.apache.commons.lang.builder.EqualsBuilder;
import org.apache.commons.lang.builder.HashCodeBuilder;
import org.apache.commons.lang.builder.ToStringBuilder;

import java.util.*;

import javacommon.base.*;
import javacommon.util.*;

import cn.org.rapid_framework.util.*;
import cn.org.rapid_framework.web.util.*;
import cn.org.rapid_framework.page.*;
import cn.org.rapid_framework.page.impl.*;

import com.ysys.www.model.*;
import com.ysys.www.dao.*;
import com.ysys.www.service.*;

/**
 * @author badqiu email:badqiu(a)gmail.com
 * @version 1.0
 * @since 1.0
 */


public class TabTask extends BaseEntity {
	
	//alias
	public static final String TABLE_ALIAS = "TabTask";
	public static final String ALIAS_USERID = "用户ID";
	public static final String ALIAS_TASKFLAG = "任务区分";
	public static final String ALIAS_TASKID = "任务别ID";
	public static final String ALIAS_CONTENT = "内容";
	public static final String ALIAS_LINK = "链接";
	public static final String ALIAS_READFLAG = "已读标志";
	public static final String ALIAS_CREATETIME = "创建时间";
	public static final String ALIAS_UPDATETIME = "更新时间";
	
	//date formats
	public static final String FORMAT_CREATETIME = DATE_TIME_FORMAT;
	public static final String FORMAT_UPDATETIME = DATE_TIME_FORMAT;
	
	//columns START
	private java.lang.String userid;
	private java.lang.Integer taskflag;
	private java.lang.String taskid;
	private java.lang.String content;
	private java.lang.String link;
	private java.lang.Integer readflag;
	private java.sql.Timestamp createtime;
	private java.sql.Timestamp updatetime;
	//columns END

	public TabTask(){
	}

	public TabTask(
		java.lang.String userid,
		java.lang.Integer taskflag,
		java.lang.String taskid
	){
		this.userid = userid;
		this.taskflag = taskflag;
		this.taskid = taskid;
	}

	public void setUserid(java.lang.String value) {
		this.userid = value;
	}
	
	public java.lang.String getUserid() {
		return this.userid;
	}
	public void setTaskflag(java.lang.Integer value) {
		this.taskflag = value;
	}
	
	public java.lang.Integer getTaskflag() {
		return this.taskflag;
	}
	public void setTaskid(java.lang.String value) {
		this.taskid = value;
	}
	
	public java.lang.String getTaskid() {
		return this.taskid;
	}
	public void setContent(java.lang.String value) {
		this.content = value;
	}
	
	public java.lang.String getContent() {
		return this.content;
	}
	public void setLink(java.lang.String value) {
		this.link = value;
	}
	
	public java.lang.String getLink() {
		return this.link;
	}
	public void setReadflag(java.lang.Integer value) {
		this.readflag = value;
	}
	
	public java.lang.Integer getReadflag() {
		return this.readflag;
	}
	public String getCreatetimeString() {
		return date2String(getCreatetime(), FORMAT_CREATETIME);
	}
	public void setCreatetimeString(String value) {
		setCreatetime(string2Date(value, FORMAT_CREATETIME,java.sql.Timestamp.class));
	}
	
	public void setCreatetime(java.sql.Timestamp value) {
		this.createtime = value;
	}
	
	public java.sql.Timestamp getCreatetime() {
		return this.createtime;
	}
	public String getUpdatetimeString() {
		return date2String(getUpdatetime(), FORMAT_UPDATETIME);
	}
	public void setUpdatetimeString(String value) {
		setUpdatetime(string2Date(value, FORMAT_UPDATETIME,java.sql.Timestamp.class));
	}
	
	public void setUpdatetime(java.sql.Timestamp value) {
		this.updatetime = value;
	}
	
	public java.sql.Timestamp getUpdatetime() {
		return this.updatetime;
	}

	public String toString() {
		return new ToStringBuilder(this)
			.append("Userid",getUserid())
			.append("Taskflag",getTaskflag())
			.append("Taskid",getTaskid())
			.append("Content",getContent())
			.append("Link",getLink())
			.append("Readflag",getReadflag())
			.append("Createtime",getCreatetime())
			.append("Updatetime",getUpdatetime())
			.toString();
	}
	
	public int hashCode() {
		return new HashCodeBuilder()
			.append(getUserid())
			.append(getTaskflag())
			.append(getTaskid())
			.append(getContent())
			.append(getLink())
			.append(getReadflag())
			.append(getCreatetime())
			.append(getUpdatetime())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof TabTask == false) return false;
		if(this == obj) return true;
		TabTask other = (TabTask)obj;
		return new EqualsBuilder()
			.append(getUserid(),other.getUserid())
			.append(getTaskflag(),other.getTaskflag())
			.append(getTaskid(),other.getTaskid())
			.append(getContent(),other.getContent())
			.append(getLink(),other.getLink())
			.append(getReadflag(),other.getReadflag())
			.append(getCreatetime(),other.getCreatetime())
			.append(getUpdatetime(),other.getUpdatetime())
			.isEquals();
	}
}

