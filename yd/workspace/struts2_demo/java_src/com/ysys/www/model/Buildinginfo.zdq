	public static final String ALIAS_DISTID = "区代号";
	public static final String ALIAS_BUILDNAME = "楼盘名称";
	public static final String ALIAS_ADDRESS = "楼盘地址";
	public static final String ALIAS_SURPLUS_NUMBER = "剩余套数";
	public static final String ALIAS_STARTING_PRICE = "起价";
	public static final String ALIAS_EVEN_PRICE = "均价";
	public static final String ALIAS_NOTICE = "开盘公告";
	public static final String ALIAS_IMAGE_PATH = "效果图路径";
	public static final String ALIAS_SALES_COMPANY = "销售公司";
