	public void setUpdatetimeString(String value) {
		setUpdatetime(string2Date(value, FORMAT_UPDATETIME,java.sql.Timestamp.class));
	}
	
	public void setUpdatetime(java.sql.Timestamp value) {
		this.updatetime = value;
	}
	
	public java.sql.Timestamp getUpdatetime() {
		return this.updatetime;
	}

	public String toString() {
		return new ToStringBuilder(this)
			.append("Buildid",getBuildid())
			.append("Cityid",getCityid())
			.append("Distid",getDistid())
			.append("Buildname",getBuildname())
			.append("Address",getAddress())
			.append("SurplusNumber",getSurplusNumber())
			.append("StartingPrice",getStartingPrice())
			.append("EvenPrice",getEvenPrice())
			.append("Notice",getNotice())
			.append("ImagePath",getImagePath())
			.append("SalesCompany",getSalesCompany())
			.append("DetailIntroduction",getDetailIntroduction())
			.append("Flag",getFlag())
			.append("Updatetime",getUpdatetime())
			.toString();
	}
	
	public int hashCode() {
		return new HashCodeBuilder()
			.append(getBuildid())
			.append(getCityid())
			.append(getDistid())
			.append(getBuildname())
			.append(getAddress())
			.append(getSurplusNumber())
			.append(getStartingPrice())
			.append(getEvenPrice())
			.append(getNotice())
			.append(getImagePath())
			.append(getSalesCompany())
			.append(getDetailIntroduction())
			.append(getFlag())
			.append(getUpdatetime())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof Buildinginfo == false) return false;
		if(this == obj) return true;
		Buildinginfo other = (Buildinginfo)obj;
		return new EqualsBuilder()
			.append(getBuildid(),other.getBuildid())
			.append(getCityid(),other.getCityid())
			.append(getDistid(),other.getDistid())
			.append(getBuildname(),other.getBuildname())
			.append(getAddress(),other.getAddress())
			.append(getSurplusNumber(),other.getSurplusNumber())
			.append(getStartingPrice(),other.getStartingPrice())
			.append(getEvenPrice(),other.getEvenPrice())
			.append(getNotice(),other.getNotice())
			.append(getImagePath(),other.getImagePath())
			.append(getSalesCompany(),other.getSalesCompany())
			.append(getDetailIntroduction(),other.getDetailIntroduction())
			.append(getFlag(),other.getFlag())
			.append(getUpdatetime(),other.getUpdatetime())
			.isEquals();
	}
