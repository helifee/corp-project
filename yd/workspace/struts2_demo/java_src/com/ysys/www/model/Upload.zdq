 * @since 1.0
 */


public class Upload extends BaseEntity {
	
	//alias
	public static final String TABLE_ALIAS = "Upload";
	public static final String ALIAS_ID = "id";
	public static final String ALIAS_FILENAME = "filename";
	public static final String ALIAS_UPFILEDATE = "upfiledate";
	public static final String ALIAS_UPFILEDIR = "upfiledir";
	public static final String ALIAS_UPFILEUSER = "upfileuser";
	
	//date formats
	public static final String FORMAT_UPFILEDATE = DATE_TIME_FORMAT;
	
	//columns START
	private java.lang.Integer id;
	private java.lang.String filename;
	private java.sql.Timestamp upfiledate;
	private java.lang.String upfiledir;
	private java.lang.String upfileuser;
	//columns END

	public Upload(){
	}

	public Upload(
		java.lang.Integer id
	){
		this.id = id;
	}

	public void setId(java.lang.Integer value) {
		this.id = value;
	}
	
	public java.lang.Integer getId() {
		return this.id;
	}
	public void setFilename(java.lang.String value) {
		this.filename = value;
	}
	
	public java.lang.String getFilename() {
		return this.filename;
	}
	public String getUpfiledateString() {
		return date2String(getUpfiledate(), FORMAT_UPFILEDATE);
	}
	public void setUpfiledateString(String value) {
		setUpfiledate(string2Date(value, FORMAT_UPFILEDATE,java.sql.Timestamp.class));
	}
	
	public void setUpfiledate(java.sql.Timestamp value) {
		this.upfiledate = value;
	}
	
	public java.sql.Timestamp getUpfiledate() {
		return this.upfiledate;
	}
	public void setUpfiledir(java.lang.String value) {
		this.upfiledir = value;
	}
	
	public java.lang.String getUpfiledir() {
		return this.upfiledir;
	}
	public void setUpfileuser(java.lang.String value) {
		this.upfileuser = value;
	}
	
	public java.lang.String getUpfileuser() {
		return this.upfileuser;
	}

	public String toString() {
		return new ToStringBuilder(this)
			.append("Id",getId())
			.append("Filename",getFilename())
			.append("Upfiledate",getUpfiledate())
			.append("Upfiledir",getUpfiledir())
			.append("Upfileuser",getUpfileuser())
			.toString();
	}
	
	public int hashCode() {
		return new HashCodeBuilder()
			.append(getId())
			.append(getFilename())
			.append(getUpfiledate())
			.append(getUpfiledir())
			.append(getUpfileuser())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof Upload == false) return false;
		if(this == obj) return true;
		Upload other = (Upload)obj;
		return new EqualsBuilder()
			.append(getId(),other.getId())
			.append(getFilename(),other.getFilename())
			.append(getUpfiledate(),other.getUpfiledate())
			.append(getUpfiledir(),other.getUpfiledir())
			.append(getUpfileuser(),other.getUpfileuser())
			.isEquals();
	}
}

