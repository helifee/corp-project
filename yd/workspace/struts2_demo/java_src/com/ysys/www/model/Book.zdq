 */


public class Book extends BaseEntity {
	
	//alias
	public static final String TABLE_ALIAS = "Book";
	public static final String ALIAS_ID = "id";
	public static final String ALIAS_NAME = "name";
	public static final String ALIAS_ISBM = "isbm";
	public static final String ALIAS_AUTHOR = "author";
	
	//date formats
	
	//columns START
	private java.lang.Double id;
	private java.lang.String name;
	private java.lang.String isbm;
	private java.lang.String author;
	//columns END

	public Book(){
	}

	public Book(
		java.lang.Double id
	){
		this.id = id;
	}

	public void setId(java.lang.Double value) {
		this.id = value;
	}
	
	public java.lang.Double getId() {
		return this.id;
	}
	public void setName(java.lang.String value) {
		this.name = value;
	}
	
	public java.lang.String getName() {
		return this.name;
	}
	public void setIsbm(java.lang.String value) {
		this.isbm = value;
	}
	
	public java.lang.String getIsbm() {
		return this.isbm;
	}
	public void setAuthor(java.lang.String value) {
		this.author = value;
	}
	
	public java.lang.String getAuthor() {
		return this.author;
	}

	public String toString() {
		return new ToStringBuilder(this)
			.append("Id",getId())
			.append("Name",getName())
			.append("Isbm",getIsbm())
			.append("Author",getAuthor())
			.toString();
	}
	
	public int hashCode() {
		return new HashCodeBuilder()
			.append(getId())
			.append(getName())
			.append(getIsbm())
			.append(getAuthor())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof Book == false) return false;
		if(this == obj) return true;
		Book other = (Book)obj;
		return new EqualsBuilder()
			.append(getId(),other.getId())
			.append(getName(),other.getName())
			.append(getIsbm(),other.getIsbm())
			.append(getAuthor(),other.getAuthor())
			.isEquals();
	}
}

