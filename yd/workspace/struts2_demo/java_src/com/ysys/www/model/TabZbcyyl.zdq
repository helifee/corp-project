public class TabZbcyyl extends BaseEntity {
	
	//alias
	public static final String TABLE_ALIAS = "TabZbcyyl";
	public static final String ALIAS_ZBID = "组别ID";
	public static final String ALIAS_ZCYID = "组成员ID";
	public static final String ALIAS_UPDATETIME = "更新时间";
	
	//date formats
	public static final String FORMAT_UPDATETIME = DATE_TIME_FORMAT;
	
	//columns START
	private java.lang.Integer zbid;
	private java.lang.String zcyid;
	private java.sql.Timestamp updatetime;
	//columns END

	public TabZbcyyl(){
	}

	public TabZbcyyl(
		java.lang.Integer zbid,
		java.lang.String zcyid
	){
		this.zbid = zbid;
		this.zcyid = zcyid;
	}

	public void setZbid(java.lang.Integer value) {
		this.zbid = value;
	}
	
	public java.lang.Integer getZbid() {
		return this.zbid;
	}
	public void setZcyid(java.lang.String value) {
		this.zcyid = value;
	}
	
	public java.lang.String getZcyid() {
		return this.zcyid;
	}
	public String getUpdatetimeString() {
		return date2String(getUpdatetime(), FORMAT_UPDATETIME);
	}
	public void setUpdatetimeString(String value) {
		setUpdatetime(string2Date(value, FORMAT_UPDATETIME,java.sql.Timestamp.class));
	}
	
	public void setUpdatetime(java.sql.Timestamp value) {
		this.updatetime = value;
	}
	
	public java.sql.Timestamp getUpdatetime() {
		return this.updatetime;
	}

	public String toString() {
		return new ToStringBuilder(this)
			.append("Zbid",getZbid())
			.append("Zcyid",getZcyid())
			.append("Updatetime",getUpdatetime())
			.toString();
	}
	
	public int hashCode() {
		return new HashCodeBuilder()
			.append(getZbid())
			.append(getZcyid())
			.append(getUpdatetime())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof TabZbcyyl == false) return false;
		if(this == obj) return true;
		TabZbcyyl other = (TabZbcyyl)obj;
		return new EqualsBuilder()
			.append(getZbid(),other.getZbid())
			.append(getZcyid(),other.getZcyid())
			.append(getUpdatetime(),other.getUpdatetime())
			.isEquals();
	}
}

