public class Mdistrict extends BaseEntity {
	
	//alias
	public static final String TABLE_ALIAS = "Mdistrict";
	public static final String ALIAS_CITYID = "市代号";
	public static final String ALIAS_DISTID = "区代号";
	public static final String ALIAS_UDFLAG = "市区标志";
	public static final String ALIAS_DISTNAME = "区名";
	public static final String ALIAS_FLAG = "可见标志";
	public static final String ALIAS_UPDATETIME = "更新时间";
	
	//date formats
	public static final String FORMAT_UPDATETIME = DATE_TIME_FORMAT;
	
	//columns START
	private java.lang.String cityid;
	private java.lang.String distid;
	private java.lang.Integer udflag;
	private java.lang.String distname;
	private java.lang.Integer flag;
	private java.sql.Timestamp updatetime;
	//columns END

	public Mdistrict(){
	}

	public Mdistrict(
		java.lang.String cityid,
		java.lang.String distid
	){
		this.cityid = cityid;
		this.distid = distid;
	}

	public void setCityid(java.lang.String value) {
		this.cityid = value;
	}
	
	public java.lang.String getCityid() {
		return this.cityid;
	}
	public void setDistid(java.lang.String value) {
		this.distid = value;
	}
	
	public java.lang.String getDistid() {
		return this.distid;
	}
	public void setUdflag(java.lang.Integer value) {
		this.udflag = value;
	}
	
	public java.lang.Integer getUdflag() {
		return this.udflag;
	}
	public void setDistname(java.lang.String value) {
		this.distname = value;
	}
	
	public java.lang.String getDistname() {
		return this.distname;
	}
	public void setFlag(java.lang.Integer value) {
		this.flag = value;
	}
	
	public java.lang.Integer getFlag() {
		return this.flag;
	}
	public String getUpdatetimeString() {
		return date2String(getUpdatetime(), FORMAT_UPDATETIME);
	}
	public void setUpdatetimeString(String value) {
		setUpdatetime(string2Date(value, FORMAT_UPDATETIME,java.sql.Timestamp.class));
	}
	
	public void setUpdatetime(java.sql.Timestamp value) {
		this.updatetime = value;
	}
	
	public java.sql.Timestamp getUpdatetime() {
		return this.updatetime;
	}

	public String toString() {
		return new ToStringBuilder(this)
			.append("Cityid",getCityid())
			.append("Distid",getDistid())
			.append("Udflag",getUdflag())
			.append("Distname",getDistname())
			.append("Flag",getFlag())
			.append("Updatetime",getUpdatetime())
			.toString();
	}
	
	public int hashCode() {
		return new HashCodeBuilder()
			.append(getCityid())
			.append(getDistid())
			.append(getUdflag())
			.append(getDistname())
			.append(getFlag())
			.append(getUpdatetime())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof Mdistrict == false) return false;
		if(this == obj) return true;
		Mdistrict other = (Mdistrict)obj;
		return new EqualsBuilder()
			.append(getCityid(),other.getCityid())
			.append(getDistid(),other.getDistid())
			.append(getUdflag(),other.getUdflag())
			.append(getDistname(),other.getDistname())
			.append(getFlag(),other.getFlag())
			.append(getUpdatetime(),other.getUpdatetime())
			.isEquals();
	}
}

