<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TabMrhyyl">

    <typeAlias alias="tabMrhyyl" type="com.ysys.www.model.TabMrhyyl"/>

    <resultMap id="tabMrhyylResult" class="tabMrhyyl">
        <result property="hyrq" column="HYRQ"/>
        <result property="hykssj" column="HYKSSJ"/>
        <result property="hyjssj" column="HYJSSJ"/>
        <result property="hyzt" column="HYZT"/>
        <result property="hysid" column="HYSID"/>
        <result property="yyrid" column="YYRID"/>
        <result property="bmbz" column="BMBZ"/>
        <result property="cjzrs" column="CJZRS"/>
        <result property="zqhyqf" column="ZQHYQF"/>
        <result property="zqhyid" column="ZQHYID"/>
        <result property="hysbg" column="HYSBG"/>
        <result property="updatetime" column="UPDATETIME"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	HYRQ ,
        	HYKSSJ ,
        	HYJSSJ ,
        	HYZT ,
        	HYSID ,
        	YYRID ,
        	BMBZ ,
        	CJZRS ,
        	ZQHYQF ,
        	ZQHYID ,
        	HYSBG ,
        	UPDATETIME 
	    ]]>
	</sql>
	
    <insert id="insert" parameterClass="tabMrhyyl">
    <![CDATA[
        INSERT INTO
        tab_mrhyyl (
        	HYRQ ,
        	HYKSSJ ,
        	HYJSSJ ,
        	HYZT ,
        	HYSID ,
        	YYRID ,
        	BMBZ ,
        	CJZRS ,
        	ZQHYQF ,
        	ZQHYID ,
        	HYSBG ,
        	UPDATETIME 
        ) VALUES (
        	#hyrq# ,
        	#hykssj# ,
        	#hyjssj# ,
        	#hyzt# ,
        	#hysid# ,
        	#yyrid# ,
        	#bmbz# ,
        	#cjzrs# ,
        	#zqhyqf# ,
        	#zqhyid# ,
        	#hysbg# ,
        	#updatetime# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
        <selectKey resultClass="java.lang.Long" type="post" keyProperty="hyrqhykssjhysid" >
            select last_insert_id()
        </selectKey>
    </insert>

    <update id="update" parameterClass="tabMrhyyl">
    <![CDATA[
        UPDATE tab_mrhyyl SET
	        HYJSSJ = #hyjssj# ,
	        HYZT = #hyzt# ,
	        YYRID = #yyrid# ,
	        BMBZ = #bmbz# ,
	        CJZRS = #cjzrs# ,
	        ZQHYQF = #zqhyqf# ,
	        ZQHYID = #zqhyid# ,
	        HYSBG = #hysbg# ,
	        UPDATETIME = #updatetime# 
        WHERE 
	        HYRQ = #hyrq# ,
	        HYKSSJ = #hykssj# ,
	        HYSID = #hysid# 
    ]]>
    </update>

    <delete id="delete" parameterClass="java.sql.Date">
    <![CDATA[
        delete from tab_mrhyyl where
        HYRQ = #hyrq# ,
        HYKSSJ = #hykssj# ,
        HYSID = #hysid# 
    ]]>
    </delete>
    
    <select id="getById" resultMap="tabMrhyylResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from tab_mrhyyl 
	        where 
		        HYRQ = #hyrq# ,
		        HYKSSJ = #hykssj# ,
		        HYSID = #hysid# 
	    ]]>
    </select>

	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	       <isNotEmpty prepend="AND" property="hyrq">
	             HYRQ = #hyrq#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="hykssj">
	             HYKSSJ = #hykssj#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="hyjssj">
	             HYJSSJ = #hyjssj#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="hyzt">
	             HYZT = #hyzt#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="hysid">
	             HYSID = #hysid#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="yyrid">
	             YYRID = #yyrid#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="bmbz">
	             BMBZ = #bmbz#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="cjzrs">
	             CJZRS = #cjzrs#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="zqhyqf">
	             ZQHYQF = #zqhyqf#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="zqhyid">
	             ZQHYID = #zqhyid#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="hysbg">
	             HYSBG = #hysbg#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="updatetime">
	             UPDATETIME = #updatetime#
	       </isNotEmpty>
	    </dynamic>	
	</sql>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from tab_mrhyyl
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tabMrhyylResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from tab_mrhyyl 
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
</sqlMap>
