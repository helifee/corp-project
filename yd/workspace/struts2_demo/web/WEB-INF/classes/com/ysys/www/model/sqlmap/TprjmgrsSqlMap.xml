<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="Tprjmgrs">

    <typeAlias alias="tprjmgrs" type="com.ysys.www.model.Tprjmgrs"/>

    <resultMap id="tprjmgrsResult" class="tprjmgrs">
        <result property="prjId" column="PRJ_ID"/>
        <result property="prjName" column="PRJ_NAME"/>
        <result property="prjDescribe" column="PRJ_DESCRIBE"/>
        <result property="prjCusname" column="PRJ_CUSNAME"/>
        <result property="prjCuscontact" column="PRJ_CUSCONTACT"/>
        <result property="prjEnvir" column="PRJ_ENVIR"/>
        <result property="prjDptid" column="PRJ_DPTID"/>
        <result property="prjPbdate" column="PRJ_PBDATE"/>
        <result property="prjPedate" column="PRJ_PEDATE"/>
        <result property="prjTbdate" column="PRJ_TBDATE"/>
        <result property="prjTedate" column="PRJ_TEDATE"/>
        <result property="prjDate" column="PRJ_DATE"/>
        <result property="prjRef" column="PRJ_REF"/>
        <result property="prjCname" column="PRJ_CNAME"/>
        <result property="prjEname" column="PRJ_ENAME"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	PRJ_ID ,
        	PRJ_NAME ,
        	PRJ_DESCRIBE ,
        	PRJ_CUSNAME ,
        	PRJ_CUSCONTACT ,
        	PRJ_ENVIR ,
        	PRJ_DPTID ,
        	PRJ_PBDATE ,
        	PRJ_PEDATE ,
        	PRJ_TBDATE ,
        	PRJ_TEDATE ,
        	PRJ_DATE ,
        	PRJ_REF ,
        	PRJ_CNAME ,
        	PRJ_ENAME 
	    ]]>
	</sql>
	
    <insert id="insert" parameterClass="tprjmgrs">
    <![CDATA[
        INSERT INTO
        tprjmgrs (
        	PRJ_ID ,
        	PRJ_NAME ,
        	PRJ_DESCRIBE ,
        	PRJ_CUSNAME ,
        	PRJ_CUSCONTACT ,
        	PRJ_ENVIR ,
        	PRJ_DPTID ,
        	PRJ_PBDATE ,
        	PRJ_PEDATE ,
        	PRJ_TBDATE ,
        	PRJ_TEDATE ,
        	PRJ_DATE ,
        	PRJ_REF ,
        	PRJ_CNAME ,
        	PRJ_ENAME 
        ) VALUES (
        	#prjId# ,
        	#prjName# ,
        	#prjDescribe# ,
        	#prjCusname# ,
        	#prjCuscontact# ,
        	#prjEnvir# ,
        	#prjDptid# ,
        	#prjPbdate# ,
        	#prjPedate# ,
        	#prjTbdate# ,
        	#prjTedate# ,
        	#prjDate# ,
        	#prjRef# ,
        	#prjCname# ,
        	#prjEname# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
        <selectKey resultClass="java.lang.Long" type="post" keyProperty="prjId" >
            select last_insert_id()
        </selectKey>
    </insert>

    <update id="update" parameterClass="tprjmgrs">
    <![CDATA[
        UPDATE tprjmgrs SET
	        PRJ_NAME = #prjName# ,
	        PRJ_DESCRIBE = #prjDescribe# ,
	        PRJ_CUSNAME = #prjCusname# ,
	        PRJ_CUSCONTACT = #prjCuscontact# ,
	        PRJ_ENVIR = #prjEnvir# ,
	        PRJ_DPTID = #prjDptid# ,
	        PRJ_PBDATE = #prjPbdate# ,
	        PRJ_PEDATE = #prjPedate# ,
	        PRJ_TBDATE = #prjTbdate# ,
	        PRJ_TEDATE = #prjTedate# ,
	        PRJ_DATE = #prjDate# ,
	        PRJ_REF = #prjRef# ,
	        PRJ_CNAME = #prjCname# ,
	        PRJ_ENAME = #prjEname# 
        WHERE 
	        PRJ_ID = #prjId# 
    ]]>
    </update>

    <delete id="delete" parameterClass="java.lang.String">
    <![CDATA[
        delete from tprjmgrs where
        PRJ_ID = #prjId# 
    ]]>
    </delete>
    
    <select id="getById" resultMap="tprjmgrsResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from tprjmgrs 
	        where 
		        PRJ_ID = #prjId# 
	    ]]>
    </select>

	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	       <isNotEmpty prepend="AND" property="prjId">
	             PRJ_ID = #prjId#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="prjName">
	             PRJ_NAME = #prjName#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="prjDescribe">
	             PRJ_DESCRIBE = #prjDescribe#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="prjCusname">
	             PRJ_CUSNAME = #prjCusname#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="prjCuscontact">
	             PRJ_CUSCONTACT = #prjCuscontact#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="prjEnvir">
	             PRJ_ENVIR = #prjEnvir#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="prjDptid">
	             PRJ_DPTID = #prjDptid#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="prjPbdate">
	             PRJ_PBDATE = #prjPbdate#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="prjPedate">
	             PRJ_PEDATE = #prjPedate#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="prjTbdate">
	             PRJ_TBDATE = #prjTbdate#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="prjTedate">
	             PRJ_TEDATE = #prjTedate#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="prjDate">
	             PRJ_DATE = #prjDate#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="prjRef">
	             PRJ_REF = #prjRef#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="prjCname">
	             PRJ_CNAME = #prjCname#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="prjEname">
	             PRJ_ENAME = #prjEname#
	       </isNotEmpty>
	    </dynamic>	
	</sql>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from tprjmgrs
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tprjmgrsResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from tprjmgrs 
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
</sqlMap>
