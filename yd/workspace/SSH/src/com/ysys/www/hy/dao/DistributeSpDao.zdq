package com.ysys.www.hy.dao;

import java.util.List;

import com.ysys.www.hy.dao.condition.TabHysyl;

/**
 * 会议室分布（数据库访问）
 * @author fengliang
 * @version 1.00 2009/09/23
 */
public interface DistributeSpDao {

	/**
	 * 会议室名称的取得
	 * 
	 * @return List 会议室相关信息
	 */
	@SuppressWarnings("unchecked")
	public abstract List findName();
	public abstract List findNamePage(long offsetFrom , long offsetTo);
	/**
	 * 会议室的删除
	 * 
	 * @param deleteObj 会议室记录
	 * @return void
	 */
	public abstract void deleteHys(TabHysyl deleteObj);

	/**
	 * 会议室插入和更新
	 * 
	 * @param deleteObj 会议室记录
	 * @return void
	 */
	public abstract void saveOrUpdateHys(TabHysyl deleteObj);

	/**
	 * 会议室信息的取得
	 * 
	 * @param hysid 会议室ID
	 * @return TabHysyl 会议室记录
	 */
	public abstract TabHysyl findById(Integer hysid);

	
	public long getRowIndexDB(String hysPx);
	
	public long getRowIndexDBSecond(String hysPx , String hysId);
	
	public long getMaxHysidDB();
	
	public long getMaxHyspxDB();
	
	public long getTotalCountDB();
}