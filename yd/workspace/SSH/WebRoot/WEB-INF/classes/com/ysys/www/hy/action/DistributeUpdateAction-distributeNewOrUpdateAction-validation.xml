<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
	<field name="hysId">
		<field-validator type="requiredstring">
			<message>会议室ID：不能为空！</message>
		</field-validator>
		<field-validator type="stringlength" short-circuit="true">
			<param name="minLength">1</param>
			<param name="maxLength">2</param>
			<message>会议室ID长度应为${minLength}到${maxLength}位！</message>
		</field-validator>		
	</field>
	<field name="hysMc">
		<field-validator type="requiredstring">
			<message>会议室名称：不能为空！</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">34</param>
			<message>会议室名称长度为${minLength}到${maxLength}位！</message>
		</field-validator>
		
	</field>
	<field name="hysRs">
		<field-validator type="requiredstring">
			<message>容纳人数：不能为空！</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">3</param>
			<message>容纳人数长度为${minLength}到${maxLength}位！</message>
		</field-validator>
		
	</field>
	<field name="hysDh">
		
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">4</param>
			<message>电话长度为${minLength}到${maxLength}位！</message>
		</field-validator>
		
	</field>
	<field name="hysSb">
		
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">255</param>
			<message>设备长度为${minLength}到${maxLength}位！</message>
		</field-validator>
		
	</field>
	<field name="hysBg">
		<field-validator type="requiredstring">
			<message>是否要会议室报告：不能为空！</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">1</param>
			<message>是否要会议室报告长度为${minLength}到${maxLength}位！</message>
		</field-validator>
		
	</field>
	<field name="hysJs">
		<field-validator type="requiredstring">
			<message>接口数量：不能为空！</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">3</param>
			<message>接口数量长度为${minLength}到${maxLength}位！</message>
		</field-validator>
		
	</field>
	<field name="hysPx">
		<field-validator type="requiredstring">
			<message>排序：不能为空！</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">2</param>
			<message>排序长度为${minLength}到${maxLength}位！</message>
		</field-validator>
		
	</field>
	<field name="hysQx">
		<field-validator type="requiredstring">
			<message>会议室标签起点X坐标：不能为空！</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">4</param>
			<message>会议室标签起点X坐标长度为${minLength}到${maxLength}位！</message>
		</field-validator>
		
	</field>
	<field name="hysQy">
		<field-validator type="requiredstring">
			<message>会议室标签起点Y坐标：不能为空！</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">4</param>
			<message>会议室标签起点Y坐标长度为${minLength}到${maxLength}位！</message>
		</field-validator>
		
	</field>
	<field name="hysZx">
		<field-validator type="requiredstring">
			<message>会议室标签终点X坐标：不能为空！</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">4</param>
			<message>会议室标签终点X坐标长度为${minLength}到${maxLength}位！</message>
		</field-validator>
		
	</field>
	<field name="hysZy">
		<field-validator type="requiredstring">
			<message>会议室标签终点Y坐标：不能为空！</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="maxLength">4</param>
			<message>会议室标签终点Y坐标长度为${minLength}到${maxLength}位！</message>
		</field-validator>
		
	</field>
</validators>