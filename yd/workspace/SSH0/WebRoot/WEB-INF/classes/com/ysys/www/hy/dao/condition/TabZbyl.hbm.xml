<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.ysys.www.hy.dao.condition">
	<class
		name="TabZbyl"
		table="tab_zbyl"
		dynamic-insert="true"
		dynamic-update="true"
		
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<id
			name="id"
			type="integer"
			column="ZBID"
		>
			<generator class="assigned"/>
		</id>

		<property
			name="jlzid"
			column="JLZID"
			type="string"
			not-null="true"
			length="8"
		/>
		<property
			name="zbqf"
			column="ZBQF"
			type="integer"
			not-null="true"
			length="10"
		/>
		<property
			name="zzwmc"
			column="ZZWMC"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="updatetime"
			column="UPDATETIME"
			type="timestamp"
			not-null="false"
			length="19"
		/>


	</class>	
</hibernate-mapping>