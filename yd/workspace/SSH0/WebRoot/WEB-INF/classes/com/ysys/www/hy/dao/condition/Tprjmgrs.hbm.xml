<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.ysys.www.hy.dao.condition">
	<class
		name="Tprjmgrs"
		table="tprjmgrs"
		dynamic-insert="true"
		dynamic-update="true"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="PRJ_ID"
		>
			<generator class="assigned"/>
		</id>

		<property
			name="prjName"
			column="PRJ_NAME"
			type="string"
			not-null="true"
			length="120"
		/>
		<property
			name="prjDescribe"
			column="PRJ_DESCRIBE"
			type="string"
			not-null="true"
		/>
		<property
			name="prjCusname"
			column="PRJ_CUSNAME"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="prjCuscontact"
			column="PRJ_CUSCONTACT"
			type="string"
			not-null="true"
			length="255"
		/>
		<property
			name="prjEnvir"
			column="PRJ_ENVIR"
			type="string"
			not-null="true"
		/>
		<property
			name="prjDptid"
			column="PRJ_DPTID"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="prjPbdate"
			column="PRJ_PBDATE"
			type="date"
			not-null="true"
			length="10"
		/>
		<property
			name="prjPedate"
			column="PRJ_PEDATE"
			type="date"
			not-null="true"
			length="10"
		/>
		<property
			name="prjTbdate"
			column="PRJ_TBDATE"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="prjTedate"
			column="PRJ_TEDATE"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="prjDate"
			column="PRJ_DATE"
			type="date"
			not-null="true"
			length="10"
		/>
		<property
			name="prjRef"
			column="PRJ_REF"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="prjCname"
			column="PRJ_CNAME"
			type="string"
			not-null="false"
			length="120"
		/>
		<property
			name="prjEname"
			column="PRJ_ENAME"
			type="string"
			not-null="false"
			length="120"
		/>


	</class>	
</hibernate-mapping>