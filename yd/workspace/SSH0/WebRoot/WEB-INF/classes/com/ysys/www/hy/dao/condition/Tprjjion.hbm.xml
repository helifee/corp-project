<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.ysys.www.hy.dao.condition">
	<class
		name="Tprjjion"
		table="tprjjion"
		dynamic-insert="true"
		dynamic-update="true"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="id" class="TprjjionPK">
			<key-property
				name="prjId"
				column="PRJ_ID"
				type="string"
			/>
			<key-property
				name="prjEmpid"
				column="PRJ_EMPID"
				type="string"
			/>
			<key-property
				name="prjBegdate"
				column="PRJ_BEGDATE"
				type="date"
			/>
		</composite-id>

		<property
			name="prjFlag"
			column="PRJ_FLAG"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="prjEnddate"
			column="PRJ_ENDDATE"
			type="date"
			not-null="true"
			length="10"
		/>


	</class>	
</hibernate-mapping>