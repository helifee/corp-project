package com.ysys.www.common.ui.bean;

import static com.ysys.www.common.ui.bean.GridConstants.HTMLSPACE;

public class GridTd {

	// ID
	private String id;

	// 水平对齐
	private String align;

	// 垂直对齐
	private String valign;

	// 是否不换行
	private boolean nowrap;

	// 背景色
	private String bgcolor;

	// div
	private String div;

	// 点击触发的事件
	private String onclick;

	// 样式
	private String span;

	// 内容
	private String content;

	// 是否含有URL
	private Boolean hasURL;

	// URL的链接
	private String url;

	public GridTd() {

		this(HTMLSPACE);
	}

	public GridTd(String content) {

		this(null, content);

	}

	public GridTd(String align, String content) {

		this(align, false, null, content);

	}

	public GridTd(String align, Boolean hasURL, String url, String content) {

		this.align = align;
		this.hasURL = hasURL;
		this.url = url;
		this.content = content;

		this.valign = null;
		this.nowrap = false;
		this.bgcolor = null;
		this.div = null;
		this.onclick = null;
		this.span = null;

	}

	/**
	 * @return the id
	 */
	public String getId() {
		return id;
	}

	/**
	 * @param id
	 *            the id to set
	 */
	public void setId(String id) {
		this.id = id;
	}

	/**
	 * @return the align
	 */
	public String getAlign() {
		return align;
	}

	/**
	 * @param align
	 *            the align to set
	 */
	public void setAlign(String align) {
		this.align = align;
	}

	/**
	 * @return the valign
	 */
	public String getValign() {
		return valign;
	}

	/**
	 * @param valign
	 *            the valign to set
	 */
	public void setValign(String valign) {
		this.valign = valign;
	}

	/**
	 * @return the nowrap
	 */
	public boolean isNowrap() {
		return nowrap;
	}

	/**
	 * @param nowrap
	 *            the nowrap to set
	 */
	public void setNowrap(boolean nowrap) {
		this.nowrap = nowrap;
	}

	/**
	 * @return the bgcolor
	 */
	public String getBgcolor() {
		return bgcolor;
	}

	/**
	 * @param bgcolor
	 *            the bgcolor to set
	 */
	public void setBgcolor(String bgcolor) {
		this.bgcolor = bgcolor;
	}

	/**
	 * @return the div
	 */
	public String getDiv() {
		return div;
	}

	/**
	 * @param div
	 *            the div to set
	 */
	public void setDiv(String div) {
		this.div = div;
	}

	/**
	 * @return the onclick
	 */
	public String getOnclick() {
		return onclick;
	}

	/**
	 * @param onclick
	 *            the onclick to set
	 */
	public void setOnclick(String onclick) {
		this.onclick = onclick;
	}

	/**
	 * @return the span
	 */
	public String getSpan() {
		return span;
	}

	/**
	 * @param span
	 *            the span to set
	 */
	public void setSpan(String span) {
		this.span = span;
	}

	/**
	 * @return the content
	 */
	public String getContent() {
		return content;
	}

	/**
	 * @param content
	 *            the content to set
	 */
	public void setContent(String content) {
		this.content = content;
	}

	/**
	 * @return the hasURL
	 */
	public Boolean getHasURL() {
		return hasURL;
	}

	/**
	 * @param hasURL
	 *            the hasURL to set
	 */
	public void setHasURL(Boolean hasURL) {
		this.hasURL = hasURL;
	}

	/**
	 * @return the uRL
	 */
	public String getUrl() {
		return url;
	}

	/**
	 * @param url
	 *            the uRL to set
	 */
	public void setUrl(String url) {
		this.url = url;
	}

}
