package com.ysys.www.hy.service.condition;

public class SitQueryString {
	private String whereString;    //查询字符串
	private String[] paramsQuanju; //参数名称
	private Object[] argsQuanju;   //参数值
	public String getWhereString() {
		return whereString;
	}
	public void setWhereString(String whereString) {
		this.whereString = whereString;
	}
	public String[] getParamsQuanju() {
		return paramsQuanju;
	}
	public void setParamsQuanju(String[] paramsQuanju) {
		this.paramsQuanju = paramsQuanju;
	}
	public Object[] getArgsQuanju() {
		return argsQuanju;
	}
	public void setArgsQuanju(Object[] argsQuanju) {
		this.argsQuanju = argsQuanju;
	}

}
