<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.ysys.www.hy.dao.condition">
	<class
		name="TabMcyl"
		table="tab_mcyl"
		dynamic-insert="true"
		dynamic-update="true"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<composite-id name="id" class="TabMcylPK">
			<key-property
				name="jnid"
				column="JNID"
				type="string"
			/>
			<key-property
				name="xmid"
				column="XMID"
				type="string"
			/>
			<key-property
				name="hmid"
				column="HMID"
				type="string"
			/>
			<key-property
				name="xmsx"
				column="XMSX"
				type="string"
			/>
		</composite-id>

		<property
			name="xmmc"
			column="XMMC"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="scqf"
			column="SCQF"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="updatetime"
			column="UPDATETIME"
			type="timestamp"
			not-null="false"
			length="19"
		/>


	</class>	
</hibernate-mapping>