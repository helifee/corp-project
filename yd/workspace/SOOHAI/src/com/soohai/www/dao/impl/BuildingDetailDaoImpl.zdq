package com.soohai.www.dao.impl;

import com.soohai.www.dao.BuildingDetailDao;
import com.soohai.www.dao.common.BaseDao;
import com.soohai.www.dao.condition.BuildingInfo;

/**
 * @see com.soohai.www.dao.BuildingDetailDao
 */
public class BuildingDetailDaoImpl extends BaseDao implements BuildingDetailDao {

	/**
	 * {@inheritDoc}
	 */
	@Override
	public BuildingInfo getBuildingDetail(int buildId) {
