<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTDSQLMap2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="NewBuildingDao">
	<typeAlias alias="BuildingMarker" type="com.soohai.www.service.common.BuildingMarker"/>

	<!-- save buildingmarker -->
	<parameterMap id="buildingmarkerpara" class="BuildingMarker">
	  <parameter property="name" jdbcType="VARCHAR" />
	  <parameter property="address" jdbcType="VARCHAR" />
	  <parameter property="lat" jdbcType="Number" />
	  <parameter property="lng" jdbcType="Number" />
	  <parameter property="type" jdbcType="VARCHAR" />
	</parameterMap> 
	<insert id="newBuilding" parameterMap="buildingmarkerpara">
		insert into marker (
			name,
			address,
			lat,
			lng,
			type
		) values (?, ?, ?, ?, ?)
	</insert>
</sqlMap>  
