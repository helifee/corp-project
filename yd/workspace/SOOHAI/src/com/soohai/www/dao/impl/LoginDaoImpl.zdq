package com.soohai.www.dao.impl;

/**
 *  @see com.soohai.www.dao.LoginDao
 */
import java.util.List;

import com.soohai.www.dao.LoginDao;
import com.soohai.www.dao.common.BaseDao;
import com.soohai.www.dao.condition.UserResult;

/**
 *  @see com.soohai.www.dao.LoginDao
 */
public class LoginDaoImpl extends BaseDao implements LoginDao  {

	 /**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<UserResult> login(String userid)  {
		log.debug("userid：" + userid + "logon...");
		return (List<UserResult>) super.queryForList("LoginDao.login",userid);


	}


}
