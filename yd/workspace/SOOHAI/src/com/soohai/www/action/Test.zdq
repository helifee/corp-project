package com.soohai.www.action;

import java.io.IOException;
import java.util.Iterator;
import java.util.List;

public class Test {

	/**
	 * @param args
	 */
	public static void main(String[] args) {

		int buildIdstart = 1;
		int buildIdend = 12;
		
		makeStaticFileBy(buildIdstart, buildIdend);
		
	}
	
	// 指定ID开始，结束测试
	public static void makeStaticFileBy(int buildIdstart, int buildIdend) {
		
		String cmd = "cmd /c start D:\\mf.bat " + buildIdstart + " " + buildIdend;
		Runtime rt = Runtime.getRuntime();
		
		try {
			Process p = rt.exec(cmd);
		} catch (IOException e) {
			
			e.printStackTrace();
		}		
	}
	
	// 指定ID开始，结束list测试
	public static void makeStaticFileBy(List buildingList) {
		Iterator iter = buildingList.iterator();
		while(iter.hasNext()) {
			int i = (Integer) iter.next();
			makeStaticFileBy(i, i);
		}
		
	}

}
