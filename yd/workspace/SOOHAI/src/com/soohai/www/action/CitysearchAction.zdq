package com.soohai.www.action;

import java.util.List;

import com.opensymphony.xwork2.ActionSupport;
import com.soohai.www.dao.condition.District;
import com.soohai.www.service.CitysearchService;
public class CitysearchAction extends ActionSupport {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1224185421685526086L;
    private CitysearchService citysearchService;
    private List<District> nDistricts;
    private List<District> wDistricts;
    private String cityName;
    private String cityId;

    @Override
	public String execute() throws Exception {

		nDistricts = citysearchService.nDisplay();
		wDistricts = citysearchService.wDisplay();
		cityName = citysearchService.getCityName();
		return SUCCESS;

	}

	
	public List<District> getNDistricts() {
		return nDistricts;
	}

	public void setNDistricts(List<District> districts) {
		nDistricts = districts;
	}

	public List<District> getWDistricts() {
		return wDistricts;
	}

	public void setWDistricts(List<District> districts) {
		wDistricts = districts;
	}


	public String getCityName() {
		return cityName;
	}

	public void setCityName(String cityName) {
		this.cityName = cityName;
	}

	public String getCityId() {
		return cityId;
	}

	public void setCityId(String cityId) {
		this.cityId = cityId;
	}



	public CitysearchService getCitysearchService() {
		return citysearchService;
	}



	public void setCitysearchService(CitysearchService citysearchService) {
		this.citysearchService = citysearchService;
	}
}
