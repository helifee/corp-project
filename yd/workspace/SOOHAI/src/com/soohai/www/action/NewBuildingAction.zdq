package com.soohai.www.action;

import com.opensymphony.xwork2.ActionSupport;
import com.soohai.www.service.NewBuildingService;
import com.soohai.www.service.common.BuildingMarker;

public class NewBuildingAction extends ActionSupport {

	private static final long serialVersionUID = 4071569266458648077L;
	private NewBuildingService newBuildingService;
	private BuildingMarker buildingMarker;
	private String name;
	private String address;
	private float lat;
	private float lng;
	private String type;
	
	public String execute() {
		buildingMarker = new BuildingMarker();
		buildingMarker.setName(this.name);
		buildingMarker.setAddress(this.address);
		buildingMarker.setLat(this.lat);
		buildingMarker.setLng(this.lng);
		buildingMarker.setType(this.type);
		newBuildingService.newBuilding(buildingMarker);
		return SUCCESS;
	}

	public void setNewBuildingService(NewBuildingService newBuildingService) {
		this.newBuildingService = newBuildingService;
	}

	public BuildingMarker getBuildingMarker() {
		return buildingMarker;
	}

	public void setBuildingMarker(BuildingMarker buildingMarker) {
		this.buildingMarker = buildingMarker;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getAddress() {
		return address;
	}

	public void setAddress(String address) {
		this.address = address;
	}

	public float getLat() {
		return lat;
	}

	public void setLat(float lat) {
		this.lat = lat;
	}

	public float getLng() {
		return lng;
	}

	public void setLng(float lng) {
		this.lng = lng;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}
}
