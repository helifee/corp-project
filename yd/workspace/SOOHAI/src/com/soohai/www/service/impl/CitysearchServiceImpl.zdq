package com.soohai.www.service.impl;

import java.util.List;

import com.soohai.www.service.CitysearchService;
import com.soohai.www.dao.CitysearchDao;
import com.soohai.www.dao.condition.District;
public class CitysearchServiceImpl implements CitysearchService{
	private CitysearchDao citysearchDao;

	@Override
    public  List<District> nDisplay(){
		return citysearchDao.qDisplay();
	}
	
	@Override
    public  List<District> wDisplay(){
		return citysearchDao.wDisplay();
	}
	
	public  String getCityName(){
		return citysearchDao.getCityName();
	}

	public CitysearchDao getCitysearchDao() {
		return citysearchDao;
	}

	public void setCitysearchDao(CitysearchDao citysearchDao) {
		this.citysearchDao = citysearchDao;
	}
}
