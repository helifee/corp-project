package com.soohai.www.service.impl;

import com.soohai.www.dao.BuildingDetailDao;
import com.soohai.www.dao.condition.BuildingInfo;
import com.soohai.www.service.BuildingDetailService;

/**
 * @see com.soohai.www.service.BuildingDetailService
 */
public class BuildingDetailServiceImpl implements BuildingDetailService {
	
	private BuildingDetailDao buildingDetailDao; 

	public void setBuildingDetailDao(BuildingDetailDao buildingDetailDao) {
		this.buildingDetailDao = buildingDetailDao;
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public BuildingInfo getBuildingInfo(int buildId) {
		
		return buildingDetailDao.getBuildingDetail(buildId);
	}

}
