package com.soohai.www.service.common;

import java.util.Date;

/**
 * 楼盘详细信息Bean
 * @author zhangdaoqiang
 * @version 1.0 2009/10/22
 */
public class BuildingInfo {
	private int buildId;		//楼盘代号
	private String cityId;		//市代号
	private String distId;		//区代号
	private String buildName;	//楼盘名称
	private String address;		//楼盘地址
	private int surplusNumber;	//剩余套数
	private int startingPrice;	//起价
	private int evenPrice;		//均价
	private String notice;		//开盘公告
	private String imagePath;	//效果图路径
	private String salesCompany;//销售公司
	private String detailIntro;	//详细介绍
	private int flag;			//可见标志
	private Date updateTime;	//更新时间
	
	public int getBuildId() {
		return buildId;
	}
	public void setBuildId(int buildId) {
		this.buildId = buildId;
	}
	public String getCityId() {
		return cityId;
	}
	public void setCityId(String cityId) {
		this.cityId = cityId;
	}
	public String getDistId() {
		return distId;
	}
	public void setDistId(String distId) {
		this.distId = distId;
	}
	public String getBuildName() {
		return buildName;
	}
	public void setBuildName(String buildName) {
		this.buildName = buildName;
	}
	public String getAddress() {
		return address;
	}
	public void setAddress(String address) {
		this.address = address;
	}
	public int getSurplusNumber() {
		return surplusNumber;
	}
	public void setSurplusNumber(int surplusNumber) {
		this.surplusNumber = surplusNumber;
	}
	public int getStartingPrice() {
		return startingPrice;
	}
	public void setStartingPrice(int startingPrice) {
		this.startingPrice = startingPrice;
	}
	public int getEvenPrice() {
		return evenPrice;
	}
	public void setEvenPrice(int evenPrice) {
		this.evenPrice = evenPrice;
	}
	public String getNotice() {
		return notice;
	}
	public void setNotice(String notice) {
		this.notice = notice;
	}
	public String getImagePath() {
		return imagePath;
	}
	public void setImagePath(String imagePath) {
		this.imagePath = imagePath;
	}
	public String getSalesCompany() {
		return salesCompany;
	}
	public void setSalesCompany(String salesCompany) {
		this.salesCompany = salesCompany;
	}
	public String getDetailIntro() {
		return detailIntro;
	}
	public void setDetailIntro(String detailIntro) {
		this.detailIntro = detailIntro;
	}
	public int getFlag() {
		return flag;
	}
	public void setFlag(int flag) {
		this.flag = flag;
	}
	public Date getUpdateTime() {
		return updateTime;
	}
	public void setUpdateTime(Date updateTime) {
		this.updateTime = updateTime;
	}

}
