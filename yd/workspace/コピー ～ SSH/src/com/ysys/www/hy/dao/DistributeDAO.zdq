package com.ysys.www.hy.dao;


import java.util.List;

import org.apache.commons.logging.Log;
import org.apache.commons.logging.LogFactory;
import org.springframework.orm.hibernate3.support.HibernateDaoSupport;


import com.ysys.www.hy.dao.condition.TabHysyl;

public class DistributeDAO extends HibernateDaoSupport {
    private static final Log log = LogFactory.getLog(DistributeDAO.class);
    @SuppressWarnings("unchecked")
	public List findName(){
	    log.debug("Init");
	    try {
	        String queryString = " from TabHysyl t order by t.sortid ";
	        return getHibernateTemplate().find(queryString);
	    } catch (RuntimeException re) {
	        log.error("find all failed", re);
	        throw re;
	    }
	}
    
    @SuppressWarnings("unchecked")
	public void deleteHys(TabHysyl deleteObj){
    	log.debug("deleting Book instance");
		try {
			
			getHibernateTemplate().delete(deleteObj);
			log.debug("delete successful");
		} catch (RuntimeException re) {
			log.error("delete failed", re);
			throw re;
		}
		
	}
    public void saveOrUpdateHys(TabHysyl deleteObj){
    	log.debug("deleting  instance");
		try {
//			getHibernateTemplate().save(deleteObj);
			getHibernateTemplate().saveOrUpdate(deleteObj);
			log.debug("delete successful");
		} catch (RuntimeException re) {
			log.error("delete failed", re);
			throw re;
		}
		
	}
    
    public TabHysyl findById(Integer hysid) {
		log.debug("getting Hys instance with id: " + hysid);
		try {
			TabHysyl instance = (TabHysyl) getHibernateTemplate()
					.get("com.ysys.www.hy.dao.condition.TabHysyl", hysid);
			return instance;
		} catch (RuntimeException re) {
			log.error("get failed", re);
			throw re;
		}
	}
    
}

