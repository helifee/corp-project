<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.ysys.www.hy.dao.condition">
	<class
		name="TabZbcyyl"
		table="tab_zbcyyl"
	>
		<meta attribute="sync-DAO">false</meta>

		<meta attribute="sync-DAO">false</meta>

		<composite-id name="id" class="TabZbcyylPK">
			<key-property
				name="zcyid"
				column="ZCYID"
				type="string"
			/>
			<key-property
				name="zbid"
				column="ZBID"
				type="integer"
			/>
		</composite-id>

		<property
			name="updatetime"
			column="UPDATETIME"
			type="timestamp"
			not-null="true"
			length="19"
		/>


	</class>	
</hibernate-mapping>