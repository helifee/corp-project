@Component
public class PerUserManager extends BaseManager<PerUser,java.lang.String>{

	private PerUserDao perUserDao;
	/**增加setXXXX()方法,spring就可以通过autowire自动设置对象属性*/
	public void setPerUserDao(PerUserDao dao) {
		this.perUserDao = dao;
	}
	public EntityDao getEntityDao() {
		return this.perUserDao;
	}
