public class PerPosition extends BaseEntity {
	
	//alias
	public static final String TABLE_ALIAS = "PerPosition";
	public static final String ALIAS_POS_ID = "职位ID";
	public static final String ALIAS_POS_NAME = "职位名称";
	public static final String ALIAS_POS_DESC = "职位描述";
	public static final String ALIAS_POS_LEVEL = "职务级别";
	public static final String ALIAS_POS_MODE = "管理与开发区分";
	public static final String ALIAS_PARENT_POS = "上级职位";
	public static final String ALIAS_UPDATETIME = "更新时间";
	
	//date formats
	public static final String FORMAT_UPDATETIME = DATE_TIME_FORMAT;
	
	//columns START
	private java.lang.String posId;
	private java.lang.String posName;
	private java.lang.String posDesc;
	private java.lang.String posLevel;
	private java.lang.String posMode;
	private java.lang.String parentPos;
	private java.sql.Timestamp updatetime;
	//columns END

	public PerPosition(){
	}

	public PerPosition(
		java.lang.String posId
	){
		this.posId = posId;
	}

	public void setPosId(java.lang.String value) {
		this.posId = value;
	}
	
	public java.lang.String getPosId() {
		return this.posId;
	}
	public void setPosName(java.lang.String value) {
		this.posName = value;
	}
	
	public java.lang.String getPosName() {
		return this.posName;
	}
	public void setPosDesc(java.lang.String value) {
		this.posDesc = value;
	}
	
	public java.lang.String getPosDesc() {
		return this.posDesc;
	}
	public void setPosLevel(java.lang.String value) {
		this.posLevel = value;
	}
	
	public java.lang.String getPosLevel() {
		return this.posLevel;
	}
	public void setPosMode(java.lang.String value) {
		this.posMode = value;
	}
	
	public java.lang.String getPosMode() {
		return this.posMode;
	}
	public void setParentPos(java.lang.String value) {
		this.parentPos = value;
	}
	
	public java.lang.String getParentPos() {
		return this.parentPos;
	}
	public String getUpdatetimeString() {
		return date2String(getUpdatetime(), FORMAT_UPDATETIME);
	}
	public void setUpdatetimeString(String value) {
		setUpdatetime(string2Date(value, FORMAT_UPDATETIME,java.sql.Timestamp.class));
	}
	
	public void setUpdatetime(java.sql.Timestamp value) {
		this.updatetime = value;
	}
	
	public java.sql.Timestamp getUpdatetime() {
		return this.updatetime;
	}

	public String toString() {
		return new ToStringBuilder(this)
			.append("PosId",getPosId())
			.append("PosName",getPosName())
			.append("PosDesc",getPosDesc())
			.append("PosLevel",getPosLevel())
			.append("PosMode",getPosMode())
			.append("ParentPos",getParentPos())
			.append("Updatetime",getUpdatetime())
			.toString();
	}
	
	public int hashCode() {
		return new HashCodeBuilder()
			.append(getPosId())
			.append(getPosName())
			.append(getPosDesc())
			.append(getPosLevel())
			.append(getPosMode())
			.append(getParentPos())
			.append(getUpdatetime())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof PerPosition == false) return false;
		if(this == obj) return true;
		PerPosition other = (PerPosition)obj;
		return new EqualsBuilder()
			.append(getPosId(),other.getPosId())
			.append(getPosName(),other.getPosName())
			.append(getPosDesc(),other.getPosDesc())
			.append(getPosLevel(),other.getPosLevel())
			.append(getPosMode(),other.getPosMode())
			.append(getParentPos(),other.getParentPos())
			.append(getUpdatetime(),other.getUpdatetime())
			.isEquals();
	}
}

