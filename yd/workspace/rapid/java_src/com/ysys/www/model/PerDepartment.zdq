public class PerDepartment extends BaseEntity {
	
	//alias
	public static final String TABLE_ALIAS = "PerDepartment";
	public static final String ALIAS_DEPT_ID = "部门ID";
	public static final String ALIAS_DEPT_NM = "部门名称";
	public static final String ALIAS_PARENT_DEPT_ID = "所属部门ID";
	public static final String ALIAS_COMPANY_ID = "所属公司ID";
	public static final String ALIAS_DEPT_DESC = "部门描述";
	public static final String ALIAS_DEPT_ROOM = "部门所在开发室";
	public static final String ALIAS_LEADER_ID = "该部门部长的社员ID";
	public static final String ALIAS_LEADER_TEL = "该部门部长的内线电话";
	public static final String ALIAS_UPDATETIME = "更新时间";
	
	//date formats
	public static final String FORMAT_UPDATETIME = DATE_TIME_FORMAT;
	
	//columns START
	private java.lang.String deptId;
	private java.lang.String deptNm;
	private java.lang.String parentDeptId;
	private java.lang.String companyId;
	private java.lang.String deptDesc;
	private java.lang.String deptRoom;
	private java.lang.String leaderId;
	private java.lang.Integer leaderTel;
	private java.sql.Timestamp updatetime;
	//columns END

	public PerDepartment(){
	}

	public PerDepartment(
		java.lang.String deptId
	){
		this.deptId = deptId;
	}

	public void setDeptId(java.lang.String value) {
		this.deptId = value;
	}
	
	public java.lang.String getDeptId() {
		return this.deptId;
	}
	public void setDeptNm(java.lang.String value) {
		this.deptNm = value;
	}
	
	public java.lang.String getDeptNm() {
		return this.deptNm;
	}
	public void setParentDeptId(java.lang.String value) {
		this.parentDeptId = value;
	}
	
	public java.lang.String getParentDeptId() {
		return this.parentDeptId;
	}
	public void setCompanyId(java.lang.String value) {
		this.companyId = value;
	}
	
	public java.lang.String getCompanyId() {
		return this.companyId;
	}
	public void setDeptDesc(java.lang.String value) {
		this.deptDesc = value;
	}
	
	public java.lang.String getDeptDesc() {
		return this.deptDesc;
	}
	public void setDeptRoom(java.lang.String value) {
		this.deptRoom = value;
	}
	
	public java.lang.String getDeptRoom() {
		return this.deptRoom;
	}
	public void setLeaderId(java.lang.String value) {
		this.leaderId = value;
	}
	
	public java.lang.String getLeaderId() {
		return this.leaderId;
	}
	public void setLeaderTel(java.lang.Integer value) {
		this.leaderTel = value;
	}
	
	public java.lang.Integer getLeaderTel() {
		return this.leaderTel;
	}
	public String getUpdatetimeString() {
		return date2String(getUpdatetime(), FORMAT_UPDATETIME);
	}
	public void setUpdatetimeString(String value) {
		setUpdatetime(string2Date(value, FORMAT_UPDATETIME,java.sql.Timestamp.class));
	}
	
	public void setUpdatetime(java.sql.Timestamp value) {
		this.updatetime = value;
	}
	
	public java.sql.Timestamp getUpdatetime() {
		return this.updatetime;
	}

	public String toString() {
		return new ToStringBuilder(this)
			.append("DeptId",getDeptId())
			.append("DeptNm",getDeptNm())
			.append("ParentDeptId",getParentDeptId())
			.append("CompanyId",getCompanyId())
			.append("DeptDesc",getDeptDesc())
			.append("DeptRoom",getDeptRoom())
			.append("LeaderId",getLeaderId())
			.append("LeaderTel",getLeaderTel())
			.append("Updatetime",getUpdatetime())
			.toString();
	}
	
	public int hashCode() {
		return new HashCodeBuilder()
			.append(getDeptId())
			.append(getDeptNm())
			.append(getParentDeptId())
			.append(getCompanyId())
			.append(getDeptDesc())
			.append(getDeptRoom())
			.append(getLeaderId())
			.append(getLeaderTel())
			.append(getUpdatetime())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof PerDepartment == false) return false;
		if(this == obj) return true;
		PerDepartment other = (PerDepartment)obj;
		return new EqualsBuilder()
			.append(getDeptId(),other.getDeptId())
			.append(getDeptNm(),other.getDeptNm())
			.append(getParentDeptId(),other.getParentDeptId())
			.append(getCompanyId(),other.getCompanyId())
			.append(getDeptDesc(),other.getDeptDesc())
			.append(getDeptRoom(),other.getDeptRoom())
			.append(getLeaderId(),other.getLeaderId())
			.append(getLeaderTel(),other.getLeaderTel())
			.append(getUpdatetime(),other.getUpdatetime())
			.isEquals();
	}
}

