public class PerAction extends BaseEntity {
	
	//alias
	public static final String TABLE_ALIAS = "PerAction";
	public static final String ALIAS_ACTION_ID = "动作ID";
	public static final String ALIAS_ACTION_NAME = "动作名称";
	public static final String ALIAS_ACTION_VALUE = "动作值";
	
	//date formats
	
	//columns START
	private java.lang.String actionId;
	private java.lang.String actionName;
	private java.lang.String actionValue;
	//columns END

	public PerAction(){
	}

	public PerAction(
		java.lang.String actionId
	){
		this.actionId = actionId;
	}

	public void setActionId(java.lang.String value) {
		this.actionId = value;
	}
	
	public java.lang.String getActionId() {
		return this.actionId;
	}
	public void setActionName(java.lang.String value) {
		this.actionName = value;
	}
	
	public java.lang.String getActionName() {
		return this.actionName;
	}
	public void setActionValue(java.lang.String value) {
		this.actionValue = value;
	}
	
	public java.lang.String getActionValue() {
		return this.actionValue;
	}

	public String toString() {
		return new ToStringBuilder(this)
			.append("ActionId",getActionId())
			.append("ActionName",getActionName())
			.append("ActionValue",getActionValue())
			.toString();
	}
	
	public int hashCode() {
		return new HashCodeBuilder()
			.append(getActionId())
			.append(getActionName())
			.append(getActionValue())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof PerAction == false) return false;
		if(this == obj) return true;
		PerAction other = (PerAction)obj;
		return new EqualsBuilder()
			.append(getActionId(),other.getActionId())
			.append(getActionName(),other.getActionName())
			.append(getActionValue(),other.getActionValue())
			.isEquals();
	}
}

