public class PerUserPermit extends BaseEntity {
	
	//alias
	public static final String TABLE_ALIAS = "PerUserPermit";
	public static final String ALIAS_USER_ID = "社员ID";
	public static final String ALIAS_ROLE_ID = "角色ID";
	public static final String ALIAS_POS_ID = "职位ID";
	public static final String ALIAS_PRO_ID = "项目ID";
	public static final String ALIAS_PERMIT_ID = "权限ID";
	public static final String ALIAS_PERMIT_REFER = "权限备考";
	public static final String ALIAS_OPERATOR = "操作人";
	public static final String ALIAS_UPDATETIME = "更新时间";
	
	//date formats
	public static final String FORMAT_UPDATETIME = DATE_TIME_FORMAT;
	
	//columns START
	private java.lang.String userId;
	private java.lang.String roleId;
	private java.lang.String posId;
	private java.lang.String proId;
	private java.lang.String permitId;
	private java.lang.String permitRefer;
	private java.lang.String operator;
	private java.sql.Timestamp updatetime;
	//columns END

	public PerUserPermit(){
	}

	public PerUserPermit(
		java.lang.String userId
	){
		this.userId = userId;
	}

	public void setUserId(java.lang.String value) {
		this.userId = value;
	}
	
	public java.lang.String getUserId() {
		return this.userId;
	}
	public void setRoleId(java.lang.String value) {
		this.roleId = value;
	}
	
	public java.lang.String getRoleId() {
		return this.roleId;
	}
	public void setPosId(java.lang.String value) {
		this.posId = value;
	}
	
	public java.lang.String getPosId() {
		return this.posId;
	}
	public void setProId(java.lang.String value) {
		this.proId = value;
	}
	
	public java.lang.String getProId() {
		return this.proId;
	}
	public void setPermitId(java.lang.String value) {
		this.permitId = value;
	}
	
	public java.lang.String getPermitId() {
		return this.permitId;
	}
	public void setPermitRefer(java.lang.String value) {
		this.permitRefer = value;
	}
	
	public java.lang.String getPermitRefer() {
		return this.permitRefer;
	}
	public void setOperator(java.lang.String value) {
		this.operator = value;
	}
	
	public java.lang.String getOperator() {
		return this.operator;
	}
	public String getUpdatetimeString() {
		return date2String(getUpdatetime(), FORMAT_UPDATETIME);
	}
	public void setUpdatetimeString(String value) {
		setUpdatetime(string2Date(value, FORMAT_UPDATETIME,java.sql.Timestamp.class));
	}
	
	public void setUpdatetime(java.sql.Timestamp value) {
		this.updatetime = value;
	}
	
	public java.sql.Timestamp getUpdatetime() {
		return this.updatetime;
	}

	public String toString() {
		return new ToStringBuilder(this)
			.append("UserId",getUserId())
			.append("RoleId",getRoleId())
			.append("PosId",getPosId())
			.append("ProId",getProId())
			.append("PermitId",getPermitId())
			.append("PermitRefer",getPermitRefer())
			.append("Operator",getOperator())
			.append("Updatetime",getUpdatetime())
			.toString();
	}
	
	public int hashCode() {
		return new HashCodeBuilder()
			.append(getUserId())
			.append(getRoleId())
			.append(getPosId())
			.append(getProId())
			.append(getPermitId())
			.append(getPermitRefer())
			.append(getOperator())
			.append(getUpdatetime())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof PerUserPermit == false) return false;
		if(this == obj) return true;
		PerUserPermit other = (PerUserPermit)obj;
		return new EqualsBuilder()
			.append(getUserId(),other.getUserId())
			.append(getRoleId(),other.getRoleId())
			.append(getPosId(),other.getPosId())
			.append(getProId(),other.getProId())
			.append(getPermitId(),other.getPermitId())
			.append(getPermitRefer(),other.getPermitRefer())
			.append(getOperator(),other.getOperator())
			.append(getUpdatetime(),other.getUpdatetime())
			.isEquals();
	}
}

