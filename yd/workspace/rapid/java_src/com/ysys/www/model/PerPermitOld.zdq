public class PerPermitOld extends BaseEntity {
	
	//alias
	public static final String TABLE_ALIAS = "PerPermitOld";
	public static final String ALIAS_PERMIT_ID = "权限ID";
	public static final String ALIAS_MODULE_ID = "模块ID";
	public static final String ALIAS_ACTION_ID = "动作ID";
	public static final String ALIAS_PERMIT_VALUE = "权限值";
	
	//date formats
	
	//columns START
	private java.lang.String permitId;
	private java.lang.String moduleId;
	private java.lang.String actionId;
	private java.lang.String permitValue;
	//columns END

	public PerPermitOld(){
	}

	public PerPermitOld(
		java.lang.String permitId
	){
		this.permitId = permitId;
	}

	public void setPermitId(java.lang.String value) {
		this.permitId = value;
	}
	
	public java.lang.String getPermitId() {
		return this.permitId;
	}
	public void setModuleId(java.lang.String value) {
		this.moduleId = value;
	}
	
	public java.lang.String getModuleId() {
		return this.moduleId;
	}
	public void setActionId(java.lang.String value) {
		this.actionId = value;
	}
	
	public java.lang.String getActionId() {
		return this.actionId;
	}
	public void setPermitValue(java.lang.String value) {
		this.permitValue = value;
	}
	
	public java.lang.String getPermitValue() {
		return this.permitValue;
	}

	public String toString() {
		return new ToStringBuilder(this)
			.append("PermitId",getPermitId())
			.append("ModuleId",getModuleId())
			.append("ActionId",getActionId())
			.append("PermitValue",getPermitValue())
			.toString();
	}
	
	public int hashCode() {
		return new HashCodeBuilder()
			.append(getPermitId())
			.append(getModuleId())
			.append(getActionId())
			.append(getPermitValue())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof PerPermitOld == false) return false;
		if(this == obj) return true;
		PerPermitOld other = (PerPermitOld)obj;
		return new EqualsBuilder()
			.append(getPermitId(),other.getPermitId())
			.append(getModuleId(),other.getModuleId())
			.append(getActionId(),other.getActionId())
			.append(getPermitValue(),other.getPermitValue())
			.isEquals();
	}
}

