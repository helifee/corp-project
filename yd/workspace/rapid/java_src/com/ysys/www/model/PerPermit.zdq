	public String toString() {
		return new ToStringBuilder(this)
			.append("PermitId",getPermitId())
			.append("Namespace",getNamespace())
			.append("ActionName",getActionName())
			.append("IsModule",getIsModule())
			.append("ModuleName",getModuleName())
			.append("ParentModule",getParentModule())
			.append("DispSeq",getDispSeq())
			.append("IsMenu",getIsMenu())
			.append("Description",getDescription())
			.toString();
	}
	
	public int hashCode() {
		return new HashCodeBuilder()
			.append(getPermitId())
			.append(getNamespace())
			.append(getActionName())
			.append(getIsModule())
			.append(getModuleName())
			.append(getParentModule())
			.append(getDispSeq())
			.append(getIsMenu())
			.append(getDescription())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof PerPermit == false) return false;
		if(this == obj) return true;
		PerPermit other = (PerPermit)obj;
		return new EqualsBuilder()
			.append(getPermitId(),other.getPermitId())
			.append(getNamespace(),other.getNamespace())
			.append(getActionName(),other.getActionName())
			.append(getIsModule(),other.getIsModule())
			.append(getModuleName(),other.getModuleName())
			.append(getParentModule(),other.getParentModule())
			.append(getDispSeq(),other.getDispSeq())
			.append(getIsMenu(),other.getIsMenu())
			.append(getDescription(),other.getDescription())
			.isEquals();
	}
