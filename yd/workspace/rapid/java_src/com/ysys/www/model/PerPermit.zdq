	//alias
	public static final String TABLE_ALIAS = "PerPermit";
	public static final String ALIAS_PERMIT_ID = "权限/模块ID";
	public static final String ALIAS_NAMESPACE = "命名空间";
	public static final String ALIAS_ACTION_NAME = "动作名称";
	public static final String ALIAS_IS_MODULE = "是否为模块";
	public static final String ALIAS_MODULE_NAME = "模块名称";
	public static final String ALIAS_PARENT_MODULE = "父模块";
	public static final String ALIAS_DISP_SEQ = "显示顺序";
	public static final String ALIAS_IS_MENU = "是否为菜单";
	public static final String ALIAS_DESCRIPTION = "描述";
	
	//date formats
	
	//columns START
	private java.lang.String permitId;
	private java.lang.String namespace;
	private java.lang.String actionName;
	private java.lang.String isModule;
	private java.lang.String moduleName;
	private java.lang.String parentModule;
	private java.lang.String dispSeq;
	private java.lang.String isMenu;
	private java.lang.String description;
