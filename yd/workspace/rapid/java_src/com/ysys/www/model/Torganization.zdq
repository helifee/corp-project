public class Torganization extends BaseEntity {
	
	//alias
	public static final String TABLE_ALIAS = "Torganization";
	public static final String ALIAS_ID = "id";
	public static final String ALIAS_NAME = "name";
	public static final String ALIAS_SN = "sn";
	public static final String ALIAS_DESCRIPTION = "description";
	public static final String ALIAS_PID = "pid";
	
	//date formats
	
	//columns START
	private java.lang.Integer id;
	private java.lang.String name;
	private java.lang.String sn;
	private java.lang.String description;
	private java.lang.Integer pid;
	//columns END

	public Torganization(){
	}

	public Torganization(
		java.lang.Integer id
	){
		this.id = id;
	}

	public void setId(java.lang.Integer value) {
		this.id = value;
	}
	
	public java.lang.Integer getId() {
		return this.id;
	}
	public void setName(java.lang.String value) {
		this.name = value;
	}
	
	public java.lang.String getName() {
		return this.name;
	}
	public void setSn(java.lang.String value) {
		this.sn = value;
	}
	
	public java.lang.String getSn() {
		return this.sn;
	}
	public void setDescription(java.lang.String value) {
		this.description = value;
	}
	
	public java.lang.String getDescription() {
		return this.description;
	}
	public void setPid(java.lang.Integer value) {
		this.pid = value;
	}
	
	public java.lang.Integer getPid() {
		return this.pid;
	}
	
	private Set torganizations = new HashSet(0);
	public void setTorganizations(Set<Torganization> torganization){
		this.torganizations = torganization;
	}
	
	public Set<Torganization> getTorganizations() {
		return torganizations;
	}
	
	private Set tpersons = new HashSet(0);
	public void setTpersons(Set<Tperson> tperson){
		this.tpersons = tperson;
	}
	
	public Set<Tperson> getTpersons() {
		return tpersons;
	}
	
	private Torganization torganization;
	
	public void setTorganization(Torganization torganization){
		this.torganization = torganization;
	}
	
	public Torganization getTorganization() {
		return torganization;
	}

	public String toString() {
		return new ToStringBuilder(this)
			.append("Id",getId())
			.append("Name",getName())
			.append("Sn",getSn())
			.append("Description",getDescription())
			.append("Pid",getPid())
			.toString();
	}
	
	public int hashCode() {
		return new HashCodeBuilder()
			.append(getId())
			.append(getName())
			.append(getSn())
			.append(getDescription())
			.append(getPid())
			.toHashCode();
	}
	
	public boolean equals(Object obj) {
		if(obj instanceof Torganization == false) return false;
		if(this == obj) return true;
		Torganization other = (Torganization)obj;
		return new EqualsBuilder()
			.append(getId(),other.getId())
			.append(getName(),other.getName())
			.append(getSn(),other.getSn())
			.append(getDescription(),other.getDescription())
			.append(getPid(),other.getPid())
			.isEquals();
	}
}

