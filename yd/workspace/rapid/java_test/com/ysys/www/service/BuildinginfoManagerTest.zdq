public class BuildinginfoManagerTest extends BaseManagerTestCase{

	private BuildinginfoManager manager;
	
	// autowire by name
	@Resource
	public void setBuildinginfoManager(BuildinginfoManager manager) {
		this.manager = manager;
	}

	@Override
	protected String[] getDbUnitDataFiles() {
		return new String[]{"classpath:common_testdata.xml","classpath:Buildinginfo_testdata.xml"};
	}

	@Test
	public void crud() {
		Buildinginfo obj = new Buildinginfo();
		
	  	obj.setCityid(new java.lang.String("1"));
	  	obj.setDistid(new java.lang.String("1"));
	  	obj.setBuildname(new java.lang.String("1"));
	  	obj.setAddress(new java.lang.String("1"));
	  	obj.setSurplusNumber(new java.lang.Integer("1"));
	  	obj.setStartingPrice(new java.lang.Integer("1"));
	  	obj.setEvenPrice(new java.lang.Integer("1"));
	  	obj.setNotice(new java.lang.String("1"));
	  	obj.setImagePath(new java.lang.String("1"));
	  	obj.setSalesCompany(new java.lang.String("1"));
	  	obj.setDetailIntroduction(new java.lang.String("1"));
	  	obj.setFlag(new java.lang.Integer("1"));
	  	obj.setUpdatetime(new java.sql.Timestamp(System.currentTimeMillis()));
		
		manager.save(obj);
		manager.getEntityDao().flush();
		
		manager.update(obj);
		manager.getEntityDao().flush();
		
		assertNotNull(obj.getBuildid());
		
		manager.removeById(obj.getBuildid());
		manager.getEntityDao().flush();
	
	}
}
