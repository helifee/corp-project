	  	obj.setDispSeq(new java.lang.String("1"));
	  	obj.setIsMenu(new java.lang.String("1"));
	  	obj.setDescription(new java.lang.String("1"));
		
		manager.save(obj);
		manager.getEntityDao().flush();
		
		manager.update(obj);
		manager.getEntityDao().flush();
		
		assertNotNull(obj.getPermitId());
		
		manager.removeById(obj.getPermitId());
		manager.getEntityDao().flush();
	
	}
