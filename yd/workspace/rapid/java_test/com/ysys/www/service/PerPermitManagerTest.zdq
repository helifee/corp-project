	}

	@Override
	protected String[] getDbUnitDataFiles() {
		return new String[]{"classpath:common_testdata.xml","classpath:PerPermit_testdata.xml"};
	}

	@Test
	public void crud() {
		PerPermit obj = new PerPermit();
		
	  	obj.setNamespace(new java.lang.String("1"));
