package pojo;

public class Upload {
	private static final long serialVersionUID = 1L;
	
	//IDフラグ
	private int id;

	//ファイル名
	private java.lang.String filename;
	
	//アップロードファイル時間
	private java.lang.String  upfiledate;
	
	//アップロードファイルのバス
	private java.lang.String upfiledir;
	
	 //アップロードファイルのユーザ
	private java.lang.String upfileuser;

	public int getId () {
		return id;
	}


	public void setId (int id) {
		this.id = id;

	}


	public java.lang.String getFilename () {
		return filename;
	}

	public void setFilename (java.lang.String filename) {
		this.filename = filename;
	}


	public java.lang.String  getUpfiledate () {
		return upfiledate;
	}

	public void setUpfiledate (java.lang.String  upfiledate) {
		this.upfiledate = upfiledate;
	}


	public java.lang.String getUpfiledir () {
		return upfiledir;
	}

	public void setUpfiledir (java.lang.String upfiledir) {
		this.upfiledir = upfiledir;
	}


	public java.lang.String getUpfileuser () {
		return upfileuser;
	}

	public void setUpfileuser (java.lang.String upfileuser) {
		this.upfileuser = upfileuser;
	}


}