<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="pojo">
	<class
		name="Pjinfo"
		table="PJINFO"
	>
	 <id name="projectid" column="PROJECTID" type="java.lang.String">
			<generator class="assigned"/>
		</id>
		<property name="enddate" type="java.lang.String">
            <column name="ENDDATE" length="50"  />
        </property>
        <property name="startdate" type="java.lang.String">
            <column name="STARTDATE" length="50" not-null="true" />
        </property>
        <property name="prjmei" type="java.lang.String">
            <column name="PRJMEI" length="50" not-null="true" />
        </property>
        
        <property name="kyakuname" type="java.lang.String">
            <column name="KYAKUNAME" length="50" not-null="true" />
        </property>	
        <property name="cancelflag" type="java.lang.String">
            <column name="CANCELFLAG" length="50" />
        </property>

<property name="intimestp" type="java.lang.String">
            <column name="INTIMESTP" length="50"/>
        </property>

<property name="uptimestp" type="java.lang.String">
            <column name="UPTIMESTP" length="50"/>
        </property>


<property name="projecttyuname" type="java.lang.String">
            <column name="PROJECTTYUNAME" length="50" not-null="true" />
        </property>
<property name="projecteiname" type="java.lang.String">
            <column name="PROJECTEINAME" length="50" not-null="true" />
        </property>
<property name="muserid" type="java.lang.String">
            <column name="MUSERID" length="50" not-null="true" />
        </property>
<property name="ystartdate" type="java.lang.String">
            <column name="YSTARTDATE" length="50" not-null="true" />
        </property>

<property name="yenddate" type="java.lang.String">
            <column name="YENDDATE" length="50" not-null="true" />
        </property>

<property name="usernum" type="java.lang.String">
            <column name="USERNUM" length="50" not-null="true" />
        </property>
<property name="pjgaiyou" type="java.lang.String">
            <column name="PJGAIYOU" length="50" not-null="true" />
        </property>

        <property name="pjem" type="java.lang.String">
            <column name="PJEM" length="50" not-null="true" />
        </property>
        <property name="statusflg" type="java.lang.String">
            <column name="STATUSFLG" length="50"  />
        </property>
        <property name="supuserid" type="java.lang.String">
            <column name="SUPUSERID" length="50" />
        </property>               
		 <property name="biko" type="java.lang.String">
            <column name="BIKO" length="50"  />
        </property> 		

	</class>	
</hibernate-mapping>