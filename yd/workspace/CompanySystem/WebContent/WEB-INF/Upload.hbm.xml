<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="pojo">
	<class
		name="Upload"
		table="UPLOAD"
	>
		<id name="id" column="ID" type="int">
			<generator class="assigned"/>
		</id>

		<property name="filename" column="FILENAME" type="string" not-null="true"
			length="300"/>
		<property name="upfiledate" column="UPFILEDATE" type="string" 
			/>
		<property name="upfiledir" column="UPFILEDIR" type="string" not-null="true"
			length="80"/>
		<property name="upfileuser" column="UPFILEUSER" type="string" 
			length="20"/>



	</class>	
</hibernate-mapping>