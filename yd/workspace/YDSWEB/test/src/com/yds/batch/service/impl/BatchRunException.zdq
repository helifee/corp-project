package com.yds.batch.service.impl;

/**
 * 测试用异常
 * 
 * @author wangduo
 * @version 1.0 2010/05/20
 */
public class BatchRunException extends Exception {

	private static final long serialVersionUID = -3507330067823066528L;

    public BatchRunException() {
        super();
    }
    
    public BatchRunException(String msg) {
        super(msg);
    }
    
    public BatchRunException(String msg, Throwable cause) {
        super(msg, cause);
    }
    
    public BatchRunException(Throwable cause) {
        super(cause);
    }
}
