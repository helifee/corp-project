package com.yds.sample.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.sample.bean.TreeTableNewAuthorBean;
import com.yds.sample.bean.TreeTableNewBookBean;
import com.yds.sample.bean.TreeTableNewStoreBean;
import com.yds.sample.dao.TreeTableNewDao;

/**
 * @see com.yds.sample.dao.TreeTableNewDao
 */
@Repository("treeTableNewDao")
public class TreeTableNewDaoImpl extends AbstractBaseDao implements TreeTableNewDao {

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<TreeTableNewAuthorBean> getAuthorList() {
		return super.queryForList("TreeTableNewDao.getAuthorList");
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<TreeTableNewBookBean> getBookList(String author) {
		return super.queryForList("TreeTableNewDao.getBookList", author);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<TreeTableNewStoreBean> getStoreList(String book) {
		return super.queryForList("TreeTableNewDao.getStoreList", book);
	}

}
