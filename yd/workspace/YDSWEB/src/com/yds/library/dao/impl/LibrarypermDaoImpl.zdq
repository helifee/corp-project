package com.yds.library.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.library.bean.LibraryPerm;
import com.yds.library.dao.LibrarypermDao;

/**
 * @see LibrarypermDao
 */
@Repository("librarypermDao")
public class LibrarypermDaoImpl extends AbstractBaseDao implements LibrarypermDao{

	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<LibraryPerm> getLibraryPerm(String userId) {
		return (List<LibraryPerm>) super.queryForList("LibrarypermDao.getLibraryPerm", userId);
	}

}
