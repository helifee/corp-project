package com.yds.library.dao.impl;

import org.springframework.stereotype.Repository;
import com.yds.base.dao.AbstractBaseDao;
import com.yds.library.bean.Finishheadinfo;
import com.yds.library.dao.BookuserinsertDao;
/**
 * 图书用户登陆处理
 * @param   Object bean
 * @return 
 */
@Repository("bookuserinsertDao")
public class BookuserinsertDaoImpl extends AbstractBaseDao implements BookuserinsertDao{

	/**
	 * 图书用户登陆处理
	 * @param  Object bean
	 * @return 
	 */
	@Override
	public void insertuserbook(Object bean) {
		insert("BookuserinsertDao.insertuserbook", bean);
	}

	/**
	 * 成功页面头部信息的取得
	 * 
	 * @param  员工id
	 * @return Booklending_userbklimit 员工借书上限
	 */
	@Override
	public Finishheadinfo getbooklimitsuccess(String userid) {
		// TODO Auto-generated method stub
		Finishheadinfo finishheadinfo =(Finishheadinfo)  super.queryForObject("BookuserinsertDao.getbooklimitsuccess", userid);
		return finishheadinfo;

	}
}
