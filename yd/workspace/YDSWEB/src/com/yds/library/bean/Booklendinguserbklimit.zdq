package com.yds.library.bean;

/**
 * 用户图书上限bean
 * @author LIYI
 * @version 1.0 2010/12/07
 */
public class Booklendinguserbklimit{
	
	private static final long serialVersionUID = -1397053695703253298L;
	/** 图书借阅上限. */
	private Integer booknumlimit;
	/** 图书已借本数. */
	private Integer booklendednum;
	/** 图书可借本数. */
	private Integer bookreadnum;
	/**
	 * @return the booknumlimit
	 */
	public Integer getBooknumlimit() {
		return booknumlimit;
	}
	/**
	 * @param booknumlimit the booknumlimit to set
	 */
	public void setBooknumlimit(Integer booknumlimit) {
		this.booknumlimit = booknumlimit;
	}
	/**
	 * @return the booklendednum
	 */
	public Integer getBooklendednum() {
		return booklendednum;
	}
	/**
	 * @param booklendednum the booklendednum to set
	 */
	public void setBooklendednum(Integer booklendednum) {
		this.booklendednum = booklendednum;
	}
	/**
	 * @return the bookreadnum
	 */
	public Integer getBookreadnum() {
		return bookreadnum;
	}
	/**
	 * @param bookreadnum the bookreadnum to set
	 */
	public void setBookreadnum(Integer bookreadnum) {
		this.bookreadnum = bookreadnum;
	}
	/**
	 * @return the serialversionuid
	 */
	public static long getSerialversionuid() {
		return serialVersionUID;
	}



}