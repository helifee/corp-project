package com.yds.employee.bean;

import com.yds.base.bean.BaseBean;

/**
 * 员工一览检索条件Bean
 * 
 * @author 远东)bixiuqing
 * @version 1.00 2010/05/18
 */
public class Yb0010CondA extends BaseBean {
	private static final long serialVersionUID = 3211114768619684337L;
	
	/** 全部部门ID. */
	private String deptId;
	
	/** 所属主管部门ID. */
	private String changeOrgId;
	
	/** 职位类别ID及职位ID. */
	private String position;
	
	/** 职位类别ID. */
	private String posTypeId;
	
	/** 职位ID. */
	private String posId;
	
	/** 类别ID. */
	private String empStatusId;
	
	/** 状态ID. */
	private String empStateId;
		
	/** 入社年份. */
	private String startYear;

	/**
	 * @return the deptId
	 */
	public String getDeptId() {
		return deptId;
	}

	/**
	 * @return the posId
	 */
	public String getPosId() {
		return posId;
	}

	/**
	 * @return the empStatusId
	 */
	public String getEmpStatusId() {
		return empStatusId;
	}

	/**
	 * @return the empStateId
	 */
	public String getEmpStateId() {
		return empStateId;
	}

	/**
	 * @return the startYear
	 */
	public String getStartYear() {
		return startYear;
	}

	/**
	 * @param deptId the deptId to set
	 */
	public void setDeptId(String deptId) {
		this.deptId = deptId;
	}

	/**
	 * @param posId the posId to set
	 */
	public void setPosId(String posId) {
		this.posId = posId;
	}

	/**
	 * @param empStatusId the empStatusId to set
	 */
	public void setEmpStatusId(String empStatusId) {
		this.empStatusId = empStatusId;
	}

	/**
	 * @param empStateId the empStateId to set
	 */
	public void setEmpStateId(String empStateId) {
		this.empStateId = empStateId;
	}

	/**
	 * @param startYear the startYear to set
	 */
	public void setStartYear(String startYear) {
		this.startYear = startYear;
	}

	/**
	 * @return the posTypeId
	 */
	public String getPosTypeId() {
		return posTypeId;
	}

	/**
	 * @param posTypeId the posTypeId to set
	 */
	public void setPosTypeId(String posTypeId) {
		this.posTypeId = posTypeId;
	}

	public String getPosition() {
		return position;
	}

	public void setPosition(String position) {
		this.position = position;
	}

	public String getChangeOrgId() {
		return changeOrgId;
	}

	public void setChangeOrgId(String changeOrgId) {
		this.changeOrgId = changeOrgId;
	}


}
