package com.yds.employee.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;
import com.yds.base.dao.AbstractBaseDao;
import com.yds.common.bean.EmpPos;
import com.yds.employee.dao.Yb0060Dao;
/**
 * @see com.yds.employee.dao.Yb0060InfoDao
 */
@Repository("yb0060Dao")
public class Yb0060DaoImpl extends AbstractBaseDao implements
		Yb0060Dao {
	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<EmpPos> getPosList(String posTypeId) {

		return (List<EmpPos>) super.queryForList(
				"yb0060Dao.getPosList", posTypeId);
	}
	/**
	 * {@inheritDoc}
	 */
	@Override
	public EmpPos getPosInfo(String posId) {

		return (EmpPos) super.queryForObject(
				"yb0060Dao.getPosInfo", posId);
	}
	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<EmpPos> getPosSortList(EmpPos empPos) {
		return (List<EmpPos>) super.queryForList(
				"yb0060Dao.getPosSortList", empPos);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Object insertPosInfo(EmpPos empPos) {
		return super.insert("yb0060Dao.insertPosInfo",empPos);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Object updatePosInfo(EmpPos empPos) {
		return super.update("yb0060Dao.updatePosInfo",empPos);
	}
	/**
	 * {@inheritDoc}
	 */
	@Override
	public int updatePosSort(EmpPos empPos) {
		return super.update("yb0060Dao.updatePosSort",empPos);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public String getMaxPosId() {

		return (String) super.queryForObject("yb0060Dao.getMaxPosId");
	}

}
