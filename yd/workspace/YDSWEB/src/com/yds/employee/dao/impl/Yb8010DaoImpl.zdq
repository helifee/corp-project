package com.yds.employee.dao.impl;
import java.util.List;
import org.springframework.stereotype.Repository;
import com.yds.base.dao.AbstractBaseDao;
import com.yds.employee.bean.EmpStateInfo;
import com.yds.employee.dao.Yb8010Dao;

/**
 * @see com.yds.employee.dao.Yb8010Dao
 */
@Repository("yb8010Dao")
public class Yb8010DaoImpl extends AbstractBaseDao implements Yb8010Dao{
	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<EmpStateInfo> getEmpStateInfoList() {

		return super.queryForList("Yb8010Dao.getEmpStateInfoList");
	}
	/**
	 * {@inheritDoc}
	 */
	@Override
	public Integer updateEmpInfo(EmpStateInfo empStateInfo) {
		return (Integer)super.update("Yb8010Dao.updateEmpInfo",empStateInfo);
	}	
}
