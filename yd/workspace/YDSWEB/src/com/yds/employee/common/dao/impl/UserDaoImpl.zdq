package com.yds.employee.common.dao.impl;

import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;
import com.yds.base.dao.AbstractBaseDao;
import com.yds.common.bean.EmpInfo;
import com.yds.common.bean.UserInfo;
import com.yds.common.bean.UserResult;
import com.yds.common.dao.UserDao;

/**
 * @see UserDao
 */
@Repository("userDao")
public class UserDaoImpl extends AbstractBaseDao implements UserDao {

	/**
	 * {@inheritDoc}
	 */
	@Override
	public String getUserNm(Map<String,String> condition) {
		return (String)super.queryForObject("UserDao.getuserNm", condition);

	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<UserResult> getExistUsers() {
		// TODO 添加人员检索条件
		return (List<UserResult>)super.queryForList("UserDao.getExistUsers");
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Long findUserId(String userId) {
		return (Long)super.queryForObject("UserDao.findUserId", userId);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<UserInfo> getTeamUserList(String teamId) {
		return (List<UserInfo>)super.queryForList("UserDao.getTeamUserList", teamId);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public EmpInfo getEmpInfoById(String id) {
		return (EmpInfo)super.queryForObject("UserDao.getEmpInfoById", id);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<EmpInfo> getEmpList() {
		return (List<EmpInfo>)super.queryForList("UserDao.getEmpList");
	}

}
