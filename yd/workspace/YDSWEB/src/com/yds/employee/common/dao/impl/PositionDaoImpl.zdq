package com.yds.employee.common.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.common.bean.EmpPos;
import com.yds.common.bean.PositionTreeInfo;
import com.yds.common.dao.PositionDao;

/**
 * @see com.yds.common.dao.PositionDao
 */
@Repository("positionDao")
public class PositionDaoImpl extends AbstractBaseDao implements
		PositionDao {

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<PositionTreeInfo> getAllPositions() {
		
		return super.queryForList("PositionDao.getAllPositions");
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<EmpPos> getPosList(String posTypeId) {

		return super.queryForList("PositionDao.getPosList", posTypeId);
	}

	@SuppressWarnings("unchecked")
	@Override
	public List<PositionTreeInfo> getPosTreeInfoList() {
		return super.queryForList("PositionDao.getPosTreeInfoList");
	}

}
