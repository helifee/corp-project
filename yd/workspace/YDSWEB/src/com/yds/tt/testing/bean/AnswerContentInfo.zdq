package com.yds.tt.testing.bean;

import com.yds.base.bean.BaseBean;

/**
 * 答案得分点表Bean
 * 
 * @author wanqiuhong
 * @version 1.0 2010/05/21
 */
public class AnswerContentInfo extends BaseBean {

	private static final long serialVersionUID = 6118205741142332124L;

	/** 选择题答案选项表示方式 */
	private String answerContentName;
	/** 答案内容 */
	private String answerContent;

	/**
	 * @return the answerContentName
	 */
	public String getAnswerContentName() {
		return answerContentName;
	}

	/**
	 * @param answerContentName
	 *            the answerContentName to set
	 */
	public void setAnswerContentName(String answerContentName) {
		this.answerContentName = answerContentName;
	}

	/**
	 * @return the answerContent
	 */
	public String getAnswerContent() {
		return answerContent;
	}

	/**
	 * @param answerContent the answerContent to set
	 */
	public void setAnswerContent(String answerContent) {
		this.answerContent = answerContent;
	}



}
