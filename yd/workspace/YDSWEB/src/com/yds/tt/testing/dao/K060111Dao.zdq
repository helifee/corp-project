package com.yds.tt.testing.dao;

import java.util.List;

import com.yds.tt.testing.bean.ExamineInfo;
import com.yds.tt.testing.bean.K060111EmpMarkInfo;
import com.yds.tt.testing.bean.K060111PaperMarkInfo;
import com.yds.tt.testing.bean.TestPaperInfo;

/**
 * 评分任务分配（数据库访问）
 * 
 * @author bixiuqing
 * @version 1.00 2010/03/23
 */

public interface K060111Dao {

	/**
	 * 取得考试信息
	 * 
	 * @param examineId
	 *            考试ID
	 * @return ExamineInfo 考试信息
	 */
	public ExamineInfo getExamineInfo(String examineId);
	/**
	 * 取得考试试卷信息
	 * 
	 * @param examineId
	 *                考试ID
	 * @return List<TestPaperInfo> 考试试卷信息
	 */
	public List<TestPaperInfo> getTestPaperInfos(String examineId);
	/**
	 * 取得大题数
	 * 
	 * @param TestPaperInfo
	 *                 考试试卷信息
	 * @return Integer 大题数
	 */
	public Integer getBigQuestCnt(TestPaperInfo testPaperInfo);
	/**
	 * 取得试卷及评分者信息
	 * 
	 * @param examineId
	 *                考试ID
	 * @return List<K060111PaperMarkInfo> 试卷及评分者信息
	 */
	public List<K060111PaperMarkInfo> getPaperMarkInfo(String examineId);
	/**
	 * 取得评卷者未完成数量
	 * 
	 * @param k060111PaperMarkInfo
	 *                试卷及评分者信息
	 * @return Integer 评卷者未完成数量
	 */
	public Integer getNotCompleteCnt(K060111PaperMarkInfo k060111PaperMarkInfo);
	/**
	 * 取得员工答卷数量
	 * 
	 * @param k060111PaperMarkInfo
	 *                试卷及评分者信息
	 * @return Integer 员工答卷数量
	 */
	public Integer getEmpAnswerCnt(K060111PaperMarkInfo k060111PaperMarkInfo);
	/**
	 * 取得所有大题及大题的评分者
	 * 
	 * @param examineId
	 *                考试ID
	 * @return List<K060111PaperMarkInfo> 大题及大题的评分者信息
	 */
	public List<K060111EmpMarkInfo> getEmpMarkInfo(String examineId);
	/**
	 * 取得大题未完成数量
	 * 
	 * @param k060111EmpMarkInfo
	 *                大题及评分者信息
	 * @return Integer 大题未完成数量
	 */
	public Integer getNotCompleteBigQuestionCnt(K060111EmpMarkInfo k060111EmpMarkInfo);
	/**
	 * 取得答案数量
	 * 
	 * @param k060111EmpMarkInfo
	 *                大题及评分者信息
	 * @return Integer 答案数量
	 */
	public Integer getAnswerCnt(K060111EmpMarkInfo k060111EmpMarkInfo);
	/**
	 * 更新考试信息表
	 * 
	 * @param examineInfo
	 *                考试信息           
	 * @return 
	 */
	public void updateExamineInfo(ExamineInfo examineInfo);
	/**
	 * 删除评分任务分配表
	 * 
	 * @param examineId
	 *                考试ID
	 * @return 
	 */
	public void deleteMarkTask(String examineId);
	/**
	 * 插入评分任务分配表(按试卷分配)
	 * 
	 * @param K060111PaperMarkInfo
	 *                大题及评分者信息
	 * @return 
	 */
	public void insertMarkPaper(K060111PaperMarkInfo paperMarkInfo);
	/**
	 * 插入评分任务分配表(按大题分配)
	 * 
	 * @param k060111EmpMarkInfo
	 *                大题及评分者信息
	 * @return 
	 */
	public void insertMarkQuestion(K060111EmpMarkInfo empMarkInfo);
}
