package com.yds.tt.testing.dao;

import java.util.List;

import com.yds.tt.testing.bean.AnswerPointInfo;
import com.yds.tt.testing.bean.ExamEmployeeInfo;
import com.yds.tt.testing.bean.K060031ExamQuestionInfo;
import com.yds.tt.testing.bean.K060031SearchInfo;
import com.yds.tt.testing.bean.K060031SingleQuesInfo;
import com.yds.tt.testing.bean.MarkTaskAssignInfo;
import com.yds.tt.testing.bean.PaperBigQuestionInfo;
import com.yds.tt.testing.bean.TestEmployeeAnswerInfo;
import com.yds.tt.testing.bean.TestPaperInfo;

/**
 * 
 * @author lijinling
 * @version 1.00 2010/04/26
 */
public interface K060101Dao {

	/**
	 * 取得评分任务分配信息.
	 * 
	 * @param markTaskAssignInfo
	 *            评分任务分配信息bean
	 * @return MarkTaskAssignInfo 评分任务分配信息bean
	 */
	public MarkTaskAssignInfo getMarkTaskAssignInfo(
			MarkTaskAssignInfo markTaskAssignInfo);

	/**
	 * 取得待评分试卷信息(key).
	 * 
	 * @param examEmployeeInfo
	 *            考试员工信息
	 * @return ExamEmployeeInfo 考试员工信息
	 */
	public ExamEmployeeInfo getExamEmployeeInfo(
			ExamEmployeeInfo examEmployeeInfo);

	/**
	 * 取得待评分试卷详细信息.
	 * 
	 * @param examEmployeeInfo
	 *            考试员工信息bean
	 * @return TestPaperInfo 试卷信息bean
	 */
	public TestPaperInfo getTestPaperInfo(ExamEmployeeInfo examEmployeeInfo);

	/**
	 * 取得试卷所有大题信息.
	 * 
	 * @param examEmployeeInfo
	 *            考试员工信息bean
	 * @return List<PaperBigQuestionInfo> 试卷大题列表
	 */
	public List<PaperBigQuestionInfo> getPaperBigQuestionInfos(
			PaperBigQuestionInfo paperBigQuestionInfo);

	/**
	 * 取得待评试卷的所有员工信息.
	 * 
	 * @param examEmployeeInfo
	 *            考试员工信息bean
	 * @return List<ExamEmployeeInfo> 考试员工信息列表
	 */
	public List<ExamEmployeeInfo> getExamEmployeeInfos(
			ExamEmployeeInfo examEmployeeInfo);

	/**
	 * 取得单道大题信息.
	 * 
	 * @param paperBigQuestionInfo
	 *            试卷大题信息bean
	 * @return PaperBigQuestionInfo 试卷大题信息bean
	 */
	public PaperBigQuestionInfo getPaperBigQuestionInfo(
			PaperBigQuestionInfo paperBigQuestionInfo);

	/**
	 * 取得待评大题的小题的信息.
	 * 
	 * @param examEmployeeInfo
	 *            考试员工信息bean
	 * @return searchInfo 检索条件bean
	 */
	public K060031ExamQuestionInfo getExamQuestionOfBigQuestion(
			K060031SearchInfo searchInfo);

	/**
	 * 取得小题答案信息.
	 * 
	 * @param k060031ExamQuestionInfo
	 *            小题信息
	 * @return K060031SingleQuesInfo 答案信息
	 */
	public List<K060031SingleQuesInfo> getSingleQuesInfos(
			K060031ExamQuestionInfo k060031ExamQuestionInfo);

	/**
	 * 取得小题答案得分点信息.
	 * 
	 * @param k060031SingleQuesInfo
	 *            答案信息
	 * @return AnswerPointInfo 得分点信息
	 */
	public List<AnswerPointInfo> getAnswerPointInfos(
			AnswerPointInfo answerPointInfo);

	/**
	 * 取得员工考试答案信息.
	 * 
	 * @param searchInfo
	 *            检索条件bean
	 * @return List<K060031SingleQuesInfo> 员工考试答案信息列表
	 */
	public List<K060031SingleQuesInfo> getExamEmployeeAnswerInfos(
			K060031SearchInfo searchInfo);

	/**
	 * 取得考试员工答卷信息.
	 * 
	 * @param testEmployeeAnswerInfo
	 *            考试员工答卷信息Bean
	 * @return 考试员工答卷信息
	 */
	public TestEmployeeAnswerInfo getTestEmployeeAnswerInfo(
			TestEmployeeAnswerInfo testEmployeeAnswerInfo);

	/**
	 * 取得自动评分题是否评分标识
	 * @param testEmployeeAnswerInfo 考试员工答卷信息Bean
	 * @return 考试员工答卷信息
	 */
	public TestEmployeeAnswerInfo getEmpAnswerInfo(
			TestEmployeeAnswerInfo testEmployeeAnswerInfo);
}
