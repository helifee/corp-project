package com.yds.tt.testing.bean;

import java.util.List;

import com.yds.base.bean.BaseBean;

/**
 * 试卷详细信息Bean
 * 
 * @author yinfuyan
 * @version 1.0 2010/04/06
 */
public class K050031PaperInfoDetail extends BaseBean {

	private static final long serialVersionUID = 7174330139120442883L;

	/** 试卷信息. */
	private TestPaperInfo paperInfo;

	/** 试卷大题信息. */
	private List<PaperBigQuestionInfo> bigQuestionInfoList;

	/** 试卷固定大题信息. */
	private List<PaperStableQuestionInfo> paperStableQuestionInfoList;

	/** 试卷随机大题信息. */
	private List<PaperRandomQuestionInfo> paperRandQuestionInfoList;

	/** 登录者用户ID. */
	private String loginUserId;

	/**
	 * @return the paperInfo
	 */
	public TestPaperInfo getPaperInfo() {
		return paperInfo;
	}

	/**
	 * @param paperInfo
	 *            the paperInfo to set
	 */
	public void setPaperInfo(TestPaperInfo paperInfo) {
		this.paperInfo = paperInfo;
	}

	/**
	 * @return the bigQuestionInfoList
	 */
	public List<PaperBigQuestionInfo> getBigQuestionInfoList() {
		return bigQuestionInfoList;
	}

	/**
	 * @param bigQuestionInfoList
	 *            the bigQuestionInfoList to set
	 */
	public void setBigQuestionInfoList(
			List<PaperBigQuestionInfo> bigQuestionInfoList) {
		this.bigQuestionInfoList = bigQuestionInfoList;
	}

	/**
	 * @return the paperStableQuestionInfoList
	 */
	public List<PaperStableQuestionInfo> getPaperStableQuestionInfoList() {
		return paperStableQuestionInfoList;
	}

	/**
	 * @param paperStableQuestionInfoList
	 *            the paperStableQuestionInfoList to set
	 */
	public void setPaperStableQuestionInfoList(
			List<PaperStableQuestionInfo> paperStableQuestionInfoList) {
		this.paperStableQuestionInfoList = paperStableQuestionInfoList;
	}

	/**
	 * @return the paperRandQuestionInfoList
	 */
	public List<PaperRandomQuestionInfo> getPaperRandQuestionInfoList() {
		return paperRandQuestionInfoList;
	}

	/**
	 * @param paperRandQuestionInfoList
	 *            the paperRandQuestionInfoList to set
	 */
	public void setPaperRandQuestionInfoList(
			List<PaperRandomQuestionInfo> paperRandQuestionInfoList) {
		this.paperRandQuestionInfoList = paperRandQuestionInfoList;
	}

	/**
	 * @return the loginUserId
	 */
	public String getLoginUserId() {
		return loginUserId;
	}

	/**
	 * @param loginUserId
	 *            the loginUserId to set
	 */
	public void setLoginUserId(String loginUserId) {
		this.loginUserId = loginUserId;
	}
}
