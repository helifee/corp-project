package com.yds.tt.testing.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.tt.testing.bean.PaperBigQuestionInfo;
import com.yds.tt.testing.bean.PaperRandomQuestionInfo;
import com.yds.tt.testing.bean.PaperStableQuestionInfo;
import com.yds.tt.testing.bean.TestPaperInfo;
import com.yds.tt.testing.dao.K050051Dao;

/**
 * @see com.yds.tt.testing.dao.K050051Dao
 */
@Repository("k050051Dao")
public class K050051DaoImpl extends AbstractBaseDao implements K050051Dao {

	/**
	 * {@inheritDoc}
	 */
	@Override
	public TestPaperInfo getTestPaperInfo(TestPaperInfo testpager) {
		TestPaperInfo testPaperInfo = (TestPaperInfo) super.queryForObject("k050051Dao."
				+ "getK050051TestPaperInfo", testpager);
		if (null != testPaperInfo) {
			testPaperInfo.setMode(testpager.getMode());
		}
		return testPaperInfo;
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<PaperBigQuestionInfo> getPaperBigQuestionInfo(
			TestPaperInfo testpager) {
		return super.queryForList("k050051Dao.getK050051BigQueList", testpager);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<PaperStableQuestionInfo> getPaperStableQuestionInfoList(
			PaperBigQuestionInfo paperBigQue) {
		return super.queryForList("k050051Dao.getK050051DudingDatiInfo",
				paperBigQue);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<PaperRandomQuestionInfo> getPaperRandomQuestionInfoList(
			PaperBigQuestionInfo paperBigQue) {

		return super.queryForList("k050051Dao.getK050051SujiDatiInfo",
				paperBigQue);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public TestPaperInfo getUpdateTime(TestPaperInfo testPaper) {

		return (TestPaperInfo) super.queryForObject("k050051Dao.getUpdateTime",
				testPaper);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void updateApproveTestPaper(TestPaperInfo testPaper) {
		super.update("k050051Dao.updateApproveTestPaper", testPaper);

	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void updateDisApproveTestPaper(TestPaperInfo testPaper) {
		super.update("k050051Dao.updateDisApproveTestPaper", testPaper);

	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void deleteTestPaper(TestPaperInfo testPaper) {
		super.update("k050051Dao.deleteTestPaper", testPaper);
	}

}
