package com.yds.tt.testing.dao;

import java.util.List;
import java.util.Map;

import com.yds.tt.testing.bean.ExamineInfo;
import com.yds.tt.testing.bean.K060131SearchInfo;

/**
 * 考试选择（数据库访问）
 * 
 * @author liyanrui
 * @version 1.00 2010/03/30
 */
public interface K060131Dao {

	/**
	 * 取得考试一览信息列表
	 * 
	 * @param param
	 *            检索条件信息
	 * @param offset
	 *            取得日志信息
	 * @param perCounts
	 *            取得翻页信息
	 * @return ExamineInfo 考试信息一览
	 */
	public List<ExamineInfo> getSearchList(K060131SearchInfo searchInfo,
			int offset, int perCounts);

	/**
	 * 取得考试一览信息总数(分页)
	 * 
	 * @param param
	 *            检索条件信息
	 * @return Long 信息总数
	 */
	public Long getTotalCount(K060131SearchInfo param);

	/**
	 * 取得考试已有的需要通过考试
	 * 
	 * @param examineId
	 *            考试ID
	 * @return List<String> 需要通过考试ID结果集
	 */
	public List<String> getHadExamines(String examineId);

	/**
	 * 插入需要通过考试信息
	 * 
	 * @param necessaryExamines
	 *            需要通过考试信息
	 */
	public void insertNecessaryExamines(Map<String, String> necessaryExamines);
}
