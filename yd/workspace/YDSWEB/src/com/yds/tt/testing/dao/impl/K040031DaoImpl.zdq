package com.yds.tt.testing.dao.impl;

import java.util.Date;
import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.tt.testing.bean.AnswerInfo;
import com.yds.tt.testing.bean.AnswerPointInfo;
import com.yds.tt.testing.bean.QuestionLibrary;
import com.yds.tt.testing.dao.K040031Dao;

@Repository("k040031Dao")
public class K040031DaoImpl extends AbstractBaseDao implements K040031Dao {

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<QuestionLibrary> getQuestionLibraryInfoList(
			List<String> questionIdList) {
		return (List<QuestionLibrary>)super.queryForList(
					"k040031Dao.getQuestionLibraryInfoList", questionIdList);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<String> getKeyWordInfo(Map<String, Integer> categoryId) {
		return (List<String>)super.queryForList("k040031Dao.getKeyWordInfo", categoryId);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void updateNewFlag(String questionId) {
		super.update("k040031Dao.updateNewFlag", questionId);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public int updateTestQuestions(QuestionLibrary questionLibrary) {
		return super.update("k040031Dao.updateTestQuestions", questionLibrary);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertTestQuestions(QuestionLibrary questionLibrary) {
		super.insert("k040031Dao.insertTestQuestions", questionLibrary);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<AnswerInfo> getAnswerInfoList(QuestionLibrary questionLibrary) {
		return (List<AnswerInfo>)super.queryForList("k040031Dao.getAnswerInfo", questionLibrary);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<AnswerPointInfo> getAnswerPointInfoList(QuestionLibrary questionLibrary) {
		return (List<AnswerPointInfo>)super.queryForList("k040031Dao.getAnswerPointInfo", questionLibrary);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertAnswerInfo(AnswerInfo answerInfo) {
		super.insert("k040031Dao.insertAnswerInfo", answerInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Date getUpateTime(String questionId) {
		return (Date)super.queryForObject("k040031Dao.getUpdateTime",questionId);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertAnswerPointInfo(AnswerPointInfo answerPointInfo) {
		super.insert("k040031Dao.insertAnswerPointInfo", answerPointInfo);
	}
}
