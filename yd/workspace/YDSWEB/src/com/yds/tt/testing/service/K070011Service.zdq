package com.yds.tt.testing.service;

import java.util.List;

import com.yds.common.bean.SysCodeInfo;
import com.yds.tt.testing.bean.ExamineInfo;
import com.yds.tt.testing.bean.K070011Info;
import com.yds.tt.testing.bean.K070011SearchPostionInfo;
import com.yds.tt.testing.bean.TestResultsInfo;

/**
 * 考试查询(业务逻辑)
 * 
 * @author chenjunshuai
 * @version 1.00 2010/04/02
 */
public interface K070011Service {
	
	/** 评定等级为1. */
	public static final int TEM_VAR_1 = 1;
	
	/** 评定等级为2. */
	public static final int TEM_VAR_2 = 2;
	
	/** 评定等级为3. */
	public static final int TEM_VAR_3 = 3;
	
	/** 评定等级为4. */
	public static final int TEM_VAR_4 = 4;

	/** 评定等级为5. */
	public static final int TEM_VAR_5 = 5;
	
	/** 分离字符串取前6位. */
	public static final int TEM_VAR_6 = 6;
	
	/** 临时变量. */
	public static final int TEM_VAR_7 = 9999;

	/** 单选按钮的数值. */
	public static final String RADIO_VALUE = "1";
	
	/** 合并单元格个数为1. */
	public static final int ROW_SPAN_CNT = 1;
	
	/**
	 * 取得考试查询考试一览信息列表 
	 * 
	 * @param  examineId 考试ID
	 * 
	 * @param  k070011Info 检索条件信息
	 * 
	 * @param  offset 取得日志信息 
	 * 
	 * @param  perCounts 取得翻页信息
	 * 
	 * @return List<ExamineInfo> 考试信息一览 
	 */
	public List<ExamineInfo> getExamineInfoList(
			String examineId, K070011Info k070011Info, int offset , int perCounts);
	
	/**
	 * 取得考试查询考试一览信息总数(分页)
	 * 
	 * @param  examineId 考试ID
	 * 
	 * @param  k070011Info 检索条件信息
	 * 
	 * @return Long 信息总数
	 */
	public Long getTotalCount(String examineId, K070011Info k070011Info);
	
	/**
	 * 取得考试查询汇总考试信息
	 * 
	 * @param  examineId 考试Id
	 * 
	 * @return ExamineInfo 汇总考试信息
	 */
	public ExamineInfo getExamineAllInfo(String examineId);
	
	/**
	 * 取得考试查询具体考试信息
	 * 
	 * @param  examineId 考试Id
	 * 
	 * @return List<CourseResultsInfo> 具体考试信息
	 */
	public List<TestResultsInfo> getExamineSpecificList(String examineId);

	/**
	 * 取得考试查询参加考试概况信息
	 * 
	 * @param  examineId 考试Id
	 * 
	 * @return TestResultsInfo 考试信息
	 */	
	public TestResultsInfo getTestResultsInfo (String examineId);
	
	/**
	 * 取得考试查询参加考试人数
	 * 
	 * @param  examineId 考试Id
	 */	
	public Integer getExamineCountNum (String examineId);
	
	/**
	 * 取得员工查询考试一览信息列表 
	 * 
	 * @param  employeesId 员工ID
	 * 
	 * @param  offset 取得日志信息 
	 * 
	 * @param  perCounts 取得翻页信息
	 * 
	 * @return List<ExamineInfo> 考试信息一览 
	 */	
	public List<ExamineInfo> getEmpExamineInfoList(String employeesId, int offset , int perCounts);

	/**
	 * 取得员工查询考试一览信息总数(分页)
	 * 
	 * @param  employeesId 员工ID
	 * 
	 * @param  employeesId 员工ID
	 * 
	 * @return Long 信息总数
	 */
	public Long getEmpTotalCount(String employeesId);

	/**
	 * 取得员工查询具体考试一览信息
	 * 
	 * @param  employeesId 员工ID
	 * 
	 * @param  parentExamineId 父考试ID
	 * 
	 * @param  examineRadioValue 单选按钮的值 
	 *  
	 * @return List<TestResultsInfo> 考试信息一览 
	 */
	public  List<TestResultsInfo> getEmpSpecificInfoList(
			String employeesId, String parentExamineId, String examineRadioValue);	
	
	/**
	 * 取得定位查询考试名称和档次
	 * 
	 * @param  k070011SearchPostionInfo 查询信息
	 * 
	 * @return  List<SysCodeInfo> 考试名称和档次 
	 */	
	public List<SysCodeInfo> getExamineNameLevelInfo(K070011SearchPostionInfo k070011SearchPostionInfo);
	
	/**
	 * 取得定位检索具体考试一览信息
	 * 
	 * @param  offset 取得日志信息 
	 * 
	 * @param  perCounts 取得翻页信息
	 * 
	 * @param  searchPostionList 查询信息
	 * 
	 * @param  postionRadioValue 单选按钮的值 
	 *  
	 * @return List<TestResultsInfo> 考试信息一览 
	 */	
	public List<TestResultsInfo> getPostionExamineList(
			String postionRadioValue, List<K070011SearchPostionInfo> searchPostionList,int offset , int perCounts);
	
	/**
	 * 取得定位检索考试一览信息总数(分页)
	 * 
	 * @param  postionRedioValue 单选按钮的值 
	 * 
	 * @param  searchPostionList 检索信息  
	 *          
	 * @return Long 信息总数 
	 */	
	public Long getPosTotalCount(String postionRadioValue, List<K070011SearchPostionInfo> searchPostionList);
}
