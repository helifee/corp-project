package com.yds.tt.testing.bean;
 
import java.util.List;

import com.yds.base.bean.BaseBean;

/**
 * 成绩明细一览信息Bean
 * 
 * @author chenzhong
 * @version 1.0 2010/04/06
 */
public class K070031DetailListInfo extends BaseBean {

	private static final long serialVersionUID = 1L;

	/** 大题数. */
	private Integer bigquestionNum;
	/** 试卷ID. */
	private String paperId;
	/** 试卷名称. */
	private String paperName;
	/** 大题标题和大题总分. */
	private List<K070031TitleAndScore> bigTitleAndScores;
	/** 个人得分信息. */
	private List<K070031PersonInfo> personInfos;
	/** 试卷平均分. */
	private Double paperAverageScore;
	/** 大题平均分. */
	private List<Double> bigAverageScores;
	
	/**
	 * @return the bigquestionNum
	 */
	public Integer getBigquestionNum() {
		return bigquestionNum;
	}
	/**
	 * @param bigquestionNum the bigquestionNum to set
	 */
	public void setBigquestionNum(Integer bigquestionNum) {
		this.bigquestionNum = bigquestionNum;
	}
	/**
	 * @return the paperId
	 */
	public String getPaperId() {
		return paperId;
	}
	/**
	 * @param paperId the paperId to set
	 */
	public void setPaperId(String paperId) {
		this.paperId = paperId;
	}
	/**
	 * @return the paperName
	 */
	public String getPaperName() {
		return paperName;
	}
	/**
	 * @param paperName the paperName to set
	 */
	public void setPaperName(String paperName) {
		this.paperName = paperName;
	}
	/**
	 * @return the bigTitleAndScores
	 */
	public List<K070031TitleAndScore> getBigTitleAndScores() {
		return bigTitleAndScores;
	}
	/**
	 * @param bigTitleAndScores the bigTitleAndScores to set
	 */
	public void setBigTitleAndScores(List<K070031TitleAndScore> bigTitleAndScores) {
		this.bigTitleAndScores = bigTitleAndScores;
	}
	/**
	 * @return the personInfos
	 */
	public List<K070031PersonInfo> getPersonInfos() {
		return personInfos;
	}
	/**
	 * @param personInfos the personInfos to set
	 */
	public void setPersonInfos(List<K070031PersonInfo> personInfos) {
		this.personInfos = personInfos;
	}
	/**
	 * @return the paperAverageScore
	 */
	public Double getPaperAverageScore() {
		return paperAverageScore;
	}
	/**
	 * @param paperAverageScore the paperAverageScore to set
	 */
	public void setPaperAverageScore(Double paperAverageScore) {
		this.paperAverageScore = paperAverageScore;
	}
	/**
	 * @return the bigAverageScores
	 */
	public List<Double> getBigAverageScores() {
		return bigAverageScores;
	}
	/**
	 * @param bigAverageScores the bigAverageScores to set
	 */
	public void setBigAverageScores(List<Double> bigAverageScores) {
		this.bigAverageScores = bigAverageScores;
	}
}
