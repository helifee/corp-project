package com.yds.tt.testing.dao;

import java.util.HashMap;
import java.util.List;
import java.util.Map;

import com.yds.tt.testing.bean.K060121SearchInfo;

/**
 * 考试选择信息一览.
 * 
 * @author 考试评分详细
 * @version 1.00 2010/05/24
 */
public interface K060121Dao {
	
	/**
	 * 初期检查处理(考试评分者检查)
	 * 
	 * @param paraMap
	 * 
	 * @return 检查结果
	 */
	public int checkExamineMarker(Map<String, String> paraMap);
	
	/**
	 * 考试详细信息取得
	 * 
	 * @param paraMap
	 * 	      flag true:实施者
	 * 			  false:评分者
	 * 
	 * @return K060121SearchInfo 考试详细信息
	 */
	public K060121SearchInfo getExamineDetailInfo(Map<String, String> paraMap, boolean flag);
	
	/**
	 * 评分任务信息取得（实施者检索处理）
	 * 
	 * @param examineId 考试ID
	 * 		  markmissionFlg 评分任务分配方式标志
	 * 
	 * @return List<K060121SearchInfo> 考试详细信息
	 */
	public List<K060121SearchInfo> getExamineDetailInfoList(String examineId,int markmissionFlg);
	
	/**
	 * 评分任务信息取得（评分者检索处理）
	 * 
	 * @param paraMap
	 * 		  markmissionFlg 评分任务分配方式标志
	 * 
	 * @return List<K060121SearchInfo> 考试详细信息
	 */
	public List<K060121SearchInfo> getExamineDetailInfoList(Map<String, String> paraMap,int markmissionFlg);
	
	/**
	 * 评分状态检查
	 * 
	 * @param pageInfo 画面信息
	 * 
	 * @return boolean 评分状态检查结果
	 */
	public String getMarkStatus(HashMap<String,Object> pageInfo);
}
