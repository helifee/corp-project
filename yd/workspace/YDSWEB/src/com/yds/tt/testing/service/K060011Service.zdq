package com.yds.tt.testing.service;

import java.util.List;

import com.yds.tt.manager.bean.AuthorityInfo;
import com.yds.tt.testing.bean.ExamineInfo;

/**
 * 考试新建及权限分配(业务逻辑)
 * 
 * @author 远东)chenzhong
 * @version 1.00 2010/04/14
 */
public interface K060011Service {
	
	/**
	 * 考试信息检索处理
	 * 
	 * @param examineId
	 *			考试ID
	 * @return ExamineInfo 
	 * 			考试信息
	 */
	public ExamineInfo getInitInfo(String examineId);
	
	/**
	 * 取得针对对象人员
	 * 
	 * @param examineId
	 *            考试ID
	 * @return String[] 课程针对对象人员
	 */
	public String[] getUserList(String examineId);
	
	/**
	 * 考试新建处理
	 * 
	 * @param ExamineInfo
	 *			考试信息
	 * @param editorInfoList
	 *			编辑者信息一览
	 * @param executantInfo
	 *			实施者信息
	 * @param strUserIdList
	 *			针对对象
	 */
	public void insertNewExamine(
			ExamineInfo examineInfo, 
			AuthorityInfo executantInfo, 
			List<AuthorityInfo> editorInfoList,
			String strUserIdList);
	
	/**
	 * 考试更新处理
	 * 
	 * @param ExamineInfo
	 *			考试信息
	 * @param editorInfoList
	 *			编辑者信息一览
	 * @param executantInfo
	 *			实施者信息
	 * @param strUserIdList
	 *			针对对象
	 */
	public void updateExamine(
			ExamineInfo examineInfo, 
			AuthorityInfo executantInfo, 
			List<AuthorityInfo> editorInfoList,
			String strUserIdList);
	
	/**
	 * 考试参照新建处理
	 * 
	 * @param ExamineInfo
	 *			考试信息
	 * @param editorInfoList
	 *			编辑者信息一览
	 * @param executantInfo
	 *			实施者信息
	 * @param strUserIdList
	 *			针对对象
	 */
	public void insertReferenceExamine(
			ExamineInfo examineInfo, 
			AuthorityInfo executantInfo, 
			List<AuthorityInfo> editorInfoList,
			String strUserIdList);
}
