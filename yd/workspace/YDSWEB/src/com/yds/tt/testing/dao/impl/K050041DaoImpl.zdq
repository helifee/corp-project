package com.yds.tt.testing.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.tt.testing.bean.AnswerInfo;
import com.yds.tt.testing.bean.PaperRandomQuestionInfo;
import com.yds.tt.testing.bean.PaperStableQuestionInfo;
import com.yds.tt.testing.dao.K050041Dao;

/**
 * @see com.yds.tt.testing.dao.K040011Dao
 */
@Repository("k050041Dao")
public class K050041DaoImpl extends AbstractBaseDao implements K050041Dao {

	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<PaperStableQuestionInfo> getQuestionInfos(
			List<String> addQuestionId) {
		return (List<PaperStableQuestionInfo>) super.queryForList(
				"K050041Dao.getQuestionInfos", addQuestionId);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Long getQuestionCount(PaperRandomQuestionInfo prqInfo) {
		return (Long) super.queryForObject("K050041Dao.getQuestionCount",
				prqInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<AnswerInfo> getAnswerInfo(String questionId) {
		return (List<AnswerInfo>) super.queryForList(
				"K050041Dao.getAnswerInfo", questionId);
	}
}
