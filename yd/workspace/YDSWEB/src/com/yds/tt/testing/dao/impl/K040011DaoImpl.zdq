package com.yds.tt.testing.dao.impl;

import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.tt.testing.bean.K040011SearchInfo;
import com.yds.tt.testing.bean.QuestionLibrary;
import com.yds.tt.testing.dao.K040011Dao;

/**
 * @see com.yds.tt.testing.dao.K040011Dao
 */
@Repository("k040011Dao")
public class K040011DaoImpl extends AbstractBaseDao implements K040011Dao {

	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<QuestionLibrary> getQuestionInfos(K040011SearchInfo searchInfo,
			int offset, int perCounts) {

		// 题库信息检索处理
		return (List<QuestionLibrary>) super.queryForList(
				"K040011Dao.searchQuestLibr", searchInfo, offset, perCounts);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Long getTotalCount(K040011SearchInfo searchInfo) {
		return (Long) super.queryForObject("K040011Dao.getTotalCount",
				searchInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<String> getKeywordList(Map<String, Integer> catemap) {
		return (List<String>) super.queryForList("K040011Dao.getKeywordList",
				catemap);
	}

}
