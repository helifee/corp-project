package com.yds.tt.testing.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.tt.testing.bean.K070031ExamineInfo;
import com.yds.tt.testing.bean.ExamineInfo;
import com.yds.tt.testing.bean.K070031BigQuestionInfo;
import com.yds.tt.testing.dao.K070031Dao;

/**
 * @see com.yds.tt.testing.dao.K070031Dao
 */
@Repository("k070031Dao")
public class K070031DaoImpl extends AbstractBaseDao implements K070031Dao{

	/**
	 * {@inheritDoc}
	 */
	@Override
	public ExamineInfo getExamineInfo(String examineId) {
		
		return (ExamineInfo) super.queryForObject(
				"K070031Dao.getExamineInfo",examineId);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<K070031BigQuestionInfo> getBigQuestionInfo(String examineId) {
		
		return (List<K070031BigQuestionInfo>) super.queryForList(
				"K070031Dao.getBigQuestionInfo",examineId);
	}

	@Override
	public K070031ExamineInfo getExamineResultInfo(String examineId) {
		return (K070031ExamineInfo) super.queryForObject(
				"K070031Dao.getExamineResultInfo",examineId);
	}

}
