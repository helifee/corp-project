package com.yds.tt.testing.bean;

import com.yds.base.bean.BaseBean;

/**
 * 评分任务分配分配信息Bean
 * 
 * @author bixiuqing
 * @version 1.0 2010/03/23
 */
public class K060111AssignInfo extends BaseBean {

	private static final long serialVersionUID = 2677565465544073524L;
	/** 分配方式. */
	private String assignMode;
	/** 分配方式可用区分. */
	private Boolean assignModeAbled;
	/** 是否第一次分配. */
	private Boolean assignFirst;
	/**
	 * @return the assignMode
	 */
	public String getAssignMode() {
		return assignMode;
	}
	/**
	 * @return the assignModeAbled
	 */
	public Boolean getAssignModeAbled() {
		return assignModeAbled;
	}
	/**
	 * @return the assignFirst
	 */
	public Boolean getAssignFirst() {
		return assignFirst;
	}
	/**
	 * @param assignMode the assignMode to set
	 */
	public void setAssignMode(String assignMode) {
		this.assignMode = assignMode;
	}
	/**
	 * @param assignModeAbled the assignModeAbled to set
	 */
	public void setAssignModeAbled(Boolean assignModeAbled) {
		this.assignModeAbled = assignModeAbled;
	}
	/**
	 * @param assignFirst the assignFirst to set
	 */
	public void setAssignFirst(Boolean assignFirst) {
		this.assignFirst = assignFirst;
	}

}
