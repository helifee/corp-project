package com.yds.tt.testing.bean;

import com.yds.base.bean.BaseBean;

/**
 * 答案得分点表Bean
 * 
 * @author wanqiuhong
 * @version 1.0 2010/04/13
 */
public class AnswerPointInfo extends BaseBean {

	private static final long serialVersionUID = 1384846968685281663L;

	/** 试题ID*/
	private String questionId;
	/** 试题版本号*/
	private Integer questionVersionNo;
	/** 答案编号*/
	private Integer answerSerialNo;
	/** 得分点编号*/
	private Integer pointSerialNo;
	/** 得分点内容*/
	private String pointContent;
	/** 得分点分数*/
	private Integer pointScore;
	/**
	 * @return the questionId
	 */
	public String getQuestionId() {
		return questionId;
	}
	/**
	 * @param questionId the questionId to set
	 */
	public void setQuestionId(String questionId) {
		this.questionId = questionId;
	}
	/**
	 * @return the questionVersionNo
	 */
	public Integer getQuestionVersionNo() {
		return questionVersionNo;
	}
	/**
	 * @param questionVersionNo the questionVersionNo to set
	 */
	public void setQuestionVersionNo(Integer questionVersionNo) {
		this.questionVersionNo = questionVersionNo;
	}
	/**
	 * @return the answerSerialNo
	 */
	public Integer getAnswerSerialNo() {
		return answerSerialNo;
	}
	/**
	 * @param answerSerialNo the answerSerialNo to set
	 */
	public void setAnswerSerialNo(Integer answerSerialNo) {
		this.answerSerialNo = answerSerialNo;
	}
	/**
	 * @return the pointSerialNo
	 */
	public Integer getPointSerialNo() {
		return pointSerialNo;
	}
	/**
	 * @param pointSerialNo the pointSerialNo to set
	 */
	public void setPointSerialNo(Integer pointSerialNo) {
		this.pointSerialNo = pointSerialNo;
	}
	/**
	 * @return the pointContent
	 */
	public String getPointContent() {
		return pointContent;
	}
	/**
	 * @param pointContent the pointContent to set
	 */
	public void setPointContent(String pointContent) {
		this.pointContent = pointContent;
	}
	/**
	 * @return the pointScore
	 */
	public Integer getPointScore() {
		return pointScore;
	}
	/**
	 * @param pointScore the pointScore to set
	 */
	public void setPointScore(Integer pointScore) {
		this.pointScore = pointScore;
	}
	
	
}
