package com.yds.tt.testing.bean;

import java.util.Date;
import java.util.List;

import com.yds.base.bean.BaseBean;

/**
 * 试卷详细信息Bean
 * 
 * @author sundefu
 * @version 1.0 2010/04/08
 */
public class K060031ExamPaperInfo extends BaseBean {

	private static final long serialVersionUID = -8089974028830849524L;
	/** 毫秒单位. */
	private static final Integer MILLISECONDS = 1000;
	/** 考试Id. */
	private String examineId;
	/** 参加次数. */
	private Integer examineJoinTimes;
	/** 考试剩余时间. */
	private Integer examineTimeLeft;
	/** 开始考试时间. */
	private Date examineStartTime;
	/** 结束考试时间. */
	private Date examineEndTime;
	/** 画面消息. */
	private List<String> screenMessage;
	/** 试卷ID. */
	private String paperId;
	/** 试卷版本号. */
	private Integer paperVersionNo;
	/** 试卷标题. */
	private String paperTitle;
	/** 试卷说明信息. */
	private String paperDescription;
	/** 大题数量. */
	private Integer bigquestionNum;
	/** 大题列表. */
	private List<PaperBigQuestionInfo> bigQuestions;	
	/** 小题列表. */
	private List<K060031ExamQuestionInfo> examQuestions;
	/** 试卷序号. */
	private Integer markingPaperNo;
	/** 试卷数. */
	private Integer paperAllNum;
	
	/**
	 * @return the examineId
	 */
	public String getExamineId() {
		return examineId;
	}
	/**
	 * @param examineId the examineId to set
	 */
	public void setExamineId(String examineId) {
		this.examineId = examineId;
	}
	/**
	 * @return the examineJoinTimes
	 */
	public Integer getExamineJoinTimes() {
		return examineJoinTimes;
	}
	/**
	 * @param examineJoinTimes the examineJoinTimes to set
	 */
	public void setExamineJoinTimes(Integer examineJoinTimes) {
		this.examineJoinTimes = examineJoinTimes;
	}
	/**
	 * @return the examineTimeLeft
	 */
	public Integer getExamineTimeLeft() {
		if (this.getExamineEndTime() != null) {
			Date currentTime = new Date();
			Long timeLeft = this.getExamineEndTime().getTime() - currentTime.getTime();
			examineTimeLeft = timeLeft.intValue()/MILLISECONDS;
			// 考试已结束，剩余时间置0
			if (examineTimeLeft < 0) {
				examineTimeLeft = 0;
			}		
		}
		return examineTimeLeft;
	}
	/**
	 * @param examineTimeLeft the examineTimeLeft to set
	 */
	public void setExamineTimeLeft(Integer examineTimeLeft) {
		this.examineTimeLeft = examineTimeLeft;
	}
	/**
	 * @return the screenMessage
	 */
	public List<String> getScreenMessage() {
		return screenMessage;
	}
	/**
	 * @param screenMessage the screenMessage to set
	 */
	public void setScreenMessage(List<String> screenMessage) {
		this.screenMessage = screenMessage;
	}
	/**
	 * @return the paperId
	 */
	public String getPaperId() {
		return paperId;
	}
	/**
	 * @param paperId the paperId to set
	 */
	public void setPaperId(String paperId) {
		this.paperId = paperId;
	}
	/**
	 * @return the paperVersionNo
	 */
	public Integer getPaperVersionNo() {
		return paperVersionNo;
	}
	/**
	 * @param paperVersionNo the paperVersionNo to set
	 */
	public void setPaperVersionNo(Integer paperVersionNo) {
		this.paperVersionNo = paperVersionNo;
	}
	/**
	 * @return the paperTitle
	 */
	public String getPaperTitle() {
		return paperTitle;
	}
	/**
	 * @param paperTitle the paperTitle to set
	 */
	public void setPaperTitle(String paperTitle) {
		this.paperTitle = paperTitle;
	}
	/**
	 * @return the paperDescription
	 */
	public String getPaperDescription() {
		return paperDescription;
	}
	/**
	 * @param paperDescription the paperDescription to set
	 */
	public void setPaperDescription(String paperDescription) {
		this.paperDescription = paperDescription;
	}
	/**
	 * @return the bigquestionNum
	 */
	public Integer getBigquestionNum() {
		return bigquestionNum;
	}
	/**
	 * @param bigquestionNum the bigquestionNum to set
	 */
	public void setBigquestionNum(Integer bigquestionNum) {
		this.bigquestionNum = bigquestionNum;
	}
	/**
	 * @return the bigQuestions
	 */
	public List<PaperBigQuestionInfo> getBigQuestions() {
		return bigQuestions;
	}
	/**
	 * @param bigQuestions the bigQuestions to set
	 */
	public void setBigQuestions(List<PaperBigQuestionInfo> bigQuestions) {
		this.bigQuestions = bigQuestions;
	}
	/**
	 * @return the examQuestions
	 */
	public List<K060031ExamQuestionInfo> getExamQuestions() {
		return examQuestions;
	}
	/**
	 * @param examQuestions the examQuestions to set
	 */
	public void setExamQuestions(List<K060031ExamQuestionInfo> examQuestions) {
		this.examQuestions = examQuestions;
	}
	/**
	 * @return the examineStartTime
	 */
	public Date getExamineStartTime() {
		return examineStartTime;
	}
	/**
	 * @param examineStartTime the examineStartTime to set
	 */
	public void setExamineStartTime(Date examineStartTime) {
		this.examineStartTime = examineStartTime;
	}
	/**
	 * @return the examineEndTime
	 */
	public Date getExamineEndTime() {
		return examineEndTime;
	}
	/**
	 * @param examineEndTime the examineEndTime to set
	 */
	public void setExamineEndTime(Date examineEndTime) {
		this.examineEndTime = examineEndTime;
	}
	/**
	 * @param markingPaperNo the markingPaperNo to set
	 */
	public void setMarkingPaperNo(Integer markingPaperNo) {
		this.markingPaperNo = markingPaperNo;
	}
	/**
	 * @return the markingPaperNo
	 */
	public Integer getMarkingPaperNo() {
		return markingPaperNo;
	}
	/**
	 * @param paperAllNum the paperAllNum to set
	 */
	public void setPaperAllNum(Integer paperAllNum) {
		this.paperAllNum = paperAllNum;
	}
	/**
	 * @return the paperAllNum
	 */
	public Integer getPaperAllNum() {
		return paperAllNum;
	}		
}
