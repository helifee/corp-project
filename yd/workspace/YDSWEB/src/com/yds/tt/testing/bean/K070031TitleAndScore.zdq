package com.yds.tt.testing.bean;

import com.yds.base.bean.BaseBean;

/**
 * 成绩明细大题标题和总分Bean
 * 
 * @author chenzhong
 * @version 1.0 2010/05/18
 */
public class K070031TitleAndScore extends BaseBean {

	private static final long serialVersionUID = 1L;

	/** 大题标题. */
	private String bigTitle;
	/** 大题总分. */
	private Integer bigScore;
	
	/**
	 * @return the bigTitle
	 */
	public String getBigTitle() {
		return bigTitle;
	}
	/**
	 * @param bigTitle the bigTitle to set
	 */
	public void setBigTitle(String bigTitle) {
		this.bigTitle = bigTitle;
	}
	/**
	 * @return the bigScore
	 */
	public Integer getBigScore() {
		return bigScore;
	}
	/**
	 * @param bigScore the bigScore to set
	 */
	public void setBigScore(Integer bigScore) {
		this.bigScore = bigScore;
	}
}
