package com.yds.tt.testing.bean;

import com.yds.base.bean.BaseBean;

/**
 * 考试评分任务分配信息单表Bean
 * 
 * @author sundefu
 * @version 1.0 2010/04/26
 */
public class MarkTaskAssignInfo extends BaseBean {

	private static final long serialVersionUID = -8932581618480880752L;

	/** 考试ID*/
	private String examineId;
	/** 考试评分者ID*/
	private String examineMarkerId;
	/** 员工ID*/
	private String employeesId;
	/** 大题编号*/
	private Integer bigquestionSerialNo;
	/** 当前评分状态*/
	private Integer markingStatus;
	/** 当前评分者*/
	private String markingEmpid;
	/**
	 * @return the examineId
	 */
	public String getExamineId() {
		return examineId;
	}
	/**
	 * @param examineId the examineId to set
	 */
	public void setExamineId(String examineId) {
		this.examineId = examineId;
	}
	/**
	 * @return the examineMarkerId
	 */
	public String getExamineMarkerId() {
		return examineMarkerId;
	}
	/**
	 * @param examineMarkerId the examineMarkerId to set
	 */
	public void setExamineMarkerId(String examineMarkerId) {
		this.examineMarkerId = examineMarkerId;
	}
	/**
	 * @return the employeesId
	 */
	public String getEmployeesId() {
		return employeesId;
	}
	/**
	 * @param employeesId the employeesId to set
	 */
	public void setEmployeesId(String employeesId) {
		this.employeesId = employeesId;
	}
	/**
	 * @return the bigquestionSerialNo
	 */
	public Integer getBigquestionSerialNo() {
		return bigquestionSerialNo;
	}
	/**
	 * @param bigquestionSerialNo the bigquestionSerialNo to set
	 */
	public void setBigquestionSerialNo(Integer bigquestionSerialNo) {
		this.bigquestionSerialNo = bigquestionSerialNo;
	}
	/**
	 * @return the markingStatus
	 */
	public Integer getMarkingStatus() {
		return markingStatus;
	}
	/**
	 * @param markingStatus the markingStatus to set
	 */
	public void setMarkingStatus(Integer markingStatus) {
		this.markingStatus = markingStatus;
	}
	/**
	 * @return the markingEmpid
	 */
	public String getMarkingEmpid() {
		return markingEmpid;
	}
	/**
	 * @param markingEmpid the markingEmpid to set
	 */
	public void setMarkingEmpid(String markingEmpid) {
		this.markingEmpid = markingEmpid;
	}
}
