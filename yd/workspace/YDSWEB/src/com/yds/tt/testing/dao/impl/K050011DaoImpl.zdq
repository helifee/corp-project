package com.yds.tt.testing.dao.impl;

import java.util.Date;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.tt.testing.bean.TestPaperInfo;
import com.yds.tt.testing.dao.K050011Dao;

/**
 * @see com.yds.tt.testing.dao.K050011Dao
 */
@Repository("k050011Dao")
public class K050011DaoImpl extends AbstractBaseDao implements K050011Dao {

	/**
	 * {@inheritDoc}
	 */
	@Override
	public TestPaperInfo getTestPaperInfo(TestPaperInfo conditionBean) {

		// <管理>模式下检索项目,取得试卷基本信息.
		return (TestPaperInfo) super.queryForObject(
				"K050011Dao.getTestPaperInfo", conditionBean);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertTestPaperInfo(TestPaperInfo testPaperInfo) {

		// 插入试卷表数据处理（<新建试卷>，<参照新建>模式下进行）.
		super.insert("K050011Dao.insertTestPaperInfo", testPaperInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void updateTestPaperInfo(TestPaperInfo testPaperInfo) {

		// 更新试卷表数据处理（<管理>模式下进行）.
		super.update("K050011Dao.updateTestPaperInfo", testPaperInfo);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertCopyTestPaperInfo(
			Map<String, Object> paraMap) {

		// 插入试卷表数据处理（<新建试卷>，<参照新建>模式下进行）.
		super.insert("K050011Dao.insertCopyTestPaperInfo", paraMap);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertCopyPaperBigQuestionInfo(
			Map<String, Object> paraMap) {

		// 插入试卷大题信息（<参照新建>模式下进行）.
		super.insert("K050011Dao.insertCopyPaperBigQuestionInfo", 
				paraMap);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertCopyPaperStableQuestionInfo(
			Map<String, Object> paraMap) {

		// 插入试卷固定大题信息（<参照新建>模式下进行）.
		super.insert("K050011Dao.insertCopyPaperStableQuestionInfo", 
				paraMap);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertCopyPaperRandQuestionInfo(
			Map<String, Object> paraMap) {

		// 插入试卷随机大题信息（<参照新建>模式下进行）.
		super.insert("K050011Dao.insertCopyPaperRandomQuestionInfo",
				paraMap);
	}
	/**
	 * {@inheritDoc}
	 */
	@Override
	public Date getUpdateTime(TestPaperInfo testPaperInfo) {
		
		// 获取最新修改时间
		return (Date)super.queryForObject("K050011Dao.getUpdateTime", 
				testPaperInfo);
	}
}
