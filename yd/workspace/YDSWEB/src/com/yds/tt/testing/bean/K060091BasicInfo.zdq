package com.yds.tt.testing.bean;

import java.util.Date;
import java.util.HashMap;
import java.util.List;
import java.util.Map;

import com.yds.base.bean.BaseBean;

/**
 * 考试阅卷&答案查看基本信息Bean
 * 
 * @author sundefu
 * @version 1.0 2010/04/23
 */
public class K060091BasicInfo extends BaseBean {

	private static final long serialVersionUID = -1536998099216491528L;
	/** 待评分试卷列表. */
	private List<String> paperList;
	/** 待评分大题列表. */
	private List<String> bigquestionList;
	/** 待评分大题Map. */
	private Map<String, String> bigquestionMap;
	/** 考试Id. */
	private String examineId;
	/** 参加次数. */
	private Integer examineJoinTimes;
	/** 试卷ID. */
	private String paperId;
	/** 试卷版本号. */
	private Integer paperVersionNo;
	/** 试卷生成号. */
	private String paperCreateNo;
	/** 试卷生成时间. */
	private Date paperCreateTime;
	/** 大题编号. */
	private Integer bigquestionSerialNo;
	/** 考试员工Id. */
	private String employeeId;
	/** 评分任务分配方式. */
	private String markTaskAsignStyle;
	/** 是否考试实施者. */
	private Boolean ifTestAdvancer;
	/** 是否初期表示. */
	private Boolean ifInit;
	
	/**
	 * @param this bigquestionList
	 */
	public void createBigquestionMap() {
		setBigquestionMap(new HashMap<String, String>());
		for (String bigquestionNo : bigquestionList) {
			bigquestionMap.put(bigquestionNo, bigquestionNo);
		}
	}
	/**
	 * @param the bigquestionSerialNo to get
	 * @return the bigquestionSerialNo
	 */
	public Object getBigquestionIfExist(String bigquestionNo) {
		return bigquestionMap.get(bigquestionNo);
	}
	/**
	 * @return the paperList
	 */
	public List<String> getPaperList() {
		return paperList;
	}
	/**
	 * @param paperList the paperList to set
	 */
	public void setPaperList(List<String> paperList) {
		this.paperList = paperList;
	}
	/**
	 * @return the bigquestionList
	 */
	public List<String> getBigquestionList() {
		return bigquestionList;
	}
	/**
	 * @param bigquestionList the bigquestionList to set
	 */
	public void setBigquestionList(List<String> bigquestionList) {
		this.bigquestionList = bigquestionList;
		createBigquestionMap();
	}
	/**
	 * @return the examineId
	 */
	public String getExamineId() {
		return examineId;
	}
	/**
	 * @param examineId the examineId to set
	 */
	public void setExamineId(String examineId) {
		this.examineId = examineId;
	}
	/**
	 * @return the examineJoinTimes
	 */
	public Integer getExamineJoinTimes() {
		return examineJoinTimes;
	}
	/**
	 * @param examineJoinTimes the examineJoinTimes to set
	 */
	public void setExamineJoinTimes(Integer examineJoinTimes) {
		this.examineJoinTimes = examineJoinTimes;
	}
	/**
	 * @return the paperId
	 */
	public String getPaperId() {
		return paperId;
	}
	/**
	 * @param paperId the paperId to set
	 */
	public void setPaperId(String paperId) {
		this.paperId = paperId;
	}
	/**
	 * @return the employeeId
	 */
	public String getEmployeeId() {
		return employeeId;
	}
	/**
	 * @param employeeId the employeeId to set
	 */
	public void setEmployeeId(String employeeId) {
		this.employeeId = employeeId;
	}
	/**
	 * @return the markTaskAsignStyle
	 */
	public String getMarkTaskAsignStyle() {
		return markTaskAsignStyle;
	}
	/**
	 * @param markTaskAsignStyle the markTaskAsignStyle to set
	 */
	public void setMarkTaskAsignStyle(String markTaskAsignStyle) {
		this.markTaskAsignStyle = markTaskAsignStyle;
	}
	/**
	 * @param ifTestAdvancer the ifTestAdvancer to set
	 */
	public void setIfTestAdvancer(Boolean ifTestAdvancer) {
		this.ifTestAdvancer = ifTestAdvancer;
	}
	/**
	 * @return the ifTestAdvancer
	 */
	public Boolean getIfTestAdvancer() {
		return ifTestAdvancer;
	}
	/**
	 * @return the paperVersionNo
	 */
	public Integer getPaperVersionNo() {
		return paperVersionNo;
	}
	/**
	 * @param paperVersionNo the paperVersionNo to set
	 */
	public void setPaperVersionNo(Integer paperVersionNo) {
		this.paperVersionNo = paperVersionNo;
	}
	/**
	 * @return the paperCreateNo
	 */
	public String getPaperCreateNo() {
		return paperCreateNo;
	}
	/**
	 * @param paperCreateNo the paperCreateNo to set
	 */
	public void setPaperCreateNo(String paperCreateNo) {
		this.paperCreateNo = paperCreateNo;
	}
	/**
	 * @return the paperCreateTime
	 */
	public Date getPaperCreateTime() {
		return paperCreateTime;
	}
	/**
	 * @param paperCreateTime the paperCreateTime to set
	 */
	public void setPaperCreateTime(Date paperCreateTime) {
		this.paperCreateTime = paperCreateTime;
	}
	/**
	 * @return the bigquestionSerialNo
	 */
	public Integer getBigquestionSerialNo() {
		return bigquestionSerialNo;
	}
	/**
	 * @param bigquestionSerialNo the bigquestionSerialNo to set
	 */
	public void setBigquestionSerialNo(Integer bigquestionSerialNo) {
		this.bigquestionSerialNo = bigquestionSerialNo;
	}
	/**
	 * @param ifInit the ifInit to set
	 */
	public void setIfInit(Boolean ifInit) {
		this.ifInit = ifInit;
	}
	/**
	 * @return the ifInit
	 */
	public Boolean getIfInit() {
		return ifInit;
	}
	/**
	 * @param bigquestionMap the bigquestionMap to set
	 */
	public void setBigquestionMap(Map<String, String> bigquestionMap) {
		this.bigquestionMap = bigquestionMap;
	}
	/**
	 * @return the bigquestionMap
	 */
	public Map<String, String> getBigquestionMap() {
		return bigquestionMap;
	}
}
