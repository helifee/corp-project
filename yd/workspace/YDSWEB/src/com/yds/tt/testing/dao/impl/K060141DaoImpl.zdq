package com.yds.tt.testing.dao.impl;

import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.tt.testing.bean.K060141SearchInfo;
import com.yds.tt.testing.dao.K060141Dao;
import com.yds.tt.training.bean.CourseInfo;

/**
 * @see com.yds.tt.testing.dao.K060141Dao
 */
@Repository("k060141Dao")
public class K060141DaoImpl extends AbstractBaseDao implements K060141Dao {

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<CourseInfo> getSearchList(K060141SearchInfo searchInfo,
			int offset, int perCounts) {

		return (List<CourseInfo>) super.queryForList("K060141Dao."
				+ "getK060141SearchInfos", searchInfo, offset, perCounts);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Long getTotalCount(K060141SearchInfo param) {

		return (Long) super.queryForObject("K060141Dao.getK060141TotalCount",
				param);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<String> getHadCourses(String examineId) {

		return super.queryForList("K060141Dao.getHadCourses", examineId);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertNecessaryCourses(Map<String, String> necessaryCourses) {
		super.insert("K060141Dao.insertNecessaryCourses", necessaryCourses);
	}
}
