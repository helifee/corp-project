package com.yds.tt.testing.dao.impl;

import java.util.Date;
import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.tt.testing.bean.ExamEmployeeInfo;
import com.yds.tt.testing.dao.G060010Dao;

/**
 * @see G060010Dao
 */
@Repository("g060010Dao")
public class G060010DaoImpl extends AbstractBaseDao implements G060010Dao{

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<ExamEmployeeInfo> getEmployeeDrop(Date examMaxDeleteTime) {
		
		return (List<ExamEmployeeInfo>) super.queryForList(
				"G060010Dao.getEmployeeDrop", examMaxDeleteTime);
		
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public void deleteDropInfo(ExamEmployeeInfo employeeInfo) {
		
		super.delete("G060010Dao.deleteDropInfo", employeeInfo);
	}

}
