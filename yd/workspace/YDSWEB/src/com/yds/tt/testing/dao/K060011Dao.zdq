package com.yds.tt.testing.dao;

import java.util.Date;
import java.util.List;
import java.util.Map;

import com.yds.common.bean.UserInfo;
import com.yds.tt.testing.bean.ExamineInfo;

/**
 * 考试新建及权限分配（数据库访问）.
 * 
 * @author chenzhong
 * @version 1.00 2010/04/14
 */
public interface K060011Dao {
	
	/**
	 * 考试信息取得
	 * @param examineId
	 *			考试ID
	 * @return ExamineInfo 
	 *   	考试信息
	 */
	public ExamineInfo getInitInfo(String examineId);
	
	/**
	 * 考试信息表数据插入(新建)
	 * @param examineInfo 
	 * 				考试信息
	 */
	public void insertNewExamineInfo(ExamineInfo examineInfo);
	
	/**
	 * 考试信息表数据更新
	 * @param examineInfo 
	 * 				考试信息
	 */
	public void updateExamineInfo(ExamineInfo examineInfo);
	
	/**
	 * 考试信息表数据插入(参照新建)
	 * @param initInfo
	 *			初期信息
	 */
	public void insertReferenceExamineInfo(ExamineInfo examineInfo);

	
	/**
	 * 考试试卷关联表数据插入
	 * @param examineInfo 
	 * 				考试信息
	 */
	public void insertExaminePaperRelation(ExamineInfo examineInfo);
	
	/**
	 * 需要通过考试关联表数据插入
	 * @param examineInfo 
	 * 				考试信息
	 */
	public void insertNecessaryExamine(ExamineInfo examineInfo);
	
	/**
	 * 需要学习课程关联表数据插入
	 * @param examineInfo 
	 * 				考试信息
	 */
	public void insertNecessaryCourse(ExamineInfo examineInfo);
	
	/**
	 * 取得针对对象人员
	 * 
	 * @param examineId
	 *            考试ID
	 * @return List<UserInfo> 针对对象人员
	 */
	public List<UserInfo> getUserList(String examineId);
	
	/**
	 * 插入针对对象人员
	 * 
	 * @param examineAttentionInfo
	 *            考试个人关联信息
	 */	
	public void insertUserList(Map<String,String> examineAttentionInfo);

	/**
	 * 删除针对对象人员
	 * 
	 * @param examineId
	 *            考试ID
	 */	
	public int deleteUserList(String examineId);
	
	/**
	 * 排他用取得最新时间
	 * 
	 * @param examineId
	 *            考试ID
	 * @return Date 最新时间
	 */
	public Date getUpdateTime(String examineId);
}
