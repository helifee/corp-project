package com.yds.tt.testing.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.tt.testing.bean.ExamineInfo;
import com.yds.tt.testing.bean.K060111EmpMarkInfo;
import com.yds.tt.testing.bean.K060111PaperMarkInfo;
import com.yds.tt.testing.bean.TestPaperInfo;
import com.yds.tt.testing.dao.K060111Dao;

/**
 * @see com.yds.tt.testing.dao.K060111Dao
 */
@Repository("k060111Dao")
public class K060111DaoImpl extends AbstractBaseDao implements K060111Dao {

	/**
	 * {@inheritDoc}
	 */
	@Override
	public ExamineInfo getExamineInfo(String examineId) {
		return (ExamineInfo) super.queryForObject("K060111Dao.getExamineInfo", examineId);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<TestPaperInfo> getTestPaperInfos(String examineId) {
		return (List<TestPaperInfo>) super.queryForList("K060111Dao.getTestPaperInfos", examineId);
	}
	/**
	 * {@inheritDoc}
	 */
	@Override
	public Integer getBigQuestCnt(TestPaperInfo testPaperInfo) {		
		return Integer.parseInt((String)super.queryForObject(
						"K060111Dao.getBigQuestCnt", testPaperInfo));
	}
	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<K060111PaperMarkInfo> getPaperMarkInfo(String examineId) {		
		return (List<K060111PaperMarkInfo>) super.queryForList(
				"K060111Dao.getPaperMarkInfos", examineId);
	}
	/**
	 * {@inheritDoc}
	 */
	@Override
	public Integer getNotCompleteCnt(K060111PaperMarkInfo k060111PaperMarkInfo) {		
		return Integer.parseInt((String) super.queryForObject(
				"K060111Dao.getNotCompleteCnt", k060111PaperMarkInfo));
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public Integer getEmpAnswerCnt(K060111PaperMarkInfo paperMarkInfo) {
		return Integer.parseInt((String) super.queryForObject(
				"K060111Dao.getEmpAnswerCnt", paperMarkInfo));
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<K060111EmpMarkInfo> getEmpMarkInfo(String examineId) {

		return (List<K060111EmpMarkInfo>) super.queryForList(
				"K060111Dao.getEmpMarkInfo", examineId);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Integer getNotCompleteBigQuestionCnt(K060111EmpMarkInfo k060111EmpMarkInfo) {
		return Integer.parseInt((String) super.queryForObject(
				"K060111Dao.getNotCompleteBigQuestionCnt", k060111EmpMarkInfo));
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public Integer getAnswerCnt(K060111EmpMarkInfo k060111EmpMarkInfo) {
		return Integer.parseInt((String) super.queryForObject(
				"K060111Dao.getAnswerCnt", k060111EmpMarkInfo));
	}
	
	/* 
	 * {@inheritDoc}
	 */
	@Override
	public void updateExamineInfo(ExamineInfo examineInfo){
		super.update("K060111Dao.updateExamineInfo",examineInfo);		
	}
	/**
	 * {@inheritDoc}
	 */
	@Override
	public void deleteMarkTask(String examineId) {
		super.delete("K060111Dao.deleteMarkTask", examineId);
	}
	
	/**
	 *{@inheritDoc}
	 */
	@Override
	public void insertMarkPaper(K060111PaperMarkInfo paperMarkInfo)  {
		super.insert("K060111Dao.insertMarkPaper", paperMarkInfo);
		
	}
	
	/**
	 *{@inheritDoc}
	 */
	@Override
	public void insertMarkQuestion(K060111EmpMarkInfo empMarkInfo) {
		super.insert("K060111Dao.insertMarkQuestion", empMarkInfo);	
	}

}
