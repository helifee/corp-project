package com.yds.tt.testing.service;

import java.util.List;

import com.yds.tt.manager.bean.TtCheckResult;
import com.yds.tt.testing.bean.ExamineInfo;
import com.yds.tt.testing.bean.K060081EnrollInfo;

/**
 * 考试报名批准信息处理
 * 
 * @author wangqingzhu
 * @version 1.00 2010/04/19
 */
public interface K060081Service {

	/**
	 * 取得考试相关信息
	 * 
	 * @param examineId
	 *            考试ID
	 * @return  ExamineInfo 编辑考试一览信息
	 */
	public ExamineInfo getExamineInfo(String examineId);

	/**
	 * 员工报名信息
	 * 
	 * @param k060081ExamineInfo
	 *            考试相关信息
	 * @return  List<K060081EnrollInfo> 员工报名信息
	 */
	public List<K060081EnrollInfo> getEnrollInfo(ExamineInfo k060081ExamineInfo);
	
	/**
	 * 更新批准信息
	 * 
	 * @param k060081EnrollList
	 *            员工相关信息
	 * @param ExamineInfo
	 * 			  考试相关信息           
	 */
	public void updateSanctionInfo(List<K060081EnrollInfo> k060081EnrollList,ExamineInfo k060081ExamineInfo);

	/**
	 * 取得员工是否通过Flg
	 * 
	 * @param k060081EnrollList
	 *            员工相关信息
	 * @param ExamineInfo
	 * 			  考试相关信息           
	 * @return String 人员信息
	 */
	public String getAllPassFlg(List<K060081EnrollInfo> k060081EnrollList,ExamineInfo k060081ExamineInfo);

	/**
	 * 更新未批准信息
	 * 
	 * @param k060081EnrollList
	 *            员工相关信息
	 * @param ExamineInfo
	 * 			  考试相关信息           
	 */
	public void updateUnsanctionInfo(List<K060081EnrollInfo> k060081EnrollList,ExamineInfo k060081ExamineInfo);

	/**
	 * 画面启动限制检查
	 * 
	 * @param examineId
	 *            考试ID
	 * @return TtCheckResult 启动限制检查结果       
	 */
	public TtCheckResult checkStartupPerm(String examineId);	
	
	/**
	 * 对象权限检查
	 * 
	 * @param examineId
	 *            考试ID
	 * @return Boolean 是否有权限          
	 */
	public Boolean checkObjectPerm(String examineId);
	
		
}
