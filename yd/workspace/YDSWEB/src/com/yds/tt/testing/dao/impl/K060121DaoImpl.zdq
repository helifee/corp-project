package com.yds.tt.testing.dao.impl;

import java.util.HashMap;
import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.tt.testing.bean.K060121SearchInfo;
import com.yds.tt.testing.dao.K060121Dao;

/**
 * @see com.yds.tt.testing.dao.K060121Dao
 */
@Repository("k060121Dao")
public class K060121DaoImpl extends AbstractBaseDao implements K060121Dao{

	/**
	 * {@inheritDoc}
	 */
	@Override
	public int checkExamineMarker(Map<String, String> paraMap) {
		return (Integer)super.queryForObject("K060121Dao.checkExamineMarker", paraMap);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public K060121SearchInfo getExamineDetailInfo(Map<String, String> paraMap, boolean flg) {
		
		if(flg){
			return (K060121SearchInfo)super.queryForObject("K060121Dao.getExamineDetailInfoExecutor", paraMap);
		}
		return (K060121SearchInfo)super.queryForObject("K060121Dao.getExamineDetailInfoMarker", paraMap);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<K060121SearchInfo> getExamineDetailInfoList(String examineId,
			int markmissionFlg) {
		if(markmissionFlg == 1){
			return (List<K060121SearchInfo>) 
				super.queryForList("K060121Dao.getExamineMarkListExecutor1", examineId);
		}else if(markmissionFlg == 2){
			return (List<K060121SearchInfo>) 
				super.queryForList("K060121Dao.getExamineMarkListExecutor2", examineId);
		}
		return (List<K060121SearchInfo>) 
			super.queryForList("K060121Dao.getExamineMarkListExecutor3", examineId);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<K060121SearchInfo> getExamineDetailInfoList(
			Map<String, String> paraMap, int markmissionFlg) {
		if(markmissionFlg == 1){
			return (List<K060121SearchInfo>) 
				super.queryForList("K060121Dao.getExamineMarkListMarker1", paraMap);
		}
		return (List<K060121SearchInfo>) 
			super.queryForList("K060121Dao.getExamineMarkListMarker2", paraMap);
	}

	@Override
	public String getMarkStatus(HashMap<String, Object> pageInfo) {
		// TODO Auto-generated method stub
		return (String) super.queryForObject("K060121Dao.getMarkStatus", pageInfo);
	}
}
