package com.yds.tt.testing.dao.impl;

import java.util.Date;
import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.common.bean.UserInfo;
import com.yds.tt.testing.bean.ExamineInfo;
import com.yds.tt.testing.dao.K060011Dao;

/**
 * @see com.yds.tt.testing.dao.K060011Dao
 */
@Repository("k060011Dao")
public class K060011DaoImpl extends AbstractBaseDao implements K060011Dao{	
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public ExamineInfo getInitInfo(String examineId){
		return (ExamineInfo) super.queryForObject("K060011Dao.getInitInfo",examineId);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertNewExamineInfo(ExamineInfo examineInfo){
		super.insert("K060011Dao.insertNewExamineInfo", examineInfo);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertReferenceExamineInfo(ExamineInfo examineInfo){
		
		super.insert("K060011Dao.insertReferenceExamineInfo", examineInfo);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public void updateExamineInfo(ExamineInfo examineInfo){
		
		super.update("K060011Dao.updateExamineInfo", examineInfo);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertExaminePaperRelation(ExamineInfo examineInfo){
		
		super.insert("K060011Dao.insertExaminePaperRelation", examineInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertNecessaryExamine(ExamineInfo examineInfo){
		
		super.insert("K060011Dao.insertNecessaryExamine", examineInfo);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertNecessaryCourse(ExamineInfo examineInfo){
		
		super.insert("K060011Dao.insertNecessaryCourse", examineInfo);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<UserInfo> getUserList(String examineId) {
		return (List<UserInfo>) super.queryForList("K060011Dao.getUserList", examineId);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public int deleteUserList(String examineId) {
		return super.delete("K060011Dao.deleteUserList", examineId);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertUserList(Map<String, String> examineAttentionInfo) {
		super.insert("K060011Dao.insertUserList", examineAttentionInfo);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public Date getUpdateTime(String examineId) {
		
		// 获取最新修改时间
		return (Date)super.queryForObject("K060011Dao.getUpdateTime", examineId);
	}
}
