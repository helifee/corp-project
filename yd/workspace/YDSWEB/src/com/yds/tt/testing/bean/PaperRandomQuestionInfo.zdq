package com.yds.tt.testing.bean;

import java.util.ArrayList;
import java.util.List;

import com.yds.base.bean.BaseBean;

/**
 * 试卷随机大题表Bean.
 * 
 * @author jiaosunquan
 * @version 1.0 2010/03/24
 */
public class PaperRandomQuestionInfo extends BaseBean {

	private static final long serialVersionUID = 2798781027044572884L;

	/** 试卷ID. */
	private String paperId;
	/** 试卷版本号. */
	private Integer paperVersionNo;
	/** 大题编号. */
	private Integer bigquestionSerialNo;
	/** 条件编号. */
	private Integer conditionSerialNo;
	/** 一级分类ID. */
	private Integer category1Id;
	/** 二级分类ID. */
	private Integer category2Id;
	/** 三级分类ID. */
	private Integer category3Id;
	/** 关键字. */
	private String keyword;
	/** 试题难度. */
	private Integer questionDifficulty;
	/** 出题数量. */
	private Integer questionNum;
	/** 试题分数. */
	private Integer questionScore;
	/** 题型. */
	private Integer questionKind;
	/** 题库试题数量. */
	private Long questionBaseCount;
	/** 关键字列表. */
	private List<String> keywordList;

	// ADD BY 李彦蕊 K050051 START

	/** 分类名. */
	private String categoryName;
	/** 试题难度名称. */
	private String questionDifficultyName;

	// ADD BY 李彦蕊 K050051 END

	// ADD BY 石岩岩 K060151 START

	/** 试卷序号标志. */
	private String paperNo;

	// ADD BY 石岩岩 K060151 END

	/**
	 * @return the paperId
	 */
	public String getPaperId() {
		return paperId;
	}

	/**
	 * @param paperId
	 *            the paperId to set
	 */
	public void setPaperId(String paperId) {
		this.paperId = paperId;
	}

	/**
	 * @return the paperVersionNo
	 */
	public Integer getPaperVersionNo() {
		return paperVersionNo;
	}

	/**
	 * @param paperVersionNo
	 *            the paperVersionNo to set
	 */
	public void setPaperVersionNo(Integer paperVersionNo) {
		this.paperVersionNo = paperVersionNo;
	}

	/**
	 * @return the bigquestionSerialNo
	 */
	public Integer getBigquestionSerialNo() {
		return bigquestionSerialNo;
	}

	/**
	 * @param bigquestionSerialNo
	 *            the bigquestionSerialNo to set
	 */
	public void setBigquestionSerialNo(Integer bigquestionSerialNo) {
		this.bigquestionSerialNo = bigquestionSerialNo;
	}

	/**
	 * @return the conditionSerialNo
	 */
	public Integer getConditionSerialNo() {
		return conditionSerialNo;
	}

	/**
	 * @param conditionSerialNo
	 *            the conditionSerialNo to set
	 */
	public void setConditionSerialNo(Integer conditionSerialNo) {
		this.conditionSerialNo = conditionSerialNo;
	}

	/**
	 * @return the category1Id
	 */
	public Integer getCategory1Id() {
		return category1Id;
	}

	/**
	 * @param category1Id
	 *            the category1Id to set
	 */
	public void setCategory1Id(Integer category1Id) {
		this.category1Id = category1Id;
	}

	/**
	 * @return the category2Id
	 */
	public Integer getCategory2Id() {
		return category2Id;
	}

	/**
	 * @param category2Id
	 *            the category2Id to set
	 */
	public void setCategory2Id(Integer category2Id) {
		this.category2Id = category2Id;
	}

	/**
	 * @return the category3Id
	 */
	public Integer getCategory3Id() {
		return category3Id;
	}

	/**
	 * @param category3Id
	 *            the category3Id to set
	 */
	public void setCategory3Id(Integer category3Id) {
		this.category3Id = category3Id;
	}

	/**
	 * @return the keyword
	 */
	public String getKeyword() {
		return keyword;
	}

	/**
	 * @param keyword
	 *            the keyword to set
	 */
	public void setKeyword(String keyword) {
		this.keyword = keyword;
	}

	/**
	 * @return the questionDifficulty
	 */
	public Integer getQuestionDifficulty() {
		return questionDifficulty;
	}

	/**
	 * @param questionDifficulty
	 *            the questionDifficulty to set
	 */
	public void setQuestionDifficulty(Integer questionDifficulty) {
		this.questionDifficulty = questionDifficulty;
	}

	/**
	 * @return the questionNum
	 */
	public Integer getQuestionNum() {
		return questionNum;
	}

	/**
	 * @param questionNum
	 *            the questionNum to set
	 */
	public void setQuestionNum(Integer questionNum) {
		this.questionNum = questionNum;
	}

	/**
	 * @return the questionScore
	 */
	public Integer getQuestionScore() {
		return questionScore;
	}

	/**
	 * @param questionScore
	 *            the questionScore to set
	 */
	public void setQuestionScore(Integer questionScore) {
		this.questionScore = questionScore;
	}

	/**
	 * @return the questionKind
	 */
	public Integer getQuestionKind() {
		return questionKind;
	}

	/**
	 * @param questionKind
	 *            the questionKind to set
	 */
	public void setQuestionKind(Integer questionKind) {
		this.questionKind = questionKind;
	}

	/**
	 * @return the questionBaseCount
	 */
	public Long getQuestionBaseCount() {
		return questionBaseCount;
	}

	/**
	 * @param questionBaseCount
	 *            the questionBaseCount to set
	 */
	public void setQuestionBaseCount(Long questionBaseCount) {
		this.questionBaseCount = questionBaseCount;
	}

	/**
	 * @param keywordList
	 *            the keywordList to set
	 */
	public void setKeywordList(List<String> keywordList) {
		this.keywordList = keywordList;
	}

	/**
	 * @return the keywordList
	 */
	public List<String> getKeywordList() {
		if (keywordList != null && keywordList.size() != 0) {
			return keywordList;
		}
		if (this.keyword == null || this.keyword.isEmpty()) {
			return keywordList;
		}
		keywordList = new ArrayList<String>();
		String[] keyWordArray = this.keyword.split(" ");
		for (String singleKeyword : keyWordArray) {
			keywordList.add(singleKeyword);
		}

		return keywordList;
	}

	/**
	 * @param category3Name
	 *            the category3Name to set
	 */
	public void setCategoryName(String categoryName) {
		this.categoryName = categoryName;
	}

	/**
	 * @return the category3Name
	 */
	public String getCategoryName() {
		return categoryName;
	}

	/**
	 * @return the paperNo
	 */
	public String getPaperNo() {
		return paperNo;
	}

	/**
	 * @param paperNo
	 *            the paperNo to set
	 */
	public void setPaperNo(String paperNo) {
		this.paperNo = paperNo;
	}

	/**
	 * @return the questionDifficultyName
	 */
	public String getQuestionDifficultyName() {
		return questionDifficultyName;
	}

	/**
	 * @param questionDifficultyName
	 *            the questionDifficultyName to set
	 */
	public void setQuestionDifficultyName(String questionDifficultyName) {
		this.questionDifficultyName = questionDifficultyName;
	}

}
