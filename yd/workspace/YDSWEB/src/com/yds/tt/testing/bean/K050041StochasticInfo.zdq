package com.yds.tt.testing.bean;

import com.yds.base.bean.BaseBean;

/**
 * 试卷试题编辑随机大题Bean
 * 
 * @author guozhizhou
 * @version 1.0 2010/04/30
 */
public class K050041StochasticInfo extends BaseBean {

	/**
	 * 
	 */
	private static final long serialVersionUID = 701177909486961601L;

	/** 试卷信息. */
	private String keyword;

	/** 随机大题出题数量. */
	private String questionNum;

	/** 随机大题试题分数. */
	private String questionScore;

	/**
	 * @return the keyword
	 */
	public final String getKeyword() {
		return keyword;
	}

	/**
	 * @param keyword the keyword to set
	 */
	public final void setKeyword(String keyword) {
		this.keyword = keyword;
	}

	/**
	 * @return the questionNum
	 */
	public final String getQuestionNum() {
		return questionNum;
	}

	/**
	 * @param questionNum the questionNum to set
	 */
	public final void setQuestionNum(String questionNum) {
		this.questionNum = questionNum;
	}

	/**
	 * @return the questionScore
	 */
	public final String getQuestionScore() {
		return questionScore;
	}

	/**
	 * @param questionScore the questionScore to set
	 */
	public final void setQuestionScore(String questionScore) {
		this.questionScore = questionScore;
	}


}
