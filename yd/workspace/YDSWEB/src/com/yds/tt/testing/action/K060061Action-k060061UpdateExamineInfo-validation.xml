<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
	<field name="examineInfo.examineName">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"考试名称"})}</message>
		</field-validator>
	</field>
		
	<field name="examineInfo.examineComment">
		<field-validator type="stringlength">
			<param name="maxLength">200</param>
			<message>${getText("yds.com.warning.0007","",{"考试说明","200"})}</message>
		</field-validator>
	</field>
	
	<field name="examineInfo.resultlevel1Score">
		<field-validator type="regex">
			<param name="expression">^[1-9]\d*$</param>
			<message>${getText("yds.tt.error.KSE54","",{"评定等级分数"})}</message>
		</field-validator>
	</field>
	
