package com.yds.tt.testing.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.tt.testing.bean.ExamEmployeeInfo;
import com.yds.tt.testing.bean.K060031ExamQuestionInfo;
import com.yds.tt.testing.bean.K060151PaperInfo;
import com.yds.tt.testing.bean.PaperRandomQuestionInfo;
import com.yds.tt.testing.dao.K060151Dao;

/**
 * @see com.yds.tt.testing.dao.K060151Dao
 */
@Repository("k060151Dao")
public class K060151DaoImpl extends AbstractBaseDao implements K060151Dao {

	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<K060151PaperInfo> getPaperInfos(String examineId) {

		return (List<K060151PaperInfo>) super.queryForList(
				"K060151Dao.getPaperInfos", examineId);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<ExamEmployeeInfo> getEmployeesInfos(String examineId) {

		return (List<ExamEmployeeInfo>) super.queryForList(
				"K060151Dao.getEmployeesInfos", examineId);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public Integer getRandomConditionNum(
			PaperRandomQuestionInfo paperRandomQuestionInfo) {
		return (Integer) super.queryForObject("K060151Dao.getRandomConditionNum",
				paperRandomQuestionInfo);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public void updateExamEmployeesInfo(ExamEmployeeInfo paperInfo) {

		super.update("K060151Dao.updateExamEmployeesInfo", paperInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Integer getPaperIdUsedSum(ExamEmployeeInfo examEmployeeInfo) {

		return (Integer) super.queryForObject("K060151Dao.getPaperIdUsedSum",
				examEmployeeInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertExamQuestionInfo(K060151PaperInfo paperInfo) {

		super.insert("K060151Dao.insertExamQuestionInfo", paperInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void updateExamInfo(String examineId) {

		super.update("K060151Dao.updateExamInfo", examineId);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<K060031ExamQuestionInfo> getRandomQuestions(
			PaperRandomQuestionInfo paperRandomQuestionInfo) {
		return (List<K060031ExamQuestionInfo>) super.queryForList(
				"K060151Dao.getRandomQuestions", paperRandomQuestionInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Integer deleteExamQuestionInfo(ExamEmployeeInfo examEmployeeInfo) {
		
		return super.delete("K060151Dao.deleteExamQuestionInfo", examEmployeeInfo);
	}
}
