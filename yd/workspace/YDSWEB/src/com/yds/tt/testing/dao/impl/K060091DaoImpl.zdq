package com.yds.tt.testing.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.tt.testing.bean.ExamEmployeeInfo;
import com.yds.tt.testing.bean.K060091BasicInfo;
import com.yds.tt.testing.bean.MarkTaskAssignInfo;
import com.yds.tt.testing.dao.K060091Dao;

/**
 * @see com.yds.tt.testing.dao.K060091Dao
 * 
 */
@Repository("k060091Dao")
public class K060091DaoImpl extends AbstractBaseDao implements K060091Dao {

	@SuppressWarnings("unchecked")
	@Override
	public List<MarkTaskAssignInfo> getMarkTaskAssignInfos(
			K060091BasicInfo k060091BasicInfo) {

		return (List<MarkTaskAssignInfo>) super.queryForList(
				"K060091Dao.getMarkTaskAssignInfos", k060091BasicInfo);
	}
	
	@SuppressWarnings("unchecked")
	@Override
	public List<ExamEmployeeInfo> getExamEmployeeInfos (ExamEmployeeInfo examEmployeeInfo) {
		return (List<ExamEmployeeInfo>) super.queryForList(
				"K060091Dao.getExamEmpInfos", examEmployeeInfo);
	}

}
