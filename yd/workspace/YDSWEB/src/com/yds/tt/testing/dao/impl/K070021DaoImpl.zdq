package com.yds.tt.testing.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.tt.testing.bean.K070021ScoreListInfo;
import com.yds.tt.testing.dao.K070021Dao;

/**
 * @see com.yds.tt.testing.dao.K070021Dao
 */
@Repository("k070021Dao")
public class K070021DaoImpl extends AbstractBaseDao implements K070021Dao{

	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<K070021ScoreListInfo> getScoreInfoListAll(String id) {
		
		return (List<K070021ScoreListInfo>) super.queryForList(
				"K070021Dao.getScoreListAll",id);
	}
	
	@Override
	@SuppressWarnings("unchecked")	
	public List<K070021ScoreListInfo> getScoreInfoListPart(String id) {
		
		return (List<K070021ScoreListInfo>) super.queryForList(
				"K070021Dao.getScoreListPart",id);	
	}
	
}
