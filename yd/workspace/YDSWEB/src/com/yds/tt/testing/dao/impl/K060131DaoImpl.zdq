package com.yds.tt.testing.dao.impl;

import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.tt.testing.bean.ExamineInfo;
import com.yds.tt.testing.bean.K060131SearchInfo;
import com.yds.tt.testing.dao.K060131Dao;

/**
 * @see com.yds.tt.testing.dao.K060131Dao
 */
@Repository("k060131Dao")
public class K060131DaoImpl extends AbstractBaseDao implements K060131Dao {

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<ExamineInfo> getSearchList(K060131SearchInfo param, int offset,
			int perCounts) {

		return (List<ExamineInfo>) super.queryForList(
				"K060131Dao.getSearchInfos", param, offset, perCounts);

	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Long getTotalCount(K060131SearchInfo param) {

		return (Long) super.queryForObject("K060131Dao.getTotalCount", param);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<String> getHadExamines(String examineId) {

		return super.queryForList("K060131Dao.getHadExamines", examineId);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertNecessaryExamines(Map<String, String> necessaryExamines) {
		super.insert("K060131Dao.insertNecessaryExamines", necessaryExamines);
	}
}
