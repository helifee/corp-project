package com.yds.tt.training.bean;

import com.yds.base.bean.BaseBean;
/**
 * 课程详细(用户)关注度信息Bean
 * 
 * @author chenjunshuai
 * @version 1.0 2010/03/26
 */
public class CourseAttentionInfo extends BaseBean{

	private static final long serialVersionUID = 8657419724899961090L;
	
	/** 课程ID. */
	private String courseId;
	
	/** 员工ID. */
	private String employeesId;
	
	/** 关注度. */
	private Integer attentionFlag;
	
	/**
	 * @return the courseId
	 */
	public String getCourseId() {
		return courseId;
	}
	
	/**
	 * @param courseId the courseId to set
	 */
	public void setCourseId(String courseId) {
		this.courseId = courseId;
	}
	
	/**
	 * @return the employeesId
	 */
	public String getEmployeesId() {
		return employeesId;
	}
	
	/**
	 * @param employeesId the employeesId to set
	 */
	public void setEmployeesId(String employeesId) {
		this.employeesId = employeesId;
	}
	
	/**
	 * @return the attentionFlag
	 */
	public Integer getAttentionFlag() {
		return attentionFlag;
	}
	
	/**
	 * @param attentionFlag the attentionFlag to set
	 */
	public void setAttentionFlag(Integer attentionFlag) {
		this.attentionFlag = attentionFlag;
	}
}
