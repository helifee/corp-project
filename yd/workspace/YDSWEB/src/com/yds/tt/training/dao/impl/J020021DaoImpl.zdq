package com.yds.tt.training.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.tt.training.bean.CourseInfo;
import com.yds.tt.training.bean.J020021SearchInfo;
import com.yds.tt.training.dao.J020021Dao;

/**
 * @see com.yds.tt.training.dao.J020021Dao
 */
@Repository("j020021Dao")
public class J020021DaoImpl extends AbstractBaseDao implements J020021Dao {

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Long selectCountCourseInfo(J020021SearchInfo courseEditableInfo) {
		return  (Long)super.queryForObject("J020021Dao.selectCountCourseInfo", courseEditableInfo);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<CourseInfo> getCourseEditableInfoList(J020021SearchInfo courseEditableInfo, int offset, int perCounts) {
		return (List<CourseInfo>) super.queryForList(
				"J020021Dao.getCourseEditableInfoList",courseEditableInfo, offset, perCounts);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<CourseInfo> getCourseConfirmableInfoList(
			J020021SearchInfo courseConfirmableInfo) {
		return (List<CourseInfo>) super.queryForList(
				"J020021Dao.getCourseConfirmableInfoList",courseConfirmableInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<CourseInfo> getCoursePaticipateInfoList(
			J020021SearchInfo coursePaticipateInfo) {
		return (List<CourseInfo>) super.queryForList(
				"J020021Dao.getCoursePaticipateInfoList",coursePaticipateInfo);
	}

}
