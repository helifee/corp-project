<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
	<field name="manaCourseInfo.courseName">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"课程名称"})}</message>
		</field-validator>
	</field>
	<field name="manaCourseInfo.courseName">
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">100</param>
			<message>${getText("yds.com.warning.0007","",{"课程名称", maxLength})}</message>
		</field-validator>
	</field>
	<field name="manaCourseInfo.courseAbstract">
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">400</param>
			<message>${getText("yds.com.warning.0007","",{"课程简介", maxLength})}</message>
		</field-validator>
	</field>
</validators>