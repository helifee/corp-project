package com.yds.tt.manager.service;

/**
 * 存在双key锁时抛出的异常.
 *
 * @author zhanghaibo
 * @version 1.00 2010/05/28
 */
public class TtExclusiveExtException extends RuntimeException {

	private static final long serialVersionUID = -855805224035229869L;
	private Object messageBean;
	
	/**
	 * @return the messageBean
	 */
	public Object getMessageBean() {
		return messageBean;
	}
	
	public TtExclusiveExtException(String message){
		super(message);
	}

	public TtExclusiveExtException(Object messageBean){
		this.messageBean = messageBean;
	}

}
