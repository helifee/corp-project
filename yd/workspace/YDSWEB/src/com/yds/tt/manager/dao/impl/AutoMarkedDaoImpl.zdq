package com.yds.tt.manager.dao.impl;

import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.tt.manager.dao.AutoMarkedDao;
import com.yds.tt.testing.bean.ExamEmployeeInfo;
import com.yds.tt.testing.bean.K060031SearchInfo;
import com.yds.tt.testing.bean.K060031SingleQuesInfo;
import com.yds.tt.testing.bean.TestEmployeeAnswerInfo;

/**
 * @see AutoMarkedDao
 */
@Repository("autoMarkedDao")
public class AutoMarkedDaoImpl extends AbstractBaseDao implements AutoMarkedDao {

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<TestEmployeeAnswerInfo> getexEmeeAnswerInfo(
			ExamEmployeeInfo examEmployeeInfo) {
		return (List<TestEmployeeAnswerInfo>) super.queryForList(
				"AutoMarkedDao.getexEmeeAnswerInfo", examEmployeeInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public Map<String, Object> getCountContentByKey(
			TestEmployeeAnswerInfo teAmswInfo) {
		return (Map<String, Object>) super.queryForObject(
				"AutoMarkedDao.getCountContentByKey", teAmswInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void updateExamineAnswer(TestEmployeeAnswerInfo teAmswInfo) {
		super.update("AutoMarkedDao.updateExamineAnswer", teAmswInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertExamineAnswer(TestEmployeeAnswerInfo teAmswInfo) {
		super.insert("AutoMarkedDao.insertExamineAnswer", teAmswInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Integer getFixedScore(K060031SearchInfo searchInfo) {
		return (Integer) super.queryForObject("AutoMarkedDao.getFixedScore",
				searchInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Integer getItemBankScore(K060031SearchInfo searchInfo) {
		return (Integer) super.queryForObject("AutoMarkedDao.getItemBankScore",
				searchInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<K060031SingleQuesInfo> getSingleQuesInfo(
			K060031SearchInfo searchInfo) {
		return (List<K060031SingleQuesInfo>) super.queryForList(
				"AutoMarkedDao.getSingleQuesInfo", searchInfo);
	}

}
