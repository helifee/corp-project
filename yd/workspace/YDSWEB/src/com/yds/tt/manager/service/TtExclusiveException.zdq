package com.yds.tt.manager.service;

/**
 * TT排他异常.
 *
 * @author qianguorong
 * @version 1.00 2010/04/22
 */
public class TtExclusiveException extends RuntimeException {
	
	private static final long serialVersionUID = 1L;
	private Object messageBean;
	
	/**
	 * @return the messageBean
	 */
	public Object getMessageBean() {
		return messageBean;
	}

	public TtExclusiveException(){}
	
	public TtExclusiveException(String message){
		super(message);
	}
	
	public TtExclusiveException(Object messageBean){
		this.messageBean = messageBean;
	}
}
