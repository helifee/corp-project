package com.yds.tt.manager.dao;

import java.util.List;
import java.util.Map;

import com.yds.tt.testing.bean.ExamEmployeeInfo;
import com.yds.tt.testing.bean.K060031SearchInfo;
import com.yds.tt.testing.bean.K060031SingleQuesInfo;
import com.yds.tt.testing.bean.TestEmployeeAnswerInfo;

/**
 * 评分处理（数据库访问）
 * 
 * @author wangqingzhu
 * @version 1.00 2010/05/06
 */
public interface AutoMarkedDao{

	/**
	 * 取得考试员工答卷信息的
	 * 
	 * @param ExamEmployeeInfo
	 *            考试信息
	 * @return List<TestEmployeeAnswerInfo> 考试员工答卷信息
	 */	
	public List<TestEmployeeAnswerInfo> getexEmeeAnswerInfo(ExamEmployeeInfo examEmployeeInfo);
	
	/**
	 * 取得考试员工答卷信息条数和考生答案内容
	 * 
	 * @param TestEmployeeAnswerInfo
	 *            考试员工答卷信息
	 * @return Map<String, Object> 考试员工答卷信息条数和考生答案内容
	 */	
	public Map<String, Object> getCountContentByKey(TestEmployeeAnswerInfo teAmswInfo);

	/**
	 * 更新考试员工答卷表
	 * 
	 * @param TestEmployeeAnswerInfo
	 *            考试员工答卷表信息
	 * @return
	 */
	public void updateExamineAnswer(TestEmployeeAnswerInfo teAmswInfo);

	/**
	 * 追加考试员工答卷信息
	 * 
	 * @param TestEmployeeAnswerInfo
	 *            考试员工答卷表信息
	 * @return
	 */
	public void insertExamineAnswer(TestEmployeeAnswerInfo teAmswInfo);
	
	/**
	 * 取得组合题单题信息
	 * 
	 * @param K060031SearchInfo
	 *            考试试题信息
	 * @return List<K060031SingleQuesInfo> 组合题单题信息
	 */	
	public List<K060031SingleQuesInfo> getSingleQuesInfo(K060031SearchInfo searchInfo);
	
	/**
	 * 取得题库试题分数
	 * 
	 * @param K060031SearchInfo
	 *            考试试题信息
	 * @return Integer 题库试题分数
	 */	
	public Integer getItemBankScore(K060031SearchInfo k631SearchInfo);
	
	/**
	 * 取得试卷固定大题试题分数
	 * 
	 * @param K060031SearchInfo
	 *            考试试题信息
	 * @return Integer 试卷固定大题试题分数
	 */	
	public Integer getFixedScore(K060031SearchInfo k631SearchInfo);
	

	
}
