package com.yds.tt.manager.bean;

import java.util.ArrayList;
import java.util.HashMap;

/**
 * 检查结果bean
 *
 * @author yukunpeng
 * @version 1.0 2010/03/30
  */
public class TtCheckResult {
	/** 检查结果flag. */
	private Boolean retFlag; 
	/** 检查结果信息. */
	private String retMessage;
	/** 错误等级 */
	private String errLevel;
	/** 用户权限Map */
	private HashMap<String,ArrayList<Integer>> checkResultMap;
	
	/**
	 * @return the retFlag
	 */
	public Boolean getRetFlag() {
		return retFlag;
	}
	/**
	 * @param retFlag the retFlag to set
	 */
	public void setRetFlag(Boolean retFlag) {
		this.retFlag = retFlag;
	}
	/**
	 * @return the retMessage
	 */
	public String getRetMessage() {
		return retMessage;
	}
	/**
	 * @param retMessage the retMessage to set
	 */
	public void setRetMessage(String retMessage) {
		this.retMessage = retMessage;
	}
	/**
	 * @return the errLevel
	 */
	public String getErrLevel() {
		return errLevel;
	}
	/**
	 * @param errLevel the errLevel to set
	 */
	public void setErrLevel(String errLevel) {
		this.errLevel = errLevel;
	}

	/**
	 * @param checkResultMap the checkResultMap to set
	 */
	public void setCheckResultMap(HashMap<String, ArrayList<Integer>> checkResultMap) {
		this.checkResultMap = checkResultMap;
	}
	/** 
	 *  根据对象ID取得其的具体权限.
	 *  @param objectId 对象ID
	 *  @return 权限List
	 */
	public ArrayList<Integer> getAuthorityList(String objectId) {
		if(null == checkResultMap) {
			return null;
		}
		return checkResultMap.get(objectId);
	}
}

