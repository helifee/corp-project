package com.yds.tt.manager.bean;

import java.util.Date;

import com.yds.base.bean.BaseBean;

/**
 * 提醒消息Bean
 * 
 * @author zhanghaibo
 * @version 1.0 2010/04/06
 */
public class MessageReminding extends BaseBean {

	private static final long serialVersionUID = -8312904945334004087L;
	/** 消息ID. */
	private String informationId;
	/** 用户ID. */
	private String userId;
	/** 消息模式. */
	private String mode;
	/** 消息参数. */
	private String parameter;
	/** 提醒内容. */
	private String content;
	/** 失效时间. */
	private Date losetime;
	/** 消息FLG. */
	private Integer informationFlg;
	/** 拼接后的URL. */
	private String redirectUrl;
	@Deprecated
	private String relatedobjectLink;

	/**
	 * @return the informationId
	 */
	public String getInformationId() {
		return informationId;
	}

	/**
	 * @param informationId
	 *            the informationId to set
	 */
	public void setInformationId(String informationId) {
		this.informationId = informationId;
	}

	/**
	 * @return the userId
	 */
	public String getUserId() {
		return userId;
	}

	/**
	 * @param userId
	 *            the userId to set
	 */
	public void setUserId(String userId) {
		this.userId = userId;
	}

	/**
	 * @return the mode
	 */
	public String getMode() {
		return mode;
	}

	/**
	 * @param mode
	 *            the mode to set
	 */
	public void setMode(String mode) {
		this.mode = mode;
	}

	/**
	 * @return the parameter
	 */
	public String getParameter() {
		return parameter;
	}

	/**
	 * @param parameter
	 *            the parameter to set
	 */
	public void setParameter(String parameter) {
		this.parameter = parameter;
	}

	/**
	 * @return the content
	 */
	public String getContent() {
		return content;
	}

	/**
	 * @param content
	 *            the content to set
	 */
	public void setContent(String content) {
		this.content = content;
	}

	/**
	 * @return the losetime
	 */
	public Date getLosetime() {
		return losetime;
	}

	/**
	 * @param losetime
	 *            the losetime to set
	 */
	public void setLosetime(Date losetime) {
		this.losetime = losetime;
	}

	/**
	 * @return the informationFlg
	 */
	public Integer getInformationFlg() {
		return informationFlg;
	}

	/**
	 * @param informationFlg
	 *            the informationFlg to set
	 */
	public void setInformationFlg(Integer informationFlg) {
		this.informationFlg = informationFlg;
	}

	/**
	 * @return the relatedobjectLink
	 */
	@Deprecated
	public String getRelatedobjectLink() {
		return relatedobjectLink;
	}

	/**
	 * @param relatedobjectLink the relatedobjectLink to set
	 */
	@Deprecated
	public void setRelatedobjectLink(String relatedobjectLink) {
		this.relatedobjectLink = relatedobjectLink;
	}

	/**
	 * @return the redirectUrl
	 */
	public String getRedirectUrl() {
		return redirectUrl;
	}

	/**
	 * @param redirectUrl the redirectUrl to set
	 */
	public void setRedirectUrl(String redirectUrl) {
		this.redirectUrl = redirectUrl;
	}

}
