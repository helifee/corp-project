package com.yds.tt.manager.bean;

import java.util.Date;

import com.yds.base.bean.BaseBean;

/**
 * 系统维护信息业务Bean
 * 
 * @author zhanghaibo
 * @version 1.0 2010/05/21
 */
public class SystemMaintainInfo extends BaseBean {

	private static final long serialVersionUID = 96518275955623477L;
	/** 维护标识 */
	private String maintainFlag;
	/** 系统维护登录者 */
	private String maintainer;
	/** 系统维护登录时间 */
	private Date maintainerLoginTime;
	/** 系统维护登录时间（更新用） */
	private String userLoginTime;

	/**
	 * @return the maintainFlag
	 */
	public String getMaintainFlag() {
		return maintainFlag;
	}

	/**
	 * @param maintainFlag
	 *            the maintainFlag to set
	 */
	public void setMaintainFlag(String maintainFlag) {
		this.maintainFlag = maintainFlag;
	}

	/**
	 * @return the maintainer
	 */
	public String getMaintainer() {
		return maintainer;
	}

	/**
	 * @param maintainer
	 *            the maintainer to set
	 */
	public void setMaintainer(String maintainer) {
		this.maintainer = maintainer;
	}

	/**
	 * @return the maintainerLoginTime
	 */
	public Date getMaintainerLoginTime() {
		return maintainerLoginTime;
	}

	/**
	 * @param maintainerLoginTime
	 *            the maintainerLoginTime to set
	 */
	public void setMaintainerLoginTime(Date maintainerLoginTime) {
		this.maintainerLoginTime = maintainerLoginTime;
	}

	/**
	 * @return the userLoginTime
	 */
	public String getUserLoginTime() {
		return userLoginTime;
	}

	/**
	 * @param userLoginTime the userLoginTime to set
	 */
	public void setUserLoginTime(String userLoginTime) {
		this.userLoginTime = userLoginTime;
	}
}
