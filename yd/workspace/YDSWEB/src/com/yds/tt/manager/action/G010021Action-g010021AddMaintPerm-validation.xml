<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
	<field name="g010021aInfo.userIdNum">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"管理员ID"})}</message>
		</field-validator>
		<field-validator type="stringlength" short-circuit="true">
			<param name="minLength">6</param>
			<param name="maxLength">6</param>
			<message>${getText("yds.com.warning.0003","",{"管理员ID","6"})}</message>
		</field-validator>
		<field-validator type="regex">
			<param name="expression">\d{6}</param>
			<message>${getText("yds.com.warning.0002","",{"管理员ID"})}</message>
		</field-validator>
	</field>
	<field name="g010021aInfo.startTimeNm">
		<field-validator type="required">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"有效时间开始时间"})}</message>
		</field-validator>
	</field>
</validators>