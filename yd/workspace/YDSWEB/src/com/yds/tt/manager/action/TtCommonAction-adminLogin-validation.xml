<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
	<field name="adminInfo.adminId">
		<field-validator type="requiredstring">
			<message>${getText("yds.com.warning.0001","",{"超级管理员帐号"})}</message>
		</field-validator>
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">20</param>
			<message>${getText("yds.com.warning.0011","",{"超级管理员帐号","20"})}</message>
		</field-validator>
	</field>
	<field name="adminInfo.adminPw">
		<field-validator type="requiredstring">
			<message>${getText("yds.com.warning.0001","",{"超级管理员密码"})}</message>
		</field-validator>
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">20</param>
			<message>${getText("yds.com.warning.0011","",{"超级管理员密码","20"})}</message>
		</field-validator>
	</field>
</validators>