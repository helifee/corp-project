package com.yds.common.action;

import org.springframework.beans.factory.config.BeanDefinition;
import org.springframework.context.annotation.Scope;
import org.springframework.stereotype.Controller;
import com.yds.base.action.AbstractBaseAction;
import com.yds.base.action.PageLockInter;


/**
 * 页面排他自动定时检测Action
 * 
 * @author fengliang
 * @version 1.00 2010/05/31
 */
@Scope(BeanDefinition.SCOPE_PROTOTYPE)
@Controller("pageArrangesAction")
public class PageArrangesAction extends AbstractBaseAction implements PageLockInter{
	
	private static final long serialVersionUID = 2835887590132360020L;	
	private String lockId; // 画面排它锁ID	
	private String pageLockUserName = ""; // 当前在线的用户名	
	private String proType = "autoCheck"; // 拦截器内流程的控制:initPro:初次进入；updatePro:进入页面后进行的操作；autoPro:定时检测
    private String pageName; // 页面名称
    
	public String getLockId() {
		return lockId;
	}
	public void setLockId(String lockId) {
		this.lockId = lockId;
	}	
	public String getProType() {
		return proType;
	}
	public String getPageLockUserName() {
		return pageLockUserName;
	}
	public void setPageLockUserName(String pageLockUserName) {
		this.pageLockUserName = pageLockUserName;
	}
	public String getPageName() {
		return pageName;
	}
	public void setPageName(String pageName) {
		this.pageName = pageName;
	}
}
