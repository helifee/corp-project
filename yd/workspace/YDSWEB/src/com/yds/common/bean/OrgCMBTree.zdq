package com.yds.common.bean;

import com.yds.base.bean.BaseBean;

/**
 * 部门信息树Bean
 * 
 * @author sun
 * @version 1.0 2010/7/26
 */
public class OrgCMBTree extends BaseBean implements TreeListResult {

	private static final long serialVersionUID = 7497514503831697870L;

	/** 一级组织ID. */	
	private String orgLev1Id;

	/** 一级组织名称. */
	private String orgLev1Nm;

	/** 二级组织ID. */	
	private String orgLev2Id;

	/** 二级组织名称. */
	private String orgLev2Nm;

	/**
	 * @return the orgLev1Id
	 */
	public String getOrgLev1Id() {
		return orgLev1Id;
	}

	/**
	 * @param orgLev1Id the orgLev1Id to set
	 */
	public void setOrgLev1Id(String orgLev1Id) {
		this.orgLev1Id = orgLev1Id;
	}

	/**
	 * @return the orgLev1Nm
	 */
	public String getOrgLev1Nm() {
		return orgLev1Nm;
	}

	/**
	 * @param orgLev1Nm the orgLev1Nm to set
	 */
	public void setOrgLev1Nm(String orgLev1Nm) {
		this.orgLev1Nm = orgLev1Nm;
	}

	/**
	 * @return the orgLev2Id
	 */
	public String getOrgLev2Id() {
		return orgLev2Id;
	}

	/**
	 * @param orgLev2Id the orgLev2Id to set
	 */
	public void setOrgLev2Id(String orgLev2Id) {
		this.orgLev2Id = orgLev2Id;
	}

	/**
	 * @return the orgLev2Nm
	 */
	public String getOrgLev2Nm() {
		return orgLev2Nm;
	}

	/**
	 * @param orgLev2Nm the orgLev2Nm to set
	 */
	public void setOrgLev2Nm(String orgLev2Nm) {
		this.orgLev2Nm = orgLev2Nm;
	}


	@Override
	public String refID1() {
		return orgLev1Id;
	}

	@Override
	public String refID2() {
		return orgLev2Id;
	}

	@Override
	public String refName1() {
		return orgLev1Nm;
	}

	@Override
	public String refName2() {
		return orgLev2Nm;
	}

	

}
