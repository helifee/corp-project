package com.yds.common.bean;

import com.yds.base.bean.BaseBean;

/**
 * 共通tree所需的DB数据Bean
 * 
 * @author zhangdaoqiang
 * @version 1.0 2010/02/05
 */
public class Node extends BaseBean {

	private static final long serialVersionUID = -8084106367915714485L;
	
	/** 记录主键.  */
	private String id;
	
	/** 显示名称.  */
	private String txt;
	
	/** 是否选中.  */
	private int check;
	
	/** 显示顺序.  */
	private String dispSeq;
	
	/**显示名称所带的URL.  */
	private String url;	
	
	/**
	 * 无参构造方法
	 */
	public Node() {}
	
	/**
	 * @param id 记录主键
	 * @param txt 显示名称
	 * @param dispSeq 显示顺序
	 */
	public Node(String id, String txt, String dispSeq) {
		this.id = id;
		this.txt = txt;
		this.dispSeq = dispSeq;
	}

	/**
	 * @param id 记录主键
	 * @param txt 显示名称
	 * @param check 是否选中
	 * @param dispSeq 显示顺序
	 * @param url 显示名称所带的URL
	 */
	public Node(String id, String txt, int check, String dispSeq, String url) {
		this.id = id;
		this.txt = txt;
		this.dispSeq = dispSeq;
		this.check = check;
		this.url = url;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getTxt() {
		return txt;
	}

	public void setTxt(String txt) {
		this.txt = txt;
	}

	public String getDispSeq() {
		return dispSeq;
	}

	public void setDispSeq(String dispSeq) {
		this.dispSeq = dispSeq;
	}

	public int getCheck() {
		return check;
	}

	public void setCheck(int check) {
		this.check = check;
	}

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

}
