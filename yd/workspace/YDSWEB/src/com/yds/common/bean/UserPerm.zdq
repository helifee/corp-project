package com.yds.common.bean;

import com.yds.base.bean.BaseBean;

/**
 * 用户权限
 * 
 * @author renlong
 * @version 1.00 2010/01/27
 */
public class UserPerm extends BaseBean {

	private static final long serialVersionUID = -3251902390162120588L;
	private String userId; // 用户ID
	private String userName;// 用户名
	private String permitId; // 权限ID
	private String permName;// 权限名
	/** 站点URL */
	private String siteURL; 
	private String nameSpace; // 命名空间
	private String actionName; // 动作名称
	private String isPerm; // 是否为权限
	private String isMenu; // 是否为菜单
	private String isControl; // 是否受控
	private String menuName; // 菜单名称
	private String parentModule; // 受控父模块
	private String dispSeq; // 显示顺序
	private String imgPath; // 位图路径

	/**
	 * @return the userId
	 */
	public String getUserId() {
		return userId;
	}

	/**
	 * @param userId
	 *            the userId to set
	 */
	public void setUserId(String userId) {
		this.userId = userId;
	}

	/**
	 * @return the userName
	 */
	public String getUserName() {
		return userName;
	}

	/**
	 * @param userName
	 *            the userName to set
	 */
	public void setUserName(String userName) {
		this.userName = userName;
	}

	/**
	 * @return the permitId
	 */
	public String getPermitId() {
		return permitId;
	}

	/**
	 * @param permitId
	 *            the permitId to set
	 */
	public void setPermitId(String permitId) {
		this.permitId = permitId;
	}

	/**
	 * @return the permName
	 */
	public String getPermName() {
		return permName;
	}

	/**
	 * @param permName
	 *            the permName to set
	 */
	public void setPermName(String permName) {
		this.permName = permName;
	}

	/**
	 * @return the nameSpace
	 */
	public String getNameSpace() {
		return nameSpace;
	}

	/**
	 * @param nameSpace
	 *            the nameSpace to set
	 */
	public void setNameSpace(String nameSpace) {
		this.nameSpace = nameSpace;
	}

	/**
	 * @return the actionName
	 */
	public String getActionName() {
		return actionName;
	}

	/**
	 * @param actionName
	 *            the actionName to set
	 */
	public void setActionName(String actionName) {
		this.actionName = actionName;
	}

	/**
	 * @return the isPerm
	 */
	public String getIsPerm() {
		return isPerm;
	}

	/**
	 * @param isPerm
	 *            the isPerm to set
	 */
	public void setIsPerm(String isPerm) {
		this.isPerm = isPerm;
	}

	/**
	 * @return the isMenu
	 */
	public String getIsMenu() {
		return isMenu;
	}

	/**
	 * @param isMenu
	 *            the isMenu to set
	 */
	public void setIsMenu(String isMenu) {
		this.isMenu = isMenu;
	}

	/**
	 * @return the isControl
	 */
	public String getIsControl() {
		return isControl;
	}

	/**
	 * @param isControl
	 *            the isControl to set
	 */
	public void setIsControl(String isControl) {
		this.isControl = isControl;
	}

	/**
	 * @return the menuName
	 */
	public String getMenuName() {
		return menuName;
	}

	/**
	 * @param menuName
	 *            the menuName to set
	 */
	public void setMenuName(String menuName) {
		this.menuName = menuName;
	}

	/**
	 * @return the siteURL
	 */
	public String getSiteURL() {
		return siteURL;
	}

	/**
	 * @param siteURL the siteURL to set
	 */
	public void setSiteURL(String siteURL) {
		this.siteURL = siteURL;
	}

	/**
	 * @return the parentModule
	 */
	public String getParentModule() {
		return parentModule;
	}

	/**
	 * @param parentModule
	 *            the parentModule to set
	 */
	public void setParentModule(String parentModule) {
		this.parentModule = parentModule;
	}

	/**
	 * @return the dispSeq
	 */
	public String getDispSeq() {
		return dispSeq;
	}

	/**
	 * @param dispSeq
	 *            the dispSeq to set
	 */
	public void setDispSeq(String dispSeq) {
		this.dispSeq = dispSeq;
	}

	/**
	 * @return the imgPath
	 */
	public String getImgPath() {
		return imgPath;
	}

	/**
	 * @param imgPath
	 *            the imgPath to set
	 */
	public void setImgPath(String imgPath) {
		this.imgPath = imgPath;
	}
}
