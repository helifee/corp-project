package com.yds.common.bean;

import com.yds.base.bean.BaseBean;

/**
 * 主题信息
 * 
 * @author lincheng
 * @version 1.00 2010/05/19
 */
public class ThemeInfo extends BaseBean {

	private static final long serialVersionUID = 7969107589487867169L;
	private String userId; // 用户ID
	private String themeId; // 主题ID
	private String themeName; // 主题名字
	private String themeURL; // 主题样式文件名
	private String themeDesc; // 主题描述
	
	public String getThemeId() {
		return themeId;
	}
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public void setThemeId(String themeId) {
		this.themeId = themeId;
	}
	public String getThemeName() {
		return themeName;
	}
	public void setThemeName(String themeName) {
		this.themeName = themeName;
	}
	public String getThemeURL() {
		return themeURL;
	}
	public void setThemeURL(String themeURL) {
		this.themeURL = themeURL;
	}
	public String getThemeDesc() {
		return themeDesc;
	}
	public void setThemeDesc(String themeDesc) {
		this.themeDesc = themeDesc;
	}

}
