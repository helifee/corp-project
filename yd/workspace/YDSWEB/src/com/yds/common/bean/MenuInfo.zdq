package com.yds.common.bean;

import java.util.List;
import com.yds.base.bean.BaseBean;

/**
 * 左侧菜单数据Bean
 * 
 * @author yuanjinling
 * @version 1.0 2010/09/16
 */
public class MenuInfo extends BaseBean{

	private static final long serialVersionUID = 3406813148200061743L;
	private String href; // 链接地址
	private String text; // 菜单名称
	private String dispSeq; // 显示顺序
	private String imgPath; // 位图路径
	private String target; // 显示位置
	private List<MenuInfo> subMenu; // 子菜单

	public String getHref() {
		return href;
	}

	public void setHref(String href) {
		this.href = href;
	}

	public String getText() {
		return text;
	}

	public void setText(String text) {
		this.text = text;
	}

	public void setTarget(String target) {
		this.target = target;
	}

	public String getDispSeq() {
		return dispSeq;
	}

	public void setDispSeq(String dispSeq) {
		this.dispSeq = dispSeq;
	}

	public String getImgPath() {
		return imgPath;
	}

	public void setImgPath(String imgPath) {
		this.imgPath = imgPath;
	}

	public String getTarget() {
		return target;
	}

	public void setTarge(String target) {
		this.target = target;
	}

	public List<MenuInfo> getSubMenu() {
		return subMenu;
	}

	public void setSubMenu(List<MenuInfo> subMenu) {
		this.subMenu = subMenu;
	}

}
