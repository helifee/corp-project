package com.yds.common.bean;

/**
 * 采番信息Bean
 * 
 * @author tianjian
 * @version 1.0 2010/03/08
 */
public class ComSeq {

	/** 表名.  */
	private String tableNm;
	
	/** 列名.  */
	private String colunmNm;
	
	/** 序列值.  */
	private Integer seqValue;
	
	/** 递增长度.  */
	private Integer seqStep;
	
	/** 最大值.  */
	private Integer seqMax;
	
	/** 最小值.  */
	private Integer seqMin;
	
	/** 循环周期区分.  */
	private String seqCycleflg;

	/**
	 * @return the tableNm
	 */
	public String getTableNm() {
		return tableNm;
	}

	/**
	 * @param tableNm the tableNm to set
	 */
	public void setTableNm(String tableNm) {
		this.tableNm = tableNm;
	}

	/**
	 * @return the colunmNm
	 */
	public String getColunmNm() {
		return colunmNm;
	}

	/**
	 * @param colunmNm the colunmNm to set
	 */
	public void setColunmNm(String colunmNm) {
		this.colunmNm = colunmNm;
	}

	/**
	 * @return the seqValue
	 */
	public Integer getSeqValue() {
		return seqValue;
	}

	/**
	 * @param seqValue the seqValue to set
	 */
	public void setSeqValue(Integer seqValue) {
		this.seqValue = seqValue;
	}

	/**
	 * @return the seqStep
	 */
	public Integer getSeqStep() {
		return seqStep;
	}

	/**
	 * @param seqStep the seqStep to set
	 */
	public void setSeqStep(Integer seqStep) {
		this.seqStep = seqStep;
	}

	/**
	 * @return the seqMax
	 */
	public Integer getSeqMax() {
		return seqMax;
	}

	/**
	 * @param seqMax the seqMax to set
	 */
	public void setSeqMax(Integer seqMax) {
		this.seqMax = seqMax;
	}

	/**
	 * @return the seqMin
	 */
	public Integer getSeqMin() {
		return seqMin;
	}

	/**
	 * @param seqMin the seqMin to set
	 */
	public void setSeqMin(Integer seqMin) {
		this.seqMin = seqMin;
	}

	/**
	 * @return the seqCycleflg
	 */
	public String getSeqCycleflg() {
		return seqCycleflg;
	}

	/**
	 * @param seqCycleflg the seqCycleflg to set
	 */
	public void setSeqCycleflg(String seqCycleflg) {
		this.seqCycleflg = seqCycleflg;
	}
	
}
