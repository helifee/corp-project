package com.yds.common.bean;

/**
 * 职位树Bean
 * 
 * @author zhangdaoqiang,yuanjinling
 * @version 1.0 2010/06/08
 */
public class PositionTreeInfo implements TreeListResult {

	/** 职位类别区分No. */
	private String diffNo;
	
	/** 职位类别区分Name */
	private String diffName;
	
	/** 职位ID */
	private String posId;
	
	/** 职位名称 */
	private String posName;

	public String getDiffNo() {
		return diffNo;
	}

	public void setDiffNo(String diffNo) {
		this.diffNo = diffNo;
	}

	public String getDiffName() {
		return diffName;
	}

	public void setDiffName(String diffName) {
		this.diffName = diffName;
	}

	public String getPosId() {
		return posId;
	}

	public void setPosId(String posId) {
		this.posId = posId;
	}

	public String getPosName() {
		return posName;
	}

	public void setPosName(String posName) {
		this.posName = posName;
	}
	
	//一级分类的ID对应职位类的ID 
	@Override
	public String refID1() {
		return diffNo;
	}
	//二级分类的ID对应职位的ID 
	@Override
	public String refID2() {
		return posId;	
	}
	//一级分类的名字对应职位类的名字 
	@Override
	public String refName1() {
        return diffName;
	}
	//二级分类的名字对应职位的名字 
	@Override
	public String refName2() {		
		return posName;
	}
}
