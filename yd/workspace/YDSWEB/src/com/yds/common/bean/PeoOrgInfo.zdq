package com.yds.common.bean;

import com.yds.base.bean.BaseBean;

/**
 * 组织表信息Bean
 * 
 * @author bixiuqing
 * @version 1.0 2010/05/24
 */
public class PeoOrgInfo extends BaseBean {

	private static final long serialVersionUID = -2706225683016938041L;

	/** 组织ID. */	
	private String orgId;

	/** 组织名称. */
