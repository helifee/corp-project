package com.yds.common.bean;

import java.util.Date;

/**
 * 系统日志表信息Bean
 * 
 * @author zhangdaoqiang
 * @version 1.0 2010/03/08
 */
public class LogInfo {

	/** 日志ID.  */
	private int logId;
	
	/** 日志等级.  */
	private String logLevel;
	
	/** 子系统ID.  */
	private String subId;
	
	/** 被操作者ID.  */
	private String userId;
	
	/** 日志信息.  */
	private String logMsg;
	
	/** 操作者登录IP.  */
	private String logIp;
	
	/** 备注.  */
	private String refer;
	
	/** 操作者ID.  */
	private String opId;
	
	/** 操作者时间.  */
	private Date opTime; 

	/**
	 * @return the logId
	 */
	public int getLogId() {
		return logId;
	}

	/**
	 * @param logId
	 *            the logId to set
	 */
	public void setLogId(int logId) {
		this.logId = logId;
	}

	/**
	 * @return the logLevel
	 */
	public String getLogLevel() {
		return logLevel;
	}

	/**
	 * @param logLevel
	 *            the logLevel to set
	 */
	public void setLogLevel(String logLevel) {
		this.logLevel = logLevel;
	}

	/**
	 * @return the subId
	 */
	public String getSubId() {
		return subId;
	}

	/**
	 * @param subId
	 *            the subId to set
	 */
	public void setSubId(String subId) {
		this.subId = subId;
	}

	/**
	 * @return the userId
	 */
	public String getUserId() {
		return userId;
	}

	/**
	 * @param userId
	 *            the userId to set
	 */
	public void setUserId(String userId) {
		this.userId = userId;
	}

	/**
	 * @return the logMsg
	 */
	public String getLogMsg() {
		return logMsg;
	}

	/**
	 * @param logMsg
	 *            the logMsg to set
	 */
	public void setLogMsg(String logMsg) {
		this.logMsg = logMsg;
	}

	/**
	 * @return the logIp
	 */
	public String getLogIp() {
		return logIp;
	}

	/**
	 * @param logIp
	 *            the logIp to set
	 */
	public void setLogIp(String logIp) {
		this.logIp = logIp;
	}

	/**
	 * @return the refer
	 */
	public String getRefer() {
		return refer;
	}

	/**
	 * @param refer
	 *            the refer to set
	 */
	public void setRefer(String refer) {
		this.refer = refer;
	}

	/**
	 * @return the opId
	 */
	public String getOpId() {
		return opId;
	}

	/**
	 * @param opId
	 *            the opId to set
	 */
	public void setOpId(String opId) {
		this.opId = opId;
	}

	/**
	 * @return the opTime
	 */
	public Date getOpTime() {
		return opTime;
	}

	/**
	 * @param opTime
	 *            the opTime to set
	 */
	public void setOpTime(Date opTime) {
		this.opTime = opTime;
	}

}
