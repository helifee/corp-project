package com.yds.common.bean;

import java.util.List;

import com.yds.base.bean.BaseBean;

/**
 * 系统参数信息Bean
 * 
 * @author bixiuqing
 * @version 1.0 2010/05/21
 */
public class ComCodeInfo extends BaseBean {

	private static final long serialVersionUID = 862030918906793033L;

	/** 子系统ID. */
	private String subSys;

	/** 类型ID. */
	private String typeId;

	/** 区分NO. */
	private String diffNo;

	/** 区分名称. */
	private String diffName;

	/** 附加属性. */
	private List<SysCodeProInfo> proList;

	/** 追加区分NO. */
	private String addDiffNo;

	/** 追加区分名称. */
	private String addDiffName;
	
	/** 显示与否FLG. */
	private String pro2;
	

	/**
	 * @return the subSys
	 */
	public String getSubSys() {
		return subSys;
	}

	/**
	 * @param subSys
	 *            the subSys to set
	 */
	public void setSubSys(String subSys) {
		this.subSys = subSys;
	}

	/**
	 * @return the typeId
	 */
	public String getTypeId() {
		return typeId;
	}

	/**
	 * @param typeId
	 *            the typeId to set
	 */
	public void setTypeId(String typeId) {
		this.typeId = typeId;
	}

	/**
	 * @return the diffNo
	 */
	public String getDiffNo() {
		return diffNo;
	}

	/**
	 * @param diffNo
	 *            the diffNo to set
	 */
	public void setDiffNo(String diffNo) {
		this.diffNo = diffNo;
	}

	/**
	 * @return the diffName
	 */
	public String getDiffName() {
		return diffName;
	}

	/**
	 * @param diffName
	 *            the diffName to set
	 */
	public void setDiffName(String diffName) {
		this.diffName = diffName;
	}

	/**
	 * @return the proList
	 */
	public List<SysCodeProInfo> getProList() {
		return proList;
	}

	/**
	 * @param proList
	 *            the proList to set
	 */
	public void setProList(List<SysCodeProInfo> proList) {
		this.proList = proList;
	}

	/**
	 * @return the addDiffNo
	 */
	public String getAddDiffNo() {
		return addDiffNo;
	}

	/**
	 * @param addDiffNo
	 *            the addDiffNo to set
	 */
	public void setAddDiffNo(String addDiffNo) {
		this.addDiffNo = addDiffNo;
	}

	/**
	 * @return the addDiffName
	 */
	public String getAddDiffName() {
		return addDiffName;
	}

	/**
	 * @param addDiffName
	 *            the addDiffName to set
	 */
	public void setAddDiffName(String addDiffName) {
		this.addDiffName = addDiffName;
	}

	public String getPro2() {
		return pro2;
	}

	public void setPro2(String pro2) {
		this.pro2 = pro2;
	}


}
