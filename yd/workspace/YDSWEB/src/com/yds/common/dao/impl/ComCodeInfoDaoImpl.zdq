package com.yds.common.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.common.bean.ComCodeInfo;
import com.yds.common.bean.ComCodeMaint;
import com.yds.common.dao.ComCodeInfoDao;

/**
 * @see com.yds.common.dao.ComCodeInfoDao
 */
@Repository("comCodeInfoDao")
public class ComCodeInfoDaoImpl extends AbstractBaseDao implements
		ComCodeInfoDao {

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<ComCodeInfo> getComCodeInfo(ComCodeInfo comCodeInfo) {

		return (List<ComCodeInfo>) super.queryForList(
				"ComCodeInfoDao.getCodeInfo", comCodeInfo);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<ComCodeMaint> getDetailComCodeInfo(ComCodeInfo comCodeInfo) {

		return (List<ComCodeMaint>) super.queryForList(
				"ComCodeInfoDao.getDetailCodeInfo", comCodeInfo);
	}
}
