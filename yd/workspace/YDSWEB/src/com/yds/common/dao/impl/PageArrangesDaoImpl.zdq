package com.yds.common.dao.impl;

import org.springframework.stereotype.Repository;
import com.yds.base.dao.AbstractBaseDao;
import com.yds.common.bean.CheckAndLockInfo;
import com.yds.common.dao.PageArrangesDao;

/**
 * @see pageArrangesDao
 */
@Repository("pageArrangesDao")
public class PageArrangesDaoImpl extends AbstractBaseDao implements PageArrangesDao{
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public CheckAndLockInfo getCheckAndLockInfo(CheckAndLockInfo checkAndLockInfo){
		return  (CheckAndLockInfo)super.queryForObject("PageArrangesDao.getAllLock" , checkAndLockInfo);
	
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public void deleteLock(CheckAndLockInfo checkAndLockInfo){
		super.delete("PageArrangesDao.deleteLock", checkAndLockInfo);
	}
		
	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertLock(CheckAndLockInfo checkAndLockInfo) {
		super.insert("PageArrangesDao.addPageLock", checkAndLockInfo);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public Long getTotalCount(CheckAndLockInfo checkAndLockInfo){
		return  (Long)super.queryForObject("PageArrangesDao.getCount", checkAndLockInfo);
		
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public  String getUserName(CheckAndLockInfo checkAndLockInfo){
		return  (String)super.queryForObject("PageArrangesDao.getUserName", checkAndLockInfo);
	}	
		
}
