package com.yds.common.dao.impl;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.common.dao.ErrorLogDao;
import com.yds.common.bean.ComErrorLog;

import org.springframework.stereotype.Repository;

/**
 * @see com.yds.common.dao.ErrorLogDao
 */
@Repository("errorLogDao")
public class ErrorLogDaoImpl extends AbstractBaseDao implements ErrorLogDao{
	/** 
	 *  {@inheritDoc}
	 */
	@Override
	public ComErrorLog getComLogByPk(ComErrorLog comLog) {

		return (ComErrorLog)super.queryForObject("comErrorLogDao.getComLogByPk", comLog);
	}	
	
	
	/**
	 *  {@inheritDoc}
	 */
	@Override
	public void insertComLog(ComErrorLog comLog) {
		
		super.insert("comErrorLogDao.insertComLog", comLog);
	}
	
	/**
	 *  {@inheritDoc}
	 */
	@Override
	public int updateComLog(ComErrorLog comLog) {
		
		return super.update("comErrorLogDao.updateComLog", comLog);
	}

}
