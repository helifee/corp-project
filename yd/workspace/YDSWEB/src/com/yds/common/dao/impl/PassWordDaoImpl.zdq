package com.yds.common.dao.impl;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.common.bean.PassWord;
import com.yds.common.dao.PassWordDao;

/**
 * @see PassWordDao
 */
@Repository("passWordDao")
public class PassWordDaoImpl extends AbstractBaseDao implements PassWordDao  {
	
	@Override
	public int updatePwd(PassWord passWord) {
		return (Integer)super.update("PassWordDao.updatePwd", passWord);
	}

	
	@Override
	public int updatePwdOld(PassWord passWord) {
		return (Integer)super.update("PassWordDao.updatePwdOld", passWord);
	}
}
