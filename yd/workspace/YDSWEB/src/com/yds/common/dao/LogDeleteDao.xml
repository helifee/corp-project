<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTDSQLMap2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="logDeleteDao">
	<typeAlias alias="logInfo" type="com.yds.common.bean.LogInfo"/>

	<!-- 删除日志 --> 
	<delete id="deleteLog" parameterClass="logInfo">  
		DELETE FROM COM_LOG_MST
		WHERE 
		LOG_LEVEL=#logLevel#
		AND <![CDATA[ OP_TIME <= #opTime# ]]>
		;
	</delete>
	
	
</sqlMap>
