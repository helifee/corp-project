package com.yds.common.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.common.bean.ComCodeConf;
import com.yds.common.bean.ComCodeMaint;
import com.yds.common.bean.ItemInfo;
import com.yds.common.bean.TypeInfo;
import com.yds.common.dao.ComCodeMaintDao;

/**
 * @see com.yds.common.dao.ComCodeInfoDao
 */
@Repository("comCodeMaintDao")
public class ComCodeMaintDaoImpl extends AbstractBaseDao implements
		ComCodeMaintDao {

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<ComCodeMaint> getDiffList(ComCodeMaint comCodeMaint) {

		return (List<ComCodeMaint>) super.queryForList(
				"ComCodeMaintDao.getDiffList", comCodeMaint);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<TypeInfo> getTypeList(String subSys) {
		return (List<TypeInfo>) super.queryForList(
				"ComCodeMaintDao.getTypeList", subSys);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<ComCodeConf> getComCodeConfList(ComCodeConf comCodeConf) {
		return (List<ComCodeConf>) super.queryForList(
				"ComCodeMaintDao.getComCodeConfList", comCodeConf);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Object insertDiffInfo(ComCodeMaint comCodeMaint) {
		return super.insert("ComCodeMaintDao.insertDiffInfo", comCodeMaint);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public int updateDiffInfo(ComCodeMaint comCodeMaint) {
		return super.update("ComCodeMaintDao.updateDiffInfo", comCodeMaint);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<ItemInfo> getItemInfoList(ItemInfo itemInfo) {
		return (List<ItemInfo>) super.queryForList(
				"ComCodeMaintDao.getItemInfoList", itemInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<ComCodeMaint> getDiffSortList(ComCodeMaint comCodeMaint) {
		return (List<ComCodeMaint>) super.queryForList(
				"ComCodeMaintDao.getDiffSortList", comCodeMaint);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public int updateDiffSort(ComCodeMaint comCodeMaint) {
		return super.update("ComCodeMaintDao.updateDiffSort", comCodeMaint);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public String getMaxDiffSort(ComCodeMaint comCodeMaint) {

		return (String) super.queryForObject("ComCodeMaintDao.getMaxDiffSort",
				comCodeMaint);
	}
}
