package com.yds.common.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.common.bean.ComSeq;
import com.yds.common.dao.ComSequenceDao;

/**
 * @see ComSequenceDao
 */
@Repository("comSequenceDao")
public class ComSequenceDaoImpl extends AbstractBaseDao implements ComSequenceDao {

	/**
	 * {@inheritDoc}
	 */
	@Override
	public ComSeq getNextSequence(ComSeq comSeq) {
		return (ComSeq) super.queryForObject("ComSequenceDao.getNextSequence", comSeq);
	}
	/**
	 * {@inheritDoc}
	 */
	@Override
	public void updateSequence(ComSeq comSeq){
	   super.update("ComSequenceDao.updateSequence", comSeq);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<ComSeq> getAllSequence(){
		return super.queryForList("ComSequenceDao.getAllSequence");
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertSequence(ComSeq comSeq) {
		 super.insert("ComSequenceDao.insertSequence", comSeq);
		
	}
}
