package com.yds.common.dao.impl;

import java.util.HashMap;
import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.common.dao.DBToExcelDao;
/**
 * @see DBToExcelDao
 */
@Repository("dbToExcelDao")
public class DBToExcelDaoImpl extends AbstractBaseDao implements DBToExcelDao{
	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<Map<String, Object>> getTableData(String tableName) {
		List<Map<String,Object>> list = super.queryForList("DBToExcelDao.getTableData",tableName);
		return list;
	}
	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<String> getTableColumn(String tableName,String tableSchema) {
		Map<String,String> map = new HashMap<String,String>();
		map.put("tableName", tableName);
		if(null == tableSchema || "".equals(tableSchema)){
			map.put("tableSchema", "test");
		}else{
			map.put("tableSchema", tableSchema);
		}
		return (List<String>)super.queryForList("DBToExcelDao.getTableColumn",map);
	}
}
