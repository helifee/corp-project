package com.yds.common.dao;

import java.util.List;
import java.util.Map;

public interface DBToExcelDao {
	/**
	 * 取得表的所有数据
	 * 
	 * @param tableName
	 *            表名
	 * @return List 表中数据List
	 */

	public List<Map<String, Object>> getTableData(String tableName);

	/**
	 * 取得表的列名
	 * 
	 * @param tableName
	 *            表名
	 * @param tableSchema
	 *            指定schema
	 * @return List 表中字段名ist
	 */
	public List<String> getTableColumn(String tableName,String tableSchema);
}
