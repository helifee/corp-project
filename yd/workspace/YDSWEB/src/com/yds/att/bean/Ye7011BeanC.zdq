package com.yds.att.bean;

import com.yds.base.bean.BaseBean;

/**
 * 考勤信息相关Bean（旧DB）
 * @author 远东) zhaodong
 * @version 1.0
 */


public class Ye7011BeanC extends BaseBean {
	
	private static final long serialVersionUID = 2289913308052974762L;

	/** 对象年 */
	private String year;
	
	/** 对象月 */
	private String month;
	
	/** 对象天 */
	private String day;
	
	/** 员工ID */
	private String empId;
	
	/** 考勤结果类型*/
	private String flag;
	
	/** NU??? */
	private String ornFlag;
	
	/** 班型 */
	private String type;
	
	/** 上班时间 */
	private String beginTime;
	
	/** 下班时间 */
	private String endTime;

	/** 考勤人ID */
	private String leaderId;

	/**
	 * @return the year
	 */
	public String getYear() {
		return year;
	}

	/**
	 * @param year the year to set
	 */
	public void setYear(String year) {
		this.year = year;
	}

	/**
	 * @return the month
	 */
	public String getMonth() {
		return month;
	}

	/**
	 * @param month the month to set
	 */
	public void setMonth(String month) {
		this.month = month;
	}

	/**
	 * @return the day
	 */
	public String getDay() {
		return day;
	}

	/**
	 * @param day the day to set
	 */
	public void setDay(String day) {
		this.day = day;
	}

	/**
	 * @return the empId
	 */
	public String getEmpId() {
		return empId;
	}

	/**
	 * @param empId the empId to set
	 */
	public void setEmpId(String empId) {
		this.empId = empId;
	}

	/**
	 * @return the flag
	 */
	public String getFlag() {
		return flag;
	}

	/**
	 * @param flag the flag to set
	 */
	public void setFlag(String flag) {
		this.flag = flag;
	}

	/**
	 * @return the ornFlag
	 */
	public String getOrnFlag() {
		return ornFlag;
	}

	/**
	 * @param ornFlag the ornFlag to set
	 */
	public void setOrnFlag(String ornFlag) {
		this.ornFlag = ornFlag;
	}

	/**
	 * @return the type
	 */
	public String getType() {
		return type;
	}

	/**
	 * @param type the type to set
	 */
	public void setType(String type) {
		this.type = type;
	}

	/**
	 * @return the beginTime
	 */
	public String getBeginTime() {
		return beginTime;
	}

	/**
	 * @param beginTime the beginTime to set
	 */
	public void setBeginTime(String beginTime) {
		this.beginTime = beginTime;
	}

	/**
	 * @return the endTime
	 */
	public String getEndTime() {
		return endTime;
	}

	/**
	 * @param endTime the endTime to set
	 */
	public void setEndTime(String endTime) {
		this.endTime = endTime;
	}

	/**
	 * @return the leaderId
	 */
	public String getLeaderId() {
		return leaderId;
	}

	/**
	 * @param leaderId the leaderId to set
	 */
	public void setLeaderId(String leaderId) {
		this.leaderId = leaderId;
	}
}
