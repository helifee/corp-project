package com.yds.att.bean;

import java.util.Date;

import com.yds.base.bean.BaseBean;

/**
 * @author 远东) 共通组
 * @version 1.0
 */


public class AttExrestInfo extends BaseBean {
	
	private static final long serialVersionUID = -217574121003157448L;

	/** 员工ID. */
	private String empId;

	/** 统计年. */
	private String calYear;

	/** 统计月. */
	private String calMonth;

	/** 发生日期. */
	private Date happenDate;

	/** 发生原因. */
	private String happenReason;

	/** 换休时间. */
	private Integer happenTime;

	/** 有效期限*/
	private Date usefulDate;
	/** 申请编号存根. */
	private String appId;


	
	/**
	 * @param empId the empId to set
	 */
	public void setEmpId(String empId) {
		this.empId = empId;
	}
	
	/**
	 * @return the empId
	 */
	public String getEmpId() {
		return this.empId;
	}
	
	/**
	 * @param calYear the calYear to set
	 */
	public void setCalYear(String calYear) {
		this.calYear = calYear;
	}
	
	/**
	 * @return the calYear
	 */
	public String getCalYear() {
		return this.calYear;
	}
	
	/**
	 * @param calMonth the calMonth to set
	 */
	public void setCalMonth(String calMonth) {
		this.calMonth = calMonth;
	}
	
	/**
	 * @return the calMonth
	 */
	public String getCalMonth() {
		return this.calMonth;
	}
	
	/**
	 * @param happenDate the happenDate to set
	 */
	public void setHappenDate(Date happenDate) {
		this.happenDate = happenDate;
	}
	
	/**
	 * @return the happenDate
	 */
	public Date getHappenDate() {
		return this.happenDate;
	}
	
	/**
	 * @param happenReason the happenReason to set
	 */
	public void setHappenReason(String happenReason) {
		this.happenReason = happenReason;
	}
	
	/**
	 * @return the happenReason
	 */
	public String getHappenReason() {
		return this.happenReason;
	}
	
	/**
	 * @param happenTime the happenTime to set
	 */
	public void setHappenTime(Integer happenTime) {
		this.happenTime = happenTime;
	}
	
	/**
	 * @return the happenTime
	 */
	public Integer getHappenTime() {
		return this.happenTime;
	}
	
	/**
	 * @param appId the appId to set
	 */
	public void setAppId(String appId) {
		this.appId = appId;
	}
	
	/**
	 * @return the appId
	 */
	public String getAppId() {
		return this.appId;
	}

	/**
	 * @param usefulDate the usefulDate to set
	 */
	public void setUsefulDate(Date usefulDate) {
		this.usefulDate = usefulDate;
	}

	/**
	 * @return the usefulDate
	 */
	public Date getUsefulDate() {
		return usefulDate;
	}

}

