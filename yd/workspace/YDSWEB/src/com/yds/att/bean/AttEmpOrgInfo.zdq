package com.yds.att.bean;

import java.util.Date;

import com.yds.base.bean.BaseBean;

public class AttEmpOrgInfo extends BaseBean implements TreeLevel, Comparable<AttEmpOrgInfo> {
	
	private static final long serialVersionUID = -563421288241626339L;
	
	/** DB项目 */
	/** 组织ID. */
	private String orgId;

	/** 组织名称. */
	private String orgNm;

	/** 组织略称. */
	private String orgSnm;

	/** 有效开始时间. */
	private Date orgStDate;

	/** 有效结束时间. */
	private Date orgEndDate;

	/** 态别FLG. */
	private String orgStateFlg;

	/** 特性ID. */
	private String orgProId;
	
	/** 特性类型. */
	private String orgProType;

	/** 阶层. */
	private String orgLev;

	/** 可见FLG. */
	private String orgViewFlg;

	/** 主管ID. */
	private String orgMnger;
	
	/** 主管姓名. */
	private String orgMngerName;

	/** 组织节点描述. */
	private String orgDesc;

	/** 更新者. */
	private String updateUser;

	/** 更新时间. */
	private Date updateTime;

	/**
	 * @param orgId the orgId to set
	 */
	public void setOrgId(String orgId) {
		this.orgId = orgId;
	}
	
	/**
	 * @return the orgId
	 */
	public String getOrgId() {
		return this.orgId;
	}
	
	/**
	 * @param orgNm the orgNm to set
	 */
	public void setOrgNm(String orgNm) {
		this.orgNm = orgNm;
	}
	
	/**
	 * @return the orgNm
	 */
	public String getOrgNm() {
		return this.orgNm;
	}
	
	/**
	 * @param orgSnm the orgSnm to set
	 */
	public void setOrgSnm(String orgSnm) {
		this.orgSnm = orgSnm;
	}
	
	/**
	 * @return the orgSnm
	 */
	public String getOrgSnm() {
		return this.orgSnm;
	}
	
	/**
	 * @param orgStDate the orgStDate to set
	 */
	public void setOrgStDate(Date orgStDate) {
		this.orgStDate = orgStDate;
	}
	
	/**
	 * @return the orgStDate
	 */
	public Date getOrgStDate() {
		return this.orgStDate;
	}
	
	/**
	 * @param orgEndDate the orgEndDate to set
	 */
	public void setOrgEndDate(Date orgEndDate) {
		this.orgEndDate = orgEndDate;
	}
	
	/**
	 * @return the orgEndDate
	 */
	public Date getOrgEndDate() {
		return this.orgEndDate;
	}
	
	/**
	 * @param orgStateFlg the orgStateFlg to set
	 */
	public void setOrgStateFlg(String orgStateFlg) {
		this.orgStateFlg = orgStateFlg;
	}
	
	/**
	 * @return the orgStateFlg
	 */
	public String getOrgStateFlg() {
		return this.orgStateFlg;
	}
	
	/**
	 * @param orgProId the orgProId to set
	 */
	public void setOrgProId(String orgProId) {
		this.orgProId = orgProId;
	}
	
	/**
	 * @return the orgProId
	 */
	public String getOrgProId() {
		return this.orgProId;
	}
	
	/**
	 * @param orgProType the orgProType to set
	 */
	public void setOrgProType(String orgProType) {
		this.orgProType = orgProType;
	}
	
	/**
	 * @return the orgProType
	 */
	public String getOrgProType() {
		return orgProType;
	}

	/**
	 * @param orgLev the orgLev to set
	 */
	public void setOrgLev(String orgLev) {
		this.orgLev = orgLev;
	}
	
	/**
	 * @return the orgLev
	 */
	public String getOrgLev() {
		return this.orgLev;
	}
	
	/**
	 * @param orgViewFlg the orgViewFlg to set
	 */
	public void setOrgViewFlg(String orgViewFlg) {
		this.orgViewFlg = orgViewFlg;
	}
	
	/**
	 * @return the orgViewFlg
	 */
	public String getOrgViewFlg() {
		return this.orgViewFlg;
	}
	
	/**
	 * @param orgMnger the orgMnger to set
	 */
	public void setOrgMnger(String orgMnger) {
		this.orgMnger = orgMnger;
	}
	
	/**
	 * @return the orgMnger
	 */
	public String getOrgMnger() {
		return this.orgMnger;
	}
	
	public String getOrgMngerName() {
		return orgMngerName;
	}

	public void setOrgMngerName(String orgMngerName) {
		this.orgMngerName = orgMngerName;
	}

	/**
	 * @param orgDesc the orgDesc to set
	 */
	public void setOrgDesc(String orgDesc) {
		this.orgDesc = orgDesc;
	}
	
	/**
	 * @return the orgDesc
	 */
	public String getOrgDesc() {
		return this.orgDesc;
	}
	
	/**
	 * @param updateUser the updateUser to set
	 */
	public void setUpdateUser(String updateUser) {
		this.updateUser = updateUser;
	}
	
	/**
	 * @return the updateUser
	 */
	public String getUpdateUser() {
		return this.updateUser;
	}
	
	/**
	 * @param updateTime the updateTime to set
	 */
	public void setUpdateTime(Date updateTime) {
		this.updateTime = updateTime;
	}
	
	/**
	 * @return the updateTime
	 */
	public Date getUpdateTime() {
		return this.updateTime;
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public String getLevel() {
		return this.getOrgLev();
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public int compareTo(AttEmpOrgInfo o) {
		if(this.orgStDate.before(o.getOrgStDate())) {
			return 1;
		} else if(this.orgStDate.after(o.getOrgStDate())) {
			return -1;
		} else {
			return 0;
		}
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public boolean equals(Object obj) {
		if(obj == null) {
			return false;
		} 
		if(!(obj instanceof AttEmpOrgInfo)) {
			return false;
		}
		
		AttEmpOrgInfo info = (AttEmpOrgInfo)obj;
		if(this.orgStDate.compareTo(info.getOrgStDate()) == 0) {
			return true;
		}
		return false;
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public int hashCode() {
		return this.orgId.hashCode();
	}

}

