package com.yds.att.dao.impl;

import java.util.HashMap;
import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.yds.att.bean.AttInfoUncor;
import com.yds.att.bean.AttMonReport;
import com.yds.att.bean.Ye0090Overtime;
import com.yds.att.dao.Ye0090Dao;
import com.yds.base.dao.AbstractBaseDao;

/**
 * @see com.yds.att.dao.Ye0090Dao
 */
@Repository("ye0090Dao")
public class Ye0090DaoImpl extends AbstractBaseDao implements Ye0090Dao{

	@Override
	@SuppressWarnings("unchecked")
	public List<AttMonReport> getAttMonReport(String year, String month) {
		Map<String, String> params = new HashMap<String, String>();
		params.put("year", year);
		params.put("month", month);
		return super.queryForList("Ye0090Dao.queryAttMonReport", params);
	}

	@Override
	@SuppressWarnings("unchecked")
	public List<Ye0090Overtime> getOvertimeReport(String year, String month) {
		Map<String, String> params = new HashMap<String, String>();
		params.put("year", year);
		params.put("month", month);
		return super.queryForList("Ye0090Dao.queryOvertimeReport", params);
	}

	@SuppressWarnings("unchecked")
	@Override
	public List<AttInfoUncor> getAttInfoUncor(String yearMonth) {
		return super.queryForList("Ye0090Dao.queryAttInfoUncor", yearMonth);
	}

}
