package com.yds.att.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.att.bean.AttBatchPar;
import com.yds.att.dao.Ye8030Dao;

/**
 * 
 * @see com.yds.ws.dao.UpdateAttInfoDao
 *
 */
@Repository("ye8030Dao")
public class Ye8030DaoImpl extends AbstractBaseDao implements
		Ye8030Dao {

	@SuppressWarnings("unchecked")
	@Override
	public List<AttBatchPar> getAttBatchPar(String attBatchYc) {
		return (List<AttBatchPar>)super.queryForList("ye8030Dao.getBatchPar", attBatchYc);
	}

	@Override
	public int deleteAttBatchPar(String attBatchYc) {
		
		return (int)super.delete("ye8030Dao.deleteAttBatchPar", attBatchYc);
	}



}
