<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTDSQLMap2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="ye8050Dao">
	<typeAlias alias="attInfoUncor" type="com.yds.att.bean.AttInfoUncor"/>
	
	<select id="getAttInfoUncor" parameterClass="String" resultClass="attInfoUncor">
		<![CDATA[
		select         	
		    ATT_YM as attYm ,
        	ATT_PER_ID as attPerId ,
        	UNCOR_TYPE as uncorType ,
        	ORG1 as org1 ,
        	ORG2 as org2 ,
        	CNT1 as cnt1 
		from ATT_INFO_UNCOR
		where
			ATT_YM = #value#
		]]>
	</select>
</sqlMap> 