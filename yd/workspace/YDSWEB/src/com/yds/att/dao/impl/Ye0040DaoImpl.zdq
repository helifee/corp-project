package com.yds.att.dao.impl;

import java.util.HashMap;
import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.yds.att.bean.AttExamin;
import com.yds.att.bean.AttInfo;
import com.yds.att.bean.AttRestApp;
import com.yds.att.bean.Ye0040CondA;
import com.yds.att.dao.Ye0040Dao;
import com.yds.base.dao.AbstractBaseDao;

/**
 * @see com.yds.att.dao.Ye0040Dao
 */
@Repository("ye0040Dao")
public class Ye0040DaoImpl extends AbstractBaseDao implements Ye0040Dao{

	/** 
	 *  {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<AttRestApp> getAttRestAppList(Ye0040CondA bean) {

		return (List<AttRestApp>)super.queryForList("Ye0040Dao.getAttRestAppList", bean);
	}
	
	/** 
	 *  {@inheritDoc}
	 */
	@Override
	public AttRestApp getAttRestAppByPk(String appId) {

		return (AttRestApp)super.queryForObject("Ye0040Dao.getAttRestAppByPk", appId);
	}	
	
	/**
	 *  {@inheritDoc}
	 */
	@Override
	public int updateAttRestApp(AttRestApp bean) {
		
		return super.update("Ye0040Dao.updateAttRestApp", bean);
	}


	@Override
	public int updateAttInfo(AttInfo bean) {

		return super.update("Ye0040Dao.updateAttInfo", bean);
	}


	@Override
	public int updateAttExamin(AttExamin bean) {

		return super.update("Ye0040Dao.updateAttExamin", bean);
	}

	@SuppressWarnings("unchecked")
	@Override
	public List<AttExamin> getAttExamin(String appId, String userId) {
		Map<String, String> aecMap = new HashMap<String, String>();
		aecMap.put("appId", appId);
		aecMap.put("userId", userId);
		return (List<AttExamin>)super.queryForList("Ye0040Dao.getAttExamin", aecMap);
	}

}
