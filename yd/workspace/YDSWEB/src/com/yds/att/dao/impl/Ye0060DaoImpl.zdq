package com.yds.att.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.att.bean.AttInfo;
import com.yds.att.bean.AttOvertime;
import com.yds.att.bean.Ye0060CondA;
import com.yds.att.bean.Ye0060Overtime;
import com.yds.att.dao.Ye0060Dao;
import com.yds.base.dao.AbstractBaseDao;
import com.yds.common.bean.UserInfo;

/**
 * @see com.yds.att.dao.Ye0060Dao
 */
@Repository("ye0060Dao")
public class Ye0060DaoImpl extends AbstractBaseDao implements Ye0060Dao{

	/** 
	 *  {@inheritDoc}
	 */
	@Override
	public long getPrjOvertimeInfoCount (Ye0060CondA ye0060CondA) {
		return  (Long)super.queryForObject("ye0060Dao.getPrjOvertimeInfoCount", ye0060CondA);
	}
	/** 
	 *  {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<Ye0060Overtime> getPrjOvertimeInfo (Ye0060CondA ye0060CondA) {

		return (List<Ye0060Overtime>)super.queryForList("ye0060Dao.getPrjOvertimeInfo", ye0060CondA);
	}

	/** 
	 *  {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<UserInfo> getAppliedEmp (AttOvertime attOvertime) {
		return (List<UserInfo>)super.queryForList("ye0060Dao.getAppliedEmp", attOvertime);
	}
	
	/** 
	 *  {@inheritDoc}
	 */
	@Override
	public Object insertApplyEmp (AttOvertime attOvertime) {
		return super.insert("attOvertimeDao.insertAttOvertime", attOvertime);
	}
	
	/** 
	 *  {@inheritDoc}
	 */
	@Override
	public AttOvertime getApplyOvertimeByPk (AttOvertime attOvertime) {
		return (AttOvertime)super.queryForObject("attOvertimeDao.getAttOvertimeByPk", attOvertime);
	}
	
	/** 
	 *  {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<AttOvertime> getApplyOvertimes (AttOvertime attOvertime) {
		return (List<AttOvertime>)super.queryForList("attOvertimeDao.getAttOvertimeList", attOvertime);
	}
	
	/** 
	 *  {@inheritDoc}
	 */
	@Override
	public Object updateApplyOvertime (AttOvertime attOvertime) {
		return super.update("attOvertimeDao.updateAttOvertime", attOvertime);
	}
	
	/** 
	 *  {@inheritDoc}
	 */
	@Override
	public Object deleteApplyOvertime (AttOvertime attOvertime) {
		return super.delete("attOvertimeDao.deleteAttOvertime", attOvertime);
	}
	
	/** 
	 *  {@inheritDoc}
	 */
	@Override
	public AttInfo getAttInfoByPk (AttInfo attInfo) {
		return (AttInfo)super.queryForObject("attInfoDao.getAttInfoByPk", attInfo);
	}
	
	/** 
	 *  {@inheritDoc}
	 */
	@Override
	public Object updateAttInfoByPk (AttInfo attInfo) {
		return super.update("attInfoDao.updateAttInfo", attInfo);
	}

	/** 
	 *  {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<String> getManageDepts(String empId) {
		return (List<String>)super.queryForList("ye0060Dao.getManageDepts", empId);
	}
}
