package com.yds.att.dao.impl;

import java.util.*;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.att.dao.Ye0050Dao;
import com.yds.att.bean.AttCorrect;
import com.yds.att.bean.AttExamin;
import com.yds.att.bean.Ye0050AttCorrectInfo;
import com.yds.att.bean.Ye0050CondA;

import org.springframework.stereotype.Repository;

/**
 * @see com.yds.att.dao.Ye0050Dao
 */
@Repository("ye0050Dao")
public class Ye0050DaoImpl extends AbstractBaseDao implements Ye0050Dao{

	/** 
	 *  {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<Ye0050AttCorrectInfo> getAttCorInfo(Ye0050CondA ye0050CondA){

		return (List<Ye0050AttCorrectInfo>)super.queryForList("Ye0050Dao.getAttCorInfo", ye0050CondA);
	}
	/** 
	 *  {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<AttExamin> gerAttExaList(String appId){

		return (List<AttExamin>)super.queryForList("Ye0050Dao.gerAttExaList", appId);
	}
	@Override
	public Object updateAttCor(AttCorrect attCorrect){
		
		return super.update("Ye0050Dao.updateAttCor", attCorrect);
	}
	@Override
	public Object updateAttInfo(AttCorrect attCorrect){
		
		return super.update("Ye0050Dao.updateAttInfo", attCorrect);
	}
	@Override
	public Object updateAttExamin(AttCorrect attCorrect){
		
		return super.update("Ye0050Dao.updateAttExamin", attCorrect);
	}
	@Override
	public String getMnger(String orgId){
		
		return (String)super.queryForObject("Ye0050Dao.getMnger", orgId);
	}
	@Override
	public String getOrgPro(String orgId){
		
		return (String)super.queryForObject("Ye0050Dao.getOrgPro", orgId);
	}
}
