package com.yds.att.dao.impl;

import java.util.*;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.att.dao.Ye0010Dao;
import com.yds.att.bean.AttInfo;
import com.yds.att.bean.AttMonReport;
import com.yds.att.bean.Ye0010CondA;

import org.springframework.stereotype.Repository;

/**
 * @see com.yds.att.dao.Ye0010Dao
 */
@Repository("ye0010Dao")
public class Ye0010DaoImpl extends AbstractBaseDao implements Ye0010Dao{

	@Override
	public AttMonReport getMonReport(Ye0010CondA ye0010CondA) {

		return (AttMonReport)super.queryForObject("Ye0010Dao.getMonReport", ye0010CondA);
	}

	@Override
	public AttMonReport getYearReport(Ye0010CondA ye0010CondA) {

		return (AttMonReport)super.queryForObject("Ye0010Dao.getMonReport", ye0010CondA);
	}

	@Override
	@SuppressWarnings("unchecked")
	public List<AttInfo> getAttInfo(Ye0010CondA ye0010CondA) {

		return (List<AttInfo>)super.queryForList("Ye0010Dao.getAttInfo", ye0010CondA);
	}
}
