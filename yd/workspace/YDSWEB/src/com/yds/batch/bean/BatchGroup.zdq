package com.yds.batch.bean;

import java.util.Date;

import com.yds.base.bean.BaseBean;

/**
 * 批处理作业组信息Bean.
 * 
 * @author wangduo
 * @version 1.0 2010/04/21
 */
public class BatchGroup extends BaseBean {

	private static final long serialVersionUID = -6458360204521689715L;
	
	/** 任务组ID. */
	private String groupId;
	
	/** 任务组名. */
	private String groupName;
	
	/** 任务组注释. */
	private String groupCommont;
	
	/** 执行日期. */
	private Date execDate;
	
	/** 执行时间. */
	private String execTime;
	
	/** 任务组执行频度. */
	private String execFreq;
	
	/** 每周或每月的第几日执行. */
	private int execDay;
	
	/** 循环执行间隔时间. */
	private int execInterval;
	
	/** 任务组最近执行时间. */
	private Date lastExecDt;

	/** 任务组执行过的次数. */
	private int execCnt;

	/** 执行状态. */
	private String execStatus;
	
	/** 生效时间. */
	private Date planStartDt;
	
	/** 失效时间. */
	private Date planEndDt;

	/** 删除标识. */
	private String validFlg;

	/**
	 * @return the groupId
	 */
	public String getGroupId() {
		return groupId;
	}

	/**
	 * @param groupId the groupId to set
	 */
	public void setGroupId(String groupId) {
		this.groupId = groupId;
	}

	/**
	 * @return the groupName
	 */
	public String getGroupName() {
		return groupName;
	}

	/**
	 * @param groupName the groupName to set
	 */
	public void setGroupName(String groupName) {
		this.groupName = groupName;
	}

	/**
	 * @return the groupCommont
	 */
	public String getGroupCommont() {
		return groupCommont;
	}

	/**
	 * @param groupCommont the groupCommont to set
	 */
	public void setGroupCommont(String groupCommont) {
		this.groupCommont = groupCommont;
	}

	/**
	 * @return the execDate
	 */
	public Date getExecDate() {
		return execDate;
	}

	/**
	 * @param execDate the execDate to set
	 */
	public void setExecDate(Date execDate) {
		this.execDate = execDate;
	}

	/**
	 * @return the execTime
	 */
	public String getExecTime() {
		return execTime;
	}

	/**
	 * @param execTime the execTime to set
	 */
	public void setExecTime(String execTime) {
		this.execTime = execTime;
	}

	/**
	 * @return the execFreq
	 */
	public String getExecFreq() {
		return execFreq;
	}

	/**
	 * @param execFreq the execFreq to set
	 */
	public void setExecFreq(String execFreq) {
		this.execFreq = execFreq;
	}

	/**
	 * @return the execDay
	 */
	public int getExecDay() {
		return execDay;
	}

	/**
	 * @param execDay the execDay to set
	 */
	public void setExecDay(int execDay) {
		this.execDay = execDay;
	}

	/**
	 * @return the execInterval
	 */
	public int getExecInterval() {
		return execInterval;
	}

	/**
	 * @param execInterval the execInterval to set
	 */
	public void setExecInterval(int execInterval) {
		this.execInterval = execInterval;
	}

	/**
	 * @return the lastExecDt
	 */
	public Date getLastExecDt() {
		return lastExecDt;
	}

	/**
	 * @param lastExecDt the lastExecDt to set
	 */
	public void setLastExecDt(Date lastExecDt) {
		this.lastExecDt = lastExecDt;
	}

	/**
	 * @return the execCnt
	 */
	public int getExecCnt() {
		return execCnt;
	}

	/**
	 * @param execCnt the execCnt to set
	 */
	public void setExecCnt(int execCnt) {
		this.execCnt = execCnt;
	}

	/**
	 * @return the execStatus
	 */
	public String getExecStatus() {
		return execStatus;
	}

	/**
	 * @param execStatus the execStatus to set
	 */
	public void setExecStatus(String execStatus) {
		this.execStatus = execStatus;
	}

	/**
	 * @return the planStartDt
	 */
	public Date getPlanStartDt() {
		return planStartDt;
	}

	/**
	 * @param planStartDt the planStartDt to set
	 */
	public void setPlanStartDt(Date planStartDt) {
		this.planStartDt = planStartDt;
	}

	/**
	 * @return the planEndDt
	 */
	public Date getPlanEndDt() {
		return planEndDt;
	}

	/**
	 * @param planEndDt the planEndDt to set
	 */
	public void setPlanEndDt(Date planEndDt) {
		this.planEndDt = planEndDt;
	}

	/**
	 * @return the validFlg
	 */
	public String getValidFlg() {
		return validFlg;
	}

	/**
	 * @param validFlg the validFlg to set
	 */
	public void setValidFlg(String validFlg) {
		this.validFlg = validFlg;
	}
	
}

