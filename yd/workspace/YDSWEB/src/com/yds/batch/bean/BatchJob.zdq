package com.yds.batch.bean;

import java.util.Date;

import com.yds.base.bean.BaseBean;

/**
 * 批处理任务信息Bean.
 * 
 * @author wangduo
 * @version 1.0 2010/04/21
 */
public class BatchJob extends BaseBean {
	
	private static final long serialVersionUID = 2692060402396137338L;

	/** 任务所在组ID. */
	private String groupId;
	
	/** 任务所在组执行过的次数. */
	private int groupExecCnt;
	
	/** 任务顺位. */
	private int jobSeq;
	
	/** 任务ID. */
	private String jobId;
	
	/** 任务名. */
	private String jobName;
	
	/** 任务执行类. */
	private String jobClass;
	
	/** 任务注释. */
	private String jobCommont;

	/** 任务执行状态. */
	private String execStatus;

	/** 任务执行开始时间. */
	private Date execStartDt;
	
	/** 任务执行结束时间. */
	private Date execEndDt;	
	
	/** 影响系统标识. */
	private String sysAffectFlg;
	
	/** 可重试标识. */
	private String retryFlg;
	
	/** 可重试次数. */
	private int retryCnt;
	
	/** 重试时间间隔. */
	private int retryInterval;
	
	/** 有效标识. */
	private String validFlg;
	
	/** 是否继续标识. */
	private String executeFlg;

	/**
	 * @return the executeFlg
	 */
	public String getExecuteFlg() {
		return executeFlg;
	}

	/**
	 * @param executeFlg the executeFlg to set
	 */
	public void setExecuteFlg(String executeFlg) {
		this.executeFlg = executeFlg;
	}

	/**
	 * @return the groupId
	 */
	public String getGroupId() {
		return groupId;
	}

	/**
	 * @param groupId the groupId to set
	 */
	public void setGroupId(String groupId) {
		this.groupId = groupId;
	}

	/**
	 * @return the groupExecCnt
	 */
	public int getGroupExecCnt() {
		return groupExecCnt;
	}

	/**
	 * @param groupExecCnt the groupExecCnt to set
	 */
	public void setGroupExecCnt(int groupExecCnt) {
		this.groupExecCnt = groupExecCnt;
	}

	/**
	 * @return the jobSeq
	 */
	public int getJobSeq() {
		return jobSeq;
	}

	/**
	 * @param jobSeq the jobSeq to set
	 */
	public void setJobSeq(int jobSeq) {
		this.jobSeq = jobSeq;
	}

	/**
	 * @return the jobId
	 */
	public String getJobId() {
		return jobId;
	}

	/**
	 * @param jobId the jobId to set
	 */
	public void setJobId(String jobId) {
		this.jobId = jobId;
	}

	/**
	 * @return the jobName
	 */
	public String getJobName() {
		return jobName;
	}

	/**
	 * @param jobName the jobName to set
	 */
	public void setJobName(String jobName) {
		this.jobName = jobName;
	}

	/**
	 * @return the jobClass
	 */
	public String getJobClass() {
		return jobClass;
	}

	/**
	 * @param jobClass the jobClass to set
	 */
	public void setJobClass(String jobClass) {
		this.jobClass = jobClass;
	}

	/**
	 * @return the jobCommont
	 */
	public String getJobCommont() {
		return jobCommont;
	}

	/**
	 * @param jobCommont the jobCommont to set
	 */
	public void setJobCommont(String jobCommont) {
		this.jobCommont = jobCommont;
	}

	/**
	 * @return the execStatus
	 */
	public String getExecStatus() {
		return execStatus;
	}

	/**
	 * @param execStatus the execStatus to set
	 */
	public void setExecStatus(String execStatus) {
		this.execStatus = execStatus;
	}

	/**
	 * @return the execStartDt
	 */
	public Date getExecStartDt() {
		return execStartDt;
	}

	/**
	 * @param execStartDt the execStartDt to set
	 */
	public void setExecStartDt(Date execStartDt) {
		this.execStartDt = execStartDt;
	}

	/**
	 * @return the execEndDt
	 */
	public Date getExecEndDt() {
		return execEndDt;
	}

	/**
	 * @param execEndDt the execEndDt to set
	 */
	public void setExecEndDt(Date execEndDt) {
		this.execEndDt = execEndDt;
	}

	/**
	 * @return the sysAffectFlg
	 */
	public String getSysAffectFlg() {
		return sysAffectFlg;
	}

	/**
	 * @param sysAffectFlg the sysAffectFlg to set
	 */
	public void setSysAffectFlg(String sysAffectFlg) {
		this.sysAffectFlg = sysAffectFlg;
	}

	/**
	 * @return the retryFlg
	 */
	public String getRetryFlg() {
		return retryFlg;
	}

	/**
	 * @param retryFlg the retryFlg to set
	 */
	public void setRetryFlg(String retryFlg) {
		this.retryFlg = retryFlg;
	}

	/**
	 * @return the retryCnt
	 */
	public int getRetryCnt() {
		return retryCnt;
	}

	/**
	 * @param retryCnt the retryCnt to set
	 */
	public void setRetryCnt(int retryCnt) {
		this.retryCnt = retryCnt;
	}

	/**
	 * @return the retryInterval
	 */
	public int getRetryInterval() {
		return retryInterval;
	}

	/**
	 * @param retryInterval the retryInterval to set
	 */
	public void setRetryInterval(int retryInterval) {
		this.retryInterval = retryInterval;
	}

	/**
	 * @return the validFlg
	 */
	public String getValidFlg() {
		return validFlg;
	}

	/**
	 * @param validFlg the validFlg to set
	 */
	public void setValidFlg(String validFlg) {
		this.validFlg = validFlg;
	}

}

