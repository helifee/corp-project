package com.yds.batch.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.batch.bean.BatchJob;
import com.yds.batch.dao.BatchJobDao;

/**
 * @see BatchJobDao
 */
@Repository("batchJobDao")
public class BatchJobDaoImpl extends AbstractBaseDao implements BatchJobDao{

	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<BatchJob> getGroupJobs(String groupId) {
		return (List<BatchJob>)super.queryForList("BatchJobDao.getGroupJobs", groupId);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertExecHis(BatchJob batchJob) {
		super.insert("BatchJobDao.insertExecHis", batchJob);
	}
	
}
