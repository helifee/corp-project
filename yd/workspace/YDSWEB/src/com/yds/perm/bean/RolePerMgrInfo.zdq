package com.yds.perm.bean;

import com.yds.base.bean.BaseBean;

/**
 * 权限管理：角色信息&授权画面用Bean
 * 
 * @author jiaosunquan
 * @version 1.0 2010/01/28
 */

public class RolePerMgrInfo extends BaseBean{

	private static final long serialVersionUID = 8069603018834165972L;
	private String permitPermitId; // 系统基本权限表.权限/模块ID
	private String checkPermitId; // 权限ID（判断是否被选中）
	private String isPerm; // 是否为权限
	private String menuName; // 菜单名称
	private String dispSeq; // 显示顺序
	private boolean checkFlg; // 是否被选中Flg
	
	/**
	 * @return the permitPermitId
	 */
	public String getPermitPermitId() {
		return permitPermitId;
	}

	/**
	 * @param permitPermitId
	 *            the permitPermitId to set
	 */
	public void setPermitPermitId(String permitPermitId) {
		this.permitPermitId = permitPermitId;
	}

	/**
	 * @return the checkPermitId
	 */
	public String getCheckPermitId() {
		return checkPermitId;
	}

	/**
	 * @param checkPermitId the checkPermitId to set
	 */
	public void setCheckPermitId(String checkPermitId) {
		this.checkPermitId = checkPermitId;
	}

	/**
	 * @return the isPerm
	 */
	public String getIsPerm() {
		return isPerm;
	}

	/**
	 * @param isPerm
	 *            the isPerm to set
	 */
	public void setIsPerm(String isPerm) {
		this.isPerm = isPerm;
	}

	/**
	 * @return the menuName
	 */
	public String getMenuName() {
		return menuName;
	}

	/**
	 * @param menuName
	 *            the menuName to set
	 */
	public void setMenuName(String menuName) {
		this.menuName = menuName;
	}

	/**
	 * @return the dispSeq
	 */
	public String getDispSeq() {
		return dispSeq;
	}

	/**
	 * @param dispSeq
	 *            the dispSeq to set
	 */
	public void setDispSeq(String dispSeq) {
		this.dispSeq = dispSeq;
	}

	/**
	 * @return the checkFlg
	 */
	public boolean isCheckFlg() {
		return checkFlg;
	}

	/**
	 * @param checkFlg the checkFlg to set
	 */
	public void setCheckFlg(boolean checkFlg) {
		this.checkFlg = checkFlg;
	}

}
