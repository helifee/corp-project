package com.yds.perm.bean;


import com.yds.base.bean.BaseBean;

/**
 * 用户授权信息Bean
 * 
 * @author yaokai
 * @version 1.0 2010/02/04
 */
public class PerUserPermitInfo extends BaseBean {

	private static final long serialVersionUID = -988470857699566776L;
	private String userId; // 用户ID
	private String permFlag; // 权限区分
	private String posRoleId; // 职位/角色ID
	private String posRoleNm; // 职位/角色名称
	private String operatorId; // 操作人
	private String permitStaDate; // 权限开始时间
	private String oldPermitStaDate; // 权限开始时间
	private String permitEndDate; // 权限终了时间
	private String insDelFlag;//插入删除判定flag 0:删除  1:修改  2:插入
	private String updateTime; // 更新时间

	/**
	 * @return the posRoleNm
	 */
	public String getPosRoleNm() {
		return posRoleNm;
	}

	/**
	 * @param posRoleNm the posRoleNm to set
	 */
	public void setPosRoleNm(String posRoleNm) {
		this.posRoleNm = posRoleNm;
	}
	
	/**
	 * @return the userId
	 */
	public String getUserId() {
		return userId;
	}

	/**
	 * @param userId
	 *            the userId to set
	 */
	public void setUserId(String userId) {
		this.userId = userId;
	}

	/**
	 * @return the permFlag
	 */
	public String getPermFlag() {
		return permFlag;
	}

	/**
	 * @param permFlag
	 *            the permFlag to set
	 */
	public void setPermFlag(String permFlag) {
		this.permFlag = permFlag;
	}

	/**
	 * @return the posRoleId
	 */
	public String getPosRoleId() {
		return posRoleId;
	}

	/**
	 * @param posRoleId
	 *            the posRoleId to set
	 */
	public void setPosRoleId(String posRoleId) {
		this.posRoleId = posRoleId;
	}

	/**
	 * @return the operatorId
	 */
	public String getOperatorId() {
		return operatorId;
	}

	/**
	 * @param operatorId
	 *            the operatorId to set
	 */
	public void setOperatorId(String operatorId) {
		this.operatorId = operatorId;
	}

	/**
	 * @return the updateTime
	 */
	public String getUpdateTime() {
		return updateTime;
	}

	/**
	 * @param updateTime the updateTime to set
	 */
	public void setUpdateTime(String updateTime) {
		this.updateTime = updateTime;
	}

	/**
	 * @return the permitStaDate
	 */
	public String getPermitStaDate() {
		return permitStaDate;
	}

	/**
	 * @param permitStaDate the permitStaDate to set
	 */
	public void setPermitStaDate(String permitStaDate) {
		this.permitStaDate = permitStaDate;
	}

	/**
	 * @return the permitEndDate
	 */
	public String getPermitEndDate() {
		return permitEndDate;
	}

	/**
	 * @param permitEndDate the permitEndDate to set
	 */
	public void setPermitEndDate(String permitEndDate) {
		this.permitEndDate = permitEndDate;
	}

	/**
	 * @return the oldPermitStaDate
	 */
	public String getOldPermitStaDate() {
		return oldPermitStaDate;
	}

	/**
	 * @param oldPermitStaDate the oldPermitStaDate to set
	 */
	public void setOldPermitStaDate(String oldPermitStaDate) {
		this.oldPermitStaDate = oldPermitStaDate;
	}

	/**
	 * @return the insDelFlag
	 */
	public String getInsDelFlag() {
		return insDelFlag;
	}

	/**
	 * @param insDelFlag the insDelFlag to set
	 */
	public void setInsDelFlag(String insDelFlag) {
		this.insDelFlag = insDelFlag;
	}

}
