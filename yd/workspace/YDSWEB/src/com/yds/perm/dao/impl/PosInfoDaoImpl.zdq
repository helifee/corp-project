package com.yds.perm.dao.impl;

import java.util.List;
import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.perm.bean.PosInfo;
import com.yds.perm.dao.PosInfoDao;

/**
 * @see com.yds.perm.dao.PosInfoDao
 */
@Repository("posInfoDao")
public class PosInfoDaoImpl extends AbstractBaseDao implements PosInfoDao {
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<PosInfo> getPosInfoList(){
		return (List<PosInfo>)super.queryForList("PosInfoDao.getPosInfoList", "");
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public void updatePosInfo(PosInfo posInfo) {
		super.update("PosInfoDao.updatePosInfo", posInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void insertPosInfo(PosInfo posInfo) {
		super.insert("PosInfoDao.insertPosInfo", posInfo);
	}	
}
