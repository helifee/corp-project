package com.yds.perm.dao;

import java.util.List;

import com.yds.perm.bean.PosInfo;

/**
 * 职位编辑处理（数据库访问）
 * 
 * @author chenjunshuai
 * @version 1.00 2010/01/28
 */
public interface PosInfoDao {
	
	/**
	 * 取得职位信息一览
	 * 
	 * @return List<PosInfo> 职位信息一览
	 */
	public List<PosInfo> getPosInfoList();
	
	/**
	 * 职位信息的更新
	 * 
	 * @param updatePosInfo 要更新的职位信息
	 */
	public abstract void updatePosInfo(PosInfo posInfo);
	
	/**
	 * 职位信息的插入
	 * 
	 * @param insertPosInfo 要插入的职位信息
	 */
	public abstract void insertPosInfo(PosInfo posInfo);
}
