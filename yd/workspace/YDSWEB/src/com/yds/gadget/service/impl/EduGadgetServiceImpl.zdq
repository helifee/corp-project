package com.yds.gadget.service.impl;

import java.util.List;

import org.springframework.stereotype.Service;

import com.yds.base.service.AbstractBaseService;
import com.yds.gadget.dao.EduGadgetDao;
import com.yds.gadget.service.EduGadgetService;
import com.yds.tt.manager.service.TtCommonUtil;
import com.yds.tt.testing.bean.ExamineInfo;
import com.yds.tt.training.bean.CourseInfo;

/**
 * @see com.yds.gadget.service.EduGadgetService
 * 
 */
@Service("eduGadgetService")
public class EduGadgetServiceImpl extends AbstractBaseService implements
		EduGadgetService {

	private EduGadgetDao eduGadgetDao;

	/**
	 * {@inheritDoc}
	 */
	@Override
	public List<ExamineInfo> getExamineInfos() {

		return eduGadgetDao.getExamineInfos(TtCommonUtil.getLoginUserId());
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public List<CourseInfo> getCourseInfos() {

		return eduGadgetDao.getCourseInfos(TtCommonUtil.getLoginUserId());
	}

	/**
	 * @param eduGadgetDao
	 *            the eduGadgetDao to set
	 */
	public void setEduGadgetDao(EduGadgetDao eduGadgetDao) {
		this.eduGadgetDao = eduGadgetDao;
	}

}
