package com.yds.gadget.service.impl;

import java.util.List;

import org.springframework.stereotype.Service;

import com.yds.base.service.AbstractBaseService;
import com.yds.gadget.service.CalendarGadgetService;
import com.yds.gadget.bean.CalendarGadgetInfo;
import com.yds.gadget.dao.CalendarGadgetDao;

/**
 * @see com.yds.gadget.service.CalendarGadgetService
 *
 */
@Service("calendarGadgetService")
public class CalendarGadgetServiceImpl extends AbstractBaseService implements
		CalendarGadgetService {

	private CalendarGadgetDao calendarGadgetDao;
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public List<CalendarGadgetInfo> getCalendarGadgetInfoList() {
		return calendarGadgetDao.getCalendarGadgetInfoList();
	}

	public void setCalendarGadgetDao(CalendarGadgetDao calendarGadgetDao) {
		this.calendarGadgetDao = calendarGadgetDao;
	}
}
