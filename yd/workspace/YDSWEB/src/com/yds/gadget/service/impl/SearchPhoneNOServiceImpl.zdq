package com.yds.gadget.service.impl;


import org.springframework.stereotype.Service;

import com.yds.base.service.AbstractBaseService;
import com.yds.gadget.dao.SearchPhoneNODao;
import com.yds.gadget.service.SearchPhoneNOService;
import com.yds.util.service.StringUtil;


/**
 * @see com.yds.gadget.service.SearchPhoneNOService
 */
@Service("searchPhoneNOService")
public class SearchPhoneNOServiceImpl extends AbstractBaseService implements SearchPhoneNOService {

	/** SearchPhoneNODao */
	private SearchPhoneNODao searchPhoneNODao;
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public String getPhoneNO(String userId){
		String ret = searchPhoneNODao.getPhoneNO(StringUtil.leftPadYd(userId));
		if (null == ret) {
			ret = "";
		}
		return ret;
	}

	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public String getSeatId(String userId){
		String ret = searchPhoneNODao.getSeatId(StringUtil.leftPadYd(userId));
		if (null == ret) {
			ret = "";
		}
		return ret;
	}

	/**
	 * @param searchPhoneNODao the searchPhoneNODao to set
	 */
	public void setSearchPhoneNODao(SearchPhoneNODao searchPhoneNODao) {
		this.searchPhoneNODao = searchPhoneNODao;
	}

}
