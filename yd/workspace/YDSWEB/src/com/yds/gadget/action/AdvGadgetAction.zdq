package com.yds.gadget.action;

import java.util.List;

import org.springframework.beans.factory.config.BeanDefinition;
import org.springframework.context.annotation.Scope;
import org.springframework.stereotype.Controller;

import com.yds.base.action.AbstractBaseAction;
import com.yds.gadget.service.AdvGadgetService;

/**
 * 广告Action
 * 
 * @author 远东)yuanjinling
 * @version 1.00 2010/09/09
 */
@Scope(BeanDefinition.SCOPE_PROTOTYPE)
@Controller("advGadgetAction")
public class AdvGadgetAction extends AbstractBaseAction{

	/**
	 * 
	 */
	private static final long serialVersionUID = 8589835958798301839L;
	private AdvGadgetService advGadgetService;
	private List<String> slideList;
	
	public String findSlideList(){
		
		slideList=advGadgetService.getSlideList();
		
		return SUCCESS;
	}

	public void setSlideList(List<String> slideList) {
		this.slideList = slideList;
	}

	public List<String> getSlideList() {
		return slideList;
	}
	
	public void setAdvGadgetService(AdvGadgetService advGadgetService) {
		this.advGadgetService = advGadgetService;
	}
	
	

}
