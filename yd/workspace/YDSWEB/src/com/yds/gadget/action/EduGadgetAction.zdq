package com.yds.gadget.action;

import java.util.List;

import org.springframework.beans.factory.config.BeanDefinition;
import org.springframework.context.annotation.Scope;
import org.springframework.stereotype.Controller;

import com.yds.base.action.AbstractBaseAction;
import com.yds.gadget.service.EduGadgetService;
import com.yds.tt.testing.bean.ExamineInfo;
import com.yds.tt.training.bean.CourseInfo;

/**
 * 公告新闻Action
 * 
 * @author 远东)liuzhongnan
 * @version 1.00 2010/08/31
 */
@Scope(BeanDefinition.SCOPE_PROTOTYPE)
@Controller("eduGadgetAction")
public class EduGadgetAction extends AbstractBaseAction {

	private static final long serialVersionUID = 8862959418989112553L;
	private EduGadgetService eduGadgetService;
	private List<ExamineInfo> examineInfoList;
	private List<CourseInfo> courseInfoList;

	public String findNewsList() {
		examineInfoList = eduGadgetService.getExamineInfos();
		courseInfoList = eduGadgetService.getCourseInfos();
		return SUCCESS;
	}

	/**
	 * @param eduGadgetService
	 *            the eduGadgetService to set
	 */
	public void setEduGadgetService(EduGadgetService eduGadgetService) {
		this.eduGadgetService = eduGadgetService;
	}

	/**
	 * @return the examineInfoList
	 */
	public List<ExamineInfo> getExamineInfoList() {
		return examineInfoList;
	}

	/**
	 * @param examineInfoList
	 *            the examineInfoList to set
	 */
	public void setExamineInfoList(List<ExamineInfo> examineInfoList) {
		this.examineInfoList = examineInfoList;
	}

	/**
	 * @return the courseInfoList
	 */
	public List<CourseInfo> getCourseInfoList() {
		return courseInfoList;
	}

	/**
	 * @param courseInfoList
	 *            the courseInfoList to set
	 */
	public void setCourseInfoList(List<CourseInfo> courseInfoList) {
		this.courseInfoList = courseInfoList;
	}
}
