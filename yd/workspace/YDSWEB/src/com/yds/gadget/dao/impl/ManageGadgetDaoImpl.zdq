package com.yds.gadget.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.gadget.bean.ActiveDeskInfo;
import com.yds.gadget.bean.GadgetInfo;
import com.yds.gadget.dao.ManageGadgetDao;

/**
 * @see com.yds.gadget.dao.ManageGadgetDao
 */
@Repository("manageGadgetDao")
public class ManageGadgetDaoImpl extends AbstractBaseDao implements ManageGadgetDao {

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<GadgetInfo> getManageGadgetInfoList(String userId) {
		return super.queryForList("ManageGadgetDao.getManageGadgetInfoList",userId);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public void addGadget(ActiveDeskInfo gadget)
	{
		super.insert("ManageGadgetDao.addGadget", gadget);
	}
	/**
	 * {@inheritDoc}
	 */
	@Override
	public int getGdsCount(int gdsId) {		
		return (Integer)super.queryForObject("ManageGadgetDao.getGdsCount", gdsId);
	}
}
