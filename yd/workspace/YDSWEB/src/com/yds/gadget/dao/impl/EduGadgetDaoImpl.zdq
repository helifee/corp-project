package com.yds.gadget.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.gadget.dao.EduGadgetDao;
import com.yds.tt.testing.bean.ExamineInfo;
import com.yds.tt.training.bean.CourseInfo;

/**
 * @see com.yds.gadget.dao.EduGadgetDao
 */
@Repository("eduGadgetDao")
public class EduGadgetDaoImpl extends AbstractBaseDao implements EduGadgetDao {

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<ExamineInfo> getExamineInfos(String userId) {
		return super.queryForList("EduGadgetDao.getExamineInfos", userId);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<CourseInfo> getCourseInfos(String userId) {
		return super.queryForList("EduGadgetDao.getCourseInfos", userId);
	}

}
