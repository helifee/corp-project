package com.yds.gadget.dao.impl;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.gadget.dao.SearchPhoneNODao;

/**
 * @see com.yds.gadget.dao.SearchPhoneNODao
 */
@Repository("searchPhoneNODao")
public class SearchPhoneNODaoImpl extends AbstractBaseDao implements SearchPhoneNODao {

	/**
	 * {@inheritDoc}
	 */
	@Override
	public String getPhoneNO(String userId) {
		return (String)super.queryForObject("SearchPhoneNODao.getPhoneNO", userId);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public String getSeatId(String userId) {
		return (String)super.queryForObject("SearchPhoneNODao.getSeatId", userId);
	}

}
