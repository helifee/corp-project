<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	<field name="tansferCondA.exMoney">
		<field-validator type="double">
			<param name="minInclusive">0.01</param>
            <param name="maxInclusive">999.99</param>
            <message>${getText("yds.com.warning.0014","",{"转账金额","0.01","999.99"})}</message>
        </field-validator>
	</field>
	
	<field name="tansferCondA.inId">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"转账对象"})}</message>
		</field-validator>
    </field>
</validators>