<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	<field name="orderInfo.orderContent">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"订单标题"})}</message>
		</field-validator>
	</field>
	<field name="orderInfo.goodsCateId">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"商品分类"})}</message>
		</field-validator>
	</field>
	<field name="exchange">
		<field-validator type="double">
			<param name="minInclusive">0.00</param>
            <param name="maxInclusive">999.99</param>
            <message>${getText("yds.com.warning.0014","",{"金额","0.00","999.99"})}</message>
        </field-validator>
	</field>
</validators>