package com.yds.gps.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.gps.bean.GpsAccount;
import com.yds.gps.bean.GpsExchangeHis;
import com.yds.gps.bean.GpsExchangeSum;
import com.yds.gps.bean.Yd0020CondA;
import com.yds.gps.dao.Yd0020Dao;

/**
 * @see com.yds.gps.dao.Yd0020Dao
 */
@Repository("yd0020Dao")
public class Yd0020DaoImpl extends AbstractBaseDao implements Yd0020Dao {

	/**
	 * {@inheritDoc}
	 */
	@Override
	public String getComRemain() {

		return (String) super.queryForObject("yd0020Dao.getComRemain");
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public String getPesonRemain(String pesAccId) {

		return (String) super.queryForObject("yd0020Dao.getPesonRemain",
				pesAccId);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public GpsAccount getCafeAccount() {

		return (GpsAccount) super.queryForObject("yd0020Dao.getCafeAccount");
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public GpsAccount getGpsAccount() {

		return (GpsAccount) super.queryForObject("yd0020Dao.getGpsAccount");
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<GpsExchangeHis> getExchangeHisList(Yd0020CondA yd0020CondA,
			int offset, int perCounts) {

		return super.queryForList("yd0020Dao.getExchangeHisList", yd0020CondA,
				offset, perCounts);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Long getTotalCount(Yd0020CondA yd0020CondA) {

		return (Long) super.queryForObject("yd0020Dao.getTotalCount",
				yd0020CondA);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public GpsExchangeSum getExchangeHisSum(Yd0020CondA yd0020CondA) {

		return (GpsExchangeSum) super.queryForObject(
				"yd0020Dao.getExchangeHisSum", yd0020CondA);

	}
}
