 */
@Repository("yd0060Dao")
public class Yd0060DaoImpl extends AbstractBaseDao implements Yd0060Dao {

	@Override
	public Yd0060OrderInfo getGpsOrderInfo(String orderId) {
		return (Yd0060OrderInfo)super.queryForObject("Yd0060Dao.getGpsOrderInfo", orderId);
	}

	@SuppressWarnings("unchecked")
	@Override
	public List<GpsOrderDetail> getGpsOrderDetail(String orderId) {
		return (List<GpsOrderDetail>)super.queryForList("Yd0060Dao.getGpsOrderDetail", orderId);
	}
	
	@Override
	public GpsOrderInfo getOrderInfo(String orderId){
		
		return (GpsOrderInfo)super.queryForObject("Yd0060Dao.getOrderInfo", orderId);
	}
	
	@Override
	public void deleteOrder(String orderId) {
		super.delete("Yd0060Dao.deleteOrder", orderId);
		
	}
	
	@SuppressWarnings("unchecked")
	@Override
	public List<GpsOrderDetail> getOrders(String orderId) {
		return (List<GpsOrderDetail>)super.queryForList("Yd0060Dao.getOrders", orderId);
	}
	
	@Override
