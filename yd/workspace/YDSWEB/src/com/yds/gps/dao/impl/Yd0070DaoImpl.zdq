package com.yds.gps.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;
import com.yds.base.dao.AbstractBaseDao;
import com.yds.gps.bean.GpsGoodsInfo;
import com.yds.gps.bean.GpsOrderDetail;
import com.yds.gps.bean.GpsOrderGoods;
import com.yds.gps.common.bean.GpsOrderInfo;
import com.yds.gps.dao.Yd0070Dao;

/**
 * @see com.yds.gps.dao.Yd0070Dao
 */
@Repository("yd0070Dao")
public class Yd0070DaoImpl extends AbstractBaseDao implements Yd0070Dao {
	
	public GpsOrderInfo getGpsOrderInfo(String orderId){
		return (GpsOrderInfo)super.queryForObject("yd0070Dao.getGpsOrderInfo", orderId);
	}
	
	public double getGpsAccountSum(String accId){
		return (Double) super.queryForObject("yd0070Dao.getGpsAccountSum", accId);
	}

	@SuppressWarnings("unchecked")
	public List<GpsOrderDetail> getDetailNowList(GpsOrderDetail gpsOrderDetail){
		return super.queryForList("yd0070Dao.getDetailNowList", gpsOrderDetail);
	}

	@Override
	public GpsGoodsInfo getGoodsInfo(String goodsId) {
		return (GpsGoodsInfo)super.queryForObject("yd0070Dao.getGoodsInfo", goodsId);
	}
	
	public GpsOrderDetail getOrderDetail(GpsOrderDetail gpsOrderDetail){
		return (GpsOrderDetail) super.queryForObject("yd0070Dao.getOrderDetail", gpsOrderDetail);
	}
	
	public String getCateName(String goodsId){
		return (String) super.queryForObject("yd0070Dao.getCateName", goodsId);
	}

	@SuppressWarnings("unchecked")
	public List<GpsOrderGoods> getDetailAllList(String orderId){
		return super.queryForList("yd0070Dao.getDetailAllList", orderId);
	}

	public Object deleteGpsOrderDetail(GpsOrderDetail gpsOrderDetail) {
		return super.delete("yd0070Dao.deleteGpsOrderDetail", gpsOrderDetail);
	}

	public Object deleteAllGpsOrderDetail(GpsOrderDetail gpsOrderDetail) {
		return super.delete("yd0070Dao.deleteAllGpsOrderDetail", gpsOrderDetail);
	}

	public Object insertGpsOrderDetail(GpsOrderDetail gpsOrderDetail) {
		return super.insert("yd0070Dao.insertGpsOrderDetail", gpsOrderDetail);
	}

	public Object insertGpsOrderGoods(GpsOrderDetail gpsOrderDetail) {
		return super.insert("yd0070Dao.insertGpsOrderGoods", gpsOrderDetail);
	}

	public Object updateGpsOrderDetail(GpsOrderDetail gpsOrderDetail) {
		return super.insert("yd0070Dao.updateGpsOrderDetail", gpsOrderDetail);
	}
	
	public Object updateGpsOrderGoods(GpsOrderDetail gpsOrderDetail) {
		return super.insert("yd0070Dao.updateGpsOrderGoods", gpsOrderDetail);
	}

}