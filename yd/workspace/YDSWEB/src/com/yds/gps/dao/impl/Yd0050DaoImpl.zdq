package com.yds.gps.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.gps.bean.Yd0050CondA;
import com.yds.gps.dao.Yd0050Dao;

/**
 * @see com.yds.gps.dao.Yd0050Dao
 */
@Repository("yd0050Dao")
public class Yd0050DaoImpl extends AbstractBaseDao implements Yd0050Dao {

	/**
	 * {@inheritDoc}
	 */

	@SuppressWarnings("unchecked")
	@Override
	public List<Yd0050CondA> getOrderInfoList(Yd0050CondA yd0050CondA,
			int offset, int perCounts) {
		return (List<Yd0050CondA>)super.queryForList("Yd0050Dao.getYd0050CondAList", yd0050CondA, offset, perCounts);
	}

	@Override
	public Long getTotalCount(Yd0050CondA yd0050CondA) {
		
		return (Long)super.queryForObject("Yd0050Dao.getTotalCount", yd0050CondA);
	}

}
