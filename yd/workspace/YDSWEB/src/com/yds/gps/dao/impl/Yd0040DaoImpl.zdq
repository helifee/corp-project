package com.yds.gps.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.gps.bean.GpsGoodsCate;
import com.yds.gps.bean.GpsGoodsInfo;
import com.yds.gps.dao.Yd0040Dao;

/**
 * @see com.yds.gps.dao.Yd0040Dao
 */
@Repository("yd0040Dao")
public class Yd0040DaoImpl extends AbstractBaseDao implements Yd0040Dao {

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<GpsGoodsInfo> getGoodsList(String goodsCateId) {

		return super.queryForList("yd0040Dao.getGoodsList", goodsCateId);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<GpsGoodsInfo> getGoodsSortList(String goodsCateId) {

		return super.queryForList("yd0040Dao.getGoodsSortList", goodsCateId);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public GpsGoodsInfo getGoodsInfo(String goodsId) {

		return (GpsGoodsInfo) super.queryForObject("yd0040Dao.getGoodsInfo",
				goodsId);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public String getGoodsCateName(String goodsCateId) {

		return (String) super.queryForObject("yd0040Dao.getGoodsCateName",
				goodsCateId);
	}

	/**
	 * {@inheritDoc}
	 */
	@SuppressWarnings("unchecked")
	@Override
	public List<String> getStoreList(String goodsCateId) {
		return super.queryForList("yd0040Dao.getStoreName", goodsCateId);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public int updateGoodsSort(GpsGoodsInfo gpsGoodsInfo) {

		return super.update("yd0040Dao.updateGoodsSort", gpsGoodsInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Object insertGoodsInfo(GpsGoodsInfo gpsGoodsInfo) {

		return super.insert("yd0040Dao.insertGoodsInfo", gpsGoodsInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Object updateGoodsInfo(GpsGoodsInfo gpsGoodsInfo) {

		return super.insert("yd0040Dao.updateGoodsInfo", gpsGoodsInfo);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Object insertGoodsCate(GpsGoodsCate gpsGoodsCate) {

		return super.insert("yd0040Dao.insertGoodsCate", gpsGoodsCate);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public String getMaxGoodsDisp() {

		return (String) super.queryForObject("yd0040Dao.getMaxGoodsDisp");
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public GpsGoodsCate getPubFlag(String goodsCateId) {

		return (GpsGoodsCate) super.queryForObject("yd0040Dao.getPubFlag",
				goodsCateId);
	}

}
