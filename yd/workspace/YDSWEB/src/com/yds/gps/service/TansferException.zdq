package com.yds.gps.service;

import com.yds.gps.service.GpsConstants.TansferStatusEnum;

public class TansferException extends RuntimeException {

	private static final long serialVersionUID = 6526530915547182750L;

	// 消息ID
	private TansferStatusEnum id;
	
	public TansferException(TansferStatusEnum id) {
		super(id.value());
		this.id = id;
	}
	
	public TansferStatusEnum getId() {
		return id;
	}

}
