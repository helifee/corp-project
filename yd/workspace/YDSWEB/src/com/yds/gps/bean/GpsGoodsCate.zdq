package com.yds.gps.bean;


import java.util.Date;

import com.yds.base.bean.BaseBean;

public class GpsGoodsCate extends BaseBean {

	private static final long serialVersionUID = -4237686591034311908L;

	/** 商品分类ID. */
	private String goodsCateId;

	/** 分类名称. */
	private String cateName;

	/**公开区分. */
	private String pubFlg;

	/**管理者ID. */
	private String mngrId;
	
	/**登录时间.*/
	private Date creatTime;

	/**
	 * @return the goodsCateId
	 */
	public String getGoodsCateId() {
		return goodsCateId;
	}

	/**
	 * @param goodsCateId the goodsCateId to set
	 */
	public void setGoodsCateId(String goodsCateId) {
		this.goodsCateId = goodsCateId;
	}

	/**
	 * @return the cateName
	 */
	public String getCateName() {
		return cateName;
	}

	/**
	 * @param cateName the cateName to set
	 */
	public void setCateName(String cateName) {
		this.cateName = cateName;
	}

	/**
	 * @return the pubFlg
	 */
	public String getPubFlg() {
		return pubFlg;
	}

	/**
	 * @param pubFlg the pubFlg to set
	 */
	public void setPubFlg(String pubFlg) {
		this.pubFlg = pubFlg;
	}

	/**
	 * @return the mngrId
	 */
	public String getMngrId() {
		return mngrId;
	}

	/**
	 * @param mngrId the mngrId to set
	 */
	public void setMngrId(String mngrId) {
		this.mngrId = mngrId;
	}

	/**
	 * @return the creatTime
	 */
	public Date getCreatTime() {
		return creatTime;
	}

	/**
	 * @param creatTime the creatTime to set
	 */
	public void setCreatTime(Date creatTime) {
		this.creatTime = creatTime;
	}
	
}
