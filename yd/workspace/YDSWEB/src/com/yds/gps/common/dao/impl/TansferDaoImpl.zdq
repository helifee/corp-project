package com.yds.gps.common.dao.impl;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.gps.bean.GpsAccount;
import com.yds.gps.bean.GpsExchangeHis;
import com.yds.gps.common.dao.TansferDao;
@Repository("tansferDao")
public class TansferDaoImpl extends AbstractBaseDao implements TansferDao {

	@Override
	public GpsAccount getGpsAccount(String accId) {
		
		return (GpsAccount) super.queryForObject("TansferDao.getGpsAccount", accId);
	}

	@Override
	public GpsAccount getGpsAccountForUpdate(String accId) {
		
		return (GpsAccount) super.queryForObject("TansferDao.getGpsAccountForUpdate", accId);
	}
	
	@Override
	public int updateGpsAccountForUpdate(GpsAccount account) {
		
		return super.update("TansferDao.updateGpsAccountForUpdate", account);
	}
	
	@Override
	public Object insertGpsAccount(GpsAccount account) {
		
		return super.insert("TansferDao.inertGpsAccount", account);
	}

	@Override
	public Object insertGpsExchangeHis(GpsExchangeHis exchangeHis) {
		
		return super.insert("TansferDao.inertGpsExchangeHis", exchangeHis);
	}

	@Override
	public int updateGpsAccount(GpsAccount account) {
		
		return super.update("TansferDao.updateGpsAccount", account);
	}

	@Override
	public double getGpsAccountSum() {

		return (Double) super.queryForObject("TansferDao.getGpsAccountSum");
	}

}
