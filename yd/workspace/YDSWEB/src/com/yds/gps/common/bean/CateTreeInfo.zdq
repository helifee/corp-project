package com.yds.gps.common.bean;

import com.yds.common.bean.TreeListResult;

/**
 * 商品分类树Bean
 * 
 * @author 彭川
 * @version 1.0 2010/10/21
 */

public class CateTreeInfo implements TreeListResult{
	
	/**分类区分No. */
	private String diffNo;
	
	/**分类区分Name.*/
	private String diffName;
	
	/**商品分类Id. */
	private String goodsCateId;
	
	/**商品分类名称. */
	private String cateName;
	
	/**
	 * @return the diffNo
	 */
	public String getDiffNo() {
		return diffNo;
	}

	/**
	 * @param diffNo the diffNo to set
	 */
	public void setDiffNo(String diffNo) {
		this.diffNo = diffNo;
	}

	/**
	 * @return the diffName
	 */
	public String getDiffName() {
		return diffName;
	}

	/**
	 * @param diffName the diffName to set
	 */
	public void setDiffName(String diffName) {
		this.diffName = diffName;
	}

	/**
	 * @return the goodsCateId
	 */
	public String getGoodsCateId() {
		return goodsCateId;
	}

	/**
	 * @param goodsCateId the goodsCateId to set
	 */
	public void setGoodsCateId(String goodsCateId) {
		this.goodsCateId = goodsCateId;
	}

	/**
	 * @return the cateName
	 */
	public String getCateName() {
		return cateName;
	}

	/**
	 * @param cateName the cateName to set
	 */
	public void setCateName(String cateName) {
		this.cateName = cateName;
	}
	
	//一级分类的ID对应职位类的ID 
	@Override
	public String refID1() {
		return diffNo;
	}
	//二级分类的ID对应职位的ID 
	@Override
	public String refID2() {
		return goodsCateId;	
	}
	//一级分类的名字对应职位类的名字 
	@Override
	public String refName1() {
        return diffName;
	}
	//二级分类的名字对应职位的名字 
	@Override
	public String refName2() {		
		return cateName;
	}
	

}


