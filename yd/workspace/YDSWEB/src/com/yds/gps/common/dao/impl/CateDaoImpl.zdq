package com.yds.gps.common.dao.impl;

import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.gps.common.bean.CateTreeInfo;
import com.yds.gps.common.dao.CateDao;

/**
 * @see com.yds.gps.common.dao.CateDao
 */
@Repository("cateDao")
public class CateDaoImpl extends AbstractBaseDao implements
      CateDao {
	
	@SuppressWarnings("unchecked")
	@Override
	public List<CateTreeInfo> getCateTreeInfoList(Map<String,String>map) {
		return super.queryForList("CateDao.getCateTreeInfoList", map);
	}

}
