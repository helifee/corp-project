package com.yds.gps.common.dao.impl;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.gps.common.dao.EnmDao;

/**
 * @see com.yds.gps.common.dao.EnmDao
 */
@Repository("enmDao")
public class EnmDaoImpl extends AbstractBaseDao implements
        EnmDao {
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public String getEmpName(String empId){
		
		return (String)super.queryForObject("enmDao.getEmpName",empId);
		
	}

}
