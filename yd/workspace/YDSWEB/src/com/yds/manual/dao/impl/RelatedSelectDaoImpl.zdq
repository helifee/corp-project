package com.yds.manual.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.manual.bean.Item;
import com.yds.manual.dao.RelatedSelectDao;

/**
 * @see com.yds.manual.dao.RelatedSelectDao
 */
@Repository("relatedSelectDao")
public class RelatedSelectDaoImpl extends AbstractBaseDao implements
		RelatedSelectDao {

	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<Item> getItemList(String preId) {
		
		return super.queryForList("RelatedSelectDao.getItemList", preId);
		
	}

}
