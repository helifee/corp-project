<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTDSQLMap2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="RelatedSelectDao">
	<typeAlias alias="item" type="com.yds.manual.bean.Item"/>
	
	<!-- 部门信息一览取得 --> 
	<select id="getItemList" parameterClass="String" resultClass="item">
