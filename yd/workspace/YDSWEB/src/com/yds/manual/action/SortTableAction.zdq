package com.yds.manual.action;

import java.util.List;

import org.springframework.beans.factory.config.BeanDefinition;
import org.springframework.context.annotation.Scope;
import org.springframework.stereotype.Controller;

import com.yds.base.action.AbstractBaseAction;
import com.yds.manual.bean.SortTableBook;
import com.yds.manual.service.SortTableService;

/**
 * 表格排序Action
 * 
 * @author 远东)zhangzheng
 * @version 1.00 2010/6/30
 */
@Scope(BeanDefinition.SCOPE_PROTOTYPE)
@Controller("sortTableAction")
public class SortTableAction extends AbstractBaseAction {

	private static final long serialVersionUID = 4072852268249251821L;
	
	private SortTableService sortTableService;
	private List<SortTableBook> bookList;
	
	public String execute(){
		setBookList(sortTableService.getBookList());
		return SUCCESS;
	}

	public void setBookList(List<SortTableBook> bookList) {
		this.bookList = bookList;
	}

	public List<SortTableBook> getBookList() {
		return bookList;
	}

	public void setSortTableService(SortTableService sortTableService) {
		this.sortTableService = sortTableService;
	}

}
