package com.yds.meet.dao.impl;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.yds.meet.bean.MetDaymetInfo;
import com.yds.meet.bean.Yc0040CondA;
import com.yds.meet.dao.Yc0040Dao;
import com.yds.base.dao.AbstractBaseDao;

/**
 * @see com.yds.meet.dao.Yc0040Dao
 */
@Repository("yc0040Dao")
public class Yc0040DaoImpl extends AbstractBaseDao implements Yc0040Dao {

	/**
	 * {@inheritDoc}
	 */
	@Override
	@SuppressWarnings("unchecked")
	public List<MetDaymetInfo> getMetDaymetLst(int offset, int perCounts,
			Yc0040CondA yc0040CondA) {

		return super.queryForList("Yc0040Dao.getMetDaymetInfoList",
				yc0040CondA, offset, perCounts);

	}

	@Override
	public Long getTotalCount(Yc0040CondA metDaymetInfo) {
		return (Long) super.queryForObject("Yc0040Dao.getTotalCount",
				metDaymetInfo);
	}

	@Override
	public String getMetRoomInfo(Integer metId) {
		return (String) super
				.queryForObject("Yc0040Dao.getMetRoomInfo", metId);
	}

}
