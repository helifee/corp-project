package com.yds.meet.bean;

import com.yds.base.bean.BaseBean;

public class Yc0040CondA extends BaseBean {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1005061760650484577L;

	/** 会议室ID. */
	private Integer metId;

	/** 选择按钮. */
	private String radioButton;

	/** 指定期间开始日期. */
	private String startDate;

	/** 指定期间结束日期. */
	private String endDate;

	/** 登录用户ID. */
	private String loginUserId;

	/** 页码. */
	private Integer pageNum;

	/** 记录开始数. */
	private Integer pageOffset;

	/**
	 * @return the radioButton
	 */
	public String getRadioButton() {
		return radioButton;
	}

	/**
	 * @param radioButton
	 *            the radioButton to set
	 */
	public void setRadioButton(String radioButton) {
		this.radioButton = radioButton;
	}

	/**
	 * @return the startDate
	 */
	public String getStartDate() {
		return startDate;
	}

	/**
	 * @param startDate
	 *            the startDate to set
	 */
	public void setStartDate(String startDate) {
		this.startDate = startDate;
	}

	/**
	 * @return the endDate
	 */
	public String getEndDate() {
		return endDate;
	}

	/**
	 * @param endDate
	 *            the endDate to set
	 */
	public void setEndDate(String endDate) {
		this.endDate = endDate;
