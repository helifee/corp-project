package com.yds.ws.dao.impl;

import org.springframework.stereotype.Repository;

import com.yds.base.dao.AbstractBaseDao;
import com.yds.common.bean.ComIoData;
import com.yds.ws.dao.IoDataSyncDao;

/**
 * 
 * @see com.yds.ws.dao.IoDataSyncDao
 *
 */
@Repository("ioDataSyncDao")
public class IoDataSyncDaoImpl extends AbstractBaseDao implements IoDataSyncDao {

	/**
	 * {@inheritDoc}
	 */
	@Override
	public Object saveIoData(ComIoData ioData) {
		return super.insert("ioDataSyncDao.saveIoData", ioData);
	}
	
	/**
	 * {@inheritDoc}
	 */
	@Override
	public Long getCount(ComIoData ioData) {
		return (Long) super.queryForObject("ioDataSyncDao.getCount", ioData);
	}

	/**
	 * {@inheritDoc}
	 */
	@Override
	public String getServiceName(String gateNo) {
		return (String) super.queryForObject("ioDataSyncDao.getService", gateNo);
	}


}
