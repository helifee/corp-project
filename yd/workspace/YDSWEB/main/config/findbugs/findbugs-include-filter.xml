<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
	<!--  所有类使用bugcode为HE的检测器  -->
	<Match>
		<BugCode name ="HE"/>
	</Match>
	<!--  所有类使用bugcode为DE,UrF,SIC的检测器  -->
	<Match>
		<Bug code="DE,UrF,SIC" />
	</Match>
	
	<Match>
		<Bug pattern="NM_CONFUSING" />
		<Priority value="2" />
	</Match>
</FindBugsFilter>