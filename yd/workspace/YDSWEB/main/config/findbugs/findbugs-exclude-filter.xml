<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
	<!-- EI_EXPOSE_REP  (EI, MALICIOUS_CODE):  May expose internal representation by returning reference to mutable object
		 EI_EXPOSE_REP2  (EI2, MALICIOUS_CODE):  May expose internal representation by incorporating reference to mutable object
         EI_EXPOSE_STATIC_REP2  (MS, MALICIOUS_CODE):  May expose internal static state by storing a mutable object into a static field
         MS_EXPOSE_REP  (MS, MALICIOUS_CODE):  Public static method may expose internal representation by returning array
		-->
	<Match>
		<Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2,EI_EXPOSE_STATIC_REP2,MS_EXPOSE_REP" />
	</Match>
	<Match>
		<Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR" />
	</Match>
	<Match>
		<Bug pattern="SE_BAD_FIELD" />
	</Match>

	<!-- checkstyle.EmptyBlock = PMD.EmptyCatchBlock = findbugs.UCF_USELESS_CONTROL_FLOW -->
	<Match>
		<Bug pattern="UCF_USELESS_CONTROL_FLOW" />
	</Match>
	
</FindBugsFilter>