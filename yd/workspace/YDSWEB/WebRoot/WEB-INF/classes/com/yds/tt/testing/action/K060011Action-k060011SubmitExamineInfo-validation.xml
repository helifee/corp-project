<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
	<field name="examineInfo.examineName">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"考试名称"})}</message>
		</field-validator>
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">200</param>
			<message>${getText("yds.com.warning.0007","",{"考试名称","200"})}</message>
		</field-validator>
	</field>
	<field name="executantInfo.userId">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"考试实施者ID"})}</message>
		</field-validator>
		<field-validator type="regex">
			<param name="expression">[0-9]{6}</param>
			<message>${getText("yds.com.warning.0002","",{"考试实施者ID"})}</message>
		</field-validator>
	</field>
</validators>