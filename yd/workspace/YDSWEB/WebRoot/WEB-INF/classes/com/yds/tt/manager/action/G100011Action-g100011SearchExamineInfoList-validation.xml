<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
	<field name="examineInfo.examineId">
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">8</param>
			<message>${getText("yds.com.warning.0010","",{"考试ID","8"})}</message>
		</field-validator>
		<field-validator type="regex">
			<param name="expression">^[A-Za-z0-9]*$</param>
			<message>${getText("yds.com.warning.0002","",{"考试ID"})}</message>
		</field-validator>
	</field>	
	<field name="examineInfo.examineName">
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">100</param>
			<message>${getText("yds.com.warning.0007","",{"考试名称","100"})}</message>
		</field-validator>
	</field>
</validators>