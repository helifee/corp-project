<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" 
"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	<field name="searchInfo.courseId">
		<field-validator type="regex">
			<param name="expression">^[A-Za-z0-9]+$</param>
			<message>${getText("yds.com.warning.0002","",{"课程ID"})}</message>
		</field-validator>
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">8</param>
			<message>${getText("yds.com.warning.0011","",{"课程ID","8"})}</message>
		</field-validator>
	</field>
	<field name="searchInfo.courseName">
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">200</param>
			<message>${getText("yds.com.warning.0007","",{"课程名称","200"})}</message>
			</field-validator>
	</field>
</validators>