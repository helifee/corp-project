<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="Ye0030Dao">

    <typeAlias alias="attCorrect" type="com.yds.att.bean.AttCorrect"/>
	<typeAlias alias="attInfo" type="com.yds.att.bean.AttInfo"/>
	<typeAlias alias="attExamin" type="com.yds.att.bean.AttExamin"/>

	


    <update id="updateAttCorrect" parameterClass="attCorrect">
    <![CDATA[
        UPDATE att_correct SET
	        APP_STATUS = #appStatus# ,
	        UPDATE_USER = #updateUser# ,
	        UPDATE_TIME = #updateTime# 
        WHERE 
	        APP_ID = #appId# 
    ]]>
    </update>

	 

	
	<update id="updateAttInfo" parameterClass="attInfo">
    <![CDATA[
        UPDATE att_info SET
	        ATT_STATUS_COR = #attStatusCor# ,
	        APP_ID_COR = #appIdCor# 
        WHERE 
	        EMP_ID = #empId# and
	        YEAR = #year# and
	        MONTH = #month# and
	        DAY = #day# 
    ]]>
    </update>
	
	<delete id="deleteAttExamin" parameterClass="attExamin">
    <![CDATA[
        delete from att_examin where
        APP_ID = #appId# 
    ]]>
    </delete>

</sqlMap>
