<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
	<field name="permRequestInfo.startTime">
		<field-validator type="requiredstring">
			<message>${getText("yds.com.warning.0001","",{"开始时间"})}</message>
		</field-validator>
	</field>
	<field name="permRequestInfo.applyReason">
		<field-validator type="requiredstring">
			<message>${getText("yds.com.warning.0001","",{"申请理由"})}</message>
		</field-validator>
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">200</param>
			<message>${getText("yds.com.warning.0007","",{"申请理由","200"})}</message>
		</field-validator>
	</field>
</validators>