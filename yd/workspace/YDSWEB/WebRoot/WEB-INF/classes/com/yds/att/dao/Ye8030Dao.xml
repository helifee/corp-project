<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTDSQLMap2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="ye8030Dao">
	<typeAlias alias="attBatchPar" type="com.yds.att.bean.AttBatchPar"/>
	
	<select id="getBatchPar" parameterClass="String" resultClass="attBatchPar">
		<![CDATA[
		select 
			APP_ID as appId,
			APP_YM as appYm,
			APP_PER_ID as appPerId,
			PRO1 as pro1,
			PRO2 as pro2
		from ATT_BATCH_PAR
		where
			APP_ID = #value#
		order by APP_YM,APP_PER_ID
		]]>
	</select>
	
	<delete id="deleteAttBatchPar" parameterClass="String">
    	<![CDATA[
			delete from att_batch_par 
			where
			APP_ID = #value# 
		]]>
    </delete>
</sqlMap> 