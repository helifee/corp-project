<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
	<field name="j030021Info.bookId">
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">8</param>
			<message>${getText("yds.com.warning.0011","",{"教材ID", maxLength})}</message>
		</field-validator>
		<field-validator type="regex">
			<param name="expression">^[A-Za-z0-9]+$</param>
			<message>${getText("yds.com.warning.0002","",{"教材ID"})}</message>
		</field-validator>
	</field>
	<field name="j030021Info.bookName">
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">100</param>
			<message>${getText("yds.com.warning.0007","",{"教材名称", maxLength})}</message>
		</field-validator>
	</field>
	<field name="j030021Info.createUserId">
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">6</param>
			<param name="minLength">6</param>
			<message>${getText("yds.com.warning.0003","",{"教材创建者ID", maxLength})}</message>
		</field-validator>
		<field-validator type="regex">
			<param name="expression">^[A-Za-z0-9]+$</param>
			<message>${getText("yds.com.warning.0002","",{"教材创建者ID"})}</message>
		</field-validator>
	</field>
</validators>