<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	<field name="empInfo.empId">
		<field-validator type="stringlength" short-circuit="true">
			<param name="minLength">6</param>
			<param name="maxLength">6</param>
			<message>${getText("yds.com.warning.0003","",{"员工ID","6"})}</message>
		</field-validator>
		<field-validator type="regex">
			<param name="expression">^[0-9]{0,6}$</param>
			<message>${getText("yds.com.warning.0002","",{"员工ID"})}</message>
		</field-validator>
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"员工ID"})}</message>
		</field-validator>
	</field>
	<field name="empInfo.empCpnm">
		<field-validator type="stringlength" short-circuit="true">
			<param name="minLength">0</param>
			<param name="maxLength">30</param>
			<message>${getText("yds.com.warning.0011","",{"姓名（拼音）","30"})}</message>
		</field-validator>
		<field-validator type="regex">
			<param name="expression">^[a-zA-Z]{0,30}$</param>
			<message>${getText("yds.com.warning.0002","",{"姓名（拼音）"})}</message>
		</field-validator>
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"姓名（拼音）"})}</message>
		</field-validator>
	</field>
	<field name="empInfo.empJrnm">
		<field-validator type="regex">
			<param name="expression">^[a-zA-Z]{0,30}$</param>
			<message>${getText("yds.com.warning.0002","",{"姓名（日文罗马字）"})}</message>
		</field-validator>
	</field>
	<field name="empInfo.empCnm">
		<field-validator type="regex">
			<param name="expression">^[^!\^@\^#\^$\^%\^^\^*\?\^~\^`]{0,20}$</param>
			<message>${getText("yds.com.warning.0002","",{"姓名（汉字）"})}</message>
		</field-validator>
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"姓名（汉字）"})}</message>
		</field-validator>
	</field>
	<field name="empInfo.empJknm">
		<field-validator type="regex">
			<param name="expression">^[ア-ヴ!ー]{0,20}$</param>
			<message>${getText("yds.com.warning.0002","",{"姓名（日文片假名）"})}</message>
		</field-validator>
	</field>
	<field name="empInfo.empCpsnm">
		<field-validator type="stringlength" short-circuit="true">
			<param name="minLength">0</param>
			<param name="maxLength">5</param>
			<message>${getText("yds.com.warning.0011","",{"姓名（拼音首字母）","5"})}</message>
		</field-validator>
		<field-validator type="regex">
			<param name="expression">^[a-zA-Z]{0,5}$</param>
			<message>${getText("yds.com.warning.0002","",{"姓名（拼音首字母）"})}</message>
		</field-validator>
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"姓名（拼音首字母）"})}</message>
		</field-validator>
	</field>
	<field name="empInfo.empJnm">
		<field-validator type="regex">
			<param name="expression">^[\u4e00-\u9fa5\ア-ヴ!ー]{0,20}$</param>
			<message>${getText("yds.com.warning.0002","",{"姓名（日文汉字）"})}</message>
		</field-validator>
	</field>
	<field name="empInfo.empSex">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"性别"})}</message>
		</field-validator>
	</field>
	<field name="empInfo.graduated">
		<field-validator type="stringlength" short-circuit="true">
			<param name="minLength">0</param>
			<param name="maxLength">40</param>
			<message>${getText("yds.com.warning.0011","",{"毕业院校","40"})}</message>
		</field-validator>
	</field>
	<field name="empInfo.major">
		<field-validator type="stringlength" short-circuit="true">
			<param name="minLength">0</param>
			<param name="maxLength">30</param>
			<message>${getText("yds.com.warning.0011","",{"专业","30"})}</message>
		</field-validator>
	</field>
	<field name="empInfo.startDate">
		<field-validator type="required">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"入社日期"})}</message>
		</field-validator>
	</field>
	<field name="empInfo.chargeOrgId">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"所属主管部门"})}</message>
		</field-validator>
	</field>
	<field name="empInfo.empDmnm">
		<field-validator type="stringlength" short-circuit="true">
			<param name="minLength">0</param>
			<param name="maxLength">25</param>
			<message>${getText("yds.com.warning.0011","",{"域用户名","25"})}</message>
		</field-validator>
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"域用户名"})}</message>
		</field-validator>
		<field-validator type="regex">
			<param name="expression">^['_A-Za-z0-9-]+(\.['_A-Za-z0-9-]+)*$</param>
			<message>${getText("yds.com.warning.0002","",{"域用户名"})}</message>
		</field-validator>
	</field>
	<field name="empInfo.empStatusId">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"员工类别"})}</message>
		</field-validator>
	</field>
	<field name="empInfo.empEmailComp">
		<field-validator type="stringlength" short-circuit="true">
			<param name="minLength">0</param>
			<param name="maxLength">25</param>
			<message>${getText("yds.com.warning.0011","",{"内部信箱","25"})}</message>
		</field-validator>
		<field-validator type="regex">
			<param name="expression">^['_A-Za-z0-9-]+(\.['_A-Za-z0-9-]+)*$</param>
			<message>${getText("yds.com.warning.0002","",{"内部信箱"})}</message>
		</field-validator>
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"内部信箱"})}</message>
		</field-validator>
	</field>
	<field name="empInfo.empEmailPub">
		<field-validator type="stringlength" short-circuit="true">
			<param name="minLength">0</param>
			<param name="maxLength">30</param>
			<message>${getText("yds.com.warning.0011","",{"外部信箱","30"})}</message>
		</field-validator>
		
		<field-validator type="regex">
			<param name="expression">^['_A-Za-z0-9-]+(\.['_A-Za-z0-9-]+)*@([A-Za-z0-9-])+(\.[A-Za-z0-9-]+)*((\.[A-Za-z0-9]{2,})|(\.[A-Za-z0-9]{2,}\.[A-Za-z0-9]{2,}))$</param>
			<message>${getText("yds.com.warning.0002","",{"外部信箱"})}</message>
		</field-validator>
		
		
	</field>
</validators>