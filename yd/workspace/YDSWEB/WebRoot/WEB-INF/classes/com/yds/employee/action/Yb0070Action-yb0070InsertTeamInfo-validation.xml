<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	<field name="teamInfo.teamNm">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"组名"})}</message>
		</field-validator>		
		<field-validator type="stringlength" short-circuit="true">
			<param name="minLength">0</param>
			<param name="maxLength">20</param>
			<message>${getText("yds.com.warning.0011","",{"组名","20"})}</message>
		</field-validator>
	</field>
	<field name="teamInfo.teamSnm">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"组略称"})}</message>
		</field-validator>		
		<field-validator type="stringlength" short-circuit="true">
			<param name="minLength">0</param>
			<param name="maxLength">10</param>
			<message>${getText("yds.com.warning.0011","",{"组略称","10"})}</message>
		</field-validator>
	</field>
	<field name="teamInfo.teamLeaderId">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"组长ID"})}</message>
		</field-validator>		
	</field>
	
</validators>