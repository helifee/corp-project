<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
		
	<field name="questionInfo.questionSource">
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">200</param>
			<message>${getText("yds.com.warning.0007","",{"试题来源","200"})}</message>
		</field-validator>
	</field>
		
	<field name="questionInfo.questionContent">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("yds.com.warning.0001","",{"试题内容"})}</message>
		</field-validator>
	</field>
</validators>