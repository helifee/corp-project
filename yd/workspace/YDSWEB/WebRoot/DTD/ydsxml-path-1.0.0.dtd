<?xml version="1.0" encoding="UTF-8"?>

<!--
  YDS Xml Path DTD.
  Used the following DOCTYPE.
  
  <!DOCTYPE root PUBLIC 
  		"YDS Xml Path 1.0.0" 
  		"http://web.yds.yd/DTD/ydsxml-path-1.0.0dtd">
-->


<!ELEMENT root (node+)>
<!ATTLIST root
	title CDATA #REQUIRED
	action CDATA #REQUIRED
	clickable (0|1) #IMPLIED
	pageId CDATA #IMPLIED
>

<!ELEMENT node (node*)>
<!ATTLIST node
	title CDATA #REQUIRED
	action CDATA #REQUIRED
	clickable (0|1) #IMPLIED
	pageId CDATA #IMPLIED
>

