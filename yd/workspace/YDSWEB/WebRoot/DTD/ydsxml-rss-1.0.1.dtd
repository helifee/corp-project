<?xml version="1.0" encoding="UTF-8"?>

<!--
  YDS Xml RSS DTD.
  Used the following DOCTYPE.
  
  <!DOCTYPE root PUBLIC 
  		"YDS Xml RSS 1.0.1" 
  		"http://web.yds.yd/DTD/ydsxml-rss-1.0.1.dtd">
-->


<!ELEMENT root (site+)>

<!ELEMENT site (#PCDATA)>
<!ATTLIST site
	id CDATA #REQUIRED
	name CDATA #REQUIRED
	type (CN|JP|EN|CH) #REQUIRED
	rssurl CDATA #REQUIRED
	encode (utf-8|gbk|shift-jis|euc|big5|gb2312) #IMPLIED
	siteurl CDATA #IMPLIED
>

