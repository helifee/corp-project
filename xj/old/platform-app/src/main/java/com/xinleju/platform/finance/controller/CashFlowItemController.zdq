	/**
	 * 科目对照
	 * @param paramater
	 * @return
	 */
	@RequestMapping(value="/querySyncFinaCoData",method={RequestMethod.POST}, consumes="application/json")
	public @ResponseBody MessageResult querySyncFinaCoData(@RequestBody Map<String,Object> map){
		MessageResult result=new MessageResult();
		String paramaterJson = JacksonUtils.toJson(map);
		try {
			//获取用户对象
			SecurityUserBeanInfo user=LoginUtils.getSecurityUserBeanInfo();
			//用户对象转json
			String userJson = JacksonUtils.toJson(user);
			List<FinaData> listResult = null;
			String dubboResultInfoCom = accountSetCompanyDtoServiceCustomer.queryList(userJson, paramaterJson);
			DubboServiceResultInfo dubboServiceResultInfoCom= JacksonUtils.fromJson(dubboResultInfoCom, DubboServiceResultInfo.class);
		    if(dubboServiceResultInfoCom.isSucess()){
				String resultInfo= dubboServiceResultInfoCom.getResult();
				List<AccountSetCompanyDto> list=JacksonUtils.fromJson(resultInfo, ArrayList.class,AccountSetCompanyDto.class);
				FinaQueryParams param = new FinaQueryParams();
				param.setCorpId(list.get(0).getCompanyId());
				//根据业务系统判断调用对照科目
				if(AppCod.CO.getCode().equals((String)map.get("appCode"))){
					FlowResult ResultInfoJson  =  syncFinaCoData.findSub(param);
					PageBean<FinaData> page=(PageBean<FinaData>)ResultInfoJson.getResult();
					listResult = page.getItems();
				}else if(AppCod.EX.getCode().equals((String)map.get("appCode"))){
					FlowResult ResultInfoJson  =  syncFinaExData.findSub(param);
					PageBean<FinaData> page=(PageBean<FinaData>)ResultInfoJson.getResult();
					listResult = page.getItems();
				}
				//测试用测试数据接口
				String dubboResultInfo=cashFlowItemDtoServiceCustomer.queryList(userJson, paramaterJson);
				
			    DubboServiceResultInfo dubboServiceResultInfo= JacksonUtils.fromJson(dubboResultInfo, DubboServiceResultInfo.class);
			    if(dubboServiceResultInfo.isSucess()){
					String resultInfoCash= dubboServiceResultInfo.getResult();
					List<CashFlowItemDto> listCash=JacksonUtils.fromJson(resultInfoCash, ArrayList.class,CashFlowItemDto.class);
					for(CashFlowItemDto dto : listCash){
						for(FinaData data : listResult){
							if(null!=dto.getSubjectCodes() && null!=data.get("code") && dto.getSubjectCodes().indexOf(data.get("code")+"")>-1){
								listResult.remove(data);
								break;
							}
						}
					}
					result.setResult(listResult);
					result.setSuccess(MessageInfo.GETSUCCESS.isResult());
					result.setMsg(MessageInfo.GETSUCCESS.getMsg());
			    }else{
			    	result.setSuccess(MessageInfo.GETERROR.isResult());
					result.setMsg(MessageInfo.GETERROR.getMsg()+"【"+dubboServiceResultInfo.getExceptionMsg()+"】");
			    }
		    }else{
		    	result.setSuccess(MessageInfo.GETERROR.isResult());
				result.setMsg(MessageInfo.GETERROR.getMsg()+"【"+dubboServiceResultInfoCom.getExceptionMsg()+"】");
		    }
		} catch (Exception e) {
			e.printStackTrace();
			log.error("调用queryList方法:  【参数"+paramaterJson+"】======"+"【"+e.getMessage()+"】");
			result.setSuccess(MessageInfo.GETERROR.isResult());
			result.setMsg(MessageInfo.GETERROR.getMsg()+"【"+e.getMessage()+"】");
		}
		return result;
	}
}
