package com.xinleju.cloud.oa.bbs.entity;

import com.xinleju.platform.base.annotation.Column;
import com.xinleju.platform.base.annotation.Table;
import com.xinleju.platform.base.entity.BaseEntity;


/**
 * @author admin
 * 
 * 
 */

@Table(value="OA_BBS_FORUM_PERMISSION",desc="论坛版块发帖权限表")
public class BbsForumPermission extends BaseEntity{
	
		
	@Column(value="forum_id",desc="版块ID")
	private String forumId;
  		
	@Column(value="role_id",desc="角色ID")
	private String roleId;
	
	@Column(value="role_name",desc="角色名称")
	private String roleName;
		
	public String getForumId() {
		return forumId;
	}
	public void setForumId(String forumId) {
		this.forumId = forumId;
	}
    
  		
	public String getRoleId() {
		return roleId;
	}
	public void setRoleId(String roleId) {
		this.roleId = roleId;
	}
	public String getRoleName() {
		return roleName;
	}
	public void setRoleName(String roleName) {
		this.roleName = roleName;
	}
    
  		
	
}
