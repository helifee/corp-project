    }

    public Boolean getIsEntireRow() {
        return isEntireRow;
    }

    public void setIsEntireRow(Boolean entireRow) {
        isEntireRow = entireRow;
    }

    public Boolean getIsExtendedField() {
        return isExtendedField;
    }

    public void setIsExtendedField(Boolean extendedField) {
        isExtendedField = extendedField;
    }

    public Boolean getIsReadonly() {
        return isReadonly;
    }

    public void setIsReadonly(Boolean isReadonly) {
        this.isReadonly = isReadonly;
    }

