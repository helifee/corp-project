package com.xinleju.cloud.oa.servey.entity;

import com.xinleju.platform.base.annotation.Column;
import com.xinleju.platform.base.annotation.Table;
import com.xinleju.platform.base.entity.BaseEntity;


/**
 * @author admin
 * 
 * 
 */

@Table(value="OA_SURVEY_QUESTION_OPTION",desc="题目选项")
public class ServeyQuestionOption extends BaseEntity{
	
	@Column(value="description",desc="选项描述")
	private String description;
    
  		
	@Column(value="question_id",desc="题目ID")
	private String questionId;
    
  		
	@Column(value="sequence_num",desc="选项序号")
	private String sequenceNum;
    
  		
	@Column(value="weight_factor",desc="权重(分值)")
	private Integer weightFactor;
    
	@Column(value="option_type",desc="类型")
	private Integer optionType;
  		

	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
    
  		
	public String getQuestionId() {
		return questionId;
	}
	public void setQuestionId(String questionId) {
		this.questionId = questionId;
	}
    
  		
	public String getSequenceNum() {
		return sequenceNum;
	}
	public void setSequenceNum(String sequenceNum) {
		this.sequenceNum = sequenceNum;
	}
    
  		
	public Integer getWeightFactor() {
		return weightFactor;
	}
	public void setWeightFactor(Integer weightFactor) {
		this.weightFactor = weightFactor;
	}
	
	public Integer getOptionType() {
		return optionType;
	}
	public void setOptionType(Integer optionType) {
		this.optionType = optionType;
	}
    
  		
	
}
