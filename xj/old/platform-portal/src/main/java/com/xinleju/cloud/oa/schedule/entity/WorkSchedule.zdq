@Table(value="OA_SCHEDULE_WORK_SCHEDULE",desc="任务表")
public class WorkSchedule extends BaseEntity{
	
		
	@Column(value="period_proceeding",desc="是否周期性事件")
	private String periodProceeding;
    
  		
	@Column(value="content",desc="内容")
	private String content;
    
  		
	@Column(value="type",desc="类型")
	private String type;//
    
  		
	@Column(value="repetition_frequency",desc="重复频率")
	private String repetitionFrequency;
    
  		
	@Column(value="begin_time",desc="任务开始时间")
	private String beginTime;
    
  		
	@Column(value="end_time",desc="任务结束时间")
	private String endTime;
    
  		
	@Column(value="period_begin_time",desc="周期开始时间")
