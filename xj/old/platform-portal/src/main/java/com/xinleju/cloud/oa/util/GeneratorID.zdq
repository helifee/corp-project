	public Map add(String key,Integer value){
		Map<String,Integer> map = new HashMap<String,Integer>();
	  if(map.get(key)!=null){
	    map.put(key,map.get(key)+value);
	  }else{
	    map.put(key,value);
	  }
	  return map;
	}
	
	
