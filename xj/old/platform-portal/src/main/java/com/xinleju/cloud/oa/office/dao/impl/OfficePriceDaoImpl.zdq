package com.xinleju.cloud.oa.office.dao.impl;

import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.xinleju.platform.base.dao.impl.BaseDaoImpl;
import com.xinleju.cloud.oa.office.dao.OfficePriceDao;
import com.xinleju.cloud.oa.office.entity.OfficePrice;

/**
 * @author sy
 * 
 * 
 */

@Repository
public class OfficePriceDaoImpl extends BaseDaoImpl<String,OfficePrice> implements OfficePriceDao{

	public OfficePriceDaoImpl() {
		super();
	}

	/* (non-Javadoc)
	 * @see com.xinleju.cloud.oa.office.dao.OfficePriceDao#getOfficePricePage(java.util.Map)
	 */
	@Override
	public List<Map<String, Object>> getOfficePricePage(Map map) {
		 return  getSqlSession().selectList("com.xinleju.cloud.oa.office.entity.OfficePrice.getOfficePricePage", map);
	}

	/* (non-Javadoc)
	 * @see com.xinleju.cloud.oa.office.dao.OfficePriceDao#getOfficePriceCount(java.util.Map)
	 */
	@Override
	public Integer getOfficePriceCount(Map map) {
		return  getSqlSession().selectOne("com.xinleju.cloud.oa.office.entity.OfficePrice.getOfficePriceCount", map);
	}

	
	
}
