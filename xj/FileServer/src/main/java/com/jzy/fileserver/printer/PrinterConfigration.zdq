package com.jzy.fileserver.printer;

import java.util.List;

import org.springframework.boot.context.properties.ConfigurationProperties;
import org.springframework.stereotype.Component;

import com.jzy.fileserver.domain.Printer;

/**
 * 打印机配置
 *
 * Created by daoqi on 2018年8月17日
 */
@Component
@ConfigurationProperties(prefix = "printer")
public class PrinterConfigration {

	private int timeout;
	
	private int headerLength;

	private List<Printer> address;

	public int getTimeout() {
		return timeout;
	}

	public void setTimeout(int timeout) {
		this.timeout = timeout;
	}

	public int getHeaderLength() {
		return headerLength;
	}

	public void setHeaderLength(int headerLength) {
		this.headerLength = headerLength;
	}

	public List<Printer> getAddress() {
		return address;
	}

	public void setAddress(List<Printer> address) {
		this.address = address;
	}
}
