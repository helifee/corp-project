package com.jzy.fileserver.dao;

import org.apache.ibatis.annotations.Param;

import com.jzy.fileserver.domain.FileVo;

/**
 * 文件表 
 *
 * Created by daoqi on 2018年8月9日
 */
public interface FileDao {

    /**
     * 根据Md5查询文件
     *  
     * @param fileMd5
     * @return
     */
    FileVo findByMd5(@Param("md5") String fileMd5);

	/**
	 * 保存文件
	 * 
	 * @param fileVo
	 */
	void save(FileVo fileVo);

	/**
	 * 根据ID查询文件
	 * 
	 * @param fileId
	 * @return
	 */
	FileVo findById(@Param("fileId")long fileId);
}
