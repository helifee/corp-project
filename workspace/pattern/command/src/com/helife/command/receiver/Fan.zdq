package com.helife.command.receiver;

public class Fan {

	private String state;

	public Fan() {
		this.state = "off";
	}
	
	public void start() {
		System.out.println("Fan is on!");
		setState("on");
	}
	
	public void stop() {
		System.out.println("Fan is off");
		setState("off");
	}

	public String getState() {
		return state;
	}

	public void setState(String state) {
		this.state = state;
	}
}
