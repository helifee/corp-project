package com.xinleju.cloud.oa.office.dao.impl;

import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.xinleju.platform.base.dao.impl.BaseDaoImpl;
import com.xinleju.cloud.oa.office.dao.OfficeRecordDao;
import com.xinleju.cloud.oa.office.entity.OfficeRecord;

/**
 * @author wangw
 * 
 * 
 */

@Repository
public class OfficeRecordDaoImpl extends BaseDaoImpl<String,OfficeRecord> implements OfficeRecordDao{

	public OfficeRecordDaoImpl() {
		super();
	}

	@Override
	public List<Map<String, Object>> getOfficeRecordPage(Map map) {
		// TODO Auto-generated method stub
		return  getSqlSession().selectList("com.xinleju.cloud.oa.office.entity.OfficeRecord.getOfficeRecordPage", map);
	}

	@Override
	public Integer getOfficeRecordCount(Map map) {
		// TODO Auto-generated method stub
		return  getSqlSession().selectOne("com.xinleju.cloud.oa.office.entity.OfficeRecord.getOfficeRecordCount", map);
	}

	
	
}
