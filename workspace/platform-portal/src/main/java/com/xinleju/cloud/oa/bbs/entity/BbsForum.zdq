package com.xinleju.cloud.oa.bbs.entity;

import com.xinleju.platform.base.annotation.Column;
import com.xinleju.platform.base.annotation.Table;
import com.xinleju.platform.base.entity.BaseEntity;


/**
 * @author admin
 * 
 * 
 */

@Table(value="OA_BBS_FORUM",desc="论坛版块表")
public class BbsForum extends BaseEntity{
	
		
	@Column(value="name",desc="版块名称")
	private String name;
    
  		
	@Column(value="description",desc="版块描述")
	private String description;
    
  		
	@Column(value="type_id",desc="类型ID")
	private String typeId;
    
  		
	@Column(value="status",desc="版块禁用启用:0:禁用;1:启用;")
	private String status;
    
  		
	@Column(value="anonymity_permission",desc="是否可匿名发帖：0:否;1:是;")
	private String anonymityPermission;
    
	@Column(value="portal_permission",desc="是否发布到首页：0:否;1:是;")
	private String portalPermission;
  		
	@Column(value="publish_score",desc="发表文章得分")
	private Integer publishScore;
    
  		
	@Column(value="reply_score",desc="回复文章得分")
	private Integer replyScore;
    
  		
	@Column(value="set_essence_score",desc="置为精华帖子得分")
	private Integer setEssenceScore;
    

	@Column(value="forum_manager",desc="版主")
	private String forumManager;
	
	@Column(value="forum_manager_id",desc="版主id")
	private String forumManagerId;
    
	@Column(value="sort_num",desc="序号")
	private Integer sortNum;
		
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
    
  		
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}


	public String getTypeId() {
		return typeId;
	}

	public void setTypeId(String typeId) {
		this.typeId = typeId;
	}

	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
    
  		
	public String getAnonymityPermission() {
		return anonymityPermission;
	}
	public void setAnonymityPermission(String anonymityPermission) {
		this.anonymityPermission = anonymityPermission;
	}
    
  		
	public Integer getPublishScore() {
		return publishScore;
	}
	public void setPublishScore(Integer publishScore) {
		this.publishScore = publishScore;
	}
    
  		
	public Integer getReplyScore() {
		return replyScore;
	}
	public void setReplyScore(Integer replyScore) {
		this.replyScore = replyScore;
	}
    
	public Integer getSetEssenceScore() {
		return setEssenceScore;
	}
	public void setSetEssenceScore(Integer setEssenceScore) {
		this.setEssenceScore = setEssenceScore;
	}

	public String getForumManager() {
		return forumManager;
	}
	public void setForumManager(String forumManager) {
		this.forumManager = forumManager;
	}
	public Integer getSortNum() {
		return sortNum;
	}
	public void setSortNum(Integer sortNum) {
		this.sortNum = sortNum;
	}
	public String getForumManagerId() {
		return forumManagerId;
	}
	public void setForumManagerId(String forumManagerId) {
		this.forumManagerId = forumManagerId;
	}
	public String getPortalPermission() {
		return portalPermission;
	}
	public void setPortalPermission(String portalPermission) {
		this.portalPermission = portalPermission;
	}
    
  		
	
}
