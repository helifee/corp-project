package com.xinleju.cloud.oa.feedback.dao.impl;

import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.xinleju.platform.base.dao.impl.BaseDaoImpl;
import com.xinleju.cloud.oa.feedback.dao.FeedBackDao;
import com.xinleju.cloud.oa.feedback.entity.FeedBack;

/**
 * @author admin
 * 
 * 
 */

@Repository
public class FeedBackDaoImpl extends BaseDaoImpl<String,FeedBack> implements FeedBackDao{

	public FeedBackDaoImpl() {
		super();
	}

	@Override
	public List<FeedBack> getFeedBackPageList(Map map) {
		return  getSqlSession().selectList("com.xinleju.cloud.oa.feedback.entity.FeedBack.getFeedBackPageList", map);
	}

	@Override
	public Integer getFeedBackPageListCount(Map map) {
		// TODO Auto-generated method stub
		return  getSqlSession().selectOne("com.xinleju.cloud.oa.feedback.entity.FeedBack.getFeedBackPageListCount", map);
	}

	
	
}
