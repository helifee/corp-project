package com.xinleju.cloud.oa.sys.service.impl;

import java.util.List;
import java.util.Map;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.xinleju.platform.base.service.impl.BaseServiceImpl;
import com.xinleju.cloud.oa.sys.dao.SysFloatWindowDao;
import com.xinleju.cloud.oa.sys.entity.SysFloatWindow;
import com.xinleju.cloud.oa.sys.service.SysFloatWindowService;

/**
 * @author admin
 * 
 * 
 */

@Service
public class SysFloatWindowServiceImpl extends  BaseServiceImpl<String,SysFloatWindow> implements SysFloatWindowService{
	

	@Autowired
	private SysFloatWindowDao sysFloatWindowDao;
	/**
	 * 模糊查询
	 */
	@Override
	public List<SysFloatWindow> fuzzySearch(Map map) {
		return sysFloatWindowDao.fuzzySearch(map);
	}
	/**
	 * 校验编号唯一
	 */
	@Override
	public Integer checkSerialNo(SysFloatWindow param) {
		final SysFloatWindow floatWindow = sysFloatWindowDao.getFloatWindowBySerialNo(param);
		return floatWindow==null?1:0;
	}
	
}
