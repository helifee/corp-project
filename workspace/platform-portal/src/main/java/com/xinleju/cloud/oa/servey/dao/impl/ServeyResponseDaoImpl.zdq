package com.xinleju.cloud.oa.servey.dao.impl;

import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.xinleju.platform.base.dao.impl.BaseDaoImpl;
import com.xinleju.cloud.oa.servey.dao.ServeyResponseDao;
import com.xinleju.cloud.oa.servey.entity.ServeyResponse;

/**
 * @author admin
 * 
 * 
 */

@Repository
public class ServeyResponseDaoImpl extends BaseDaoImpl<String,ServeyResponse> implements ServeyResponseDao{

	public ServeyResponseDaoImpl() {
		super();
	}

	@Override
	public List<Map<String, Object>> getServeyResponsePage(Map map) {
		// TODO Auto-generated method stub
		return getSqlSession().selectList("com.xinleju.cloud.oa.servey.entity.ServeyResponse.getServeyResponsePage", map);
	}

	@Override
	public Integer getServeyResponsePageCount(Map map) {
		// TODO Auto-generated method stub
		Integer serveyResponsePageCount = 0;
		List<Map<String, Object>> objList = getSqlSession().selectList("com.xinleju.cloud.oa.servey.entity.ServeyResponse.getServeyResponsePageCount", map);
		if(objList != null && objList.size() > 0){
			serveyResponsePageCount = objList.size();
		}
		return serveyResponsePageCount;
	}

	
	
}
