package com.xinleju.cloud.oa.favorite.dao.impl;

import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.xinleju.cloud.oa.favorite.dao.FavoriteDao;
import com.xinleju.cloud.oa.favorite.entity.Favorite;
import com.xinleju.platform.base.dao.impl.BaseDaoImpl;

/**
 * @author admin
 * 
 * 
 */

@Repository
public class FavoriteDaoImpl extends BaseDaoImpl<String,Favorite> implements FavoriteDao{

	public FavoriteDaoImpl() {
		super();
	}

	@Override
	public Favorite getFavoriteRoot(Map<String, Object> paramMap) {
		List<Favorite> list=getSqlSession().selectList("com.xinleju.cloud.oa.favorite.entity.Favorite.getFavoriteRoot", paramMap);
		if(list!=null && list.size()>0){
			return list.get(0);
		}else{
			return null;
		}
	}

	
	
}
