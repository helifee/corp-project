		<dubbo:service  interface="com.xinleju.cloud.oa.bbs.dto.service.BbsBoardDtoServiceCustomer" ref="bbsBoardDtoServiceProducer"/>
		<!-- 讨论区禁言表 -->
		<dubbo:service interface="com.xinleju.cloud.oa.bbs.dto.service.BbsForbidDtoServiceCustomer" ref="bbsForbidDtoServiceProducer"/>

    	<bean id="bbsArticleDtoServiceProducer" class="com.xinleju.cloud.oa.bbs.dto.service.impl.BbsArticleDtoServiceProducer"></bean>
    	<bean id="bbsArticleIssueAreaDtoServiceProducer" class="com.xinleju.cloud.oa.bbs.dto.service.impl.BbsArticleIssueAreaDtoServiceProducer"></bean>
    	<bean id="bbsArticleReplyDtoServiceProducer" class="com.xinleju.cloud.oa.bbs.dto.service.impl.BbsArticleReplyDtoServiceProducer"></bean>
    	<bean id="bbsBoardAuthDtoServiceProducer" class="com.xinleju.cloud.oa.bbs.dto.service.impl.BbsBoardAuthDtoServiceProducer"></bean>
    	<bean id="bbsBoardDtoServiceProducer" class="com.xinleju.cloud.oa.bbs.dto.service.impl.BbsBoardDtoServiceProducer"></bean>
    	<bean id="bbsForbidDtoServiceProducer" class="com.xinleju.cloud.oa.bbs.dto.service.impl.BbsForbidDtoServiceProducer"></bean>
