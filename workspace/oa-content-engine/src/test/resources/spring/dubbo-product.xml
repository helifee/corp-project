		<dubbo:service  interface="com.xinleju.cloud.oa.inquiry.dto.service.InquirySurveytypeExtendDtoServiceCustomer" ref="inquirySurveytypeExtendDtoServiceProducer"/>

	   	<bean id="inqueyVotedefIniteDtoServiceProducer" class="com.xinleju.cloud.oa.inquiry.dto.service.impl.InqueyVotedefIniteDtoServiceProducer"></bean>
	   	<bean id="inquiryAuthorItlesDtoServiceProducer" class="com.xinleju.cloud.oa.inquiry.dto.service.impl.InquiryAuthorItlesDtoServiceProducer"></bean>
	   	<bean id="inquiryScopeDtoServiceProducer" class="com.xinleju.cloud.oa.inquiry.dto.service.impl.InquiryScopeDtoServiceProducer"></bean>
	   	<bean id="inquirySubSurveyDtoServiceProducer" class="com.xinleju.cloud.oa.inquiry.dto.service.impl.InquirySubSurveyDtoServiceProducer"></bean>
	   	<bean id="inquirySubsurveyItemDtoServiceProducer" class="com.xinleju.cloud.oa.inquiry.dto.service.impl.InquirySubsurveyItemDtoServiceProducer"></bean>
	   	<bean id="inquirySurveyBasicDtoServiceProducer" class="com.xinleju.cloud.oa.inquiry.dto.service.impl.InquirySurveyBasicDtoServiceProducer"></bean>
	   	<bean id="inquirySurveyDiscussDtoServiceProducer" class="com.xinleju.cloud.oa.inquiry.dto.service.impl.InquirySurveyDiscussDtoServiceProducer"></bean>
	   	<bean id="inquirySurveyTypeDtoServiceProducer" class="com.xinleju.cloud.oa.inquiry.dto.service.impl.InquirySurveyTypeDtoServiceProducer"></bean>
	   	<bean id="inquirySurveytypeExtendDtoServiceProducer" class="com.xinleju.cloud.oa.inquiry.dto.service.impl.InquirySurveytypeExtendDtoServiceProducer"></bean>
	   	    <!-- 问卷调查 服务提供者  End  -->
    <dubbo:provider filter="DubboServiceFilter"></dubbo:provider>
</beans>