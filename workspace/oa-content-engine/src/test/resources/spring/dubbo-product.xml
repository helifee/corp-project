    	<bean id="officeHouseDtoServiceProducer" class="com.xinleju.cloud.oa.office.dto.service.impl.OfficeHouseDtoServiceProducer"></bean>
    	<bean id="officeInfoDtoServiceProducer" class="com.xinleju.cloud.oa.office.dto.service.impl.OfficeInfoDtoServiceProducer"></bean>
    	<bean id="officeRecordDtoServiceProducer" class="com.xinleju.cloud.oa.office.dto.service.impl.OfficeRecordDtoServiceProducer"></bean>
    	<bean id="officeOrgDtoServiceProducer" class="com.xinleju.cloud.oa.office.dto.service.impl.OfficeOrgDtoServiceProducer"></bean>
    <!-- 办公用品服务提供者  End  -->

    <!-- 日程事件服务提供者  Start  -->
		<!-- 日程事件具体信息列表 -->
		<dubbo:service  interface="com.xinleju.cloud.oa.event.dto.service.CalEventDtoServiceCustomer" ref="calEventDtoServiceProducer"/>
