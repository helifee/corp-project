		<dubbo:service  interface="com.xinleju.cloud.oa.event.dto.service.CalReplyDtoServiceCustomer" ref="calReplyDtoServiceProducer"/>

 	    <bean id="calEventDtoServiceProducer" class="com.xinleju.cloud.oa.event.dto.service.impl.CalEventDtoServiceProducer"></bean>
