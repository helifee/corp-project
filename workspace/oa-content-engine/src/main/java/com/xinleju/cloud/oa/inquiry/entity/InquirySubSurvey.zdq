package com.xinleju.cloud.oa.inquiry.entity;

import com.xinleju.platform.base.annotation.Column;
import com.xinleju.platform.base.annotation.Table;
import com.xinleju.platform.base.entity.BaseEntity;


/**
 * @author wangw
 * 
 * 
 */

@Table(value="OA_INQUIRY_SUB_SURVEY",desc="调查问题题目选择项信息")
public class InquirySubSurvey extends BaseEntity{
	
		
	@Column(value="survey_basic_id",desc="基本表ID")
	private String surveyBasicId;
    
  		
	@Column(value="max_select",desc="最大选择数")
	private String maxSelect;
    
  		
	@Column(value="title",desc="调查问题项标题")
	private String title;
    
  		
	@Column(value="discess",desc="是否允许评论")
	private String discess;
    
  		
	@Column(value="single_many",desc="单选多选标识")
	private String singleMany;
    
  		
	@Column(value="other_item",desc="是否允许其他选项")
	private String otherItem;
    
  		
	@Column(value="min_select",desc="多选最小选项数")
	private String minSelect;
    
  		
	@Column(value="sort",desc="选项排序")
	private String sort;
    
  		
	@Column(value="sub_survey_desc",desc="调查问题项描述")
	private String subSurveyDesc;
    
  		
		
	public String getSurveyBasicId() {
		return surveyBasicId;
	}
	public void setSurveyBasicId(String surveyBasicId) {
		this.surveyBasicId = surveyBasicId;
	}
    
  		
	public String getMaxSelect() {
		return maxSelect;
	}
	public void setMaxSelect(String maxSelect) {
		this.maxSelect = maxSelect;
	}
    
  		
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
    
  		
	public String getDiscess() {
		return discess;
	}
	public void setDiscess(String discess) {
		this.discess = discess;
	}
    
  		
	public String getSingleMany() {
		return singleMany;
	}
	public void setSingleMany(String singleMany) {
		this.singleMany = singleMany;
	}
    
  		
	public String getOtherItem() {
		return otherItem;
	}
	public void setOtherItem(String otherItem) {
		this.otherItem = otherItem;
	}
    
  		
	public String getMinSelect() {
		return minSelect;
	}
	public void setMinSelect(String minSelect) {
		this.minSelect = minSelect;
	}
    
  		
	public String getSort() {
		return sort;
	}
	public void setSort(String sort) {
		this.sort = sort;
	}
    
  		
	public String getSubSurveyDesc() {
		return subSurveyDesc;
	}
	public void setSubSurveyDesc(String subSurveyDesc) {
		this.subSurveyDesc = subSurveyDesc;
	}
    
  		
	
}
