package com.xinleju.cloud.oa.sys.quick.dao.impl;

import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.xinleju.cloud.oa.sys.quick.dao.EntryDao;
import com.xinleju.cloud.oa.sys.quick.entity.Entry;
import com.xinleju.platform.base.dao.impl.BaseDaoImpl;

/**
 * @author admin
 * 
 * 
 */

@Repository
public class EntryDaoImpl extends BaseDaoImpl<String,Entry> implements EntryDao{

	public EntryDaoImpl() {
		super();
	}

	@Override
	public List<Map<String, Object>> queryTreeList(Map<String,Object> map) {
		return  getSqlSession().selectList("com.xinleju.cloud.oa.sys.quick.entity.Entry.queryTreeList", map);
	}

	@Override
	public List<Map<String, Object>> fuzzySearch(Map<String,Object> map) {
		return  getSqlSession().selectList("com.xinleju.cloud.oa.sys.quick.entity.Entry.fuzzySearch", map);
	}

	@Override
	public List<Map<String, Object>> getRulerSortNum(Map<String, Object> paramater) {
		return  getSqlSession().selectList("com.xinleju.cloud.oa.sys.quick.entity.Entry.getRulerSortNum", paramater);
	}

	@Override
	public List<Entry> getEntryListByParentId(String parentId) {
		return  getSqlSession().selectList("com.xinleju.cloud.oa.sys.quick.entity.Entry.getEntryListByParentId", parentId);
	}

	
	
}
