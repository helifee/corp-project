package com.xinleju.cloud.oa.sys.dao.impl;

import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.xinleju.platform.base.dao.impl.BaseDaoImpl;
import com.xinleju.cloud.oa.sys.dao.SysCommonToolsDao;
import com.xinleju.cloud.oa.sys.entity.SysCommonTools;

/**
 * @author admin
 * 
 * 
 */

@Repository
public class SysCommonToolsDaoImpl extends BaseDaoImpl<String,SysCommonTools> implements SysCommonToolsDao{

	public SysCommonToolsDaoImpl() {
		super();
	}

	@Override
	public Integer getMaxSort() {
		Integer i= getSqlSession().selectOne("com.xinleju.cloud.oa.sys.entity.SysCommonTools.getmaxSort");
		return i;
	}

	/* (non-Javadoc)
	 * @see com.xinleju.cloud.oa.sys.dao.SysCommonToolsDao#getCommonToolsCount()
	 */
	@Override
	public Integer getCommonToolsCount() {
		Integer i= getSqlSession().selectOne("com.xinleju.cloud.oa.sys.entity.SysCommonTools.getCommonToolsCount");
		return i;
	}

	/* (non-Javadoc)
	 * @see com.xinleju.cloud.oa.sys.dao.SysCommonToolsDao#getCommonToolsList(java.util.Map)
	 */
	@Override
	public List<SysCommonTools> getCommonToolsList(Map map) {
	
		return getSqlSession().selectList("com.xinleju.cloud.oa.sys.entity.SysCommonTools.getCommonToolsList", map);
	}

	
	
}
