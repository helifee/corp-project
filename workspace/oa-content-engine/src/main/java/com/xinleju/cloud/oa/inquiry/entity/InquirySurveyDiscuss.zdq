		this.orgDepartmentId = orgDepartmentId;
	}
    
  		
	public String getOrgPostId() {
		return orgPostId;
	}
	public void setOrgPostId(String orgPostId) {
		this.orgPostId = orgPostId;
	}
    
  		
	
