	@Column(value="discuss_content",desc="评论内容")
	private String discussContent;
    
  		
	@Column(value="org_account_id",desc="投票人所属单位ID")
	private String orgAccountId;
    
  		
	@Column(value="org_department_id",desc="投票人所属部门ID")
