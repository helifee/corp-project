package com.xinleju.cloud.oa.meeting.entity;

import com.xinleju.platform.base.annotation.Column;
import com.xinleju.platform.base.annotation.Table;
import com.xinleju.platform.base.entity.BaseEntity;


/**
 * @author wangw
 * 
 * 
 */

@Table(value="OA_MEETING_SUMMARY",desc="会议纪要表")
public class MeetingSummary extends BaseEntity{
	
		
	@Column(value="meeting_id",desc="会议ID")
	private String meetingId;
    
  		
	@Column(value="data_format",desc="正文类型")
	private String dataFormat;
    
  		
	@Column(value="content",desc="正文内容")
	private String content;
    
  		
	@Column(value="is_audit",desc="是否需要审核")
	private String isAudit;
    
  		
	@Column(value="scope",desc="阅权查看人 ID")
	private String scope;
    
  		
	@Column(value="autid_state",desc="审核状态")
	private String autidState;
	
	public String getMeetingId() {
		return meetingId;
	}
	public void setMeetingId(String meetingId) {
		this.meetingId = meetingId;
	}
    
  		
	public String getDataFormat() {
		return dataFormat;
	}
	public void setDataFormat(String dataFormat) {
		this.dataFormat = dataFormat;
	}
    
  		
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
    
  		
	public String getIsAudit() {
		return isAudit;
	}
	public void setIsAudit(String isAudit) {
		this.isAudit = isAudit;
	}
    
  		
	public String getScope() {
		return scope;
	}
	public void setScope(String scope) {
		this.scope = scope;
	}
    
  		
	public String getAutidState() {
		return autidState;
	}
	public void setAutidState(String autidState) {
		this.autidState = autidState;
	}
    
  		
	
}
