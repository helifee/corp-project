package com.xinleju.cloud.oa.bbs.entity;

import com.xinleju.platform.base.annotation.Column;
import com.xinleju.platform.base.annotation.Table;
import com.xinleju.platform.base.entity.BaseEntity;


/**
 * @author wangw
 * 
 * 
 */

@Table(value="OA_BBS_BOARD",desc="讨论区版块主表")
public class BbsBoard extends BaseEntity{
	
		
	@Column(value="description",desc="版块描述")
	private String description;
    
  		
	@Column(value="name",desc="版块名称")
	private String name;
    
  		
	@Column(value="top_number",desc="置顶个数")
	private String topNumber;
    
  		
	@Column(value="anonymous_flag",desc="是否匿名发帖")
	private String anonymousFlag;
    
  		
	@Column(value="anonymous_reply_flag",desc="是否匿名回复")
	private String anonymousReplyFlag;
    
  		
	@Column(value="sort",desc="排序字段")
	private String sort;
    
  		
	@Column(value="order_flag",desc="回复排序")
	private String orderFlag;
    
  		
	@Column(value="image_id",desc="板块封面ID")
	private String imageId;
    
  		
	@Column(value="auth_type",desc="发起授权类型【人员、部门、单位】")
	private String authType;
    
  		
		
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
    
  		
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
    
  		
	public String getTopNumber() {
		return topNumber;
	}
	public void setTopNumber(String topNumber) {
		this.topNumber = topNumber;
	}
    
  		
	public String getAnonymousFlag() {
		return anonymousFlag;
	}
	public void setAnonymousFlag(String anonymousFlag) {
		this.anonymousFlag = anonymousFlag;
	}
    
  		
	public String getAnonymousReplyFlag() {
		return anonymousReplyFlag;
	}
	public void setAnonymousReplyFlag(String anonymousReplyFlag) {
		this.anonymousReplyFlag = anonymousReplyFlag;
	}
    
  		
	public String getSort() {
		return sort;
	}
	public void setSort(String sort) {
		this.sort = sort;
	}
    
  		
	public String getOrderFlag() {
		return orderFlag;
	}
	public void setOrderFlag(String orderFlag) {
		this.orderFlag = orderFlag;
	}
    
  		
	public String getImageId() {
		return imageId;
	}
	public void setImageId(String imageId) {
		this.imageId = imageId;
	}
    
  		
	public String getAuthType() {
		return authType;
	}
	public void setAuthType(String authType) {
		this.authType = authType;
	}
    
  		
	
}
