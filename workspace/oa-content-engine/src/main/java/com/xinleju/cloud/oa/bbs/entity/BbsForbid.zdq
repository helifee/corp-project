package com.xinleju.cloud.oa.bbs.entity;

import com.xinleju.platform.base.annotation.Column;
import com.xinleju.platform.base.annotation.Table;
import com.xinleju.platform.base.entity.BaseEntity;


/**
 * @author wangw
 * 
 * 
 */

@Table(value="OA_BBS_FORBID",desc="讨论区禁言表")
public class BbsForbid extends BaseEntity{
	
		
	@Column(value="article_id",desc="讨论主题ID")
	private String articleId;
    
  		
	@Column(value="forbid_personnel",desc="禁言对应人员")
	private String forbidPersonnel;
    
  		
	@Column(value="forbid_state_date",desc="禁言开始时间")
	private String forbidStateDate;
    
  		
	@Column(value="forbid_endi_date",desc="禁言结束时间")
	private String forbidEndiDate;
    
  		
	@Column(value="forbid_personnel_id",desc="禁言人员")
	private String forbidPersonnelId;
    
  		
		
	public String getArticleId() {
		return articleId;
	}
	public void setArticleId(String articleId) {
		this.articleId = articleId;
	}
    
  		
	public String getForbidPersonnel() {
		return forbidPersonnel;
	}
	public void setForbidPersonnel(String forbidPersonnel) {
		this.forbidPersonnel = forbidPersonnel;
	}
    
  		
	public String getForbidStateDate() {
		return forbidStateDate;
	}
	public void setForbidStateDate(String forbidStateDate) {
		this.forbidStateDate = forbidStateDate;
	}
    
  		
	public String getForbidEndiDate() {
		return forbidEndiDate;
	}
	public void setForbidEndiDate(String forbidEndiDate) {
		this.forbidEndiDate = forbidEndiDate;
	}
    
  		
	public String getForbidPersonnelId() {
		return forbidPersonnelId;
	}
	public void setForbidPersonnelId(String forbidPersonnelId) {
		this.forbidPersonnelId = forbidPersonnelId;
	}
    
  		
	
}
