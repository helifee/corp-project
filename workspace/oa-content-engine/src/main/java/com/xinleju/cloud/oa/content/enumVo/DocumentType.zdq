package com.xinleju.cloud.oa.content.enumVo;

/**
 * 定义文档类型的枚举
 * @author Administrator
 *
 */
public enum DocumentType {
	
	NEWS("新闻",1), DOCUMENT("文档",2);
	
	private String name;
	
	private int index;
	
	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public int getIndex() {
		return index;
	}


	public void setIndex(int index) {
		this.index = index;
	}


	private DocumentType(String name,int index){
		name = this.name;
		index = this.index;
		
	}
	
	

	public static String getType(int key){
		switch (key) {
		case 1:
			return " 新闻";
		case 2:
			return "文档";
		default:
			return null;
		}
	}
	
	
}
