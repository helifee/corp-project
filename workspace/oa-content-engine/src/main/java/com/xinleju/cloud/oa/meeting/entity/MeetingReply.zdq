package com.xinleju.cloud.oa.meeting.entity;

import com.xinleju.platform.base.annotation.Column;
import com.xinleju.platform.base.annotation.Table;
import com.xinleju.platform.base.entity.BaseEntity;


/**
 * @author wangw
 * 
 * 
 */

@Table(value="OA_MEETING_REPLY",desc="与会人员表")
public class MeetingReply extends BaseEntity{
	
		
	@Column(value="meeting_id",desc="会议ID")
	private String meetingId;
    
  		
	@Column(value="user_id",desc="与会者")
	private String userId;
    
  		
	@Column(value="feed_back_flag",desc="是否参加")
	private String feedBackFlag;
    
  		
	@Column(value="feed_back",desc="回执意见")
	private String feedBack;
    
  		
	@Column(value="reda_date",desc="回执时间")
	private String redaDate;
    
  		
	@Column(value="look_state",desc="会议回执状态")
	private String lookState;
	
	
	@Column(value="replace_person_id",desc="指定替代人id")
	private String replacePersonId;
    
  		
	public String getReplacePersonId() {
		return replacePersonId;
	}
	public void setReplacePersonId(String replacePersonId) {
		this.replacePersonId = replacePersonId;
	}
	@Column(value="look_time",desc="会议回执时间")
	private String lookTime;
    
  		
	@Column(value="nottic_state",desc="状态【是否通知】")
	private String notticState;
	
	public String getMeetingId() {
		return meetingId;
	}
	public void setMeetingId(String meetingId) {
		this.meetingId = meetingId;
	}
    
  		
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
    
  		
	public String getFeedBackFlag() {
		return feedBackFlag;
	}
	public void setFeedBackFlag(String feedBackFlag) {
		this.feedBackFlag = feedBackFlag;
	}
    
  		
	public String getFeedBack() {
		return feedBack;
	}
	public void setFeedBack(String feedBack) {
		this.feedBack = feedBack;
	}
    
  		
	public String getRedaDate() {
		return redaDate;
	}
	public void setRedaDate(String redaDate) {
		this.redaDate = redaDate;
	}
    
  		
	public String getLookState() {
		return lookState;
	}
	public void setLookState(String lookState) {
		this.lookState = lookState;
	}
    
  		
	public String getLookTime() {
		return lookTime;
	}
	public void setLookTime(String lookTime) {
		this.lookTime = lookTime;
	}
    
  		
	public String getNotticState() {
		return notticState;
	}
	public void setNotticState(String notticState) {
		this.notticState = notticState;
	}
    
  		
	
}
