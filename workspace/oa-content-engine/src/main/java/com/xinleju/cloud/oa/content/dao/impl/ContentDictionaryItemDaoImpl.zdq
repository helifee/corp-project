package com.xinleju.cloud.oa.content.dao.impl;

import java.util.List;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.xinleju.platform.base.dao.impl.BaseDaoImpl;
import com.xinleju.cloud.oa.content.dao.ContentDictionaryItemDao;
import com.xinleju.cloud.oa.content.entity.ContentDictionaryItem;

/**
 * @author admin
 * 
 * 
 */

@Repository
public class ContentDictionaryItemDaoImpl extends BaseDaoImpl<String,ContentDictionaryItem> implements ContentDictionaryItemDao{

	public ContentDictionaryItemDaoImpl() {
		super();
	}

	@Override
	public int deleteAllObjectByDictionaryId(String strId) {
		return  getSqlSession().delete("com.xinleju.cloud.oa.content.entity.ContentDictionaryItem.deleteAllObjectByDictionaryId", strId);
	}

	@Override
	public List<Map<String, Object>> getPageData(Map<String, Object> map) {
		return  getSqlSession().selectList("com.xinleju.cloud.oa.content.entity.ContentDictionaryItem.getPageData", map);
	}

	@Override
	public Integer getPageDataCount(Map<String, Object> map) {
		return  getSqlSession().selectOne("com.xinleju.cloud.oa.content.entity.ContentDictionaryItem.getPageDataCount", map);
	}

	
	
}
