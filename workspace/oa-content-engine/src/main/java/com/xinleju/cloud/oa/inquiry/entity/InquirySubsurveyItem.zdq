package com.xinleju.cloud.oa.inquiry.entity;

import com.xinleju.platform.base.annotation.Column;
import com.xinleju.platform.base.annotation.Table;
import com.xinleju.platform.base.entity.BaseEntity;


/**
 * @author wangw
 * 
 * 
 */

@Table(value="OA_INQUIRY_SUBSURVEY_ITEM",desc="调查项信息")
public class InquirySubsurveyItem extends BaseEntity{
	
		
	@Column(value="survey_basic_id",desc="基本表D")
	private String surveyBasicId;
    
  		
	@Column(value="subsurvey_id",desc="调查问题项ID")
	private String subsurveyId;
    
  		
	@Column(value="content",desc="调查项内容")
	private String content;
    
  		
	@Column(value="vote_count",desc="投票数")
	private String voteCount;
    
  		
	@Column(value="other_option",desc="其他选项")
	private String otherOption;
    
  		
	@Column(value="content_ext1",desc="扩展字段 目前存图片ID")
	private String contentExt1;
    
  		
	@Column(value="sort",desc="排序")
	private String sort;
    
  		
	@Column(value="content_ext2",desc="显示文本框属性")
	private String contentExt2;
    
  		
		
	public String getSurveyBasicId() {
		return surveyBasicId;
	}
	public void setSurveyBasicId(String surveyBasicId) {
		this.surveyBasicId = surveyBasicId;
	}
    
  		
	public String getSubsurveyId() {
		return subsurveyId;
	}
	public void setSubsurveyId(String subsurveyId) {
		this.subsurveyId = subsurveyId;
	}
    
  		
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
    
  		
	public String getVoteCount() {
		return voteCount;
	}
	public void setVoteCount(String voteCount) {
		this.voteCount = voteCount;
	}
    
  		
	public String getOtherOption() {
		return otherOption;
	}
	public void setOtherOption(String otherOption) {
		this.otherOption = otherOption;
	}
    
  		
	public String getContentExt1() {
		return contentExt1;
	}
	public void setContentExt1(String contentExt1) {
		this.contentExt1 = contentExt1;
	}
    
  		
	public String getSort() {
		return sort;
	}
	public void setSort(String sort) {
		this.sort = sort;
	}
    
  		
	public String getContentExt2() {
		return contentExt2;
	}
	public void setContentExt2(String contentExt2) {
		this.contentExt2 = contentExt2;
	}
    
  		
	
}
