package com.xinleju.cloud.oa.meeting.entity;

import com.xinleju.platform.base.annotation.Column;
import com.xinleju.platform.base.annotation.Table;
import com.xinleju.platform.base.entity.BaseEntity;


/**
 * @author wangw
 * 
 * 
 */

@Table(value="OA_MEETING_TYPE",desc="会议类型表")
public class MeetingType extends BaseEntity{
	
		
	@Column(value="name",desc="类型名称")
	private String name;
    
  		
	@Column(value="sort_id",desc="排序号")
	private String sortId;
    
  		
	@Column(value="content",desc="类型说明")
	private String content;
	
		
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
    
  		
	public String getSortId() {
		return sortId;
	}
	public void setSortId(String sortId) {
		this.sortId = sortId;
	}
    
  		
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
    
  		
	
}
