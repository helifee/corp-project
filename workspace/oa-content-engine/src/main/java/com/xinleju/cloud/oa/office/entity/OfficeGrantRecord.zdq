package com.xinleju.cloud.oa.office.entity;

import com.xinleju.platform.base.annotation.Column;
import com.xinleju.platform.base.annotation.Table;
import com.xinleju.platform.base.entity.BaseEntity;


/**
 * @author wangw
 *
 *
 */

@Table(value="OA_OFFICE_GRANT_RECORD",desc="办公用品出库表")
public class OfficeGrantRecord extends BaseEntity{


	@Column(value="stock_apply_id",desc="申请")
	private String stockApplyId;


	@Column(value="stock_info_id",desc="用品ID")
	private String stockInfoId;

	@Column(value="stock_num",desc="用品编号")
	private String stockNum;

	@Column(value="stock_name",desc="名称")
	private String stockName;


	@Column(value="stock_specifications",desc="规格")
	private String stockSpecifications;


	@Column(value="metering_unit",desc="计量单位")
	private String meteringUnit;


	@Column(value="house_name",desc="类别")
	private String houseName;

	@Column(value="house_id",desc="用品ID")
	private String houseId;


	@Column(value="stock_brand",desc="品牌")
	private String stockBrand;


	@Column(value="appky_amount",desc="申请数量")
	private String appkyAmount;


	@Column(value="grant_amount",desc="发放数量")
	private String grantAmount;


	@Column(value="company_person_id",desc="申请人ID")
	private String companyPersonId;


	@Column(value="grant_state",desc="发放状态")
	private String grantState;


	@Column(value="apply_state",desc="申请部门")
	private String applyState;



	public String getStockApplyId() {
		return stockApplyId;
	}
	public void setStockApplyId(String stockApplyId) {
		this.stockApplyId = stockApplyId;
	}


	public String getStockInfoId() {
		return stockInfoId;
	}
	public void setStockInfoId(String stockInfoId) {
		this.stockInfoId = stockInfoId;
	}

	public String getStockNum() {
		return stockNum;
	}
	public void setStockNum(String stockNum) {
		this.stockNum = stockNum;
	}

	public String getStockName() {
		return stockName;
	}
	public void setStockName(String stockName) {
		this.stockName = stockName;
	}


	public String getStockSpecifications() {
		return stockSpecifications;
	}
	public void setStockSpecifications(String stockSpecifications) {
		this.stockSpecifications = stockSpecifications;
	}


	public String getMeteringUnit() {
		return meteringUnit;
	}
	public void setMeteringUnit(String meteringUnit) {
		this.meteringUnit = meteringUnit;
	}


	public String getHouseName() {
		return houseName;
	}
	public void setHouseName(String houseName) {
		this.houseName = houseName;
	}
	public String getHouseId() {
		return houseId;
	}
	public void setHouseId(String houseId) {
		this.houseId = houseId;
	}
	public String getStockBrand() {
		return stockBrand;
	}
	public void setStockBrand(String stockBrand) {
		this.stockBrand = stockBrand;
	}


	public String getAppkyAmount() {
		return appkyAmount;
	}
	public void setAppkyAmount(String appkyAmount) {
		this.appkyAmount = appkyAmount;
	}


	public String getGrantAmount() {
		return grantAmount;
	}
	public void setGrantAmount(String grantAmount) {
		this.grantAmount = grantAmount;
	}


	public String getCompanyPersonId() {
		return companyPersonId;
	}
	public void setCompanyPersonId(String companyPersonId) {
		this.companyPersonId = companyPersonId;
	}


	public String getGrantState() {
		return grantState;
	}
	public void setGrantState(String grantState) {
		this.grantState = grantState;
	}


	public String getApplyState() {
		return applyState;
	}
	public void setApplyState(String applyState) {
		this.applyState = applyState;
	}



}
