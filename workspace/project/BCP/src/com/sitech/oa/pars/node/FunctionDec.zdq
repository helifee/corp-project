package com.sitech.oa.pars.node;

public class FunctionDec implements Node {
	public Function function;
	public Body body;
	public String path;//当前函数所在文件路径名
	public FunctionDec(Function function,Body body,String path){
		this.function=function;
		this.body=body;
		this.path=path;
	}
	public void accept(Visitor v) {
		v.visitFunctionDec(this);

	}

}
