package com.sitech.sql.parse.node;

public class BooleanExp implements Node {
	public ColumnExp columnExp1;
	public ColumnExp columnExp2;
	public Check check;
	public NodeToken in;
	public Select select;
	public ParamList paramList;
	public Between between;
	public WhereColumns whereColumns;
	

	public BooleanExp(ColumnExp columnExp1, ColumnExp columnExp2, Check check,
			NodeToken in,  Select select, ParamList paramList,
			Between between,WhereColumns whereColumns) {
		this.columnExp1 = columnExp1;
		this.columnExp2 = columnExp2;
		this.check = check;
		this.in = in;
		this.select = select;
		this.paramList = paramList;
		this.between = between;
		this.whereColumns=whereColumns;
	}


	public void accept(Visitor v) {
		v.visitBooleanExp(this);

	}

}
