package com.sitech.sql.parse.node;

public class ScalarExp implements Node {
	public ColumnExp columnExp;
	public ColumnRef columnRef;
	public FunctionCall funCall;
	public Select seelct;
	public NodeToken string;
	public Var var;
	public NodeToken number;
	public CaseExp caseExp;
	


	public ScalarExp(ColumnExp columnExp, ColumnRef columnRef,
			FunctionCall funCall, Select seelct, NodeToken string, Var var,
			NodeToken number,CaseExp caseExp) {
		this.columnExp = columnExp;
		this.columnRef = columnRef;
		this.funCall = funCall;
		this.seelct = seelct;
		this.string = string;
		this.var = var;
		this.number = number;
		this.caseExp=caseExp;
	}




	public void accept(Visitor v) {
		v.visitScalarExp(this);

	}

}
