package com.sitech.sql.parse.node;

public class TableRef implements Node {
	public TableExp tableExp;
	public NodeToken otherName;
	
	public TableRef(TableExp tableExp, NodeToken otherName) {
		this.tableExp = tableExp;
		this.otherName = otherName;
	}

	public void accept(Visitor v) {
		v.visitTableRef(this);

	}

}
