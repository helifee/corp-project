package com.sitech.sql.parse.node;

public class SinSelect implements Node {
	public DistinctAll distinctAll;
	public Columns columns;
	public ExpTable expTable;
	public WhereExp whereExp;
	
	public SinSelect(DistinctAll distinctAll,
			Columns columns, ExpTable expTable, WhereExp whereExp) {
		this.distinctAll = distinctAll;
		this.columns = columns;
		this.expTable = expTable;
		this.whereExp = whereExp;
	}


	public void accept(Visitor v) {
		v.visitSinSelect(this);

	}

}
