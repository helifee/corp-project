package com.sitech.sql.parse.node;

public class CaseExp implements Node {
	public NodeList whenThenExpList;
	public ElseExp elseExp;
	
	public CaseExp(NodeList whenThenExpList, ElseExp elseExp) {
		this.whenThenExpList=whenThenExpList;
		this.elseExp = elseExp;
	}

	public void accept(Visitor v) {
		v.visitCaseExp(this);

	}

}
