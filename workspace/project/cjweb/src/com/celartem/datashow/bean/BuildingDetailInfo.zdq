package com.celartem.datashow.bean;

/**
 * 建筑物详细信息
 * 
 * @author zhangdaoqiang
 *
 */
public class BuildingDetailInfo {

	/**
	 * 描述
	 */
	private int dataType;
	
	/**
	 * 用电量
	 */
	private double electric;
	
	/**
	 * 用热量
	 */
	private double heat;
	
	/**
	 * 用冷量
	 */
	private double cool;
	
	/**
	 * 热水量
	 */
	private double hotWater;
	
	/**
	 * 冷水量
	 */
	private double coldWater;

	/**
	 * @return the dataType
	 */
	public int getDataType() {
		return dataType;
	}

	/**
	 * @param dataType the dataType to set
	 */
	public void setDataType(int dataType) {
		this.dataType = dataType;
	}

	/**
	 * @return the electric
	 */
	public double getElectric() {
		return electric;
	}

	/**
	 * @param electric the electric to set
	 */
	public void setElectric(double electric) {
		this.electric = electric;
	}

	/**
	 * @return the heat
	 */
	public double getHeat() {
		return heat;
	}

	/**
	 * @param heat the heat to set
	 */
	public void setHeat(double heat) {
		this.heat = heat;
	}

	/**
	 * @return the cool
	 */
	public double getCool() {
		return cool;
	}

	/**
	 * @param cool the cool to set
	 */
	public void setCool(double cool) {
		this.cool = cool;
	}

	/**
	 * @return the hotWater
	 */
	public double getHotWater() {
		return hotWater;
	}

	/**
	 * @param hotWater the hotWater to set
	 */
	public void setHotWater(double hotWater) {
		this.hotWater = hotWater;
	}

	/**
	 * @return the coldWater
	 */
	public double getColdWater() {
		return coldWater;
	}

	/**
	 * @param coldWater the coldWater to set
	 */
	public void setColdWater(double coldWater) {
		this.coldWater = coldWater;
	}
	
}
