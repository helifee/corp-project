package com.celartem.datashow.bean;

public class PoPowerRoomRate {

	private int fid;
	private String fname;
	private int fromFid;
	private String servBuildName;
	private double amount;

	/**
	 * @return the fid
	 */
	public int getFid() {
		return fid;
	}

	/**
	 * @param fid
	 *            the fid to set
	 */
	public void setFid(int fid) {
		this.fid = fid;
	}

	/**
	 * @return the fname
	 */
	public String getFname() {
		return fname;
	}

	/**
	 * @param fname
	 *            the fname to set
	 */
	public void setFname(String fname) {
		this.fname = fname;
	}

	/**
	 * @return the fromFid
	 */
	public int getFromFid() {
		return fromFid;
	}

	/**
	 * @param fromFid
	 *            the fromFid to set
	 */
	public void setFromFid(int fromFid) {
		this.fromFid = fromFid;
	}

	/**
	 * @return the servBuildName
	 */
	public String getServBuildName() {
		return servBuildName;
	}

	/**
	 * @param servBuildName
	 *            the servBuildName to set
	 */
	public void setServBuildName(String servBuildName) {
		this.servBuildName = servBuildName;
	}

	/**
	 * @return the amount
	 */
	public double getAmount() {
		return amount;
	}

	/**
	 * @param amount
	 *            the amount to set
	 */
	public void setAmount(double amount) {
		this.amount = amount;
	}
}
