package com.xinleju.erp.frame.models;

import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.FetchType;
import javax.persistence.JoinColumn;
import javax.persistence.Lob;
import javax.persistence.ManyToOne;
import javax.persistence.Table;

import org.hibernate.annotations.Formula;

import com.xinleju.erp.base.app.models.IdEntity;

/**
 * 版权：(C) 版权所有 2000-2014 上海天好电子商务有限公司苏州分公司
 * 消息发送记录
 * @author zhengxj
 * @version $Id: Msg.java 3384 2015-05-21 08:48:36Z zhongjun $
 * @since
 * @see
 */
@Entity
@Table(name = "SM_OA_MSG_SEND_LOG")
public class OaMsgSendLog extends IdEntity {

    /**
     * 序列化ID
     */
    private static final long serialVersionUID = 1L;

    /**
     * 消息类型-待办,需要人工处理的工作，譬如流程发出的待
     */
    public static final Integer MSG_TYPE_DB = 0;

    /**
     * 消息类型-通知,仅查看，譬如预警消息
     */
    public static final Integer MSG_TYPE_TZ = 1;
    

    
    /*创建状态*/
    public static final int RETURN_STATE_CREATE    = 0;
    
    /*调用失败*/
    public static final int RETURN_STATE_FAIL      = 1;
    
    /*创建成功*/
    public static final int RETURN_STATE_SUCCESS   = 2;
    
    /**
     * MSG_TYPE =1 的时候 ， 待办状态
     */
    public static final String STATUS_DB = "1";
    
    /**
     * MSG_TYPE =1 的时候 ， 已办状态
     */
    public static final String STATUS_YB = "2";
    
    /**
     * 消息操作类型 DB(1待办) YB(2已办) YD(3已读) WD(4未读) RM(5删除)
     */
    private String opType;

    /**
     * 调用OA返回状态(0表示未操作，1表示操作失败，2表示操作成功)
     */
    @Column(name = "return_state", columnDefinition = "int default 0")
    private Integer         returnState;
    
    
    private String name;
    
    @Column(name = "start_at")
    private Date startAt;

    /**
     * 调用OA返回消息
     */
    private String          returnMessage;

    /**
     * 错误次数
     */
    @Column(name = "error_times", columnDefinition = "int default 0")
    private Integer         errorTimes;

    /**
     * 最新的异常原因
     */
    @Lob
    private String          errorMsg;
    
    /**
     * 
     */
    @Column(name = "msg_id")
    private Long msgId;
    
    @ManyToOne(fetch = FetchType.LAZY)
    @JoinColumn(name="msg_id",updatable=false,insertable=false)
    private Msg msg;
    
    private String loginName;
    
    
	/**
	 *标题
	 */
	@Column(nullable = true)
    private String title;
    
	/**
	 * 模块编码
	 */
	@Column(nullable = true)
	private String moduleCode;
	/**
	 * msgType
	 */
	@Column(nullable = true)
	private String msgType;
	/**
	 * url
	 */
	@Column(nullable = true)
	private String url;
	/**
	 * 发送时间
	 */
	@Column(nullable = true)
	private Date typedDate;
	

	public String getMsgType() {
		return msgType;
	}

	public void setMsgType(String msgType) {
		this.msgType = msgType;
	}

	public Date getTypedDate() {
		return typedDate;
	}

	public void setTypedDate(Date typedDate) {
		this.typedDate = typedDate;
	}

	public String getLoginName() {
		return loginName;
	}

	public void setLoginName(String loginName) {
		this.loginName = loginName;
	}

	public Long getMsgId() {
        return msgId;
    }

    public void setMsgId(Long msgId) {
        this.msgId = msgId;
    }

    public Msg getMsg() {
        return msg;
    }

    public void setMsg(Msg msg) {
        this.msg = msg;
    }

    public Integer getReturnState() {
        return returnState;
    }

    public void setReturnState(Integer returnState) {
        this.returnState = returnState;
    }

    public String getReturnMessage() {
        return returnMessage;
    }

    public void setReturnMessage(String returnMessage) {
        this.returnMessage = returnMessage;
    }

    public Integer getErrorTimes() {
        return errorTimes;
    }

    public void setErrorTimes(Integer errorTimes) {
        this.errorTimes = errorTimes;
    }

    public String getErrorMsg() {
        return errorMsg;
    }

    public void setErrorMsg(String errorMsg) {
        this.errorMsg = errorMsg;
    }

    public String getOpType() {
        return opType;
    }

    public void setOpType(String opType) {
        this.opType = opType;
    }

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getModuleCode() {
		return moduleCode;
	}

	public void setModuleCode(String moduleCode) {
		this.moduleCode = moduleCode;
	}

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Date getStartAt() {
		return startAt;
	}

	public void setStartAt(Date startAt) {
		this.startAt = startAt;
	}


	
    
}
