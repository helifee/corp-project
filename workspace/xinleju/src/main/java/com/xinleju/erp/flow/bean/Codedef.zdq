package com.xinleju.erp.flow.bean;

import javax.xml.bind.annotation.XmlAccessType;
import javax.xml.bind.annotation.XmlAccessorType;
import javax.xml.bind.annotation.XmlAttribute;

/**
 * 
 * 
 * <p>
 * 外部编码目录,参考outCode.xml
 * </p>
 * <p>
 * </p>
 * 
 * @author 孙朝辉
 * @version $Id: Codedef.java 583 2014-06-13 09:38:22Z sunchaohui $
 * @since
 * 
 */
@XmlAccessorType(XmlAccessType.FIELD)
public class Codedef {
    public Codedef() {

    }

    public Codedef(String code, String name) {
        this.code = code;
        this.name = name;
    }

    /**
     * 编码
     */
    @XmlAttribute
    private String code;

    /**
     * 名称
     */
    @XmlAttribute
    private String name;

    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }
}
