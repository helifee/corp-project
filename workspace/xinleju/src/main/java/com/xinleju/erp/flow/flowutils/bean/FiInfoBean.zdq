public class FiInfoBean implements IResultBean {

    /**
     * 序列化ID
     */
    private static final long serialVersionUID = -7808427960082352764L;


    /**
     * 流程实例Id
     */
    private Long fiId;
    
    /**
     * 流程模板信息
     */
    private FlBean flb;

    /**
     * 流程实例信息
     */
    private FiBean fib;
    /**
     * 正在运行的用户集合
     */
    private List<WiBean> runningWis;
    
    /**
     * 正在运行的wp
     */
    private List<WpBean> runningWps;
    
    /**
     * 即将流向的wp
     */
    private WpBean nextWp;

    public Long getFiId() {
        return fiId;
    }

    public void setFiId(Long fiId) {
        this.fiId = fiId;
    }

    public FlBean getFlb() {
        return flb;
    }

    public void setFlb(FlBean flb) {
        this.flb = flb;
    }

    public FiBean getFib() {
        return fib;
    }

    public void setFib(FiBean fib) {
        this.fib = fib;
    }

    public List<WpBean> getRunningWps() {
        return runningWps;
    }

    public void setRunningWps(List<WpBean> runningWps) {
        this.runningWps = runningWps;
    }

    public WpBean getNextWp() {
        return nextWp;
    }

    public void setNextWp(WpBean nextWp) {
        this.nextWp = nextWp;
    }

    public List<WiBean> getRunningWis() {
        return runningWis;
    }

    public void setRunningWis(List<WiBean> runningWis) {
        this.runningWis = runningWis;
    }
}
