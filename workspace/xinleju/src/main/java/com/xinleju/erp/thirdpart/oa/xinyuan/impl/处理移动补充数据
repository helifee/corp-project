select fd_link+'http://xljerp.xyre.com/mobiletodo///%2Fweixin%2Fshenpi.jsp%3Fwiid'+replace(SUBSTRING ( fd_link ,charindex('approve.do%3FtId%',fd_link)+16,charindex('&phoneUrl=',fd_link)-50),'&phoneUrl=','')+'%26which%3DOA%26userId%3D'+SUBSTRING ( fd_link ,charindex('username',fd_link)+9,(charindex('http://xljerp.xyre.com:100///FlowInstanceApprove',fd_link)-55))+'%26from%3D0' from sys_notify_todo  t where fd_create_time>'2016-07-22 12:40:00.000' and fd_model_name='xinleju' and fd_type=1 and charindex('&phoneUrl=http',fd_link)<=0
and SUBSTRING ( fd_link ,charindex('username',fd_link),(charindex('http://xljerp.xyre.com:100///FlowInstanceApprove',fd_link))) !=''


//处理完成的
update t
  set t.fd_link=(fd_link+'http://xljerp.xyre.com/mobiletodo///%2Fweixin%2Fshenpi.jsp%3Fwiid'+replace(SUBSTRING ( fd_link ,charindex('approve.do%3FtId%',fd_link)+16,charindex('&phoneUrl=',fd_link)-50),'&phoneUrl=','')+'%26which%3DOA%26userId%3D'+SUBSTRING ( fd_link ,charindex('username',fd_link)+9,(charindex('http://xljerp.xyre.com:100///FlowInstanceApprove',fd_link)-55))+'%26from%3D1')
from sys_notify_todo  t where fd_create_time>'2016-07-22 12:40:00.000' and fd_model_name='xinleju' and fd_type=2 and charindex('&phoneUrl=http',fd_link)<=0
and SUBSTRING ( fd_link ,charindex('username',fd_link),(charindex('http://xljerp.xyre.com:100///FlowInstanceApprove',fd_link))) !=''

