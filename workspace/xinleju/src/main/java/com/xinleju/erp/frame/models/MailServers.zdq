package com.xinleju.erp.frame.models;

import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name="SM_MAIL_SERVERS")
public class MailServers extends BaseEntity{

private static final long serialVersionUID = -87228792006507894L;
	
	/**
	 * 服务器名称
	 */
	private String name;
	
	/**
	 * 服务器地址
	 */
	private String smtpAddr;
	
	/**
	 * 用户名
	 */
	private String userName;
	
	/**
	 * 密码
	 */
	private String password;
	

	public String getSmtpAddr() {
		return smtpAddr;
	}

	public void setSmtpAddr(String smtpAddr) {
		this.smtpAddr = smtpAddr;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}
}
