package com.xinleju.erp.frame.test;

import java.util.ArrayList;
import java.util.List;

import com.xinleju.erp.frame.dto.FiBillDTO;

public enum ReceivablerEnum {
		VOUCHER_1("1",100000.00d,"1","1","1","1"),
		VOUCHER_2("2",120000.00d,"1","1","1","1"),
		VOUCHER_3("3",130000.00d,"2","2","2","2"),
		VOUCHER_4("4",140000.00d,"2","2","2","2");
		private static List<FiBillDTO> list = null;
		private FiBillDTO dto =  null;
		private ReceivablerEnum(String id,Double paidinmny,String bankid,String funsetid, String houseid ,String userid){
			dto = new FiBillDTO();
			dto.setAttribute("id", id);
			dto.setAttribute("paidinmny", paidinmny);
			dto.setAttribute("bankid",bankid);
			dto.setAttribute("funsetid",funsetid );
			dto.setAttribute("houseid",houseid);
			dto.setAttribute("userid",userid);
		}
		
		public FiBillDTO getDto() {
			return dto;
		}
		public synchronized static  List<FiBillDTO> getList() {
			if(list == null){
				list= new ArrayList<FiBillDTO>();
				for(ReceivablerEnum item : values()){
					list.add(item.getDto());
				}
			}
			return list;
		}
		
		
		
}
