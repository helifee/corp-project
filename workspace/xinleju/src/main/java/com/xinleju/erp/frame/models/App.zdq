package com.xinleju.erp.frame.models;

import javax.persistence.Entity;
import javax.persistence.NamedQuery;
import javax.persistence.QueryHint;
import javax.persistence.Table;

import org.hibernate.annotations.Cache;
import org.hibernate.annotations.CacheConcurrencyStrategy;

@SuppressWarnings("serial")
@Entity
@Table(name="SM_app")
@NamedQuery(name = "App.selectAll", query = "select o from App o", hints={@QueryHint(name = "org.hibernate.cacheable", value ="true")})
@Cache(usage=CacheConcurrencyStrategy.READ_WRITE)
public class App extends BaseEntity{
    
    private String name;
    private String code;
   
    /**
     * pc系统路径
     */
    private String depUrl;
    /**
     * phone系统路径
     */
    private String phoneUrl;
    /**
     * 主页链接
     */
    private String indexUrl;
    
    public String getDepUrl() {
		return depUrl;
	}

	public void setDepUrl(String depUrl) {
		this.depUrl = depUrl;
	}

	public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }

	public String getIndexUrl() {
		return indexUrl;
	}

	public void setIndexUrl(String indexUrl) {
		this.indexUrl = indexUrl;
	}

	public String getPhoneUrl() {
		return phoneUrl;
	}

	public void setPhoneUrl(String phoneUrl) {
		this.phoneUrl = phoneUrl;
	}
	
	
}
