package com.xinleju.erp.frame.test;

import java.util.HashMap;
import java.util.Map;

public enum FiAccSubtypeEnum {
	SUBTYPE_1("资产类","1"),
	SUBTYPE_2("负债类","2");
	private String name;
	private String key;
	private static Map<Object,Object> map;
	private FiAccSubtypeEnum(String name ,String key){
		this.name = name;
		this.key = key;
	}
	public String getKey() {
		return key;
	}
	public void setKey(String key) {
		this.key = key;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return key;
	}
	
	public synchronized static Map<Object,Object> getMap(){
		if(map == null || map.isEmpty()){
			map = new HashMap<Object,Object>();
			for(FiAccSubtypeEnum fbtEnum : values())
				map.put(fbtEnum.getKey(), fbtEnum.getName());
		}
		return map;
	}
	
	public static Object getElement(Object key){
		return getMap().get(key);
	}
	 
		
}
