@SuppressWarnings("serial")
@Entity
@Table(name="SM_role_module")
public class RoleModule extends BaseEntity{
    
    @Column(name = "role_id")
    private Long roleId;
    
    @ManyToOne(fetch = FetchType.LAZY)
    @JoinColumn(name="role_id",updatable=false,insertable=false)
    private Role role;
    
    @Column(name = "func_module_id")
    private Long funcModuleId;
    
    @ManyToOne(fetch = FetchType.LAZY)
    @JoinColumn(name="func_module_id",updatable=false,insertable=false)
    private FuncModule funcModule;

    //private List<FuncButton> funcChildButtons;
    
    public Role getRole() {
        return role;
    }

    public void setRole(Role role) {
        this.role = role;
    }

    public Long getRoleId() {
        return roleId;
    }

    public void setRoleId(Long roleId) {
        this.roleId = roleId;
    }

    public Long getFuncModuleId() {
        return funcModuleId;
    }

    public void setFuncModuleId(Long funcModuleId) {
        this.funcModuleId = funcModuleId;
    }

    public FuncModule getFuncModule() {
        return funcModule;
    }

    public void setFuncModule(FuncModule funcModule) {
        this.funcModule = funcModule;
    }

/*    public List<FuncButton> getFuncChildButtons() {
        return funcChildButtons;
    }

    public void setFuncChildButtons(List<FuncButton> funcChildButtons) {
        this.funcChildButtons = funcChildButtons;
    }*/
    
}
