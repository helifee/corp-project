package com.xinleju.erp.flow.models;

import javax.persistence.Entity;
import javax.persistence.FetchType;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;
import javax.persistence.Table;

import com.xinleju.erp.base.app.models.IdEntity;

/**
 * 
 * 
 * <p>
 * 活动实例历史备份
 * </p>
 * <p>
 * </p>
 * 
 * @author 孙朝辉
 * @version $Id: Hai.java 2315 2015-01-19 05:25:13Z zhongjun $
 * @since
 * 
 */
@Entity
@Table(name = "tf_hai")
public class Hai extends IdEntity {

    /**
     * 序列化
     */
    private static final long serialVersionUID = 3302035937806469985L;

    /**
     * logCode:环节结束时
     */
    public static final String LOG_CODE_COMPLETE = "A_C";
    
    /**
     * logCode:环节结束时
     */
    public static final String LOG_CODE_OVERDUE = "A_O";

    /**
     * 外键:Ai,环节实例ID
     */
    private Long aiId;

    /**
     * 记录编码
     */
    private String logCode;

    /**
     * 当时的时间戳
     */
    private String tstamp;

    /**
     * 当时状态
     */
    private Integer status;

    /**
     * 外键:Ac,活动
     */
    private Long acId;

    /**
     * 级联对象
     */
    @ManyToOne(fetch = FetchType.EAGER)
    @JoinColumn(updatable = false, insertable = false)
    private Ac ac;

    /**
     * 外键:Fl,流程定义
     */
    private Long flId;

    /**
     * 级联对象,流程
     */
    @ManyToOne(fetch = FetchType.EAGER)
    @JoinColumn(updatable = false, insertable = false)
    private Fl fl;

    /**
     * 外键:Fi,流程实例
     */
    private Long fiId;

    /**
     * 级联对象,流程实例
     */
    @ManyToOne(fetch = FetchType.EAGER)
    @JoinColumn(updatable = false, insertable = false)
    private Fi fi;

    public Ac getAc() {
        return ac;
    }

    public void setAc(Ac ac) {
        this.ac = ac;
    }

    public Long getAcId() {
        return acId;
    }

    public void setAcId(Long acId) {
        this.acId = acId;
    }

    public Long getAiId() {
        return aiId;
    }

    public void setAiId(Long aiId) {
        this.aiId = aiId;
    }

    public Fi getFi() {
        return fi;
    }

    public void setFi(Fi fi) {
        this.fi = fi;
    }

    public Long getFiId() {
        return fiId;
    }

    public void setFiId(Long fiId) {
        this.fiId = fiId;
    }

    public Fl getFl() {
        return fl;
    }

    public void setFl(Fl fl) {
        this.fl = fl;
    }

    public Long getFlId() {
        return flId;
    }

    public void setFlId(Long flId) {
        this.flId = flId;
    }

    public Integer getStatus() {
        return status;
    }

    public void setStatus(Integer status) {
        this.status = status;
    }

    public String getTstamp() {
        return tstamp;
    }

    public void setTstamp(String tstamp) {
        this.tstamp = tstamp;
    }

    public String getLogCode() {
        return logCode;
    }

    public void setLogCode(String logCode) {
        this.logCode = logCode;
    }
}
