package com.xinleju.erp.flowengine.models;

import java.util.Date;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;
@Entity
@Table(name="flow_instance_step_work_task_extend")
public class FlowInstanceStepWorkTaskExtend {
	//主键
	@Id
	private String guid;
	//流程实例ID
	private String relationFiId;
	//催办，需要关联任务
	private String relationTaskId;
	//阅读人Id
    private Long readUserId;
	//阅读人登陆名
	private String readUserLoginName;
	//阅读人
	private String readUserName;
	//抄送，关联环节ID
	private String relationStepId;
	//创建时间
	private Date createTime;
	//接受时间
	private Date acceptTime;
	//状态
	private Integer status;
	//任务类型 
	private String type;

	
	public String getGuid() {
		return guid;
	}
	public void setGuid(String guid) {
		this.guid = guid;
	}
	
	public String getRelationFiId() {
		return relationFiId;
	}
	public void setRelationFiId(String relationFiId) {
		this.relationFiId = relationFiId;
	}
	public String getRelationStepId() {
		return relationStepId;
	}
	public void setRelationStepId(String relationStepId) {
		this.relationStepId = relationStepId;
	}
	public String getRelationTaskId() {
		return relationTaskId;
	}
	public void setRelationTaskId(String relationTaskId) {
		this.relationTaskId = relationTaskId;
	}
	
	public Long getReadUserId() {
		return readUserId;
	}
	public void setReadUserId(Long readUserId) {
		this.readUserId = readUserId;
	}
	public String getReadUserLoginName() {
		return readUserLoginName;
	}
	public void setReadUserLoginName(String readUserLoginName) {
		this.readUserLoginName = readUserLoginName;
	}
	public String getReadUserName() {
		return readUserName;
	}
	public void setReadUserName(String readUserName) {
		this.readUserName = readUserName;
	}
	
	public Date getCreateTime() {
		return createTime;
	}
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
	public Date getAcceptTime() {
		return acceptTime;
	}
	public void setAcceptTime(Date acceptTime) {
		this.acceptTime = acceptTime;
	}
	public Integer getStatus() {
		return status;
	}
	public void setStatus(Integer status) {
		this.status = status;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}

	
	
}
