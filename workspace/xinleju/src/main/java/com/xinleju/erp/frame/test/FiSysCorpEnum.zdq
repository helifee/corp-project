package com.xinleju.erp.frame.test;

import java.util.ArrayList;
import java.util.List;

import com.xinleju.erp.frame.dto.FiSysCorpDTO;

/**
 * 财务系统公司
 */
public enum FiSysCorpEnum {
		CORP_1001("1","01","1001","鑫苑中国总部",""),
		CORP_100101("2","01","100101","鑫苑中国河南分公司","1"),
		CORP_10010101("3","01","10010101","河南鑫苑万卓置业有限公司","2"),
		CORP_10010102("4","01","10010102","河南鑫苑基业置业有限公司","2"),
		CORP_100102("5","01","100102","济南鑫苑万卓置业有限公司","1"),
		CORP_1002("6","01","1002","鑫乐居","");
		private static List<FiSysCorpDTO> list = null;
		private FiSysCorpDTO dto =  null;
		private FiSysCorpEnum(String id ,String ds,String code,String name,String pid){
			dto = new FiSysCorpDTO();
			dto.setId(id);
			dto.setDs(ds);
			dto.setName(name);
			dto.setCode(code);
			dto.setPid(pid);
		}
		public FiSysCorpDTO getDto() {
			return dto;
		}
		public synchronized static  List<FiSysCorpDTO> getList() {
			if(list == null){
				list= new ArrayList<FiSysCorpDTO>();
				for(FiSysCorpEnum item : values()){
					list.add(item.getDto());
				}
			}
			return list;
		}
}
