package com.xinleju.erp.finance.dao;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.xinleju.erp.finance.models.FiVoucherEntry;
import com.xinleju.erp.flow.dao.impl.BaseDAO;

@Repository
public class FiVoucherEntryDao extends BaseDAO {
	/**
	 * 根据模板id获得分录列表
	 * 
	 * @param id
	 * @return
	 */
	public List<FiVoucherEntry> getVoucherEntryByTempId(Long id) {
		return getDao().findByHQL(FiVoucherEntry.class, "from FiVoucherEntry where voucherTemplateId =?", id);
	}

}
