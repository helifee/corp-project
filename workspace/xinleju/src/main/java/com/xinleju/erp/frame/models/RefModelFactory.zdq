package com.xinleju.erp.frame.models;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

import com.xinleju.erp.frame.itf.IRefModel;
import com.xinleju.erp.frame.models.ref.AppRefModel;
import com.xinleju.erp.frame.models.ref.AssTypeRefModel;
import com.xinleju.erp.frame.models.ref.BMRefModel;
import com.xinleju.erp.frame.models.ref.BODataRefModel;
import com.xinleju.erp.frame.models.ref.BORefModel;
import com.xinleju.erp.frame.models.ref.FiAccSubRefModel;
import com.xinleju.erp.frame.models.ref.FiSysInfoRefModel;
import com.xinleju.erp.frame.models.ref.FormBORefModel;
import com.xinleju.erp.frame.models.ref.MDCorpRefModel;
import com.xinleju.erp.frame.models.ref.MDLegalPersonRefModel;
import com.xinleju.erp.frame.models.ref.MDProjectRefModel;
import com.xinleju.erp.frame.utils.BeanUtil;

@Component
public class RefModelFactory {
	@Autowired
	private BORefModel boRefModel;
	@Autowired
	private FiSysInfoRefModel fiSysInfoRefModel;
	@Autowired
	private MDProjectRefModel  mdProjectRefModel;
	@Autowired
	private MDCorpRefModel  mdCorpRefModel;
	@Autowired
	private MDLegalPersonRefModel mdLegaPersonRefModel;
	@Autowired
	private AppRefModel appRefModel;
	@Autowired
	private FormBORefModel  formBORefModel;
	
	public FormBORefModel getFormBORefModel() {
		return formBORefModel;
	}

	public void setFormBORefModel(FormBORefModel formBORefModel) {
		this.formBORefModel = formBORefModel;
	}

	
	public IRefModel getFiAccSubRefModel(Long fisysinfoid,String targsyscorpid){
		return new FiAccSubRefModel(fisysinfoid,targsyscorpid);
	}
	
	public IRefModel getRefModel(String modelName){
		IRefModel model = null;
		Object obj = BeanUtil.getAttribute(modelName+"Model", this);
		if(obj != null && obj instanceof IRefModel){
			model = (IRefModel) obj;
		}
		return model;
	}

	public BODataRefModel getBODataRefModel(Long boid){
		return new BODataRefModel(boid);
	}
	
	public AssTypeRefModel getAssTypeRefModel(Long fisysinfoid,String targsyscorpid){
		return new AssTypeRefModel(fisysinfoid,targsyscorpid);
	}
	
	public BMRefModel getBMRefModel(Long fisysinfoid,String targsyscorpid){
		return new BMRefModel(fisysinfoid,targsyscorpid);
	}
	
	public AppRefModel getAppRefModel() {
		return appRefModel;
	}

	public void setAppRefModel(AppRefModel appRefModel) {
		this.appRefModel = appRefModel;
	}

	public BORefModel getBoRefModel() {
		return boRefModel;
	}

	public void setBoRefModel(BORefModel boRefModel) {
		this.boRefModel = boRefModel;
	}

	public FiSysInfoRefModel getFiSysInfoRefModel() {
		return fiSysInfoRefModel;
	}

	public void setFiSysInfoRefModel(FiSysInfoRefModel fiSysInfoRefModel) {
		this.fiSysInfoRefModel = fiSysInfoRefModel;
	}

	public MDProjectRefModel getMdProjectRefModel() {
		return mdProjectRefModel;
	}

	public void setMdProjectRefModel(MDProjectRefModel mdProjectRefModel) {
		this.mdProjectRefModel = mdProjectRefModel;
	}

	public MDCorpRefModel getMdCorpRefModel() {
		return mdCorpRefModel;
	}

	public void setMdCorpRefModel(MDCorpRefModel mdCorpRefModel) {
		this.mdCorpRefModel = mdCorpRefModel;
	}

	public MDLegalPersonRefModel getMdLegaPersonRefModel() {
		return mdLegaPersonRefModel;
	}

	public void setMdLegaPersonRefModel(MDLegalPersonRefModel mdLegaPersonRefModel) {
		this.mdLegaPersonRefModel = mdLegaPersonRefModel;
	}
	
	
}
