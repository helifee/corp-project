package com.xinleju.erp.frame.enums;

import java.util.HashMap;
import java.util.Map;

public enum OperatorEnum {
		OP_ADD("加","+"),
		OP_SUB("减","-"),
		OP_MUIL("乘","*"),
		OP_DIV("除","/"),
		OP_AND("逻辑与","&&"),
		OP_OR("逻辑或","||"),
		OP_NOT("逻辑非","!"),
		OP_EQ("等于","=="),
		OP_NE("不等于","!="),
		OP_GT("大于",">"),
		OP_LT("小于","<"),
		OP_GE("大于等于",">="),
		OP_LE("小于等于","<="),
		OP_SELECT("选择"," ? : ");
		private String name;
		private String code;
		private static Map<Object,Object> map;
		private OperatorEnum(String name ,String key){
				this.name = name;
				this.code = key;
		}
		public String getCode() {
			return code;
		}
		public void setCode(String code) {
			this.code = code;
		}
		public String getName() {
			return name;
		}
		public void setName(String name) {
			this.name = name;
		}
	
		@Override
		public String toString() {
			return code;
		}
		
		public synchronized static Map<Object,Object> getMap(){
			if(map == null || map.isEmpty()){
				map = new HashMap<Object,Object>();
				for(OperatorEnum fbtEnum : OperatorEnum.values())
					map.put(fbtEnum.getCode(), fbtEnum.getName());
			}
			return map;
		}
		
		public static Object getElement(Object code){
			return getMap().get(code);
		}
		 
		
}
