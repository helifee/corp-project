package com.xinleju.erp.frame.dto;

import java.util.ArrayList;
import java.util.List;

public class BizAuthListDTO {
	/**
	 * 业务控制点名称
	 */
	private String dataCtrlName;
	/**
	 * 业务控制点维度名称
	 */
	private String dataFieldName;
	/**
	 * 业务控制点维度ID
	 */
	private Long dataFieldId;
	/**
	 * 业务控制点范围列表
	 */
	private List<BizAuthScopeListDTO> dataScopeList = new ArrayList<BizAuthScopeListDTO>();
	public String getDataCtrlName() {
		return dataCtrlName;
	}
	public void setDataCtrlName(String dataCtrlName) {
		this.dataCtrlName = dataCtrlName;
	}
	public String getDataFieldName() {
		return dataFieldName;
	}
	public void setDataFieldName(String dataFieldName) {
		this.dataFieldName = dataFieldName;
	}
	public List<BizAuthScopeListDTO> getDataScopeList() {
		return dataScopeList;
	}
	public void setDataScopeList(List<BizAuthScopeListDTO> dataScopeList) {
		this.dataScopeList = dataScopeList;
	}
	public Long getDataFieldId() {
		return dataFieldId;
	}
	public void setDataFieldId(Long dataFieldId) {
		this.dataFieldId = dataFieldId;
	}
}
