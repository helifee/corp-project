package com.xinleju.erp.frame.models;

import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.FetchType;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;
import javax.persistence.Table;
@Entity
@Table(name="sm_flow_auth")
public class FlowAuth extends BaseEntity{
    
    private static final long serialVersionUID = 3682517320264110330L;
    
    /** 是否立即转移-否 **/
    public static final Integer TRANS_FLAG_REAL = 0;
    /** 是否立即转移-是 */
    public static final Integer TRANS_FLAG_NOT_REAL = 1;
    
    /** 授权类型 */
    private Integer authType;
    
    /** 授权人 */
    @Column(name = "auth_user_id")
    private Long authUserId;
    
    @ManyToOne(fetch = FetchType.LAZY )
    @JoinColumn(name="auth_user_id",updatable=false,insertable=false)
    private User authUser;
    
    /** 被授权人 */
    @Column(name = "auth2_user_id")
    private Long auth2UserId;

    @ManyToOne(fetch = FetchType.LAZY )
    @JoinColumn(name="auth2_user_id",updatable=false,insertable=false)
    private User auth2User;
    
    /** 授权项 */
    private String authItems;
    
    /** 授权范围 */
    private String authScope;
    
    /** 授权开始时间 */
    private Date authBeginDate;
    
    /** 授权类结束时间*/
    private Date authEndDate;

    /** 是否立即转移(0:是,1:否) */
    private Integer transFlg;

    @ManyToOne(fetch = FetchType.LAZY )
    @JoinColumn(name="create_user_id",updatable=false,insertable=false)
    private User createUser;

	public Integer getAuthType() {
		return authType;
	}

	public void setAuthType(Integer authType) {
		this.authType = authType;
	}

	public Long getAuthUserId() {
		return authUserId;
	}

	public void setAuthUserId(Long authUserId) {
		this.authUserId = authUserId;
	}

	public User getAuthUser() {
		return authUser;
	}

	public void setAuthUser(User authUser) {
		this.authUser = authUser;
	}

	public Long getAuth2UserId() {
		return auth2UserId;
	}

	public void setAuth2UserId(Long auth2UserId) {
		this.auth2UserId = auth2UserId;
	}

	public User getAuth2User() {
		return auth2User;
	}

	public void setAuth2User(User auth2User) {
		this.auth2User = auth2User;
	}

	public String getAuthItems() {
		return authItems;
	}

	public void setAuthItems(String authItems) {
		this.authItems = authItems;
	}

	public String getAuthScope() {
		return authScope;
	}

	public void setAuthScope(String authScope) {
		this.authScope = authScope;
	}

	public Date getAuthBeginDate() {
		return authBeginDate;
	}

	public void setAuthBeginDate(Date authBeginDate) {
		this.authBeginDate = authBeginDate;
	}

	public Date getAuthEndDate() {
		return authEndDate;
	}

	public void setAuthEndDate(Date authEndDate) {
		this.authEndDate = authEndDate;
	}

	public User getCreateUser() {
		return createUser;
	}

	public void setCreateUser(User createUser) {
		this.createUser = createUser;
	}

	public Integer getTransFlg() {
		return transFlg;
	}

	public void setTransFlg(Integer transFlg) {
		this.transFlg = transFlg;
	}
}
