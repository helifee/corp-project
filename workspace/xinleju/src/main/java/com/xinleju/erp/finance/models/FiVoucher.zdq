package com.xinleju.erp.finance.models;

import java.util.List;

/**
 * 凭证数据，保存凭证基本信息以及分录信息
 * @author songjunjie
 */
public class FiVoucher {
	private FiVoucherData fiVoucherData;
	private List<FiEntryData> fiEntryDataList;
	private List<FiVoucherBill> fiVoucherBillList;
	private List<FiVoucherExBill> voucherBillList;

	public FiVoucherData getFiVoucherData() {
		return fiVoucherData;
	}

	public void setFiVoucherData(FiVoucherData fiVoucherData) {
		this.fiVoucherData = fiVoucherData;
	}

	public List<FiEntryData> getFiEntryDataList() {
		return fiEntryDataList;
	}

	public void setFiEntryDataList(List<FiEntryData> fiEntryDataList) {
		this.fiEntryDataList = fiEntryDataList;
	}

	public List<FiVoucherBill> getFiVoucherBillList() {
		return fiVoucherBillList;
	}

	public void setFiVoucherBillList(List<FiVoucherBill> fiVoucherBillList) {
		this.fiVoucherBillList = fiVoucherBillList;
	}

	public List<FiVoucherExBill> getVoucherBillList() {
		return voucherBillList;
	}

	public void setVoucherBillList(List<FiVoucherExBill> voucherBillList) {
		this.voucherBillList = voucherBillList;
	}
	
	
}
