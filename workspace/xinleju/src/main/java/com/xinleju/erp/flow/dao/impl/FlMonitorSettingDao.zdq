package com.xinleju.erp.flow.dao.impl;

import org.springframework.stereotype.Repository;

@Repository
public class FlMonitorSettingDao extends BaseDAO {
	/**
	 * 
	 * @param args
	 * @return
	 */
	public int update(Object... args) {
		String hql = "update FlMonitorSetting set approverOvertime =?,approverRate=?, leaderId=?,leaderName=?,leaderOvertime=?,leaderRate=?,status=? " +
				"where flId=?";
		return getDao().execute(hql, args);
	}
}
