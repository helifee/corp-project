package com.xinleju.erp.frame.dao;

import org.springframework.stereotype.Repository;

import com.xinleju.erp.flow.dao.impl.BaseDAO;
import com.xinleju.erp.frame.models.SmContent;

@Repository
public class SmContentDao extends BaseDAO{
	
	public SmContent findOne(Long id){
		return getDao().findFirstByHQL(SmContent.class,"from SmContent s where s.id=?", id);
	}
	
	public void save(SmContent sc){
		getDao().saveOrUpdate(sc);
	}

}
