package com.xinleju.erp.flow.models;


import java.util.Date;

import javax.persistence.Entity;
import javax.persistence.Table;

import org.hibernate.annotations.DynamicInsert;
import org.hibernate.annotations.DynamicUpdate;

import com.xinleju.erp.base.app.models.IdEntity;


/**
 * 
 * 版权：(C) 版权所有 2000-2014 上海天好电子商务有限公司苏州分公司
 * 阅读记录
 * @author   zhongjun
 * @version  $Id$
 * @since
 * @see
 */
@Entity
@Table(name = "tf_user_read_log")
@DynamicInsert(true)
@DynamicUpdate(true)
public class UserReadLog extends IdEntity {

    /**
     * @Fields serialVersionUID : uid
     */
    private static final long serialVersionUID = 1L;

    /**
     * 阅读者id
     */
    private Long readerId;

    /**
     * 阅读者名称
     */
    private String readerName;

    /**
     * 阅读者登陆名
     */
    private String readerLoginName;

    /**
     * 阅读时流程的状态（对照Fi.java里的status）
     */
    private Integer fiStatus;

    /**
     * 阅读时间
     */
    private Date createDate;

    /**
     * 最后一次阅读时间
     */
    private Date lastReadDate;

    /**
     * 阅读次数
     */
    private Integer readCount;

    /**
     * 审批对应的流程id
     */
    private Long fiId;

    public Long getReaderId() {
        return readerId;
    }

    public void setReaderId(Long readerId) {
        this.readerId = readerId;
    }

    public String getReaderName() {
        return readerName;
    }

    public void setReaderName(String readerName) {
        this.readerName = readerName;
    }

    public String getReaderLoginName() {
        return readerLoginName;
    }

    public void setReaderLoginName(String readerLoginName) {
        this.readerLoginName = readerLoginName;
    }

    public Integer getFiStatus() {
        return fiStatus;
    }

    public void setFiStatus(Integer fiStatus) {
        this.fiStatus = fiStatus;
    }

    public Date getCreateDate() {
        return createDate;
    }

    public void setCreateDate(Date createDate) {
        this.createDate = createDate;
    }

    public Date getLastReadDate() {
        return lastReadDate;
    }

    public void setLastReadDate(Date lastReadDate) {
        this.lastReadDate = lastReadDate;
    }

    public Integer getReadCount() {
        return readCount;
    }

    public void setReadCount(Integer readCount) {
        this.readCount = readCount;
    }

    public Long getFiId() {
        return fiId;
    }

    public void setFiId(Long fiId) {
        this.fiId = fiId;
    }

}
