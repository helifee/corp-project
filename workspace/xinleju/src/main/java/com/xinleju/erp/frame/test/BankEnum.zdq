package com.xinleju.erp.frame.test;

import java.util.ArrayList;
import java.util.List;

import com.xinleju.erp.frame.dto.FiBillDTO;

public enum BankEnum {
		VOUCHER_1("1","ICBC", "工商银行北京市分行"),
		VOUCHER_2("2", "CMB","招商银行四川省绵阳市分行");
		private static List<FiBillDTO> list = null;
		private FiBillDTO dto =  null;
		private BankEnum(String id,String code,String name){
			dto = new FiBillDTO();
			dto.setAttribute("id", id);
			dto.setAttribute("code", code);
			dto.setAttribute("name",name);
		}
		
		public FiBillDTO getDto() {
			return dto;
		}
		public synchronized static  List<FiBillDTO> getList() {
			if(list == null){
				list= new ArrayList<FiBillDTO>();
				for(BankEnum item : values()){
					list.add(item.getDto());
				}
			}
			return list;
		}
		
		
		
}
