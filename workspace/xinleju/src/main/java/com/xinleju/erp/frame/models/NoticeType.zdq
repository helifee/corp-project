package com.xinleju.erp.frame.models;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.FetchType;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;
import javax.persistence.Table;


/**
 * 通知公告类型
 * @author chenhongbin
 *
 */
@Table(name = "SM_NOTICE_TYPE")
@Entity
public class NoticeType extends BaseEntity {
	/**
	 * 序列化
	 */
	private static final long serialVersionUID = -5309247727644607041L;
	/**
	 * 类别名称
	 */
	private String name;
	
	/**
	 * 路径
	 */
	private String prefix;
	
	/**
	 * 父ID
	 */
	@Column(name = "parent_id")
	private Long parentId;
	
	/**
	 * 父类型
	 */
	@ManyToOne(fetch = FetchType.LAZY)
	@JoinColumn(name = "parent_id", insertable = false, updatable = false)
	private NoticeType parent;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPrefix() {
		return prefix;
	}

	public void setPrefix(String prefix) {
		this.prefix = prefix;
	}

	public Long getParentId() {
		return parentId;
	}

	public void setParentId(Long parentId) {
		this.parentId = parentId;
	}

	public NoticeType getParent() {
		return parent;
	}

	public void setParent(NoticeType parent) {
		this.parent = parent;
	}
	
}
