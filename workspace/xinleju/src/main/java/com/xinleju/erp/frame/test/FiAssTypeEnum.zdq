package com.xinleju.erp.frame.test;

import java.util.ArrayList;
import java.util.List;

import com.xinleju.erp.frame.dto.FiAssTypeDTO;

public enum FiAssTypeEnum {
	//TODO 目前没有找到对应的Voucher业务类 暂时注释
		//ASSTYPE_BM("1","4","1","BM","部门",0),
		ASSTYPE_YHZH("3","4","3","YHZH","银行账户",0),
		ASSTYPE_HTBM("2","10","2","HTBM","合同编码",1),
		ASSTYPE_GYSFZHS("5","10","5","GYSFZHS","供应商辅助核算",2),
		ASSTYPE_GCXM("6","10","6","GCXM","工程项目",3);
		private static List<FiAssTypeDTO> list = null;
		private FiAssTypeDTO dto =  null;
		private FiAssTypeEnum(String id, String pid,String assid,String asscode,String assname,Integer sort){
			dto = new FiAssTypeDTO();
			dto.setId(id);
			dto.setPid(pid);
			dto.setAssid(assid);
			dto.setAsscode(asscode);
			dto.setAssname(assname);
			dto.setSort(sort);
		}
		
		public synchronized static  List<FiAssTypeDTO> getList() {
			if(list == null){
				list= new ArrayList<FiAssTypeDTO>();
				for(FiAssTypeEnum item : values()){
					list.add(item.getDto());
				}
			}
			return list;
		}

		public FiAssTypeDTO getDto() {
			return dto;
		}

}
	