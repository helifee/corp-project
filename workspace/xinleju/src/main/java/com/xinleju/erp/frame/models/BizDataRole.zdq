package com.xinleju.erp.frame.models;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.FetchType;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;
import javax.persistence.Table;

@Entity
@Table(name = "SM_BIZ_DATA_ROLE")
public class BizDataRole extends BaseEntity {
	/**
	 * @Fields serialVersionUID : TODO(序列化)
	 */
	private static final long serialVersionUID = -4372327018900646100L;
	/**
	 * 角色ID
	 */
	@Column(name = "role_id")
	private Long roleId;
	/**
	 * 业务控制点ID
	 */
	@Column(name = "biz_data_field_id")
	private Long fieldId;
	
	@ManyToOne(fetch = FetchType.LAZY)
	@JoinColumn(name = "biz_data_field_id", updatable = false, insertable = false)
	private BizDataField bizDataField;
	
	/**
	 * 业务控制点范围ID
	 */
	@Column(name = "biz_data_scope_id")
	private Long scopeId;

	@ManyToOne(fetch = FetchType.LAZY)
	@JoinColumn(name = "biz_data_scope_id", updatable = false, insertable = false)
	private BizDataScope bizDataScope;
	
	/**
	 * 更多分类选择
	 */
	@Column(name = "biz_data_scope_other_options")
	private String otherOptions;

	public Long getRoleId() {
		return roleId;
	}

	public void setRoleId(Long roleId) {
		this.roleId = roleId;
	}

	public Long getScopeId() {
		return scopeId;
	}

	public void setScopeId(Long scopeId) {
		this.scopeId = scopeId;
	}

	public String getOtherOptions() {
		return otherOptions;
	}

	public void setOtherOptions(String otherOptions) {
		this.otherOptions = otherOptions;
	}

	public Long getFieldId() {
		return fieldId;
	}

	public void setFieldId(Long fieldId) {
		this.fieldId = fieldId;
	}

	public BizDataField getBizDataField() {
		return bizDataField;
	}

	public void setBizDataField(BizDataField bizDataField) {
		this.bizDataField = bizDataField;
	}

	public BizDataScope getBizDataScope() {
		return bizDataScope;
	}

	public void setBizDataScope(BizDataScope bizDataScope) {
		this.bizDataScope = bizDataScope;
	}
}
