package com.xinleju.erp.frame.models;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

import com.xinleju.erp.annotation.Children;
import com.xinleju.erp.annotation.PageField;

//@Entity
//@Table(name="FI_ASSMAPPING")
@Children(name={FiAssDataMapping.class})
public class FiAssMapping extends BaseEntity { 
 
	private static final long serialVersionUID = 1L;
	@PageField(showname="财务系统",unique = true)
	private Long fisysinfoid;
	@PageField(showname="财务系统公司",unique = true)
	private String targsyscorpid;
	@PageField(showname="科目辅助核算",unique = true,must = true)
	@Column(length = 50)
	private String assid;
	@PageField(showname="业务对象",unique = true,must = true)
	private Long boid;

	public String getAssid() {
		return assid;
	}

	public void setAssid(String assid) {
		this.assid = assid;
	}

	public Long getBoid() {
		return boid;
	}

	public void setBoid(Long boid) {
		this.boid = boid;
	}

	public Long getFisysinfoid() {
		return fisysinfoid;
	}

	public void setFisysinfoid(Long fisysinfoid) {
		this.fisysinfoid = fisysinfoid;
	}

	public String getTargsyscorpid() {
		return targsyscorpid;
	}

	public void setTargsyscorpid(String targsyscorpid) {
		this.targsyscorpid = targsyscorpid;
	}


	
	
}
