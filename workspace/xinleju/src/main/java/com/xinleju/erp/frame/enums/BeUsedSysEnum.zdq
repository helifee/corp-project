package com.xinleju.erp.frame.enums;

import java.util.HashMap;
import java.util.Map;

public enum BeUsedSysEnum {
		SYS_SA("销售系统","SA"),
		SYS_CO("成本系统","CO"),
		SYS_EX("费用系统","EX");
		private String name;
		private String key;
		private static Map<Object,Object> map;
		private BeUsedSysEnum(String name ,String key){
				this.name = name;
				this.key = key;
		}
		public String getKey() {
			return key;
		}
		public void setKey(String key) {
			this.key = key;
		}
		public String getName() {
			return name;
		}
		public void setName(String name) {
			this.name = name;
		}
	
		@Override
		public String toString() {
			return key;
		}
		
		public synchronized static Map<Object,Object> getMap(){
			if(map == null || map.isEmpty()){
				map = new HashMap<Object,Object>();
				for(BeUsedSysEnum fbtEnum : BeUsedSysEnum.values())
					map.put(fbtEnum.getKey(), fbtEnum.getName());
			}
			return map;
		}
		
		public static Object getElement(Object key){
			return getMap().get(key);
		}
		 
		
}
