package com.xinleju.erp.frame.enums;

import java.util.Map;
import java.util.concurrent.ConcurrentHashMap;

import net.sf.json.JSONArray;

import com.xinleju.erp.frame.utils.CommonUtil;

public enum FiBusinessTypeEnum {
	//TYPE_BD("基本档案", "bdobject"), TYPE_FORM("业务表单", "formobject");
	TYPE_BD("基本档案", "1"), TYPE_FORM("业务表单", "2");
	private String name;
	private String key;
	private static Map<Object, Object> map = new ConcurrentHashMap<Object, Object>();;

	private FiBusinessTypeEnum(String name, String key) {
		this.name = name;
		this.key = key;
	}

	public String getKey() {
		return key;
	}

	public void setKey(String key) {
		this.key = key;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return name;
	}

	public static Map<Object, Object> getMap() {
		if (map.isEmpty()) {
			for (FiBusinessTypeEnum fbtEnum : FiBusinessTypeEnum.values())
				map.put(fbtEnum.getKey(), fbtEnum.getName());
		}
		return map;
	}

	public static JSONArray getJSONObject() {
		return CommonUtil.transRef(getMap());
	}

	public static Object getElement(Object key) {
		return getMap().get(key);
	}
}
