package com.xinleju.erp.frame.dao;

import java.util.List;

import org.springframework.stereotype.Repository;

import com.xinleju.erp.flow.dao.impl.BaseDAO;
import com.xinleju.erp.frame.models.BizDataField;

/**
 * 业务控制点维度DAO
 * @author lixj
 *
 */
@Repository
public class BizDataFieldDao extends BaseDAO {
	/**
	 * 通过模块名业务控制点维度
	 * @param moduleCode
	 * @return
	 */
	public List<BizDataField> getByModuleCode(String moduleCode){
		return getDao().findByHQL(BizDataField.class, "from BizDataField where moduleCode = ?",
				moduleCode);
	}
}
