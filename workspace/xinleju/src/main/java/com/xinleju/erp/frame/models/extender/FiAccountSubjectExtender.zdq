package com.xinleju.erp.frame.models.extender;

import com.xinleju.erp.frame.dto.FiAccountSubjectDTO;
import com.xinleju.erp.frame.models.EntityExtender;

public class FiAccountSubjectExtender extends EntityExtender {

	@SuppressWarnings("unused")
	private String text = "";
	@SuppressWarnings("unused")
	private String bizData;
	@SuppressWarnings("unused")
	private String bizDatamx;
	@SuppressWarnings("unused")
	private String assName;

	public String getText(FiAccountSubjectDTO dto) {
		return dto.getSubcode() + " " + dto.getSubname();
	}

	public String getBizData(FiAccountSubjectDTO dto) {
		return dto.getBizData();
	}

	public String getBizDatamx(FiAccountSubjectDTO dto) {
		return dto.getBizDatamx();
	}

	public String getAssName(FiAccountSubjectDTO dto) {
		return dto.getAssName();
	}

}
