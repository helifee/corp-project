package com.xinleju.erp.frame.dto;

public class BizAuthScopeListDTO {
	/**
	 * 业务控制点范围名称
	 */
	private String dataScopeName;
	/**
	 * 业务控制点范围获取数据URL
	 */
	private String dataScopeUrl;
	/**
	 * 业务控制点范围ID
	 */
	private Long dataScopeId;
	/**
	 * 是否选中
	 */
	private byte isSelected;
	/**
	 * 其他选项
	 */
	private String otherOptions;
	public String getDataScopeName() {
		return dataScopeName;
	}
	public void setDataScopeName(String dataScopeName) {
		this.dataScopeName = dataScopeName;
	}
	public String getDataScopeUrl() {
		return dataScopeUrl;
	}
	public void setDataScopeUrl(String dataScopeUrl) {
		this.dataScopeUrl = dataScopeUrl;
	}
	public Long getDataScopeId() {
		return dataScopeId;
	}
	public void setDataScopeId(Long dataScopeId) {
		this.dataScopeId = dataScopeId;
	}
	public byte getIsSelected() {
		return isSelected;
	}
	public void setIsSelected(byte isSelected) {
		this.isSelected = isSelected;
	}
	public String getOtherOptions() {
		return otherOptions;
	}
	public void setOtherOptions(String otherOptions) {
		this.otherOptions = otherOptions;
	}
}
