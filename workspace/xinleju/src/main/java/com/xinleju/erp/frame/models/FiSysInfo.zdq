package com.xinleju.erp.frame.models;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

import com.xinleju.erp.annotation.PageField;

//@Entity
//@Table(name="FI_SYSINFO")
public class FiSysInfo extends BaseEntity { 
 
	private static final long serialVersionUID = 1L;
	@Column(length=20)
	@PageField(showname="财务系统标识",must=true,unique=true)
	private String vtargetsyscode ;
	@Column(length=50)
	@PageField(showname="财务系统名称",must=true,unique=true)
	private String vtargetsysname;
	@Column(length=20)
	@PageField(showname="接口类型",must=true)  
	private String vtype;
	@Column(length=200)
	@PageField(showname="财务系统地址",must=true,unique=true)
	private String vtargetsysadd;
	@Column(length=20)
	@PageField(showname="业务系统标识",must=true)
	private String vbusisyscode;
	
	public String getVtargetsyscode() {
		return vtargetsyscode;
	}
	public void setVtargetsyscode(String vtargetsyscode) {
		this.vtargetsyscode = vtargetsyscode;
	}
	public String getVtargetsysname() {
		return vtargetsysname;
	}
	public void setVtargetsysname(String vtargetsysname) {
		this.vtargetsysname = vtargetsysname;
	}
	public String getVtype() {
		return vtype;
	}
	public void setVtype(String vtype) {
		this.vtype = vtype;
	}
	public String getVtargetsysadd() {
		return vtargetsysadd;
	}
	public void setVtargetsysadd(String vtargetsysadd) {
		this.vtargetsysadd = vtargetsysadd;
	}
	public String getVbusisyscode() {
		return vbusisyscode;
	}
	public void setVbusisyscode(String vbusisyscode) {
		this.vbusisyscode = vbusisyscode;
	}

}
