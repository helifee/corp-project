    public static enum EventType {

        AS("AS", "环节开始"), AE("AE", "环节结束"), WS("WS", "工作开始"), WR("WR", "工作重置"), WO("WO", "工作过期"), WE("WE", "工作结束"), FS("FS", "流程开始"), FE("FE", "流程结束"),FO("FO", "流程作废") ;

        private EventType(String code, String name) {
            this.code = code;
            this.name = name;
        }

        private String code;
        private String name;

        public String getCode() {
            return code;
        }

        public void setCode(String code) {
            this.code = code;
        }

        public String getName() {
            return name;
        }

        public void setName(String name) {
            this.name = name;
        }
    }
    
    public static enum WiType {

        /**
         * 
         * 1,影响流程
         * 0,不影响流程
         * 枚举所有的工作类型
         */
        ZC("正常", "ZC", 1), CS("抄送", "CS", 0), ZF("转发", "ZF", 0), LG("日志", "LG", 0), CY("传阅", "CY", 0), CB("催办", "CB", 0);

        private WiType(String name, String code, int isEffectFlow) {
            this.name = name;
            this.code = code;
            this.isEffectFlow = isEffectFlow;
        }

        /**
         * 
         * <p>
         * 返回指定编码的工作类型
         * </p>
         * <p>
         * </p>
         * 
         * @author 孙朝辉
         * @param code
         *            编码
         * @return WiType 工作类型
         */
        public static WiType get(String code) {
            for (WiType t : WiType.values()) {
                if (t.getCode().equals(code)) {
                    return t;
                }
            }
            return null;
        }

        /**
         * 是否影响流程流转
         */
        private int isEffectFlow;

        /**
         * 名称
         */
        private String name;

        /**
         * 编码
         */
        private String code;

        public String getName() {
            return name;
        }

        public void setName(String name) {
            this.name = name;
        }

        public String getCode() {
            return code;
        }

        public void setCode(String code) {
            this.code = code;
        }

        public int getIsEffectFlow() {
            return isEffectFlow;
        }

        public void setIsEffectFlow(int isEffectFlow) {
            this.isEffectFlow = isEffectFlow;
        }
    }
    
