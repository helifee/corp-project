 * @version  $Id$
 * @since
 * @see
 */
@SuppressWarnings("serial")
@Entity
@Table(name="SM_OP")
public class FuncButton extends BaseEntity{
    
    private String name;

    // 按钮的url 防止直接输入按钮的url
    private String buttonUrl;
    
    private String code;
    /**
     * @Fields funcModuleId : 按钮属于那个模块下面
     */
    @Column(name = "func_id")
    private Long funcModuleId;
    
    @ManyToOne(fetch = FetchType.LAZY)
    @JoinColumn(name="func_id",updatable=false,insertable=false)
    private FuncModule funcModule;
    @Lob
    private String note;
    
    @Column(name="MODULE_CODE")
    private String moduleCode;
    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }
 

    public Long getFuncModuleId() {
        return funcModuleId;
    }

    public void setFuncModuleId(Long funcModuleId) {
        this.funcModuleId = funcModuleId;
    }

    public FuncModule getFuncModule() {
        return funcModule;
    }

    public void setFuncModule(FuncModule funcModule) {
        this.funcModule = funcModule;
    }

    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }

    public String getNote() {
        return note;
    }

    public void setNote(String note) {
        this.note = note;
    }

    public String getButtonUrl() {
        return buttonUrl;
    }

    public void setButtonUrl(String buttonUrl) {
        this.buttonUrl = buttonUrl;
    }
}
