package com.xinleju.erp.flow.models;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

import org.hibernate.annotations.DynamicInsert;
import org.hibernate.annotations.DynamicUpdate;

import com.xinleju.erp.base.app.models.IdEntity;
import com.xinleju.erp.base.app.utils.HibernateUtils;

/**
 * 
 * 
 * <p>
 * 业务参与人
 * </p>
 * <p>
 * </p>
 * 
 * @author 孙朝辉
 * @version $Id: BizParticipant.java 583 2014-06-13 09:38:22Z sunchaohui $
 * @since
 * 
 */
@Entity
@Table(name = "TF_BIZ_PARTICIPANT")
@DynamicInsert(true)
@DynamicUpdate(true)
public class BizParticipant extends IdEntity {

    /**
     * 序列化
     */
    private static final long serialVersionUID = -5924132347945451283L;

    /**
     * 编码
     */
    private String code;

    /**
     * 名称
     */
    private String name;

    /**
     * SQL语句
     */
    @Column(name="sql_data",length = HibernateUtils.COLOUMN_LENGTH_1500)
    private String sql;

    /**
     * 是否禁用
     */
    @Column(columnDefinition = "int(10) default 0")
    private Integer isDisabled;

    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getSql() {
        return sql;
    }

    public void setSql(String sql) {
        this.sql = sql;
    }

    public Integer getIsDisabled() {
        return isDisabled;
    }

    public void setIsDisabled(Integer isDisabled) {
        this.isDisabled = isDisabled;
    }

}
