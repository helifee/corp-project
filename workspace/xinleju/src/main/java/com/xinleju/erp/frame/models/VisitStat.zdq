package com.xinleju.erp.frame.models;

import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

import com.xinleju.erp.base.app.models.IdEntity;


@Entity
@Table(name = "SM_VISIT_STAT")
public class VisitStat extends IdEntity {
	
	/**
	 * 序列化
	 */
	private static final long serialVersionUID = -7291768065459242113L;


	/**
	 * 模块编码
	 */
	@Column(nullable = false)
	private String moduleCode;
	
	
	/**
	 * 功能名称，如：用户管理
	 */
	@Column(nullable = false)
	private String funcName;
	
	
	/**
	 * 动作，如：保存用户
	 */
	@Column(nullable = false)
	private String actionName;
	
	/**
	 *动作编码
	 */
	@Column(nullable = false)
	private String actionId;
	
	
	/**
	 * 访问次数
	 */
	@Column(nullable = false)
	private Long visitTimes;
	
	/**
	 * 访问日期
	 */
	@Column(nullable = false)
	private Date visitDate;

	public String getModuleCode() {
		return moduleCode;
	}

	public void setModuleCode(String moduleCode) {
		this.moduleCode = moduleCode;
	}

	public String getFuncName() {
		return funcName;
	}

	public void setFuncName(String funcName) {
		this.funcName = funcName;
	}

	public String getActionName() {
		return actionName;
	}

	public void setActionName(String actionName) {
		this.actionName = actionName;
	}

	public String getActionId() {
		return actionId;
	}

	public void setActionId(String actionId) {
		this.actionId = actionId;
	}

	public Long getVisitTimes() {
		return visitTimes;
	}

	public void setVisitTimes(Long visitTimes) {
		this.visitTimes = visitTimes;
	}

	public Date getVisitDate() {
		return visitDate;
	}

	public void setVisitDate(Date visitDate) {
		this.visitDate = visitDate;
	}
}
