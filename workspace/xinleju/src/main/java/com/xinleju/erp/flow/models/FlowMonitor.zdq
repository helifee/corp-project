package com.xinleju.erp.flow.models;

import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;
import javax.persistence.Temporal;
import javax.persistence.TemporalType;

import com.xinleju.erp.base.app.models.IdEntity;

/**
 * 流程监控
 * 
 * @author zhengxj
 *
 */
@Entity
@Table(name = "tf_flow_monitor")
public class FlowMonitor extends IdEntity {

    /**
     * 序列化ID
     */
    private static final long serialVersionUID = 1L;

    /**
     * 监控点-流程发起
     */
    public final static int MONITOR_POINT_FLOW_START = 1;

    /**
     * 监控点-收到审批
     */
    public final static int MONITOR_POINT_ACCEPT_SP = 2;

    /**
     * 监控点-审批完成
     */
    public final static int MONITOR_POINT_SP_FINISH = 3;

    /**
     * 监控点-流程完成
     */
    public final static int MONITOR_POINT_FLOW_FINISH = 4;
    
    /**
     * 挂起
     */
    public final static int MONITOR_POINT_FLOW_SUSPEND = 5;
    
    /**
     * 流程模板
     */
    public final static int MONITOR_POINT_FLOW_TEMP = 6;
    
    /**
     * 禁用
     */
    public static int USER_STATUS_N = 1;
    
    /**
     * 启用
     */
    public static int USER_STATUS_Y = 2;

    /**
     * 名称
     */
    private String name;

    /**
     * 监控人(标准角色/岗位/人员)
     */
    @Column(length = 4000)
    private String monitor;

    /**
     * 被监控人(公司部门/标准角色/岗位/人员)
     */
    @Column(length = 4000)
    private String beMonitored;
    
    /**
     * 挂起(公司部门/标准角色/岗位/人员)
     */
    @Column(length = 4000)
    private String suspend;

    /**
     * 流程模块集合
     */
    private String flowTemps;

    /**
     * 监控点
     */
    private String monitorPoint;
    
    /**
     * 状态(启用/禁用)
     */
    private Integer useStatus;
    
    @Column(length = 4000)
    private String realBeMonitored;

    @Temporal(TemporalType.TIMESTAMP)
    private Date createTime;

    @Temporal(TemporalType.TIMESTAMP)
    private Date updateTime;

    public FlowMonitor() {
        super();
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getMonitor() {
        return monitor;
    }

    public void setMonitor(String monitor) {
        this.monitor = monitor;
    }

    public String getBeMonitored() {
        return beMonitored;
    }

    public void setBeMonitored(String beMonitored) {
        this.beMonitored = beMonitored;
    }

    public String getFlowTemps() {
        return flowTemps;
    }

    public void setFlowTemps(String flowTemps) {
        this.flowTemps = flowTemps;
    }

    public String getMonitorPoint() {
        return monitorPoint;
    }

    public void setMonitorPoint(String monitorPoint) {
        this.monitorPoint = monitorPoint;
    }

    public Date getCreateTime() {
        return createTime;
    }

    public void setCreateTime(Date createTime) {
        this.createTime = createTime;
    }

    public Date getUpdateTime() {
        return updateTime;
    }

    public void setUpdateTime(Date updateTime) {
        this.updateTime = updateTime;
    }

    public Integer getUseStatus() {
        return useStatus;
    }

    public void setUseStatus(Integer useStatus) {
        this.useStatus = useStatus;
    }

    public String getSuspend() {
        return suspend;
    }

    public void setSuspend(String suspend) {
        this.suspend = suspend;
    }

    public String getRealBeMonitored() {
        return realBeMonitored;
    }

    public void setRealBeMonitored(String realBeMonitored) {
        this.realBeMonitored = realBeMonitored;
    }
    
}
