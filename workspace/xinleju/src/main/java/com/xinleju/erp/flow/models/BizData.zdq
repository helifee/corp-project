package com.xinleju.erp.flow.models;

import javax.persistence.Entity;
import javax.persistence.Table;

import com.xinleju.erp.base.app.models.IdEntity;

/**
 * 业务数据
 * @author jun
 *
 */
@Entity
@Table(name = "tf_biz_data")
public class BizData extends IdEntity {

    /**
     * 序列化
     */
    private static final long serialVersionUID = -6980322890740184291L;
    
    private String bizId;
    
    private String soCode;
    
    private String typeClass;
    
    /**
     * 外键:流程实例ID
     */
    private Long fiId;

    /**
     * 外键:环节实例ID
     */
    private Long aiId;

    /**
     * 编码
     */
    private String name;
    
    /**
     * 名称
     */
    private String displayName;
    /**
     * 值
     */
    private String val;
    /**
     * 类型,取值java类,譬如:java.lang.Double
     */
    private String type;
    
    /**
     * 数据类型
     */
    private String dataType;

    /**
     * 数据版本
     */
    private String dataVsersion;
    
    private String flowInstanceId;
    
    public Long getAiId() {
        return aiId;
    }

    public void setAiId(Long aiId) {
        this.aiId = aiId;
    }

 


	public Long getFiId() {
		return fiId;
	}

	public void setFiId(Long fiId) {
		this.fiId = fiId;
	}

	public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getVal() {
        return val;
    }

    public void setVal(String val) {
        this.val = val;
    }

    public String getType() {
        return type;
    }

    public void setType(String type) {
        this.type = type;
    }

    public String getDisplayName() {
        return displayName;
    }

    public void setDisplayName(String displayName) {
        this.displayName = displayName;
    }

    public String getBizId() {
        return bizId;
    }

    public void setBizId(String bizId) {
        this.bizId = bizId;
    }

    public String getSoCode() {
        return soCode;
    }

    public void setSoCode(String soCode) {
        this.soCode = soCode;
    }

    public String getTypeClass() {
        return typeClass;
    }

    public void setTypeClass(String typeClass) {
        this.typeClass = typeClass;
    }

	public String getDataType() {
		return dataType;
	}

	public void setDataType(String dataType) {
		this.dataType = dataType;
	}

	public String getDataVsersion() {
		return dataVsersion;
	}

	public void setDataVsersion(String dataVsersion) {
		this.dataVsersion = dataVsersion;
	}

	public String getFlowInstanceId() {
		return flowInstanceId;
	}

	public void setFlowInstanceId(String flowInstanceId) {
		this.flowInstanceId = flowInstanceId;
	}

}
