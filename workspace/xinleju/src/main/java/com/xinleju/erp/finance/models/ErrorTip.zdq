package com.xinleju.erp.finance.models;

/**
 * @author wjf
 *
 * 错误信息类：导入导出功能错误信息通用entity
 */
public class ErrorTip {
	
	/**
	 * 
	 */
	public ErrorTip() {
	}
	
	/**
	 * 
	 */
	public ErrorTip(String rowno,String itemname,String reasion) {
		super();
		this.rowno = rowno;
		this.itemname=itemname;
		this.reasion = reasion;
	}

	
	private String rowno; //行号   存储格式  2:3  表示2行3列
	
	private String colno; //列号(预留)
	
	private String itemname; //字段名
	
	private String reasion; //错误原因

	/**
	 * 123
	 */
	public String getRowno() {
		return rowno;
	}

	/**
	 * @param rowno the rowno to set
	 */
	public void setRowno(String rowno) {
		this.rowno = rowno;
	}

	/**
	 * 123
	 */
	public String getColno() {
		return colno;
	}

	/**
	 * @param colno the colno to set
	 */
	public void setColno(String colno) {
		this.colno = colno;
	}

	/**
	 * 123
	 */
	public String getItemname() {
		return itemname;
	}

	/**
	 * @param itemname the itemname to set
	 */
	public void setItemname(String itemname) {
		this.itemname = itemname;
	}

	/**
	 * 123
	 */
	public String getReasion() {
		return reasion;
	}

	/**
	 * @param reasion the reasion to set
	 */
	public void setReasion(String reasion) {
		this.reasion = reasion;
	}
	
	
}
