@Entity
@Table(name = "tf_fl")
@Cache(usage=CacheConcurrencyStrategy.READ_WRITE)
public class Fl extends IdEntity {

    /**
     * 序列化
     */
    private static final long serialVersionUID = 9220064956326266277L;

    /**
     * 当参与人重复时，执行的动作 0:不执行任何动作, 1:前置跳过、后置参与, 2:前置参与、后置跳过
     */
    private Integer doWhenRepeatInAcs;

    
    /**
     * 当节点的岗位参与人为空时策略
     */
    private Integer doWhenNoUsersAtWp;
    
    /**
     * 流程图显示左上修正像素
     */
    private String fixLeft;

    /**
     * 流程图显示右上修正像素
     */
    private String fixTop;

    /**
     * 流程名
     */
    private String flowName;

    /**
     * 流程编码
     */
    private String flowCode;

    /**
     * 流程版本
     */
    private String flowVersion;

    /**
     * 是否禁用
     */
    @Column(name = "is_disabled", columnDefinition = "int(10) default 0")
    private Integer isDisabled;

    /**
     * 是否可以加签
     */
    private Integer jqStrategy;

    /**
     * 是否提交，已经提交的流程可以使用，未提交的处于编辑状态
     */
    private Integer isCommited;

    /**
     * 是否启用时限
     */
    private Integer isTimeLimit;
    /**
     * 时限
     */
    private BigDecimal timeLimit;

    /**
     * 时限单位
     */
    private Integer timeUnit;

    /**
     * 发布时间
     */
    @Column(name = "publish_time", columnDefinition = "timestamp default current_timestamp()")
    private Date publishTime;

    
    /**
     * 所属目录
     */
    private Long ctId;
    /**
     * 所属目录
     */
    @ManyToOne(fetch = FetchType.LAZY)
    @JoinColumn(name = "ct_id", updatable = false, insertable = false)
    private Ct  ct;
    
    /**
     * 所属应用系统
     */
    private String appCode;
    
    /**
     *可阅人员:格式化参与人字符串
     */
    @Column(length = HibernateUtils.COLOUMN_LENGTH_3000)
    private String kyList;
    
    
    /**
     * 默认发起人员:格式化参与人字符串
     */
    @Column(length = HibernateUtils.COLOUMN_LENGTH_3000)
    private String fqList;
    
    /**
     * 流程说明
     */
    @Column(length = HibernateUtils.COLOUMN_LENGTH_3000)
    private String flRemark;
    
    /**
     * 排序
     */
    private Integer flSort;
    
    /**
     * 发起方式（0：集成，1：手动）
     */
    @Column(name = "start_type", columnDefinition = "int(10) default 0")
    private Integer startType;
    
    /**
     * 编号规则-前缀
     */
    private String codePrefix;
    /**
     * 编号规则编码
     */
    private String codeTypeCode;
    /**
     * 默认主题
     */
    private String titleType;
    
    /**
     * 允许用户修改流程主题（0：不允许，1：允许）
     */
    @Column(name = "can_change_title", columnDefinition = "int(10) default 0")
    private Integer canChangeTitle;
    
    /**
     * 是否显示流程图（0：显示，1：不显示）
     */
    @Column(name = "show_graph", columnDefinition = "int(10) default 0")
    private Integer showGraph;
    
    /**
     * 是否显示发起人补充附件（0：显示，1：不显示）
     */
    @Column(name = "show_relation_file", columnDefinition = "int(10) default 0")
    private Integer showRelationFile;
    
    /**
     * 审批意见排序（0：正序，1：倒序）
     */
    @Column(name = "wis_sort_type", columnDefinition = "int(10) default 0")
    private Integer wisSortType;
    
    /**
     * 创建人ID
     */
    @Column(name = "create_user_id", updatable = false)
    private Long createUserId;

    /**
     * 创建人登录名
     */
    @Column(name = "create_user_login_name", updatable = false)
    private String createUserLoginName;

    /**
     * 创建人姓名
     */
    @Column(name = "create_user_name", updatable = false)
    private String createUserName;
    
    /**
     * 修改人ID
     */
    private Long editUserId;

    /**
     * 修改人登录名
     */
    private String editUserLoginName;

    /**
     * 修改人姓名
     */
    private String editUserName;
    
    /**
     * 是否当前版本（1：为当前版本 0：等待启用）
     */
    private Integer isCurrent;
    
    /**
     * 创建时间
     */
    @Column(name = "create_time", updatable = false)
    private Date createTime;
    
    /**
     * 修改时间
     */
    @Column(name = "update_time")
    private Date updateTime;
    
    /**
     * 所属业务对象id
     */
    @Column(name = "service_object_define_id")
    private Long serviceObjectDefineId;
    /**
     * 所属业务对象
     */
    @ManyToOne(fetch = FetchType.LAZY)
    @JoinColumn(name = "service_object_define_id", updatable = false, insertable = false)
    private ServiceObjectDefine serviceObjectDefine;
    
    /**
     * 是否默认（0：否，1：是）
     */
    @Column(name = "is_default", columnDefinition = "int(10) default 0")
    private Integer isDefault;
    
    /**
     * 是否是业务流程
     */
    private Boolean isBizFl;
    
  

	public String getFlowName() {
        return flowName;
    }

    public void setFlowName(String flowName) {
        this.flowName = flowName;
    }

