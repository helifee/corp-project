    }

    public void setFixLeft(String fixLeft) {
        this.fixLeft = fixLeft;
    }

    public String getFixTop() {
        return fixTop;
    }

    public void setFixTop(String fixTop) {
        this.fixTop = fixTop;
    }

    public Integer getJqStrategy() {
        return jqStrategy;
    }

    public void setJqStrategy(Integer jqStrategy) {
        this.jqStrategy = jqStrategy;
    }

    /**
     * @return Returns the publishTime.
     */
    public Date getPublishTime() {
        return publishTime;
    }

    /**
