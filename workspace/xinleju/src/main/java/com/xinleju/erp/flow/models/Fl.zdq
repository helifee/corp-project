    }

    public void setFqList(String fqList) {
        this.fqList = fqList;
    }

    public Integer getStartType() {
        return startType;
    }

    public void setStartType(Integer startType) {
        this.startType = startType;
    }

    public Integer getShowRelationFile() {
        return showRelationFile;
    }

    public void setShowRelationFile(Integer showRelationFile) {
