package com.xinleju.erp.flow.models;

import javax.persistence.Entity;
import javax.persistence.Table;

import com.xinleju.erp.base.app.models.IdEntity;

/**
 * <p>
 * 工作类别
 * </p>
 * <p>
 * </p>
 * 
 * @author 孙朝辉
 * @version $Id: Wt.java 1104 2014-07-29 00:56:18Z sunchaohui $
 * @since
 * @see
 */
@Entity
@Table(name = "tf_wt")
public class Wt extends IdEntity {

    /**
     * 序列化<br>
     */
    private static final long serialVersionUID = -6387433256569970250L;

    /**
     * 名称
     */
    private String name;
    
    /**
     * 编码
     */
    private String code;
    
    
    /**
     * 是否影响流程
     */
    private int isEffectFlow;
    
    public String getName() {
        return name;
    }
    public void setName(String name) {
        this.name = name;
    }
    public String getCode() {
        return code;
    }
    public void setCode(String code) {
        this.code = code;
    }
    public int getIsEffectFlow() {
        return isEffectFlow;
    }
    public void setIsEffectFlow(int isEffectFlow) {
        this.isEffectFlow = isEffectFlow;
    }
}
