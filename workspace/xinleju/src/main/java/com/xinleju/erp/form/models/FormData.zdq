package com.xinleju.erp.form.models;

import javax.persistence.Entity;
import javax.persistence.FetchType;
import javax.persistence.JoinColumn;
import javax.persistence.Lob;
import javax.persistence.ManyToOne;
import javax.persistence.Table;

import com.xinleju.erp.frame.models.BaseEntity;

/**
 * 表单数据
 * @author chenhongbin
 *
 */
@Table(name = "EF_FORM_DATA")
@Entity
public class FormData extends BaseEntity{
	
	/**
	 * 序列化
	 */
	private static final long serialVersionUID = -5493394438739518324L;

	/**
	 * 表单内容
	 */
	@Lob
	private String dataHtml;
	
	/**
	 * 关联的表单ID
	 */
	private Long formDefineId;
	
	/**
	 * 表单
	 */
	@ManyToOne(fetch = FetchType.LAZY)
	@JoinColumn(name = "form_define_id", updatable = false, insertable = false, nullable = false)
	private FormDefine formDefine;

	public String getDataHtml() {
		return dataHtml;
	}

	public void setDataHtml(String dataHtml) {
		this.dataHtml = dataHtml;
	}

	public Long getFormDefineId() {
		return formDefineId;
	}

	public void setFormDefineId(Long formDefineId) {
		this.formDefineId = formDefineId;
	}

	public FormDefine getFormDefine() {
		return formDefine;
	}

	public void setFormDefine(FormDefine formDefine) {
		this.formDefine = formDefine;
	}
}
