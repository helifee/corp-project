package com.xinleju.erp.frame.models.extender;

import com.xinleju.erp.frame.enums.FiBusinessTypeEnum;
import com.xinleju.erp.frame.models.BusinessObject;
import com.xinleju.erp.frame.models.EntityExtender;

public class BusinessObjectExtender extends EntityExtender {
	

	private String botype;

	public String getBotype(BusinessObject exObj) {
		botype = "";
		if(exObj.getBdobject().equals("true"))
			botype += FiBusinessTypeEnum.TYPE_BD.getName()+" ";
		if(exObj.getFormobject().equals("true"))
			botype += FiBusinessTypeEnum.TYPE_FORM.getName();
		return botype;
	}

	public void setBotype(String botype) {
		this.botype = botype;
	}
	
	
}
