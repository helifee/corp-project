package com.xinleju.erp.frame.models;

import java.util.Date;
import java.util.HashMap;
import java.util.Map;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Lob;
import javax.persistence.Table;

import com.xinleju.erp.base.app.models.IdEntity;
import com.xinleju.erp.base.app.utils.HibernateUtils;

/**
 * 接口日志
 * 
 * @author chenhb
 *
 */
@Entity
@Table(name="SM_INTERFACE_LOG")
public class InterfaceLog extends IdEntity {

	/**
	 * 序列化
	 */
	private static final long serialVersionUID = 5401554547216466241L;
	/**
	 * 所有
	 */
	public static final int STATUS_ALL = 0;
	/**
	 * 初始
	 */
	public static final int STATUS_CREATE = 0;

	/**
	 * 执行中
	 */
	public static final int STATUS_RUNNING = 1;

	/**
	 * 执行成功
	 */
	public static final int STATUS_SUCCESS = 2;

	/**
	 * 异常结束
	 */
	public static final int STATUS_FAIL = 9;

	/**
	 * 名称
	 */
	@Column(nullable=false)
	private String name;

	/**
	 * 参数
	 */
	@Column(name = "params", length = HibernateUtils.COLOUMN_LENGTH_3000)
	private String params;

	/**
	 * 创建时间
	 */
	private Date createAt;

	/**
	 * 任务开始执行时间
	 */
	private Date startAt;

	/**
	 * 任务完成时间
	 */
	private Date finishAt;

	/**
	 * 执行状态（0表示未执行，1表示执行成功，2表示执行失败）
	 */
	@Column(name = "status", columnDefinition = "int default 0",nullable=false)
	private Integer status;

	/**
	 * 异常原因
	 */
    @Lob
	private String failureNote;

	/**
	 * 重试次数(建议最大重次3次)
	 */
	@Column(name = "retry_times", columnDefinition = "int default 0")
	private Integer retryTimes;

	/**
	 * 模块编码
	 */
	@Column(nullable = true)
	private String moduleCode;
	/**
	 * 消息ID
	 */
	@Column(nullable = true)
	private String messageId;
	/**
	 * 发起人
	 */
	@Column(nullable = true)
	private String initiatorName;
	/**
	 * 处理人
	 */
	@Column(nullable = true)
	private String userName;
	/**
	 * 标题
	 */
	@Column(nullable = true)
	private String title;
	/**
	 * 操作类型
	 */
	@Column(nullable = true)
	private String operationType;
	
	public static final int SIGN_ALL = 0;// 所有
	public static final int SIGN_DS = 1;// 待审
	public static final int SIGN_dy = 2;// 待阅
	public static final int SIGN_YD = 3;// 已读
	public static final int SIGN_YS = 4;// 已审
	public static final int SIGN_SC =5;// 删除
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getParams() {
		return params;
	}

	public void setParams(String params) {
		this.params = params;
	}

	public Date getCreateAt() {
		return createAt;
	}

	public void setCreateAt(Date createAt) {
		this.createAt = createAt;
	}

	public Date getStartAt() {
		return startAt;
	}

	public void setStartAt(Date startAt) {
		this.startAt = startAt;
	}

	public Date getFinishAt() {
		return finishAt;
	}

	public void setFinishAt(Date finishAt) {
		this.finishAt = finishAt;
	}

	public Integer getStatus() {
		return status;
	}

	public void setStatus(Integer status) {
		this.status = status;
	}

	public String getFailureNote() {
		return failureNote;
	}

	public void setFailureNote(String failureNote) {
		this.failureNote = failureNote;
	}

	public Integer getRetryTimes() {
		return retryTimes;
	}

	public void setRetryTimes(Integer retryTimes) {
		this.retryTimes = retryTimes;
	}

	public String getModuleCode() {
		return moduleCode;
	}

	public void setModuleCode(String moduleCode) {
		this.moduleCode = moduleCode;
	}

	public static Map<Integer, String> getStatusMap() {
		Map<Integer, String> statusMap = new HashMap<Integer, String>();
		statusMap.put(STATUS_ALL, "所有");
		statusMap.put(STATUS_SUCCESS,"成功");
		statusMap.put(STATUS_FAIL, "失败");
		return statusMap;
	}
	public static Map<Integer, String> getTypeMap() {
		Map<Integer, String> getTypeMap = new HashMap<Integer, String>();
		getTypeMap.put(SIGN_ALL,"所有");
		getTypeMap.put(SIGN_DS, "待审");
		getTypeMap.put(SIGN_YD, "待阅");
		getTypeMap.put(SIGN_YD, "已读");
		getTypeMap.put(SIGN_YS, "已审");
		getTypeMap.put(SIGN_SC, "删除");
		return getTypeMap;
	}

	public String getInitiatorName() {
		return initiatorName;
	}

	public void setInitiatorName(String initiatorName) {
		this.initiatorName = initiatorName;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getMessageId() {
		return messageId;
	}

	public void setMessageId(String messageId) {
		this.messageId = messageId;
	}

	public String getOperationType() {
		return operationType;
	}

	public void setOperationType(String operationType) {
		this.operationType = operationType;
	}
	

}
