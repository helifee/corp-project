package cn.helife.struts2.vo;

public class UserVO {
	private String name;
	private String password;
	private String configpw;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public String getConfigpw() {
		return configpw;
	}

	public void setConfigpw(String configpw) {
		this.configpw = configpw;
	}

}
