package remote.dict.service.impl;

import java.util.concurrent.ConcurrentHashMap;

import dictquery.QueryService;

public class RemoteDictQueryServiceImpl implements QueryService {
	
	private static final ConcurrentHashMap<String, String> dict =
		new ConcurrentHashMap<String, String>();
	
	static {
		dict.put("sky", "Ìì¿Õ");
		dict.put("computer", "¼ÆËã»ú");
	}

	@Override
	public String queryWorld(String word) {
		System.out.println("RemoteDictQueryServiceImpl.queryWorld called!");
		String result = dict.get(word);
		if(null == result) {
			result = "N/A";
		}
		return result;
	}

}
