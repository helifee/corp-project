public class ToDoButtonBean implements IResultBean {
    
    /**
     * @Fields serialVersionUID : TODO(目的和意义)
     */
    private static final long serialVersionUID = 8749126793759634078L;
    
    /**
     * 意见填写要求-不能填写
     */
    public static final int NOTE_TYPE_NOT = 0;
    
    /**
     * 意见填写要求-可以填写
     */
    public static final int NOTE_TYPE_NEED = 1;

    
    /**
     * 意见填写要求-必须填
     */
    public static final int NOTE_TYPE_MUST = 2;
    
    /**
     * 操作编码
     */
    private String code;
    
    /**
     * 操作名
     */
    private String name;
    /**
     * 排序
     */
    private Integer sort;
    
    /**
     * 默认意见
     */
    private String note;
    
    /**
     * 是否一定要填写意见
     */
    private Integer noteType;

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public Integer getSort() {
        return sort;
    }

    public void setSort(Integer sort) {
        this.sort = sort;
    }

    public String getNote() {
        return note;
    }

    public void setNote(String note) {
        this.note = note;
    }

    public Integer getNoteType() {
        return noteType;
    }

    public void setNoteType(Integer noteType) {
        this.noteType = noteType;
    }

    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }
}
