package com.xinleju.erp.flow.service.api.extend.dto;

import java.io.Serializable;

public class AuthDTO implements Serializable {

    private static final long serialVersionUID = -8811760455584168459L;

    /** 功能授权 **/
    private FuncAuthDTO funcAuth;
    
    /** 数据授权 **/
    private DataAuthDTO dataAuth;

    public FuncAuthDTO getFuncAuth() {
        return funcAuth;
    }

    public void setFuncAuth(FuncAuthDTO funcAuth) {
        this.funcAuth = funcAuth;
    }

    public DataAuthDTO getDataAuth() {
        return dataAuth;
    }

    public void setDataAuth(DataAuthDTO dataAuth) {
        this.dataAuth = dataAuth;
    }

}
