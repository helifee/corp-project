package com.xinleju.erp.flow.service.api.extend.dto;


import java.io.Serializable;
import java.util.ArrayList;
import java.util.List;


/**
 * 公司信息
 * @author chenhb
 *
 */
public class CompanyDTO implements Serializable{
	
	/**
	 * 序列化
	 */
	private static final long serialVersionUID = -3917853891410426086L;


	/**
	 * id
	 */
	private String id;
	
	
	/**
	 * 名称
	 */
	private String name;
	
	
	/**
	 * 编码
	 */
	private String code;
	
	
	/**
	 * 
	 * 子公司
	 */
	private List<CompanyDTO> childrenCompanyDTO=new ArrayList();
	
	
	/**
	 * 状态（0或空表示启用，1表示禁用）
	 */
	private Integer status;
	private String parentId;


	public String getParentId() {
		return parentId;
	}


	public void setParentId(String parentId) {
		this.parentId = parentId;
	}


	public String getId() {
		return id;
	}


	public void setId(String id) {
		this.id = id;
	}


	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public Integer getStatus() {
		return status;
	}


	public void setStatus(Integer status) {
		this.status = status;
	}


	public String getCode() {
		return code;
	}


	public void setCode(String code) {
		this.code = code;
	}


	public List<CompanyDTO> getChildrenCompanyDTO() {
		return childrenCompanyDTO;
	}


	public void setChildrenCompanyDTO(List<CompanyDTO> childrenCompanyDTO) {
		this.childrenCompanyDTO = childrenCompanyDTO;
	}
	
	
	
}
