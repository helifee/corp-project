package com.xinleju.erp.flow.flowutils.bean;

import java.io.Serializable;

/**
 * <p></p>
 * <p></p>
 * @author   Administrator
 * @version  $Id$
 * @since
 * @see
 */
public class BizDataDTO implements Serializable{

    /**
     * 意义，目的和功能，以及被用到的地方<br>
     */
    private static final long serialVersionUID = 953279114383956921L;
    
    /**
     * 数据类型-业务数据
     */
    public static final String DATA_TYPE_BIZ = "DATA_TYPE_BIZ";
    
    /**
     * 数据类型-流程数据
     */
    public static final String DATA_TYPE_FLOW = "DATA_TYPE_FLOW";
    
    
    /**
     * 数据列名称Key
     */
    private String dataName;
    
    
    /**
     * 显示名称
     * 
     */
    private String displayDataName;
    /**
     * 数据类型参考DATA_TYPE_FLOW
     */
    private String dataType;
    
    /**
     * 数据值（只支持简单的数据类型如：String、Integer、Long等）
     */
    private Object dataValue;
    
    

    
    /**
     * 数据版本
     */
    private String dataVersion;

	public String getDataName() {
		return dataName;
	}

	public void setDataName(String dataName) {
		this.dataName = dataName;
	}

	public String getDataType() {
		return dataType;
	}

	public void setDataType(String dataType) {
		this.dataType = dataType;
	}

	public Object getDataValue() {
		return dataValue;
	}

	public void setDataValue(Object dataValue) {
		this.dataValue = dataValue;
	}

	public String getDataVersion() {
		return dataVersion;
	}

	public void setDataVersion(String dataVersion) {
		this.dataVersion = dataVersion;
	}

	public String getDisplayDataName() {
		return displayDataName;
	}

	public void setDisplayDataName(String displayDataName) {
		this.displayDataName = displayDataName;
	}
	
	

}
