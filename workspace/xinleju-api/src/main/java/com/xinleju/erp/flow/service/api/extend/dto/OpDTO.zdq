public class OpDTO implements Serializable{

    private static final long serialVersionUID = 702582886663731739L;
    
    /**
     * 名称
     */
    private String name;
    /**
     * 编码
     */
    private String code;
    
    /**
     *  按钮属于那个模块下面
     */
    private String funcModuleId;
    

    // 按钮的url 防止直接输入按钮的url
    private String buttonUrl;
    
    
    
    public String getButtonUrl() {
		return buttonUrl;
	}
    
	public void setButtonUrl(String buttonUrl) {
		this.buttonUrl = buttonUrl;
	}
	
	public String getName() {
        return name;
    }
    public void setName(String name) {
        this.name = name;
    }
    public String getCode() {
        return code;
    }
    public void setCode(String code) {
        this.code = code;
    }
	public String getFuncModuleId() {
		return funcModuleId;
	}
	public void setFuncModuleId(String funcModuleId) {
		this.funcModuleId = funcModuleId;
	}
    
    
}
