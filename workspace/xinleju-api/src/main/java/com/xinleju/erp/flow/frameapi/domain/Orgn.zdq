package com.xinleju.erp.flow.frameapi.domain;

import java.io.Serializable;

import com.xinleju.erp.flow.utils.ReflectionUtils;

@SuppressWarnings("serial")
public class Orgn implements Serializable{
	
	private String orgnName;
	private String orgnCode;
	private String orgnId;
	private Integer isLeaf;
	private Integer isUserEmpty;
	public String getOrgnName() {
		return orgnName;
	}
	public void setOrgnName(String orgnName) {
		this.orgnName = orgnName;
	}
	public String getOrgnCode() {
		return orgnCode;
	}
	public void setOrgnCode(String orgnCode) {
		this.orgnCode = orgnCode;
	}
	public String getOrgnId() {
		return orgnId;
	}
	public void setOrgnId(String orgnId) {
		this.orgnId = orgnId;
	}
	public Integer getIsLeaf() {
		return isLeaf;
	}
	public void setIsLeaf(Integer isLeaf) {
		this.isLeaf = isLeaf;
	}
	public Integer getIsUserEmpty() {
		return isUserEmpty;
	}
	public void setIsUserEmpty(Integer isUserEmpty) {
		this.isUserEmpty = isUserEmpty;
	}
	public boolean equals(Object object) {
		if (null == object)
			return false;
		if (!(object.getClass().getName().equals(this.getClass().getName())))
			return false;
		else {
			String object_id = (String) ReflectionUtils.getFieldValue(object,
					"orgnId");
			if (null == getOrgnId() || null == object_id)
				return false;
			else
				return (this.getOrgnId().equals(object_id));
		}
	}
}
