package com.xinleju.erp.sm.extend.dto;

import java.io.Serializable;
import java.util.ArrayList;
import java.util.Date;
import java.util.List;

/**
 * 同步财务接口数据查询参数对象
 * 
 * @author walden
 */
public class FinaQueryParams implements Serializable {

	private static final long serialVersionUID = 1394822800219217241L;

	public static final int QUERY_ALL = 0;
	/** 当前页 从1开始 如果是0 不做分页查询，查询所有 */
	private Integer currentPage;
	/** 页面长度 */
	private Integer pageSize = 50;
	/** 这个值取值为 FINADATATYPE_ */
	private Integer finaDataType = 0;
	/********** 业务对象查询参数 *********/
	/** 公司id */
	private String corpId;
	/** 项目id */
	private String projectId;

	/********** 业务表单参数 begin *********/
	/** 开票日期起 */
	private String billDateBegin;
	/** 开票日期止 */
	private String billDateEnd;

	/** 审核日期起 */
	private String auditDateBegin;
	/** 审核日期起 */
	private String auditDateEnd;
	/** 所属项目 */
	private String project;
	/** 开票人 */
	private String billMan;
	/** 审核人 */
	private String auditMan;
	/** 票据类型 */
	private String billType;
	/** 票据编号 */
	private String billNumber;
	/** 入账银行 */
	private String billBank;
	/** 是否生成凭证 */
	private boolean createVouch;
	/** 凭证编号 */
	private String voucherNumber;
	/** 所属系统 */
	private Integer system;
	/** 生成凭证状态 1：未生成凭证   2：生成凭证失败  3：全部 */
	private Integer createVouchStatus;
	/** 项目id列表 */
	private List<String> projectIdList = new ArrayList<String>();
	/** 付款单位列表 */
	private List<String> payUnitIdList = new ArrayList<String>();
	
	private String cretateStatus;
	
	private String flag;

	/********** 业务表单参数 end *********/

	public FinaQueryParams() {
		this.currentPage = QUERY_ALL;
	}

	public FinaQueryParams(int currentPage) {
		this.currentPage = currentPage;
	}

	public FinaQueryParams(int currentPage, int pageSize) {
		this.currentPage = currentPage;
		this.pageSize = pageSize;
	}

	public Integer getCurrentPage() {
		return currentPage;
	}

	public void setCurrentPage(Integer currentPage) {
		this.currentPage = currentPage;
	}

	public Integer getPageSize() {
		return pageSize;
	}

	public void setPageSize(Integer pageSize) {
		this.pageSize = pageSize;
	}

	/**
	 * 是否分页查询
	 * 
	 * @return false/true
	 */
	public boolean isQueryPage() {
		return this.currentPage > QUERY_ALL;
	}

	

	public String getProject() {
		return project;
	}

	public void setProject(String project) {
		this.project = project;
	}

	public String getBillMan() {
		return billMan;
	}

	public void setBillMan(String billMan) {
		this.billMan = billMan;
	}

	public String getBillType() {
		return billType;
	}

	public void setBillType(String billType) {
		this.billType = billType;
	}

	public String getBillNumber() {
		return billNumber;
	}

	public void setBillNumber(String billNumber) {
		this.billNumber = billNumber;
	}

	public String getBillBank() {
		return billBank;
	}

	public void setBillBank(String billBank) {
		this.billBank = billBank;
	}

	public boolean isCreateVouch() {
		return createVouch;
	}

	public void setCreateVouch(boolean createVouch) {
		this.createVouch = createVouch;
	}

	public String getVoucherNumber() {
		return voucherNumber;
	}

	public void setVoucherNumber(String voucherNumber) {
		this.voucherNumber = voucherNumber;
	}

	public static long getSerialversionuid() {
		return serialVersionUID;
	}

	public String getCorpId() {
		return corpId;
	}

	public void setCorpId(String corpId) {
		this.corpId = corpId;
	}

	public String getProjectId() {
		return projectId;
	}

	public void setProjectId(String projectId) {
		this.projectId = projectId;
	}

	public Integer getFinaDataType() {
		return finaDataType;
	}

	public void setFinaDataType(Integer finaDataType) {
		this.finaDataType = finaDataType;
	}

	public Integer getSystem() {
		return system;
	}

	public void setSystem(Integer system) {
		this.system = system;
	}

	public String getBillDateBegin() {
		return billDateBegin;
	}

	public void setBillDateBegin(String billDateBegin) {
		this.billDateBegin = billDateBegin;
	}

	public String getBillDateEnd() {
		return billDateEnd;
	}

	public void setBillDateEnd(String billDateEnd) {
		this.billDateEnd = billDateEnd;
	}

	public String getAuditDateBegin() {
		return auditDateBegin;
	}

	public void setAuditDateBegin(String auditDateBegin) {
		this.auditDateBegin = auditDateBegin;
	}

	public String getAuditDateEnd() {
		return auditDateEnd;
	}

	public void setAuditDateEnd(String auditDateEnd) {
		this.auditDateEnd = auditDateEnd;
	}

	public String getAuditMan() {
		return auditMan;
	}

	public void setAuditMan(String auditMan) {
		this.auditMan = auditMan;
	}

	public List<String> getProjectIdList() {
		return projectIdList;
	}

	public void setProjectIdList(List<String> projectIdList) {
		this.projectIdList = projectIdList;
	}

	public List<String> getPayUnitIdList() {
		return payUnitIdList;
	}

	public void setPayUnitIdList(List<String> payUnitIdList) {
		this.payUnitIdList = payUnitIdList;
	}

	public Integer getCreateVouchStatus() {
		return createVouchStatus;
	}

	public void setCreateVouchStatus(Integer createVouchStatus) {
		this.createVouchStatus = createVouchStatus;
	}

	public String getCretateStatus() {
		return cretateStatus;
	}

	public void setCretateStatus(String cretateStatus) {
		this.cretateStatus = cretateStatus;
	}

	public String getFlag() {
		return flag;
	}

	public void setFlag(String flag) {
		this.flag = flag;
	}

	
	

}
