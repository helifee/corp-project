package com.xinleju.erp.sm.cache.api;

import com.xinleju.erp.flow.flowutils.bean.FlowResult;
import com.xinleju.erp.flow.flowutils.bean.PageBean;
import com.xinleju.erp.sm.extend.dto.FinaData;
import com.xinleju.erp.sm.extend.dto.FinaQueryParams;

/**
 * 同步财务接口需要的数据
 * 主数据使用
 * @author walden
 * 
 */
public interface SyncFinaData {
	/**
	 * 通过FinaQueryParams中的参数获得<b>财务接口</b>所需要的业务对象或者业务表单数据数据
	 * <p>
	 * 如果params中的currentPage==0不做分页查询，返回对象中只有items是有效数据；
	 * <p>
	 * 如果params中的currentPage>0做分页查询，需要返回PageBean做所有有效数据字段；
	 * <p>
	 * 目前业务对象 只需要实现，当currentPage查询所有数据项
	 * 
	 * @param params
	 * @return
	 */
	public FlowResult<PageBean<FinaData>> getFinaData(FinaQueryParams params);
}