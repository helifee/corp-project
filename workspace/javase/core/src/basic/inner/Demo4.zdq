package basic.inner;

class Outer4 {
	private static String name = "zhangsan!";
	
	public void fun() {
		class Inner {
			public void print() {
				System.out.println("Name = " + name);
			}
		}
		
		new Inner().print();
	}
}

public class Demo4 {

	public static void main(String[] args) {
		new Outer4().fun();
	}

}
