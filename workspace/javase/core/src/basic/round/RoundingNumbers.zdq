package basic.round;

public class RoundingNumbers {

	public static void main(String[] args) {
		double a = 0.7, b = 0.4;
		float c = 0.7f, d = 0.4f;
		System.out.println(Math.round(a));
		System.out.println(Math.round(b));
		System.out.println(Math.round(c));
		System.out.println(Math.round(d));
	}

}
