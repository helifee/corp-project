package io;

import java.io.File;

public class ListFile2 {

	public static void main(String[] args) {
		File file = new File("D:" + File.separator + "software");
		
		File[] files = file.listFiles();
		for(File p : files) {
			
			System.out.print(p.getParent() + "-->");
			System.out.println(p.getPath());
		}
	}

}
