package class_collection;

import java.util.Arrays;

public class ArraysDemo {
	public static void main(String[] args) {
		int a1[] = { 1, 2, 3, 4, 5, 6 };
		int a2[] = { 6, 5, 4, 3, 2, 1 };
		Arrays.sort(a2);
		System.out.println(Arrays.equals(a1, a2));
		
		Arrays.fill(a2, 3);
		System.out.println(Arrays.toString(a2));
	}
}
