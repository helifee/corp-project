package tips;

public class RegularExpression {
	public static void main(String[] arga) {
		String text = "abcdebcadxbc";
		
		String[] tokens = text.split(".bc");
		for(String token : tokens) {
			System.out.print(token + " ");
		}
		System.out.println();
		
		tokens = text.split("..cd");
		for(String token : tokens) {
			System.out.print(token + " ");
		}
		System.out.println();
	}
}